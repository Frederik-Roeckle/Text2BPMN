```xml
<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="P2P_Process_Definition"
    targetNamespace="http://example.com/bpmn/p2p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_p2p">
    <semantic:participant id="pool_purchasing" name="Purchasing Department" processRef="process_purchasing"/>
    <semantic:participant id="pool_vendor" name="Vendor" processRef="process_vendor"/>
  </semantic:collaboration>

  <semantic:error id="PR_DISCREPANCY_ERROR" name="PR Discrepancy Error" errorCode="E001"/>
  <semantic:error id="BUDGET_OVERDRAFT_ERROR" name="Budget Overdraft Error" errorCode="E002"/>
  <semantic:error id="DATA_BREACH_ERROR" name="Data Breach Error" errorCode="E003"/>
  <semantic:error id="INVOICE_DISCREPANCY_ERROR" name="Invoice Discrepancy Error" errorCode="E004"/>

  <semantic:message id="MESSAGE_PO_FOR_REVIEW" name="PO for Review"/>
  <semantic:message id="MESSAGE_VENDOR_PO_DECISION" name="Vendor PO Decision"/>
  <semantic:message id="MESSAGE_INVOICE" name="Invoice"/>
  <semantic:message id="MESSAGE_WRONG_INVOICE" name="Wrong Invoice for Correction"/>
  <semantic:message id="MESSAGE_CORRECTED_INVOICE" name="Corrected Invoice"/>

  <semantic:process id="process_purchasing" isExecutable="true">
    <semantic:laneSet id="laneSet_purchasing">
      <semantic:lane id="lane_requestor" name="Requestor">
        <semantic:flowNodeRef>start_event_req_1</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_1_define_needs</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_2_create_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_1_initial_pr_approval</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_3_issue_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>error_be_pr_discrepancy</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_err_pr_disc</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_4_send_pr_to_hod</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_5_create_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_6_send_po_to_poa</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_7_dispatch_po_to_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>event_mc_req_vendor_po_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_4_vendor_po_outcome</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_8_search_new_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_9_receive_goods_services</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_5_goods_or_service</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_10_issue_gr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_11_issue_ses</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_6_merge_gr_ses</semantic:flowNodeRef>
        <semantic:flowNodeRef>event_mc_req_invoice_from_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_12_process_invoice_3way_match</semantic:flowNodeRef>
        <semantic:flowNodeRef>error_be_invoice_discrepancy</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_err_inv_disc</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_7_invoice_correct</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_13_return_invoice_to_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>event_mc_req_corrected_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_8_merge_invoice_paths</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_14_post_invoice_sap</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_15_send_invoice_to_ap</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_hod" name="Heads of Departments">
        <semantic:flowNodeRef>task_hod_1_review_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>error_be_budget_overdraft</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_err_bud_over</semantic:flowNodeRef>
        <semantic:flowNodeRef>error_be_data_breach</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_err_data_br</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_hod_2_document_submit_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_hod_1_hod_pr_decision</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_poa" name="PO Approvers">
        <semantic:flowNodeRef>task_poa_1_review_po_specs</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_poa_2_document_submit_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_poa_1_poa_po_decision</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_ap" name="Accounts Payable Team">
        <semantic:flowNodeRef>task_ap_1_receive_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_2_decide_payment_method</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_ap_1_payment_method</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_3_perform_payment</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_event_ap_1_process_completed</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <semantic:startEvent id="start_event_req_1" name="Start P2P Process">
      <semantic:outgoing>sf_req_1</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_req_1_define_needs" name="Determine and define business requirements &amp; needs">
      <semantic:incoming>sf_req_1</semantic:incoming>
      <semantic:incoming>sf_gw_req1_not_approved</semantic:incoming>
      <semantic:incoming>sf_gw_hod1_declined</semantic:incoming>
      <semantic:incoming>sf_gw_hod1_rejected_incomplete</semantic:incoming>
      <semantic:incoming>sf_gw_poa1_rejected</semantic:incoming>
      <semantic:outgoing>sf_req_2</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_2_create_pr" name="Request to issue Purchase Requisition (PR)">
      <semantic:incoming>sf_req_2</semantic:incoming>
      <semantic:outgoing>sf_req_3</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_1_initial_pr_approval" name="Initial PR Approved?">
      <semantic:incoming>sf_req_3</semantic:incoming>
      <semantic:outgoing>sf_gw_req1_approved</semantic:outgoing>
      <semantic:outgoing>sf_gw_req1_not_approved</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_3_issue_pr" name="Issue Purchase Requisition (PR)">
      <semantic:incoming>sf_gw_req1_approved</semantic:incoming>
      <semantic:outgoing>sf_req_4</semantic:outgoing>
    </semantic:task>
    <semantic:boundaryEvent id="error_be_pr_discrepancy" name="PR Discrepancy Occurred" attachedToRef="task_req_3_issue_pr" cancelActivity="true">
      <semantic:outgoing>sf_err_pr_disc</semantic:outgoing>
      <semantic:errorEventDefinition errorRef="PR_DISCREPANCY_ERROR"/>
    </semantic:boundaryEvent>
    <semantic:endEvent id="end_err_pr_disc" name="PR Discrepancy Processed">
        <semantic:incoming>sf_err_pr_disc</semantic:incoming>
    </semantic:endEvent>
    <semantic:task id="task_req_4_send_pr_to_hod" name="Send issued PR to Heads of Departments for approval">
      <semantic:incoming>sf_req_4</semantic:incoming>
      <semantic:outgoing>sf_req_hod_1</semantic:outgoing>
    </semantic:task>

    <semantic:task id="task_hod_1_review_pr" name="Review PR (needs, budget, form)">
      <semantic:incoming>sf_req_hod_1</semantic:incoming>
      <semantic:outgoing>sf_hod_2</semantic:outgoing>
    </semantic:task>
    <semantic:boundaryEvent id="error_be_budget_overdraft" name="Budget Overdraft Occurred" attachedToRef="task_hod_1_review_pr" cancelActivity="true">
      <semantic:outgoing>sf_err_bud_over</semantic:outgoing>
      <semantic:errorEventDefinition errorRef="BUDGET_OVERDRAFT_ERROR"/>
    </semantic:boundaryEvent>
    <semantic:endEvent id="end_err_bud_over" name="Budget Overdraft Processed">
        <semantic:incoming>sf_err_bud_over</semantic:incoming>
    </semantic:endEvent>
    <semantic:boundaryEvent id="error_be_data_breach" name="Data Breach Occurred" attachedToRef="task_hod_1_review_pr" cancelActivity="true">
      <semantic:outgoing>sf_err_data_br</semantic:outgoing>
      <semantic:errorEventDefinition errorRef="DATA_BREACH_ERROR"/>
    </semantic:boundaryEvent>
    <semantic:endEvent id="end_err_data_br" name="Data Breach Processed">
        <semantic:incoming>sf_err_data_br</semantic:incoming>
    </semantic:endEvent>
    <semantic:task id="task_hod_2_document_submit_decision" name="Document/submit HoD final decision to requestor">
      <semantic:incoming>sf_hod_2</semantic:incoming>
      <semantic:outgoing>sf_hod_3</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_hod_1_hod_pr_decision" name="HoD PR Decision">
      <semantic:incoming>sf_hod_3</semantic:incoming>
      <semantic:outgoing>sf_gw_hod1_approved</semantic:outgoing>
      <semantic:outgoing>sf_gw_hod1_declined</semantic:outgoing>
      <semantic:outgoing>sf_gw_hod1_rejected_incomplete</semantic:outgoing>
    </semantic:exclusiveGateway>

    <semantic:task id="task_req_5_create_po" name="Create Purchase Order (PO)">
      <semantic:incoming>sf_gw_hod1_approved</semantic:incoming>
      <semantic:outgoing>sf_req_5</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_6_send_po_to_poa" name="Send PO to PO approvers for approval">
      <semantic:incoming>sf_req_5</semantic:incoming>
      <semantic:outgoing>sf_req_poa_1</semantic:outgoing>
    </semantic:task>

    <semantic:task id="task_poa_1_review_po_specs" name="Review PO specifications">
      <semantic:incoming>sf_req_poa_1</semantic:incoming>
      <semantic:outgoing>sf_poa_2</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_poa_2_document_submit_decision" name="Document/submit POA final decision to requestor">
      <semantic:incoming>sf_poa_2</semantic:incoming>
      <semantic:outgoing>sf_poa_3</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_poa_1_poa_po_decision" name="POA PO Decision">
      <semantic:incoming>sf_poa_3</semantic:incoming>
      <semantic:outgoing>sf_gw_poa1_approved</semantic:outgoing>
      <semantic:outgoing>sf_gw_poa1_rejected</semantic:outgoing>
    </semantic:exclusiveGateway>

    <semantic:task id="task_req_7_dispatch_po_to_vendor" name="Dispatch approved PO to vendor">
      <semantic:incoming>sf_gw_poa1_approved</semantic:incoming>
      <semantic:incoming>sf_req_search_new_vendor_done</semantic:incoming>
      <semantic:outgoing>sf_req_wait_vendor_decision</semantic:outgoing> <!-- Connects to event_mc_req_vendor_po_decision -->
      <!-- Message flow to vendor is separate -->
    </semantic:task>
    
    <semantic:intermediateCatchEvent id="event_mc_req_vendor_po_decision" name="Receive Vendor PO Decision">
      <semantic:incoming>sf_req_wait_vendor_decision</semantic:incoming>
      <!-- Incoming message flow from vendor -->
      <semantic:outgoing>sf_req_vendor_decision_received</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="MESSAGE_VENDOR_PO_DECISION"/>
    </semantic:intermediateCatchEvent>
    <semantic:exclusiveGateway id="gw_req_4_vendor_po_outcome" name="Vendor PO Outcome?">
      <semantic:incoming>sf_req_vendor_decision_received</semantic:incoming>
      <semantic:outgoing>sf_gw_req4_rejected</semantic:outgoing>
      <semantic:outgoing>sf_gw_req4_approved</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_8_search_new_vendor" name="Search for a new vendor">
      <semantic:incoming>sf_gw_req4_rejected</semantic:incoming>
      <semantic:outgoing>sf_req_search_new_vendor_done</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_9_receive_goods_services" name="Receive agreed goods/services">
      <semantic:incoming>sf_gw_req4_approved</semantic:incoming>
      <semantic:outgoing>sf_req_10</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_5_goods_or_service" name="Goods or Service Received?">
      <semantic:incoming>sf_req_10</semantic:incoming>
      <semantic:outgoing>sf_gw_req5_goods</semantic:outgoing>
      <semantic:outgoing>sf_gw_req5_service</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_10_issue_gr" name="Issue Goods Receipt (GR)">
      <semantic:incoming>sf_gw_req5_goods</semantic:incoming>
      <semantic:outgoing>sf_req_11_gr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_11_issue_ses" name="Issue Service Entry Sheet (SES)">
      <semantic:incoming>sf_gw_req5_service</semantic:incoming>
      <semantic:outgoing>sf_req_11_ses</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_6_merge_gr_ses" name="Merge GR/SES Path">
      <semantic:incoming>sf_req_11_gr</semantic:incoming>
      <semantic:incoming>sf_req_11_ses</semantic:incoming>
      <semantic:outgoing>sf_req_wait_invoice</semantic:outgoing>
    </semantic:exclusiveGateway>
    
    <semantic:intermediateCatchEvent id="event_mc_req_invoice_from_vendor" name="Receive Invoice from Vendor">
      <semantic:incoming>sf_req_wait_invoice</semantic:incoming>
      <!-- Incoming message flow from vendor -->
      <semantic:outgoing>sf_req_invoice_received</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="MESSAGE_INVOICE"/>
    </semantic:intermediateCatchEvent>
    <semantic:task id="task_req_12_process_invoice_3way_match" name="Perform three-way match (Invoice, PO, GR/SES)">
      <semantic:incoming>sf_req_invoice_received</semantic:incoming>
      <semantic:outgoing>sf_req_13</semantic:outgoing>
    </semantic:task>
    <semantic:boundaryEvent id="error_be_invoice_discrepancy" name="Invoice Discrepancy Occurred" attachedToRef="task_req_12_process_invoice_3way_match" cancelActivity="true">
      <semantic:outgoing>sf_err_inv_disc</semantic:outgoing>
      <semantic:errorEventDefinition errorRef="INVOICE_DISCREPANCY_ERROR"/>
    </semantic:boundaryEvent>
    <semantic:endEvent id="end_err_inv_disc" name="Invoice Discrepancy Processed">
        <semantic:incoming>sf_err_inv_disc</semantic:incoming>
    </semantic:endEvent>
    <semantic:exclusiveGateway id="gw_req_7_invoice_correct" name="Invoice Correct?">
      <semantic:incoming>sf_req_13</semantic:incoming>
      <semantic:outgoing>sf_gw_req7_wrong</semantic:outgoing>
      <semantic:outgoing>sf_gw_req7_correct</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_13_return_invoice_to_vendor" name="Return invoice to vendor for correction">
      <semantic:incoming>sf_gw_req7_wrong</semantic:incoming>
      <!-- Outgoing message flow to vendor -->
      <semantic:outgoing>sf_req_wait_corrected_invoice</semantic:outgoing>
    </semantic:task>

    <semantic:intermediateCatchEvent id="event_mc_req_corrected_invoice" name="Receive corrected invoice from vendor">
      <semantic:incoming>sf_req_wait_corrected_invoice</semantic:incoming>
      <!-- Incoming message flow from vendor -->
      <semantic:outgoing>sf_req_corrected_invoice_received</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="MESSAGE_CORRECTED_INVOICE"/>
    </semantic:intermediateCatchEvent>
    <semantic:exclusiveGateway id="gw_req_8_merge_invoice_paths" name="Merge Invoice Paths">
      <semantic:incoming>sf_gw_req7_correct</semantic:incoming>
      <semantic:incoming>sf_req_corrected_invoice_received</semantic:incoming>
      <semantic:outgoing>sf_req_14</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_14_post_invoice_sap" name="Post invoice in SAP">
      <semantic:incoming>sf_req_14</semantic:incoming>
      <semantic:outgoing>sf_req_15</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_15_send_invoice_to_ap" name="Send invoice to Accounts Payable (AP) team">
      <semantic:incoming>sf_req_15</semantic:incoming>
      <semantic:outgoing>sf_req_ap_1</semantic:outgoing>
    </semantic:task>

    <semantic:task id="task_ap_1_receive_invoice" name="Receive invoice (AP)">
      <semantic:incoming>sf_req_ap_1</semantic:incoming>
      <semantic:outgoing>sf_ap_2</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ap_2_decide_payment_method" name="Decide on payment method">
      <semantic:incoming>sf_ap_2</semantic:incoming>
      <semantic:outgoing>sf_ap_3</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_ap_1_payment_method" name="Payment Method Decision">
      <semantic:incoming>sf_ap_3</semantic:incoming>
      <semantic:outgoing>sf_gw_ap1_full</semantic:outgoing>
      <semantic:outgoing>sf_gw_ap1_installments</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_ap_3_perform_payment" name="Perform payment to vendor">
      <semantic:incoming>sf_gw_ap1_full</semantic:incoming>
      <semantic:incoming>sf_gw_ap1_installments</semantic:incoming>
      <semantic:outgoing>sf_ap_4</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="end_event_ap_1_process_completed" name="P2P Process Successfully Completed">
      <semantic:incoming>sf_ap_4</semantic:incoming>
    </semantic:endEvent>

    <!-- Sequence Flows for Purchasing Department -->
    <semantic:sequenceFlow id="sf_req_1" sourceRef="start_event_req_1" targetRef="task_req_1_define_needs"/>
    <semantic:sequenceFlow id="sf_req_2" sourceRef="task_req_1_define_needs" targetRef="task_req_2_create_pr"/>
    <semantic:sequenceFlow id="sf_req_3" sourceRef="task_req_2_create_pr" targetRef="gw_req_1_initial_pr_approval"/>
    <semantic:sequenceFlow id="sf_gw_req1_approved" name="Approved" sourceRef="gw_req_1_initial_pr_approval" targetRef="task_req_3_issue_pr"/>
    <semantic:sequenceFlow id="sf_gw_req1_not_approved" name="Not Approved" sourceRef="gw_req_1_initial_pr_approval" targetRef="task_req_1_define_needs"/>
    <semantic:sequenceFlow id="sf_req_4" sourceRef="task_req_3_issue_pr" targetRef="task_req_4_send_pr_to_hod"/>
    <semantic:sequenceFlow id="sf_err_pr_disc" sourceRef="error_be_pr_discrepancy" targetRef="end_err_pr_disc"/>
    
    <semantic:sequenceFlow id="sf_req_hod_1" sourceRef="task_req_4_send_pr_to_hod" targetRef="task_hod_1_review_pr"/>
    <semantic:sequenceFlow id="sf_hod_2" sourceRef="task_hod_1_review_pr" targetRef="task_hod_2_document_submit_decision"/>
    <semantic:sequenceFlow id="sf_err_bud_over" sourceRef="error_be_budget_overdraft" targetRef="end_err_bud_over"/>
    <semantic:sequenceFlow id="sf_err_data_br" sourceRef="error_be_data_breach" targetRef="end_err_data_br"/>
    <semantic:sequenceFlow id="sf_hod_3" sourceRef="task_hod_2_document_submit_decision" targetRef="gw_hod_1_hod_pr_decision"/>
    <semantic:sequenceFlow id="sf_gw_hod1_approved" name="PR Approved" sourceRef="gw_hod_1_hod_pr_decision" targetRef="task_req_5_create_po"/>
    <semantic:sequenceFlow id="sf_gw_hod1_declined" name="PR Declined" sourceRef="gw_hod_1_hod_pr_decision" targetRef="task_req_1_define_needs"/>
    <semantic:sequenceFlow id="sf_gw_hod1_rejected_incomplete" name="PR Rejected Incomplete" sourceRef="gw_hod_1_hod_pr_decision" targetRef="task_req_1_define_needs"/>

    <semantic:sequenceFlow id="sf_req_5" sourceRef="task_req_5_create_po" targetRef="task_req_6_send_po_to_poa"/>
    <semantic:sequenceFlow id="sf_req_poa_1" sourceRef="task_req_6_send_po_to_poa" targetRef="task_poa_1_review_po_specs"/>
    <semantic:sequenceFlow id="sf_poa_2" sourceRef="task_poa_1_review_po_specs" targetRef="task_poa_2_document_submit_decision"/>
    <semantic:sequenceFlow id="sf_poa_3" sourceRef="task_poa_2_document_submit_decision" targetRef="gw_poa_1_poa_po_decision"/>
    <semantic:sequenceFlow id="sf_gw_poa1_approved" name="PO Approved" sourceRef="gw_poa_1_poa_po_decision" targetRef="task_req_7_dispatch_po_to_vendor"/>
    <semantic:sequenceFlow id="sf_gw_poa1_rejected" name="PO Rejected" sourceRef="gw_poa_1_poa_po_decision" targetRef="task_req_1_define_needs"/>

    <semantic:sequenceFlow id="sf_req_wait_vendor_decision" name="Wait for Vendor Decision" sourceRef="task_req_7_dispatch_po_to_vendor" targetRef="event_mc_req_vendor_po_decision"/>
    <semantic:sequenceFlow id="sf_req_vendor_decision_received" sourceRef="event_mc_req_vendor_po_decision" targetRef="gw_req_4_vendor_po_outcome"/>
    <semantic:sequenceFlow id="sf_gw_req4_rejected" name="Vendor Rejected PO" sourceRef="gw_req_4_vendor_po_outcome" targetRef="task_req_8_search_new_vendor"/>
    <semantic:sequenceFlow id="sf_req_search_new_vendor_done" name="New vendor found, dispatch same PO" sourceRef="task_req_8_search_new_vendor" targetRef="task_req_7_dispatch_po_to_vendor"/>
    <semantic:sequenceFlow id="sf_gw_req4_approved" name="Vendor Approved/Negotiated PO" sourceRef="gw_req_4_vendor_po_outcome" targetRef="task_req_9_receive_goods_services"/>
    
    <semantic:sequenceFlow id="sf_req_10" sourceRef="task_req_9_receive_goods_services" targetRef="gw_req_5_goods_or_service"/>
    <semantic:sequenceFlow id="sf_gw_req5_goods" name="Goods Received" sourceRef="gw_req_5_goods_or_service" targetRef="task_req_10_issue_gr"/>
    <semantic:sequenceFlow id="sf_gw_req5_service" name="Service Received" sourceRef="gw_req_5_goods_or_service" targetRef="task_req_11_issue_ses"/>
    <semantic:sequenceFlow id="sf_req_11_gr" sourceRef="task_req_10_issue_gr" targetRef="gw_req_6_merge_gr_ses"/>
