<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="InternalRefurbishmentProcessDefs"
    targetNamespace="http://example.com/bpmn/internal-refurbishment"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_internal_refurbishment">
    <semantic:participant id="pool_main_process" name="Internal Refurbishment Process" processRef="internalRefurbishmentProcess"/>
    <semantic:participant id="pool_subprocess_planning" name="Planning &amp; Scheduling Process" processRef="planningSchedulingProcess"/>
    <semantic:messageFlow id="message_flow_to_manager" sourceRef="task_save_process_mail" targetRef="intermediate_catch_request_received"/>
    <semantic:messageFlow id="message_flow_decision_approved_to_rm" sourceRef="task_mgr_save_entries_approved" targetRef="intermediate_catch_decision_received"/>
    <semantic:messageFlow id="message_flow_decision_rejected_to_rm" sourceRef="task_mgr_save_entries_rejected" targetRef="intermediate_catch_decision_received"/>
  </semantic:collaboration>

  <semantic:process id="internalRefurbishmentProcess" isExecutable="true">
    <semantic:laneSet id="laneSet_main">
      <semantic:lane id="lane_responsible_maintenance" name="Responsible Maintenance">
        <semantic:flowNodeRef>start_event_request_received</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_create_pm_notification</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_choose_notification_type</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_define_notification_characteristics</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_confirm_entries_warranty</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_define_reference_object_tab</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_define_malfunction_data_tab</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_breakdown_case</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_mark_breakdown_checkbox</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_not_mark_breakdown_checkbox</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_merge_breakdown</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_save_pm_notification</semantic:flowNodeRef>
        <semantic:flowNodeRef>pg_split_send_details</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_define_receiver_details</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_define_notice_text</semantic:flowNodeRef>
        <semantic:flowNodeRef>pg_merge_send_details</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_confirm_send_entries</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_save_process_mail</semantic:flowNodeRef>
        <semantic:flowNodeRef>intermediate_catch_decision_received</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_request_approved_check</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_manage_work_order</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_create_internal_refurb_order</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_transfer_info_to_wo</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_define_order_type_mc04</semantic:flowNodeRef>
        <semantic:flowNodeRef>pg_split_plants_valuation</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_define_refurb_plants</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_define_valuation_type</semantic:flowNodeRef>
        <semantic:flowNodeRef>pg_merge_plants_valuation</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_define_operations_main</semantic:flowNodeRef>
        <semantic:flowNodeRef>pg_split_internal_external_ops</semantic:flowNodeRef>
        <semantic:flowNodeRef>call_activity_planning_scheduling</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_maintain_external_ops</semantic:flowNodeRef>
        <semantic:flowNodeRef>pg_merge_internal_external_ops</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_define_estimated_costs</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_maintain_involved_partners</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_define_allocated_objects</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_define_additional_data</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_define_location_data</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_save_work_order</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_assign_wo_to_wbs</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_event_request_handled</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_event_rm_rejected</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_maintenance_manager" name="Maintenance Manager">
        <semantic:flowNodeRef>intermediate_catch_request_received</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_evaluate_request</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_document_submit_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_decision_approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_mgr_change_pm_notification_approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_mgr_set_status_approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_mgr_confirm_entries_approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_mgr_select_put_in_process</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_mgr_save_entries_approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_mgr_change_pm_notification_rejected</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_mgr_set_status_cancelled</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_mgr_save_entries_rejected</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_event_request_rejected</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <!-- Responsible Maintenance Lane Elements -->
    <semantic:startEvent id="start_event_request_received" name="Internal Refurbishment Request Received">
      <semantic:outgoing>flow_to_create_pm</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_create_pm_notification" name="Create PM Notification in SAP">
      <semantic:incoming>flow_to_create_pm</semantic:incoming>
      <semantic:outgoing>flow_to_choose_type</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_choose_notification_type" name="Choose Notification Type P1">
      <semantic:incoming>flow_to_choose_type</semantic:incoming>
      <semantic:outgoing>flow_to_define_chars</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_define_notification_characteristics" name="Define Notification Characteristics Tab">
      <semantic:incoming>flow_to_define_chars</semantic:incoming>
      <semantic:outgoing>flow_to_confirm_warranty</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_confirm_entries_warranty" name="Confirm Entries (Warranty Check)">
      <semantic:incoming>flow_to_confirm_warranty</semantic:incoming>
      <semantic:outgoing>flow_to_define_ref_obj</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_define_reference_object_tab" name="Define Reference Object Tab">
      <semantic:incoming>flow_to_define_ref_obj</semantic:incoming>
      <semantic:outgoing>flow_to_define_malf_data</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_define_malfunction_data_tab" name="Define Malfunction Data Tab">
      <semantic:incoming>flow_to_define_malf_data</semantic:incoming>
      <semantic:outgoing>flow_to_gw_breakdown</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_breakdown_case" name="Breakdown Case?">
      <semantic:incoming>flow_to_gw_breakdown</semantic:incoming>
      <semantic:outgoing>flow_breakdown_yes</semantic:outgoing>
      <semantic:outgoing>flow_breakdown_no</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_mark_breakdown_checkbox" name="Mark Breakdown Checkbox">
      <semantic:incoming>flow_breakdown_yes</semantic:incoming>
      <semantic:outgoing>flow_to_merge_breakdown_1</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_not_mark_breakdown_checkbox" name="Do Not Mark Breakdown Checkbox">
      <semantic:incoming>flow_breakdown_no</semantic:incoming>
      <semantic:outgoing>flow_to_merge_breakdown_2</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_merge_breakdown">
      <semantic:incoming>flow_to_merge_breakdown_1</semantic:incoming>
      <semantic:incoming>flow_to_merge_breakdown_2</semantic:incoming>
      <semantic:outgoing>flow_to_save_pm_notif</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_save_pm_notification" name="Save PM Notification">
      <semantic:incoming>flow_to_save_pm_notif</semantic:incoming>
      <semantic:outgoing>flow_to_pg_split_send</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="pg_split_send_details" name="Define Send Details">
      <semantic:incoming>flow_to_pg_split_send</semantic:incoming>
      <semantic:outgoing>flow_to_define_receiver</semantic:outgoing>
      <semantic:outgoing>flow_to_define_notice_text</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:task id="task_define_receiver_details" name="Define Receiver Details">
      <semantic:incoming>flow_to_define_receiver</semantic:incoming>
      <semantic:outgoing>flow_to_pg_merge_send_1</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_define_notice_text" name="Define Notice Text">
      <semantic:incoming>flow_to_define_notice_text</semantic:incoming>
      <semantic:outgoing>flow_to_pg_merge_send_2</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="pg_merge_send_details">
      <semantic:incoming>flow_to_pg_merge_send_1</semantic:incoming>
      <semantic:incoming>flow_to_pg_merge_send_2</semantic:incoming>
      <semantic:outgoing>flow_to_confirm_send_entries</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:task id="task_confirm_send_entries" name="Confirm Send Entries">
      <semantic:incoming>flow_to_confirm_send_entries</semantic:incoming>
      <semantic:outgoing>flow_to_save_process_mail</semantic:outgoing>
    </semantic:task>
    <semantic:userTask id="task_save_process_mail" name="Save Process and Send Mail"> <!-- Changed to UserTask as it initiates a message -->
      <semantic:incoming>flow_to_save_process_mail</semantic:incoming>
      <!-- Outgoing is the message flow -->
    </semantic:userTask>

    <semantic:intermediateCatchEvent id="intermediate_catch_decision_received" name="Receive Maintenance Request Decision">
      <!-- Incoming is message flow -->
      <semantic:outgoing>flow_to_gw_req_approved_check</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="message_decision"/>
    </semantic:intermediateCatchEvent>
    <semantic:exclusiveGateway id="gateway_request_approved_check" name="Request Approved?">
      <semantic:incoming>flow_to_gw_req_approved_check</semantic:incoming>
      <semantic:outgoing>flow_req_approved_yes</semantic:outgoing>
      <semantic:outgoing>flow_req_approved_no</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_manage_work_order" name="Manage Work Order">
      <semantic:incoming>flow_req_approved_yes</semantic:incoming>
      <semantic:outgoing>flow_to_create_refurb_order</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_create_internal_refurb_order" name="Create Internal Refurbishment Order">
      <semantic:incoming>flow_to_create_refurb_order</semantic:incoming>
      <semantic:outgoing>flow_to_transfer_info</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_transfer_info_to_wo" name="Transfer Information to Work Order">
      <semantic:incoming>flow_to_transfer_info</semantic:incoming>
      <semantic:outgoing>flow_to_define_order_type</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_define_order_type_mc04" name="Define Order Type MC04">
      <semantic:incoming>flow_to_define_order_type</semantic:incoming>
      <semantic:outgoing>flow_to_pg_split_plants_val</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="pg_split_plants_valuation" name="Define Plants and Valuation">
      <semantic:incoming>flow_to_pg_split_plants_val</semantic:incoming>
      <semantic:outgoing>flow_to_define_refurb_plants</semantic:outgoing>
      <semantic:outgoing>flow_to_define_val_type</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:task id="task_define_refurb_plants" name="Define Interval of Refurbishment Plants">
      <semantic:incoming>flow_to_define_refurb_plants</semantic:incoming>
      <semantic:outgoing>flow_to_pg_merge_plants_val_1</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_define_valuation_type" name="Define Valuation Type">
      <semantic:incoming>flow_to_define_val_type</semantic:incoming>
      <semantic:outgoing>flow_to_pg_merge_plants_val_2</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="pg_merge_plants_valuation">
      <semantic:incoming>flow_to_pg_merge_plants_val_1</semantic:incoming>
      <semantic:incoming>flow_to_pg_merge_plants_val_2</semantic:incoming>
      <semantic:outgoing>flow_to_define_ops_main</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:task id="task_define_operations_main" name="Define Operations">
      <semantic:incoming>flow_to_define_ops_main</semantic:incoming>
      <semantic:outgoing>flow_to_pg_split_int_ext_ops</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="pg_split_internal_external_ops" name="Maintain Operations">
      <semantic:incoming>flow_to_pg_split_int_ext_ops</semantic:incoming>
      <semantic:outgoing>flow_to_call_activity_planning</semantic:outgoing>
      <semantic:outgoing>flow_to_maintain_ext_ops</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:callActivity id="call_activity_planning_scheduling" name="Maintain Internal Operations" calledElement="planningSchedulingProcess">
      <semantic:incoming>flow_to_call_activity_planning</semantic:incoming>
      <semantic:outgoing>flow_to_pg_merge_int_ext_ops_1</semantic:outgoing>
    </semantic:callActivity>
    <semantic:task id="task_maintain_external_ops" name="Maintain External Operations">
      <semantic:incoming>flow_to_maintain_ext_ops</semantic:incoming>
      <semantic:outgoing>flow_to_pg_merge_int_ext_ops_2</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="pg_merge_internal_external_ops">
      <semantic:incoming>flow_to_pg_merge_int_ext_ops_1</semantic:incoming>
      <semantic:incoming>flow_to_pg_merge_int_ext_ops_2</semantic:incoming>
      <semantic:outgoing>flow_to_define_est_costs</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:task id="task_define_estimated_costs" name="Define Estimated Costs">
      <semantic:incoming>flow_to_define_est_costs</semantic:incoming>
      <semantic:outgoing>flow_to_maintain_partners</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_maintain_involved_partners" name="Maintain Involved Partners">
      <semantic:incoming>flow_to_maintain_partners</semantic:incoming>
      <semantic:outgoing>flow_to_define_alloc_obj</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_define_allocated_objects" name="Define Allocated Objects">
      <semantic:incoming>flow_to_define_alloc_obj</semantic:incoming>
      <semantic:outgoing>flow_to_define_add_data</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_define_additional_data" name="Define Additional Data">
      <semantic:incoming>flow_to_define_add_data</semantic:incoming>
      <semantic:outgoing>flow_to_define_loc_data</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_define_location_data" name="Define Location Data">
      <semantic:incoming>flow_to_define_loc_data</semantic:incoming>
      <semantic:outgoing>flow_to_save_wo</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_save_work_order" name="Save Work Order">
      <semantic:incoming>flow_to_save_wo</semantic:incoming>
      <semantic:outgoing>flow_to_assign_wbs</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_assign_wo_to_wbs" name="Assign Work Order to WBS Element">
      <semantic:incoming>flow_to_assign_wbs</semantic:incoming>
      <semantic:outgoing>flow_to_end_handled</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="end_event_request_handled" name="Internal Refurbishment Request Handled">
      <semantic:incoming>flow_to_end_handled</semantic:incoming>
    </semantic:endEvent>
    <semantic:endEvent id="end_event_rm_rejected" name="Process Ended (RM - Rejected)">
      <semantic:incoming>flow_req_approved_no</semantic:incoming>
    </semantic:endEvent>

    <!-- Maintenance Manager Lane Elements -->
    <semantic:intermediateCatchEvent id="intermediate_catch_request_received" name="Maintenance Request Received">
      <!-- Incoming is message flow -->
      <semantic:outgoing>flow_to_evaluate_request</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="message_request"/>
    </semantic:intermediateCatchEvent>
    <semantic:task id="task_evaluate_request" name="Evaluate Request">
      <semantic:incoming>flow_to_evaluate_request</semantic:incoming>
      <semantic:outgoing>flow_to_doc_decision</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_document_submit_decision" name="Document/Submit Decision">
      <semantic:incoming>flow_to_doc_decision</semantic:incoming>
      <semantic:outgoing>flow_to_gw_decision_approved</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_decision_approved" name="Decision Approved?">
      <semantic:incoming>flow_to_gw_decision_approved</semantic:incoming>
      <semantic:outgoing>flow_decision_approved_yes</semantic:outgoing>
      <semantic:outgoing>flow_decision_approved_no</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_mgr_change_pm_notification_approved" name="Change PM Notification (Approved)">
      <semantic:incoming>flow_decision_approved_yes</semantic:incoming>
      <semantic:outgoing>flow_to_mgr_set_status_appr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_mgr_set_status_approved" name="Set Notification Status to Approved">
      <semantic:incoming>flow_to_mgr_set_status_appr</semantic:incoming>
      <semantic:outgoing>flow_to_mgr_confirm_appr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_mgr_confirm_entries_approved" name="Confirm Entries (Approved)">
      <semantic:incoming>flow_to_mgr_confirm_appr</semantic:incoming>
      <semantic:outgoing>flow_to_mgr_put_in_process</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_mgr_select_put_in_process" name="Select Put in Process Icon">
      <semantic:incoming>flow_to_mgr_put_in_process</semantic:incoming>
      <semantic:outgoing>flow_to_mgr_save_appr</semantic:outgoing>
    </semantic:task>
    <semantic:userTask id="task_mgr_save_entries_approved" name="Save Entries (Approved)"> <!-- Changed to UserTask as it initiates a message -->
      <semantic:incoming>flow_to_mgr_save_appr</semantic:incoming>
      <!-- Outgoing is message flow -->
    </semantic:userTask>
    <semantic:task id="task_mgr_change_pm_notification_rejected" name="Change PM Notification (Cancelled)">
      <semantic:incoming>flow_decision_approved_no</semantic:incoming>
      <semantic:outgoing>flow_to_mgr_set_status_cancel</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_mgr_set_status_cancelled" name="Set Notification Status to Cancelled">
      <semantic:incoming>flow_to_mgr_set_status_cancel</semantic:incoming>
      <semantic:outgoing>flow_to_mgr_save_cancel</semantic:outgoing>
    </semantic:task>
    <semantic:userTask id="task_mgr_save_entries_rejected" name="Save Entries (Cancelled)"> <!-- Changed to UserTask as it initiates a message -->
      <semantic:incoming>flow_to_mgr_save_cancel</semantic:incoming>
      <semantic:outgoing>flow_to_end_rejected_mgr</semantic:outgoing> <!-- Also sends message -->
    </semantic:userTask>
    <semantic:endEvent id="end_event_request_rejected" name="Request Rejected and Process Terminated">
      <semantic:incoming>flow_to_end_rejected_mgr</semantic:incoming>
    </semantic:endEvent>

    <!-- Sequence Flows for Responsible Maintenance -->
    <semantic:sequenceFlow id="flow_to_create_pm" sourceRef="start_event_request_received" targetRef="task_create_pm_notification"/>
    <semantic:sequenceFlow id="flow_to_choose_type" sourceRef="task_create_pm_notification" targetRef="task_choose_notification_type"/>
    <semantic:sequenceFlow id="flow_to_define_chars" sourceRef="task_choose_notification_type" targetRef="task_define_notification_characteristics"/>
    <semantic:sequenceFlow id="flow_to_confirm_warranty" sourceRef="task_define_notification_characteristics" targetRef="task_confirm_entries_warranty"/>
    <semantic:sequenceFlow id="flow_to_define_ref_obj" sourceRef="task_confirm_entries_warranty" targetRef="task_define_reference_object_tab"/>
    <semantic:sequenceFlow id="flow_to_define_malf_data" sourceRef="task_define_reference_object_tab" targetRef="task_define_malfunction_data_tab"/>
    <semantic:sequenceFlow id="flow_to_gw_breakdown" sourceRef="task_define_malfunction_data_tab" targetRef="gateway_breakdown_case"/>
    <semantic:sequenceFlow id="flow_breakdown_yes" name="Yes" sourceRef="gateway_breakdown_case" targetRef="task_mark_breakdown_checkbox"/>
    <semantic:sequenceFlow id="flow_breakdown_no" name="No" sourceRef="gateway_breakdown_case" targetRef="task_not_mark_breakdown_checkbox"/>
    <semantic:sequenceFlow id="flow_to_merge_breakdown_1" sourceRef="task_mark_breakdown_checkbox" targetRef="gateway_merge_breakdown"/>
    <semantic:sequenceFlow id="flow_to_merge_breakdown_2" sourceRef="task_not_mark_breakdown_checkbox" targetRef="gateway_merge_breakdown"/>
    <semantic:sequenceFlow id="flow_to_save_pm_notif" sourceRef="gateway_merge_breakdown" targetRef="task_save_pm_notification"/>
    <semantic:sequenceFlow id="flow_to_pg_split_send" sourceRef="task_save_pm_notification" targetRef="pg_split_send_details"/>
    <semantic:sequenceFlow id="flow_to_define_receiver" sourceRef="pg_split_send_details" targetRef="task_define_receiver_details"/>
    <semantic:sequenceFlow id="flow_to_define_notice_text" sourceRef="pg_split_send_details" targetRef="task_define_notice_text"/>
    <semantic:sequenceFlow id="flow_to_pg_merge_send_1" sourceRef="task_define_receiver_details" targetRef="pg_merge_send_details"/>
    <semantic:sequenceFlow id="flow_to_pg_merge_send_2" sourceRef="task_define_notice_text" targetRef="pg_merge_send_details"/>
    <semantic:sequenceFlow id="flow_to_confirm_send_entries" sourceRef="pg_merge_send_details" targetRef="task_confirm_send_entries"/>
    <semantic:sequenceFlow id="flow_to_save_process_mail" sourceRef="task_confirm_send_entries" targetRef="task_save_process_mail"/>

    <semantic:sequenceFlow id="flow_to_gw_req_approved_check" sourceRef="intermediate_catch_decision_received" targetRef="gateway_request_approved_check"/>
    <semantic:sequenceFlow id="flow_req_approved_yes" name="Yes" sourceRef="gateway_request_approved_check" targetRef="task_manage_work_order"/>
