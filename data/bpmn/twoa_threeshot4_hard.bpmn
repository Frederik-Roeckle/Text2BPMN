<?xml version="1.0" encoding="UTF-8"?>
<semantic:definitions xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
                      xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                      xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                      xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                      id="definitions_p2p"
                      targetNamespace="http://example.com/bpmn/p2p_process">

  <semantic:collaboration id="collab_p2p">
    <semantic:participant id="pool_purchasing_dept" name="Purchasing Department" processRef="process_purchasing"/>
    <semantic:participant id="pool_vendor" name="Vendor" processRef="process_vendor"/>

    <semantic:messageFlow id="msgflow_pr_to_hod" sourceRef="task_req_send_pr_hod" targetRef="task_hod_receive_pr"/>
    <semantic:messageFlow id="msgflow_pr_decision_to_req" sourceRef="task_hod_submit_decision" targetRef="task_req_receive_pr_decision"/>
    <semantic:messageFlow id="msgflow_po_to_poa" sourceRef="task_req_send_po_poa" targetRef="task_poa_receive_po"/>
    <semantic:messageFlow id="msgflow_po_decision_to_req" sourceRef="task_poa_submit_decision" targetRef="task_req_receive_po_decision"/>
    <semantic:messageFlow id="msgflow_po_to_vendor" sourceRef="task_req_dispatch_po_vendor" targetRef="task_ven_receive_po"/>
    <semantic:messageFlow id="msgflow_vendor_decision_to_req" sourceRef="task_ven_send_po_decision" targetRef="task_req_receive_vendor_decision"/>
    <semantic:messageFlow id="msgflow_invoice_to_req" sourceRef="task_ven_send_invoice" targetRef="task_req_receive_invoice"/>
    <semantic:messageFlow id="msgflow_wrong_invoice_to_vendor" sourceRef="task_req_return_invoice" targetRef="task_ven_receive_wrong_invoice"/>
    <semantic:messageFlow id="msgflow_corrected_invoice_to_req" sourceRef="task_ven_send_corrected_invoice" targetRef="task_req_receive_corrected_invoice"/>
    <semantic:messageFlow id="msgflow_invoice_to_ap" sourceRef="task_req_send_invoice_ap" targetRef="task_ap_receive_invoice"/>
  </semantic:collaboration>

  <semantic:process id="process_purchasing" isExecutable="true">
    <semantic:laneSet id="laneset_purchasing">
      <semantic:lane id="lane_requestor" name="Requestor">
        <semantic:flowNodeRef>event_start_p2p</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_define_requirements</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_create_pr_draft</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_send_pr_hod</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_receive_pr_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_pr_approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_issue_formal_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_create_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_send_po_poa</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_receive_po_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_po_approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_dispatch_po_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_receive_vendor_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_vendor_po_outcome</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_search_new_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_receive_items</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_item_type</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_issue_gr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_issue_ses</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_merge_gr_ses</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_receive_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_3way_match</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_invoice_correct</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_return_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_receive_corrected_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_merge_invoice_check</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_post_invoice_sap</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_send_invoice_ap</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_hod" name="Heads of Departments">
        <semantic:flowNodeRef>task_hod_receive_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_hod_review_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_hod_document_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_hod_pr_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_hod_submit_decision</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_po_approvers" name="PO Approvers">
        <semantic:flowNodeRef>task_poa_receive_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_poa_review_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_poa_document_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_poa_po_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_poa_submit_decision</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_ap_team" name="Accounts Payable Team">
        <semantic:flowNodeRef>task_ap_receive_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_decide_payment_method</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_ap_payment_type</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_pay_full</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_pay_installments</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_ap_merge_payment</semantic:flowNodeRef>
        <semantic:flowNodeRef>event_end_p2p_success</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <!-- Requestor Lane Elements -->
    <semantic:startEvent id="event_start_p2p" name="P2P Process Started">
      <semantic:outgoing>flow_start_to_define_req</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_req_define_requirements" name="Define Requirements">
      <semantic:incoming>flow_start_to_define_req</semantic:incoming>
      <semantic:incoming>flow_gw_pr_approved_to_define_req</semantic:incoming>
      <semantic:incoming>flow_gw_po_approved_to_define_req</semantic:incoming>
      <semantic:outgoing>flow_define_req_to_create_pr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_create_pr_draft" name="Create PR Draft">
      <semantic:incoming>flow_define_req_to_create_pr</semantic:incoming>
      <semantic:outgoing>flow_create_pr_to_send_pr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_send_pr_hod" name="Send PR for HoD Approval">
      <semantic:incoming>flow_create_pr_to_send_pr</semantic:incoming>
      <semantic:outgoing>flow_req_send_pr_hod_done</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_receive_pr_decision" name="Receive PR Decision from HoD">
      <semantic:incoming>flow_req_send_pr_hod_done</semantic:incoming> <!-- Placeholder, actual trigger is message -->
      <semantic:outgoing>flow_receive_pr_decision_to_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_pr_approved" name="PR Approved by HoD?">
      <semantic:incoming>flow_receive_pr_decision_to_gw</semantic:incoming>
      <semantic:outgoing>flow_gw_pr_approved_to_issue_pr</semantic:outgoing>
      <semantic:outgoing>flow_gw_pr_approved_to_define_req</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_issue_formal_pr" name="Issue Formal PR">
      <semantic:incoming>flow_gw_pr_approved_to_issue_pr</semantic:incoming>
      <semantic:outgoing>flow_issue_pr_to_create_po</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_create_po" name="Create PO">
      <semantic:incoming>flow_issue_pr_to_create_po</semantic:incoming>
      <semantic:outgoing>flow_create_po_to_send_po</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_send_po_poa" name="Send PO for POA Approval">
      <semantic:incoming>flow_create_po_to_send_po</semantic:incoming>
      <semantic:outgoing>flow_req_send_po_poa_done</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_receive_po_decision" name="Receive PO Decision from POA">
      <semantic:incoming>flow_req_send_po_poa_done</semantic:incoming> <!-- Placeholder -->
      <semantic:outgoing>flow_receive_po_decision_to_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_po_approved" name="PO Approved by POA?">
      <semantic:incoming>flow_receive_po_decision_to_gw</semantic:incoming>
      <semantic:outgoing>flow_gw_po_approved_to_dispatch</semantic:outgoing>
      <semantic:outgoing>flow_gw_po_approved_to_define_req</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_dispatch_po_vendor" name="Dispatch PO to Vendor">
      <semantic:incoming>flow_gw_po_approved_to_dispatch</semantic:incoming>
      <semantic:incoming>flow_search_vendor_to_dispatch</semantic:incoming>
      <semantic:outgoing>flow_req_dispatch_po_done</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_receive_vendor_decision" name="Receive Vendor PO Decision">
      <semantic:incoming>flow_req_dispatch_po_done</semantic:incoming> <!-- Placeholder -->
      <semantic:outgoing>flow_receive_vendor_decision_to_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_vendor_po_outcome" name="Vendor Approved/Negotiated?">
      <semantic:incoming>flow_receive_vendor_decision_to_gw</semantic:incoming>
      <semantic:outgoing>flow_gw_vendor_outcome_to_receive_items</semantic:outgoing>
      <semantic:outgoing>flow_gw_vendor_outcome_to_search_vendor</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_search_new_vendor" name="Search for New Vendor">
      <semantic:incoming>flow_gw_vendor_outcome_to_search_vendor</semantic:incoming>
      <semantic:outgoing>flow_search_vendor_to_dispatch</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_receive_items" name="Receive Agreed Goods/Services">
      <semantic:incoming>flow_gw_vendor_outcome_to_receive_items</semantic:incoming>
      <semantic:outgoing>flow_receive_items_to_gw_type</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_item_type" name="Goods or Service?">
      <semantic:incoming>flow_receive_items_to_gw_type</semantic:incoming>
      <semantic:outgoing>flow_gw_type_to_gr</semantic:outgoing>
      <semantic:outgoing>flow_gw_type_to_ses</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_issue_gr" name="Issue Goods Receipt (GR)">
      <semantic:incoming>flow_gw_type_to_gr</semantic:incoming>
      <semantic:outgoing>flow_gr_to_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_issue_ses" name="Issue Service Entry Sheet (SES)">
      <semantic:incoming>flow_gw_type_to_ses</semantic:incoming>
      <semantic:outgoing>flow_ses_to_merge</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_merge_gr_ses" name="Merge GR/SES">
      <semantic:incoming>flow_gr_to_merge</semantic:incoming>
      <semantic:incoming>flow_ses_to_merge</semantic:incoming>
      <semantic:outgoing>flow_merge_gr_ses_to_receive_invoice</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_receive_invoice" name="Receive Invoice from Vendor">
      <semantic:incoming>flow_merge_gr_ses_to_receive_invoice</semantic:incoming>
      <semantic:outgoing>flow_receive_invoice_to_3way</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_3way_match" name="Perform 3-Way Match">
      <semantic:incoming>flow_receive_invoice_to_3way</semantic:incoming>
      <semantic:outgoing>flow_3way_to_gw_invoice_correct</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_invoice_correct" name="Invoice Correct?">
      <semantic:incoming>flow_3way_to_gw_invoice_correct</semantic:incoming>
      <semantic:outgoing>flow_gw_invoice_correct_to_merge_check</semantic:outgoing>
      <semantic:outgoing>flow_gw_invoice_correct_to_return</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_return_invoice" name="Return Wrong Invoice to Vendor">
      <semantic:incoming>flow_gw_invoice_correct_to_return</semantic:incoming>
      <semantic:outgoing>flow_req_return_invoice_done</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_receive_corrected_invoice" name="Receive Corrected Invoice">
      <semantic:incoming>flow_req_return_invoice_done</semantic:incoming> <!-- Placeholder -->
      <semantic:outgoing>flow_receive_corrected_to_merge_check</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_merge_invoice_check" name="Merge Invoice Check">
      <semantic:incoming>flow_gw_invoice_correct_to_merge_check</semantic:incoming>
      <semantic:incoming>flow_receive_corrected_to_merge_check</semantic:incoming>
      <semantic:outgoing>flow_merge_check_to_post_invoice</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_post_invoice_sap" name="Post Invoice in SAP">
      <semantic:incoming>flow_merge_check_to_post_invoice</semantic:incoming>
      <semantic:outgoing>flow_post_invoice_to_send_ap</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_send_invoice_ap" name="Send Invoice to AP Team">
      <semantic:incoming>flow_post_invoice_to_send_ap</semantic:incoming>
    </semantic:task>

    <!-- HoD Lane Elements -->
    <semantic:task id="task_hod_receive_pr" name="Receive PR for Approval">
      <semantic:outgoing>flow_hod_receive_to_review</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_hod_review_pr" name="Review PR">
      <semantic:incoming>flow_hod_receive_to_review</semantic:incoming>
      <semantic:outgoing>flow_hod_review_to_document</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_hod_document_decision" name="Document PR Decision">
      <semantic:incoming>flow_hod_review_to_document</semantic:incoming>
      <semantic:outgoing>flow_hod_document_to_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_hod_pr_decision" name="HoD PR Decision?">
      <semantic:incoming>flow_hod_document_to_gw</semantic:incoming>
      <semantic:outgoing>flow_hod_gw_to_submit</semantic:outgoing> <!-- Simplified: always submit -->
    </semantic:exclusiveGateway>
    <semantic:task id="task_hod_submit_decision" name="Submit PR Decision to Requestor">
      <semantic:incoming>flow_hod_gw_to_submit</semantic:incoming>
    </semantic:task>

    <!-- POA Lane Elements -->
    <semantic:task id="task_poa_receive_po" name="Receive PO for Approval">
      <semantic:outgoing>flow_poa_receive_to_review</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_poa_review_po" name="Review PO">
      <semantic:incoming>flow_poa_receive_to_review</semantic:incoming>
      <semantic:outgoing>flow_poa_review_to_document</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_poa_document_decision" name="Document PO Decision">
      <semantic:incoming>flow_poa_review_to_document</semantic:incoming>
      <semantic:outgoing>flow_poa_document_to_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_poa_po_decision" name="POA PO Decision?">
      <semantic:incoming>flow_poa_document_to_gw</semantic:incoming>
      <semantic:outgoing>flow_poa_gw_to_submit</semantic:outgoing> <!-- Simplified: always submit -->
    </semantic:exclusiveGateway>
    <semantic:task id="task_poa_submit_decision" name="Submit PO Decision to Requestor">
      <semantic:incoming>flow_poa_gw_to_submit</semantic:incoming>
    </semantic:task>

    <!-- AP Team Lane Elements -->
    <semantic:task id="task_ap_receive_invoice" name="Receive Invoice for Payment">
      <semantic:outgoing>flow_ap_receive_to_decide</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ap_decide_payment_method" name="Decide Payment Method">
      <semantic:incoming>flow_ap_receive_to_decide</semantic:incoming>
      <semantic:outgoing>flow_ap_decide_to_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_ap_payment_type" name="Payment Method?">
      <semantic:incoming>flow_ap_decide_to_gw</semantic:incoming>
      <semantic:outgoing>flow_ap_gw_to_pay_full</semantic:outgoing>
      <semantic:outgoing>flow_ap_gw_to_pay_install</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_ap_pay_full" name="Perform Payment in Full">
      <semantic:incoming>flow_ap_gw_to_pay_full</semantic:incoming>
      <semantic:outgoing>flow_ap_pay_full_to_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ap_pay_installments" name="Perform Payment in Installments">
      <semantic:incoming>flow_ap_gw_to_pay_install</semantic:incoming>
      <semantic:outgoing>flow_ap_pay_install_to_merge</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_ap_merge_payment" name="Merge Payment">
      <semantic:incoming>flow_ap_pay_full_to_merge</semantic:incoming>
      <semantic:incoming>flow_ap_pay_install_to_merge</semantic:incoming>
      <semantic:outgoing>flow_ap_merge_to_end</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:endEvent id="event_end_p2p_success" name="P2P Process Successfully Completed">
      <semantic:incoming>flow_ap_merge_to_end</semantic:incoming>
    </semantic:endEvent>

    <!-- Sequence Flows for Purchasing Process -->
    <semantic:sequenceFlow id="flow_start_to_define_req" sourceRef="event_start_p2p" targetRef="task_req_define_requirements"/>
    <semantic:sequenceFlow id="flow_define_req_to_create_pr" sourceRef="task_req_define_requirements" targetRef="task_req_create_pr_draft"/>
    <semantic:sequenceFlow id="flow_create_pr_to_send_pr" sourceRef="task_req_create_pr_draft" targetRef="task_req_send_pr_hod"/>
    <semantic:sequenceFlow id="flow_req_send_pr_hod_done" sourceRef="task_req_send_pr_hod" targetRef="task_req_receive_pr_decision"/>
    <semantic:sequenceFlow id="flow_receive_pr_decision_to_gw" sourceRef="task_req_receive_pr_decision" targetRef="gw_req_pr_approved"/>
    <semantic:sequenceFlow id="flow_gw_pr_approved_to_issue_pr" name="Approved" sourceRef="gw_req_pr_approved" targetRef="task_req_issue_formal_pr"/>
    <semantic:sequenceFlow id="flow_gw_pr_approved_to_define_req" name="Not Approved" sourceRef="gw_req_pr_approved" targetRef="task_req_define_requirements"/>
    <semantic:sequenceFlow id="flow_issue_pr_to_create_po" sourceRef="task_req_issue_formal_pr" targetRef="task_req_create_po"/>
    <semantic:sequenceFlow id="flow_create_po_to_send_po" sourceRef="task_req_create_po" targetRef="task_req_send_po_poa"/>
    <semantic:sequenceFlow id="flow_req_send_po_poa_done" sourceRef="task_req_send_po_poa" targetRef="task_req_receive_po_decision"/>
    <semantic:sequenceFlow id="flow_receive_po_decision_to_gw" sourceRef="task_req_receive_po_decision" targetRef="gw_req_po_approved"/>
    <semantic:sequenceFlow id="flow_gw_po_approved_to_dispatch" name="Approved" sourceRef="gw_req_po_approved" targetRef="task_req_dispatch_po_vendor"/>
    <semantic:sequenceFlow id="flow_gw_po_approved_to_define_req" name="Not Approved" sourceRef="gw_req_po_approved" targetRef="task_req_define_requirements"/>
    <semantic:sequenceFlow id="flow_req_dispatch_po_done" sourceRef="task_req_dispatch_po_vendor" targetRef="task_req_receive_vendor_decision"/>
    <semantic:sequenceFlow id="flow_receive_vendor_decision_to_gw" sourceRef="task_req_receive_vendor_decision" targetRef="gw_req_vendor_po_outcome"/>
    <semantic:sequenceFlow id="flow_gw_vendor_outcome_to_receive_items" name="Approved/Negotiated" sourceRef="gw_req_vendor_po_outcome" targetRef="task_req_receive_items"/>
    <semantic:sequenceFlow id="flow_gw_vendor_outcome_to_search_vendor" name="Rejected" sourceRef="gw_req_vendor_po_outcome" targetRef="task_req_search_new_vendor"/>
    <semantic:sequenceFlow id="flow_search_vendor_to_dispatch" sourceRef="task_req_search_new_vendor" targetRef="task_req_dispatch_po_vendor"/>
    <semantic:sequenceFlow id="flow_receive_items_to_gw_type" sourceRef="task_req_receive_items" targetRef="gw_req_item_type"/>
    <semantic:sequenceFlow id="flow_gw_type_to_gr" name="Goods" sourceRef="gw_req_item_type" targetRef="task_req_issue_gr"/>
    <semantic:sequenceFlow id="flow_gw_type_to_ses" name="Service" sourceRef="gw_req_item_type" targetRef="task_req_issue_ses"/>
    <semantic:sequenceFlow id="flow_gr_to_merge" sourceRef="task_req_issue_gr" targetRef="gw_req_merge_gr_ses"/>
    <semantic:sequenceFlow id="flow_ses_to_merge" sourceRef="task_req_issue_ses" targetRef="gw_req_merge_gr_ses"/>
    <semantic:sequenceFlow id="flow_merge_gr_ses_to_receive_invoice" sourceRef="gw_req_merge_gr_ses" targetRef="task_req_receive_invoice"/>
    <semantic:sequenceFlow id="flow_receive_invoice_to_3way" sourceRef="task_req_receive_invoice" targetRef="task_req_3way_match"/>
    <semantic:sequenceFlow id="flow_3way_to_gw_invoice_correct" sourceRef="task_req_3way_match" targetRef="gw_req_invoice_correct"/>
    <semantic:sequenceFlow id="flow_gw_invoice_correct_to_merge_check" name="Correct" sourceRef="gw_req_invoice_correct" targetRef="gw_req_merge_invoice_check"/>
    <semantic:sequenceFlow id="flow_gw_invoice_correct_to_return" name="Wrong" sourceRef="gw_req_invoice_correct" targetRef="task_req_return_invoice"/>
    <semantic:sequenceFlow id="flow_req_return_invoice_done" sourceRef="task_req_return_invoice" targetRef="task_req_receive_corrected_invoice"/>
    <semantic:sequenceFlow id="flow_receive_corrected_to_merge_check" sourceRef="task_req_receive_corrected_invoice" targetRef="gw_req_merge_invoice_check"/>
