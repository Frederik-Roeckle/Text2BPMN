    <semantic:messageFlow id="mf_decline_to_employee" name="Decline Notification" sourceRef="task_notify_decline" targetRef="event_decline_received" messageRef="msg_decline_notification"/>
  </semantic:collaboration>

  <semantic:message id="msg_overtime_request" name="OvertimeRegistrationRequestMessage"/>
