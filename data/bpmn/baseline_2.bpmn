<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="OvertimeRegistrationProcess"
    targetNamespace="http://example.com/bpmn/overtime-registration"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_overtimeRegistration">
    <semantic:participant id="participant_employee" name="Employee (Sales Department)" processRef="process_employee"/>
    <semantic:participant id="participant_manager" name="Manager" processRef="process_manager"/>
  </semantic:collaboration>

  <semantic:process id="process_employee" isExecutable="true">
    <semantic:laneSet id="laneSet_employee">
      <semantic:lane id="lane_employee" name="Employee (Sales Department)">
        <semantic:flowNodeRef>start_employee</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_createOvertimeRegistration</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_sendOvertimeRegistration</semantic:flowNodeRef>
        <semantic:flowNodeRef>receive_declinedRequest</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_employee</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <semantic:startEvent id="start_employee" name="Start Overtime Registration Request">
      <semantic:outgoing>flow_start_create</semantic:outgoing>
    </semantic:startEvent>

    <semantic:task id="task_createOvertimeRegistration" name="Create Overtime Registration">
      <semantic:incoming>flow_start_create</semantic:incoming>
      <semantic:outgoing>flow_create_send</semantic:outgoing>
    </semantic:task>

    <semantic:task id="task_sendOvertimeRegistration" name="Send Overtime Registration to Manager">
      <semantic:incoming>flow_create_send</semantic:incoming>
      <semantic:outgoing>flow_send_end</semantic:outgoing>
    </semantic:task>

    <semantic:intermediateCatchEvent id="receive_declinedRequest" name="Receive Declined Overtime Registration">
      <semantic:incoming>flow_declined_employee</semantic:incoming>
      <semantic:outgoing>flow_declined_end</semantic:outgoing>
    </semantic:intermediateCatchEvent>

    <semantic:endEvent id="end_employee" name="End Overtime Registration Process">
      <semantic:incoming>flow_send_end</semantic:incoming>
      <semantic:incoming>flow_declined_end</semantic:incoming>
    </semantic:endEvent>

    <semantic:sequenceFlow id="flow_start_create" sourceRef="start_employee" targetRef="task_createOvertimeRegistration"/>
    <semantic:sequenceFlow id="flow_create_send" sourceRef="task_createOvertimeRegistration" targetRef="task_sendOvertimeRegistration"/>
    <semantic:sequenceFlow id="flow_send_end" sourceRef="task_sendOvertimeRegistration" targetRef="end_employee"/>
    <semantic:sequenceFlow id="flow_declined_employee" sourceRef="receive_declinedRequest" targetRef="receive_declinedRequest"/>
    <semantic:sequenceFlow id="flow_declined_end" sourceRef="receive_declinedRequest" targetRef="end_employee"/>
  </semantic:process>

  <semantic:process id="process_manager" isExecutable="true">
    <semantic:laneSet id="laneSet_manager">
      <semantic:lane id="lane_manager" name="Manager">
        <semantic:flowNodeRef>receive_overtimeRequest</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_evaluateOvertimeRequest</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_decisionApproval</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_sendDeclineNotification</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_declined</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <semantic:intermediateCatchEvent id="receive_overtimeRequest" name="Receive Overtime Registration Request">
      <semantic:outgoing>flow_receive_evaluate</semantic:outgoing>
    </semantic:intermediateCatchEvent>

    <semantic:task id="task_evaluateOvertimeRequest" name="Evaluate Overtime Registration Request">
      <semantic:incoming>flow_receive_evaluate</semantic:incoming>
      <semantic:outgoing>flow_evaluate_decision</semantic:outgoing>
    </semantic:task>

    <semantic:exclusiveGateway id="gateway_decisionApproval" name="Approve or Decline Overtime Request?">
      <semantic:incoming>flow_evaluate_decision</semantic:incoming>
      <semantic:outgoing>flow_approved_end</semantic:outgoing>
      <semantic:outgoing>flow_declined_notify</semantic:outgoing>
    </semantic:exclusiveGateway>

    <semantic:endEvent id="end_approved" name="End Process - Request Approved">
      <semantic:incoming>flow_approved_end</semantic:incoming>
    </semantic:endEvent>

    <semantic:task id="task_sendDeclineNotification" name="Send Decline Notification to Employee">
      <semantic:incoming>flow_declined_notify</semantic:incoming>
      <semantic:outgoing>flow_notify_end</semantic:outgoing>
    </semantic:task>

    <semantic:endEvent id="end_declined" name="End Process - Request Declined">
      <semantic:incoming>flow_notify_end</semantic:incoming>
    </semantic:endEvent>

    <semantic:sequenceFlow id="flow_receive_evaluate" sourceRef="receive_overtimeRequest" targetRef="task_evaluateOvertimeRequest"/>
    <semantic:sequenceFlow id="flow_evaluate_decision" sourceRef="task_evaluateOvertimeRequest" targetRef="gateway_decisionApproval"/>
    <semantic:sequenceFlow id="flow_approved_end" sourceRef="gateway_decisionApproval" targetRef="end_approved"/>
    <semantic:sequenceFlow id="flow_declined_notify" sourceRef="gateway_decisionApproval" targetRef="task_sendDeclineNotification"/>
    <semantic:sequenceFlow id="flow_notify_end" sourceRef="task_sendDeclineNotification" targetRef="end_declined"/>
  </semantic:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_overtimeRegistration" name="Overtime Registration Diagram" resolution="96.0">
    <bpmndi:BPMNPlane bpmnElement="collab_overtimeRegistration">

      <bpmndi:BPMNShape id="participant_employee_shape" bpmnElement="participant_employee" isHorizontal="true">
        <dc:Bounds x="20" y="20" width="450" height="300"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="start_employee_shape" bpmnElement="start_employee">
        <dc:Bounds x="60" y="100" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="task_createOvertimeRegistration_shape" bpmnElement="task_createOvertimeRegistration">
        <dc:Bounds x="130" y="90" width="140" height="56"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="task_sendOvertimeRegistration_shape" bpmnElement="task_sendOvertimeRegistration">
        <dc:Bounds x="290" y="90" width="160" height="56"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="receive_declinedRequest_shape" bpmnElement="receive_declinedRequest">
        <dc:Bounds x="180" y="190" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="end_employee_shape" bpmnElement="end_employee">
        <dc:Bounds x="370" y="190" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNEdge id="flow_start_create_edge" bpmnElement="flow_start_create">
        <di:waypoint x="96" y="118"/>
        <di:waypoint x="130" y="118"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_create_send_edge" bpmnElement="flow_create_send">
        <di:waypoint x="270" y="118"/>
        <di:waypoint x="290" y="118"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_send_end_edge" bpmnElement="flow_send_end">
        <di:waypoint x="450" y="118"/>
        <di:waypoint x="370" y="208"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_declined_employee_edge" bpmnElement="flow_declined_employee">
        <di:waypoint x="198" y="208"/>
        <di:waypoint x="198" y="208"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_declined_end_edge" bpmnElement="flow_declined_end">
        <di:waypoint x="216" y="208"/>
        <di:waypoint x="370" y="208"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNShape id="participant_manager_shape" bpmnElement="participant_manager" isHorizontal="true">
        <dc:Bounds x="20" y="350" width="450" height="300"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="receive_overtimeRequest_shape" bpmnElement="receive_overtimeRequest">
        <dc:Bounds x="60" y="400" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="task_evaluateOvertimeRequest_shape" bpmnElement="task_evaluateOvertimeRequest">
        <dc:Bounds x="130" y="390" width="180" height="56"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="gateway_decisionApproval_shape" bpmnElement="gateway_decisionApproval" isMarkerVisible="true">
        <dc:Bounds x="340" y="395" width="50" height="50"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="end_approved_shape" bpmnElement="end_approved">
        <dc:Bounds x="430" y="370" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="task_sendDeclineNotification_shape" bpmnElement="task_sendDeclineNotification">
        <dc:Bounds x="430" y="460" width="180" height="56"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="end_declined_shape" bpmnElement="end_declined">
        <dc:Bounds x="640" y="470" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNEdge id="flow_receive_evaluate_edge" bpmnElement="flow_receive_evaluate">
        <di:waypoint x="96" y="418"/>
        <di:waypoint x="130" y="418"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_evaluate_decision_edge" bpmnElement="flow_evaluate_decision">
        <di:waypoint x="310" y="418"/>
        <di:waypoint x="340" y="420"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_approved_end_edge" bpmnElement="flow_approved_end">
        <di:waypoint x="390" y="420"/>
        <di:waypoint x="430" y="388"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_declined_notify_edge" bpmnElement="flow_declined_notify">
        <di:waypoint x="390" y="445"/>
        <di:waypoint x="430" y="488"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_notify_end_edge" bpmnElement="flow_notify_end">
        <di:waypoint x="610" y="488"/>
        <di:waypoint x="640" y="488"/>
      </bpmndi:BPMNEdge>

    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</semantic:definitions>
