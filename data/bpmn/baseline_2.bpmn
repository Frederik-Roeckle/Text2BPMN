        <semantic:flowNodeRef>employee_end_approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>event_decline_received</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_process_decline</semantic:flowNodeRef>
        <semantic:flowNodeRef>employee_end_declined</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <semantic:startEvent id="employee_start" name="Overtime Registration Initiated">
      <semantic:outgoing>flow_start_to_create</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_create_reg" name="Create Overtime Registration">
