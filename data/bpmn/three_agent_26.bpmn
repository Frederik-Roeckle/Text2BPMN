```xml
<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="CommitmentInvoiceProcessDefs"
    targetNamespace="http://example.com/bpmn/commitment-invoice"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_commitment_invoice">
    <semantic:participant id="participant_commitment_invoice_ops" name="Commitment &amp; Invoice Process Operations" processRef="proc_commitment_invoice"/>
  </semantic:collaboration>

  <semantic:process id="proc_commitment_invoice" name="Commitment and Invoice Process" isExecutable="true">
    <semantic:laneSet id="laneSet_main">
      <semantic:lane id="lane_monetary_dept" name="Monetary Department">
        <semantic:flowNodeRef>se_main_start</semantic:flowNodeRef>
        <semantic:flowNodeRef>sp_funds_commitment</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_mon_view_report</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_ap_dept" name="Accounts Payable Department">
        <semantic:flowNodeRef>task_ap_receive_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>pg_ap_prereq_fork</semantic:flowNodeRef>
        <semantic:flowNodeRef>sp_ap_maintain_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>sp_ap_create_cost_centre</semantic:flowNodeRef>
        <semantic:flowNodeRef>pg_ap_prereq_join</semantic:flowNodeRef>
        <semantic:flowNodeRef>sp_ap_post_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_display_doc_check_status</semantic:flowNodeRef>
        <semantic:flowNodeRef>xg_ap_doc_parked</semantic:flowNodeRef>
        <semantic:flowNodeRef>sp_ap_release_parked_doc</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_post_payment_req_define_details</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_select_post_final_doc</semantic:flowNodeRef>
        <semantic:flowNodeRef>ee_main_end</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <!-- Main Process Elements -->
    <semantic:startEvent id="se_main_start" name="Commitment &amp; Invoice Received">
      <semantic:outgoing>flow_se_main_start_to_sp_funds_commitment</semantic:outgoing>
    </semantic:startEvent>

    <semantic:subProcess id="sp_funds_commitment" name="Post Funds Commitment in SAP ECC" triggeredByEvent="false">
      <semantic:incoming>flow_se_main_start_to_sp_funds_commitment</semantic:incoming>
      <semantic:outgoing>flow_sp_funds_commitment_to_task_mon_view_report</semantic:outgoing>
      <semantic:startEvent id="sp_fc_start_event" name="Start Funds Commitment">
        <semantic:outgoing>flow_sp_fc_start_to_task_fc_define_docs</semantic:outgoing>
      </semantic:startEvent>
      <semantic:task id="task_fc_define_docs_codes_currency" name="Define Document Type, Company Code, and Currency">
        <semantic:incoming>flow_sp_fc_start_to_task_fc_define_docs</semantic:incoming>
        <semantic:outgoing>flow_task_fc_define_docs_to_task_fc_define_posting</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_fc_define_posting_details" name="Define Commitment Posting Details (Item, Centre, Fund, Area)">
        <semantic:incoming>flow_task_fc_define_docs_to_task_fc_define_posting</semantic:incoming>
        <semantic:outgoing>flow_task_fc_define_posting_to_task_fc_save</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_fc_save_document" name="Save Funds Commitment Document">
        <semantic:incoming>flow_task_fc_define_posting_to_task_fc_save</semantic:incoming>
        <semantic:outgoing>flow_task_fc_save_to_sp_fc_end</semantic:outgoing>
      </semantic:task>
      <semantic:endEvent id="sp_fc_end_event" name="End Funds Commitment">
        <semantic:incoming>flow_task_fc_save_to_sp_fc_end</semantic:incoming>
      </semantic:endEvent>
      <semantic:sequenceFlow id="flow_sp_fc_start_to_task_fc_define_docs" sourceRef="sp_fc_start_event" targetRef="task_fc_define_docs_codes_currency"/>
      <semantic:sequenceFlow id="flow_task_fc_define_docs_to_task_fc_define_posting" sourceRef="task_fc_define_docs_codes_currency" targetRef="task_fc_define_posting_details"/>
      <semantic:sequenceFlow id="flow_task_fc_define_posting_to_task_fc_save" sourceRef="task_fc_define_posting_details" targetRef="task_fc_save_document"/>
      <semantic:sequenceFlow id="flow_task_fc_save_to_sp_fc_end" sourceRef="task_fc_save_document" targetRef="sp_fc_end_event"/>
    </semantic:subProcess>

    <semantic:task id="task_mon_view_report" name="Refer to Posting Report (Funds Commitment)">
      <semantic:incoming>flow_sp_funds_commitment_to_task_mon_view_report</semantic:incoming>
      <semantic:outgoing>flow_task_mon_view_report_to_task_ap_receive_invoice</semantic:outgoing>
    </semantic:task>

    <semantic:task id="task_ap_receive_invoice" name="Receive Invoice from External Service Provider">
      <semantic:incoming>flow_task_mon_view_report_to_task_ap_receive_invoice</semantic:incoming>
      <semantic:outgoing>flow_task_ap_receive_invoice_to_pg_ap_prereq_fork</semantic:outgoing>
    </semantic:task>

    <semantic:parallelGateway id="pg_ap_prereq_fork" name="Fork for Pre-requisites">
      <semantic:incoming>flow_task_ap_receive_invoice_to_pg_ap_prereq_fork</semantic:incoming>
      <semantic:outgoing>flow_pg_ap_prereq_fork_to_sp_ap_maintain_vendor</semantic:outgoing>
      <semantic:outgoing>flow_pg_ap_prereq_fork_to_sp_ap_create_cost_centre</semantic:outgoing>
    </semantic:parallelGateway>

    <semantic:subProcess id="sp_ap_maintain_vendor" name="Maintain Vendor Master Data" triggeredByEvent="false">
      <semantic:incoming>flow_pg_ap_prereq_fork_to_sp_ap_maintain_vendor</semantic:incoming>
      <semantic:outgoing>flow_sp_ap_maintain_vendor_to_pg_ap_prereq_join</semantic:outgoing>
      <semantic:startEvent id="sp_vm_start_event" name="Start Vendor Maintenance">
        <semantic:outgoing>flow_sp_vm_start_to_task_vm_specify_initial_data</semantic:outgoing>
      </semantic:startEvent>
      <semantic:task id="task_vm_specify_initial_data" name="Specify Initial Vendor Data (Vendor, CC, Purch. Org, Acc. Group)">
        <semantic:incoming>flow_sp_vm_start_to_task_vm_specify_initial_data</semantic:incoming>
        <semantic:outgoing>flow_task_vm_specify_initial_data_to_task_vm_specify_address</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_vm_specify_address" name="Specify Vendor Address Data">
        <semantic:incoming>flow_task_vm_specify_initial_data_to_task_vm_specify_address</semantic:incoming>
        <semantic:outgoing>flow_task_vm_specify_address_to_task_vm_maintain_vat_tax</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_vm_maintain_vat_tax" name="Maintain European VAT ID and Tax File Number">
        <semantic:incoming>flow_task_vm_specify_address_to_task_vm_maintain_vat_tax</semantic:incoming>
        <semantic:outgoing>flow_task_vm_maintain_vat_tax_to_task_vm_define_bank</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_vm_define_bank" name="Define Bank Details">
        <semantic:incoming>flow_task_vm_maintain_vat_tax_to_task_vm_define_bank</semantic:incoming>
        <semantic:outgoing>flow_task_vm_define_bank_to_task_vm_specify_reco_account</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_vm_specify_reco_account" name="Specify Reconciliation Account">
        <semantic:incoming>flow_task_vm_define_bank_to_task_vm_specify_reco_account</semantic:incoming>
        <semantic:outgoing>flow_task_vm_specify_reco_account_to_task_vm_define_payment_terms</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_vm_define_payment_terms" name="Define Payment Terms and Incoterms">
        <semantic:incoming>flow_task_vm_specify_reco_account_to_task_vm_define_payment_terms</semantic:incoming>
        <semantic:outgoing>flow_task_vm_define_payment_terms_to_task_vm_save_vendor</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_vm_save_vendor" name="Save New Vendor">
        <semantic:incoming>flow_task_vm_define_payment_terms_to_task_vm_save_vendor</semantic:incoming>
        <semantic:outgoing>flow_task_vm_save_vendor_to_sp_vm_end</semantic:outgoing>
      </semantic:task>
      <semantic:endEvent id="sp_vm_end_event" name="End Vendor Maintenance">
        <semantic:incoming>flow_task_vm_save_vendor_to_sp_vm_end</semantic:incoming>
      </semantic:endEvent>
      <semantic:sequenceFlow id="flow_sp_vm_start_to_task_vm_specify_initial_data" sourceRef="sp_vm_start_event" targetRef="task_vm_specify_initial_data"/>
      <semantic:sequenceFlow id="flow_task_vm_specify_initial_data_to_task_vm_specify_address" sourceRef="task_vm_specify_initial_data" targetRef="task_vm_specify_address"/>
      <semantic:sequenceFlow id="flow_task_vm_specify_address_to_task_vm_maintain_vat_tax" sourceRef="task_vm_specify_address" targetRef="task_vm_maintain_vat_tax"/>
      <semantic:sequenceFlow id="flow_task_vm_maintain_vat_tax_to_task_vm_define_bank" sourceRef="task_vm_maintain_vat_tax" targetRef="task_vm_define_bank"/>
      <semantic:sequenceFlow id="flow_task_vm_define_bank_to_task_vm_specify_reco_account" sourceRef="task_vm_define_bank" targetRef="task_vm_specify_reco_account"/>
      <semantic:sequenceFlow id="flow_task_vm_specify_reco_account_to_task_vm_define_payment_terms" sourceRef="task_vm_specify_reco_account" targetRef="task_vm_define_payment_terms"/>
      <semantic:sequenceFlow id="flow_task_vm_define_payment_terms_to_task_vm_save_vendor" sourceRef="task_vm_define_payment_terms" targetRef="task_vm_save_vendor"/>
      <semantic:sequenceFlow id="flow_task_vm_save_vendor_to_sp_vm_end" sourceRef="task_vm_save_vendor" targetRef="sp_vm_end_event"/>
    </semantic:subProcess>

    <semantic:subProcess id="sp_ap_create_cost_centre" name="Create Cost Centre (CC)" triggeredByEvent="false">
      <semantic:incoming>flow_pg_ap_prereq_fork_to_sp_ap_create_cost_centre</semantic:incoming>
      <semantic:outgoing>flow_sp_ap_create_cost_centre_to_pg_ap_prereq_join</semantic:outgoing>
      <semantic:startEvent id="sp_cc_start_event" name="Start Cost Centre Creation">
        <semantic:outgoing>flow_sp_cc_start_to_task_cc_define_name_validity</semantic:outgoing>
      </semantic:startEvent>
      <semantic:task id="task_cc_define_name_validity" name="Define CC Name and Validity Period">
        <semantic:incoming>flow_sp_cc_start_to_task_cc_define_name_validity</semantic:incoming>
        <semantic:outgoing>flow_task_cc_define_name_validity_to_task_cc_confirm_entries</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_cc_confirm_entries" name="Confirm CC Entries">
        <semantic:incoming>flow_task_cc_define_name_validity_to_task_cc_confirm_entries</semantic:incoming>
        <semantic:outgoing>flow_task_cc_confirm_entries_to_task_cc_define_master_data</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_cc_define_master_data" name="Define CC Master Data (Name, Resp., Category, Area, Currency, Group Assign)">
        <semantic:incoming>flow_task_cc_confirm_entries_to_task_cc_define_master_data</semantic:incoming>
        <semantic:outgoing>flow_task_cc_define_master_data_to_task_cc_save_entries</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_cc_save_entries" name="Save CC Entries">
        <semantic:incoming>flow_task_cc_define_master_data_to_task_cc_save_entries</semantic:incoming>
        <semantic:outgoing>flow_task_cc_save_entries_to_sp_cc_end</semantic:outgoing>
      </semantic:task>
      <semantic:endEvent id="sp_cc_end_event" name="End Cost Centre Creation">
        <semantic:incoming>flow_task_cc_save_entries_to_sp_cc_end</semantic:incoming>
      </semantic:endEvent>
      <semantic:sequenceFlow id="flow_sp_cc_start_to_task_cc_define_name_validity" sourceRef="sp_cc_start_event" targetRef="task_cc_define_name_validity"/>
      <semantic:sequenceFlow id="flow_task_cc_define_name_validity_to_task_cc_confirm_entries" sourceRef="task_cc_define_name_validity" targetRef="task_cc_confirm_entries"/>
      <semantic:sequenceFlow id="flow_task_cc_confirm_entries_to_task_cc_define_master_data" sourceRef="task_cc_confirm_entries" targetRef="task_cc_define_master_data"/>
      <semantic:sequenceFlow id="flow_task_cc_define_master_data_to_task_cc_save_entries" sourceRef="task_cc_define_master_data" targetRef="task_cc_save_entries"/>
      <semantic:sequenceFlow id="flow_task_cc_save_entries_to_sp_cc_end" sourceRef="task_cc_save_entries" targetRef="sp_cc_end_event"/>
    </semantic:subProcess>

    <semantic:parallelGateway id="pg_ap_prereq_join" name="Join Pre-requisites">
      <semantic:incoming>flow_sp_ap_maintain_vendor_to_pg_ap_prereq_join</semantic:incoming>
      <semantic:incoming>flow_sp_ap_create_cost_centre_to_pg_ap_prereq_join</semantic:incoming>
      <semantic:outgoing>flow_pg_ap_prereq_join_to_sp_ap_post_invoice</semantic:outgoing>
    </semantic:parallelGateway>

    <semantic:subProcess id="sp_ap_post_invoice" name="Prepare and Post Invoice" triggeredByEvent="false">
      <semantic:incoming>flow_pg_ap_prereq_join_to_sp_ap_post_invoice</semantic:incoming>
      <semantic:outgoing>flow_sp_ap_post_invoice_to_task_ap_display_doc_check_status</semantic:outgoing>
      <semantic:startEvent id="sp_pi_start_event" name="Start Invoice Posting">
        <semantic:outgoing>flow_sp_pi_start_to_task_pi_define_initial_details</semantic:outgoing>
      </semantic:startEvent>
      <semantic:task id="task_pi_define_initial_details" name="Define Initial Invoice Details (Doc Type, Vendor, Period, CI, Fund, Funds Center)">
        <semantic:incoming>flow_sp_pi_start_to_task_pi_define_initial_details</semantic:incoming>
        <semantic:outgoing>flow_task_pi_define_initial_details_to_task_pi_define_tax_assign_cc</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_pi_define_tax_assign_cc" name="Define Tax Details and Assign Cost Centre">
        <semantic:incoming>flow_task_pi_define_initial_details_to_task_pi_define_tax_assign_cc</semantic:incoming>
        <semantic:outgoing>flow_task_pi_define_tax_assign_cc_to_task_pi_save_posting</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_pi_save_posting" name="Save Invoice Posting">
        <semantic:incoming>flow_task_pi_define_tax_assign_cc_to_task_pi_save_posting</semantic:incoming>
        <semantic:outgoing>flow_task_pi_save_posting_to_sp_pi_end</semantic:outgoing>
      </semantic:task>
      <semantic:endEvent id="sp_pi_end_event" name="End Invoice Posting">
        <semantic:incoming>flow_task_pi_save_posting_to_sp_pi_end</semantic:incoming>
      </semantic:endEvent>
      <semantic:sequenceFlow id="flow_sp_pi_start_to_task_pi_define_initial_details" sourceRef="sp_pi_start_event" targetRef="task_pi_define_initial_details"/>
      <semantic:sequenceFlow id="flow_task_pi_define_initial_details_to_task_pi_define_tax_assign_cc" sourceRef="task_pi_define_initial_details" targetRef="task_pi_define_tax_assign_cc"/>
      <semantic:sequenceFlow id="flow_task_pi_define_tax_assign_cc_to_task_pi_save_posting" sourceRef="task_pi_define_tax_assign_cc" targetRef="task_pi_save_posting"/>
      <semantic:sequenceFlow id="flow_task_pi_save_posting_to_sp_pi_end" sourceRef="task_pi_save_posting" targetRef="sp_pi_end_event"/>
    </semantic:subProcess>

    <semantic:task id="task_ap_display_doc_check_status" name="Display Document and Check Status">
      <semantic:incoming>flow_sp_ap_post_invoice_to_task_ap_display_doc_check_status</semantic:incoming>
      <semantic:outgoing>flow_task_ap_display_doc_check_status_to_xg_ap_doc_parked</semantic:outgoing>
    </semantic:task>

    <semantic:exclusiveGateway id="xg_ap_doc_parked" name="Document Parked?">
      <semantic:incoming>flow_task_ap_display_doc_check_status_to_xg_ap_doc_parked</semantic:incoming>
      <semantic:outgoing>flow_xg_ap_doc_parked_to_sp_ap_release_parked_doc</semantic:outgoing>
      <semantic:outgoing>flow_xg_ap_doc_parked_to_task_ap_post_payment_req_define_details</semantic:outgoing>
    </semantic:exclusiveGateway>

    <semantic:subProcess id="sp_ap_release_parked_doc" name="Release Parked Document" triggeredByEvent="false">
      <semantic:incoming>flow_xg_ap_doc_parked_to_sp_ap_release_parked_doc</semantic:incoming>
      <semantic:outgoing>flow_sp_ap_release_parked_doc_to_task_ap_post_payment_req_define_details</semantic:outgoing>
      <semantic:startEvent id="sp_rpd_start_event" name="Start Document Release">
        <semantic:outgoing>flow_sp_rpd_start_to_task_rpd_customize_doc</semantic:outgoing>
      </semantic:startEvent>
      <semantic:task id="task_rpd_customize_doc" name="Customize Document">
        <semantic:incoming>flow_sp_rpd_start_to_task_rpd_customize_doc</semantic:incoming>
        <semantic:outgoing>flow_task_rpd_customize_doc_to_task_rpd_assign_req_cat_fi</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_rpd_assign_req_cat_fi" name="Assign Request Category to FI Document Type">
        <semantic:incoming>flow_task_rpd_customize_doc_to_task_rpd_assign_req_cat_fi</semantic:incoming>
        <semantic:outgoing>flow_task_rpd_assign_req_cat_fi_to_task_rpd_link_req_cat_doc_type</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_rpd_link_req_cat_doc_type" name="Link Request Category to Document Type">
        <semantic:incoming>flow_task_rpd_assign_req_cat_fi_to_task_rpd_link_req_cat_doc_type</semantic:incoming>
        <semantic:outgoing>flow_task_rpd_link_req_cat_doc_type_to_task_rpd_control_proc_req_cat</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_rpd_control_proc_req_cat" name="Control Processing of Request Categories">
        <semantic:incoming>flow_task_rpd_link_req_cat_doc_type_to_task_rpd_control_proc_req_cat</semantic:incoming>
        <semantic:outgoing>flow_task_rpd_control_proc_req_cat_to_task_rpd_assign_workflow</semantic:outgoing>
      </semantic:task>
      <semantic:task id="task_rpd_assign_workflow" name="Assign Workflow to Request Type">
        <semantic:incoming>flow_task_rpd_control_proc_req_cat_to_task_rpd_assign_workflow</semantic:incoming>
        <semantic:outgoing>flow_task_rpd_assign_workflow_to_sp_rpd_end</semantic:outgoing>
      </semantic:task>
      <semantic:endEvent id="sp_rpd_end_event" name="End Document Release">
        <semantic:incoming>flow_task_rpd_assign_workflow_to_sp_rpd_end</semantic:incoming>
      </semantic:endEvent>
      <semantic:sequenceFlow id="flow_sp_rpd_start_to_task_rpd_customize_doc" sourceRef="sp_rpd_start_event" targetRef="task_rpd_customize_doc"/>
      <semantic:sequenceFlow id="flow_task_rpd_customize_doc_to_task_rpd_assign_req_cat_fi" sourceRef="task_rpd_customize_doc" targetRef="task_rpd_assign_req_cat_fi"/>
      <semantic:sequenceFlow id="flow_task_rpd_assign_req_cat_fi_to_task_rpd_link_req_cat_doc_type" sourceRef="task_rpd_assign_req_cat_fi" targetRef="task_rpd_link_req_cat_doc_type"/>
      <semantic:sequenceFlow id="flow_task_rpd_link_req_cat_doc_type_to_task_rpd_control_proc_req_cat" sourceRef="task_rpd_link_req_cat_doc_type" targetRef="task_rpd_control_proc_req_cat"/>
      <semantic:sequenceFlow id="flow_task_rpd_control_proc_req_cat_to_task_rpd_assign_workflow" sourceRef="task_rpd_control_proc_req_cat" targetRef="task_rpd_assign_workflow"/>
      <semantic:sequenceFlow id="flow_task_rpd_assign_workflow_to_sp_rpd_end" sourceRef="task_rpd_assign_workflow" targetRef="sp_rpd_end_event"/>
    </semantic:subProcess>

    <semantic:task id="task_ap_post_payment_req_define_details" name="Post Payment Request (Define Details &amp; Execute)">
      <semantic:incoming>flow_sp_ap_release_parked_doc_to_task_ap_post_payment_req_define_details</semantic:incoming>
      <semantic:incoming>flow_xg_ap_doc_parked_to_task_ap_post_payment_req_define_details</semantic:incoming>
      <semantic:outgoing>flow_task_ap_post_payment_req_define_details_to_task_ap_select_post_final_doc</semantic:outgoing>
    </semantic:task>

    <semantic:task id="task_ap_select_post_final_doc" name="Select and Post Corresponding Document">
      <semantic:incoming>flow_task_ap_post_payment_req_define_details_to_task_ap_select_post_final_doc</semantic:incoming>
      <semantic:outgoing>flow_task_ap_select_post_final_doc_to_ee_main_end</semantic:outgoing>
    </semantic:task>

    <semantic:endEvent id="ee_main_end" name="Process Maintained">
      <semantic:incoming>flow_task_ap_select_post_final_doc_to_ee_main_end</semantic:incoming>
    </semantic:endEvent>

    <!-- Main Process Sequence Flows -->
    <semantic:sequenceFlow id="flow_se_main_start_to_sp_funds_commitment" sourceRef="se_main_start" targetRef="sp_funds_commitment"/>
    <semantic:sequenceFlow id="flow_sp_funds_commitment_to_task_mon_view_report" sourceRef="sp_funds_commitment" targetRef="task_mon_view_report"/>
    <semantic:sequenceFlow id="flow_task_mon_view_report_to_task_ap_receive_invoice" sourceRef="task_mon_view_report" targetRef="task_ap_receive_invoice"/>
    <semantic:sequenceFlow id="flow_task_ap_receive_invoice_to_pg_ap_prereq_fork" sourceRef="task_ap_receive_invoice" targetRef="pg_ap_prereq_fork"/>
    <semantic:sequenceFlow id="flow_pg_ap_prereq_fork_to_sp_ap_maintain_vendor" sourceRef="pg_ap_prereq_fork" targetRef="sp_ap_maintain_vendor"/>
    <semantic:sequenceFlow id="flow_pg_ap_prereq_fork_to_sp_ap_create_cost_centre" sourceRef="pg_ap_prereq_fork" targetRef="sp_ap_create_cost_centre"/>
