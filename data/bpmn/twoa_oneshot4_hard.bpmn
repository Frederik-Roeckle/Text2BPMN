<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="P2P_Process_Definition"
    targetNamespace="http://example.com/bpmn/p2p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_p2p">
    <semantic:participant id="pool_purchasing" name="Purchasing Department" processRef="process_purchasing"/>
    <semantic:participant id="pool_vendor" name="Vendor" processRef="process_vendor"/>
    <semantic:messageFlow id="msgflow_req_pr_hod" sourceRef="task_req_send_pr_hod" targetRef="task_hod_receive_pr"/>
    <semantic:messageFlow id="msgflow_hod_decision_req" sourceRef="task_hod_submit_pr_decision" targetRef="gw_req_pr_approved"/>
    <semantic:messageFlow id="msgflow_req_po_poa" sourceRef="task_req_send_po_poa" targetRef="task_poa_receive_po"/>
    <semantic:messageFlow id="msgflow_poa_decision_req" sourceRef="task_poa_submit_po_decision" targetRef="gw_req_po_approved"/>
    <semantic:messageFlow id="msgflow_req_po_vendor" sourceRef="task_req_dispatch_po_vendor" targetRef="task_ven_receive_po"/>
    <semantic:messageFlow id="msgflow_ven_decision_req" sourceRef="task_ven_send_decision_req" targetRef="gw_req_vendor_decision"/>
    <semantic:messageFlow id="msgflow_ven_invoice_req" sourceRef="task_ven_send_invoice" targetRef="task_req_receive_invoice"/>
    <semantic:messageFlow id="msgflow_req_badinvoice_ven" sourceRef="task_req_return_invoice_vendor" targetRef="task_ven_receive_bad_invoice"/>
    <semantic:messageFlow id="msgflow_ven_goodinvoice_req" sourceRef="task_ven_send_good_invoice" targetRef="task_req_receive_good_invoice"/>
    <semantic:messageFlow id="msgflow_req_invoice_ap" sourceRef="task_req_send_invoice_ap" targetRef="task_ap_receive_invoice"/>
  </semantic:collaboration>

  <semantic:process id="process_purchasing" isExecutable="true">
    <semantic:laneSet id="laneset_purchasing">
      <semantic:lane id="lane_requestor" name="Requestor">
        <semantic:flowNodeRef>event_start_p2p</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_determine_needs</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_create_pr_draft</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_send_pr_hod</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_pr_approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_issue_formal_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_create_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_send_po_poa</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_po_approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_dispatch_po_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_vendor_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_search_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_receive_items</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_item_type</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_issue_gr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_issue_ses</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_merge_gr_ses</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_receive_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_3way_match</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_invoice_ok</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_return_invoice_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_receive_good_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_merge_invoice_check</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_post_invoice_sap</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_send_invoice_ap</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_hod" name="Heads of Departments">
        <semantic:flowNodeRef>task_hod_receive_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_hod_review_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_hod_doc_pr_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_hod_pr_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_hod_submit_pr_decision</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_poa" name="PO Approvers">
        <semantic:flowNodeRef>task_poa_receive_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_poa_review_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_poa_doc_po_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_poa_po_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_poa_submit_po_decision</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_ap" name="Accounts Payable Team">
        <semantic:flowNodeRef>task_ap_receive_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_decide_payment</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_ap_payment_type</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_pay_full</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_pay_install</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_ap_merge_payment</semantic:flowNodeRef>
        <semantic:flowNodeRef>event_end_p2p_success</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <semantic:startEvent id="event_start_p2p" name="P2P Process Started">
      <semantic:outgoing>flow_start_to_determine</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_req_determine_needs" name="Determine Business Needs">
      <semantic:incoming>flow_start_to_determine</semantic:incoming>
      <semantic:incoming>flow_gwpr_to_determine</semantic:incoming>
      <semantic:incoming>flow_gwpo_to_determine</semantic:incoming>
      <semantic:outgoing>flow_determine_to_createpr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_create_pr_draft" name="Create PR Draft">
      <semantic:incoming>flow_determine_to_createpr</semantic:incoming>
      <semantic:outgoing>flow_createpr_to_sendpr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_send_pr_hod" name="Send PR to HoD">
      <semantic:incoming>flow_createpr_to_sendpr</semantic:incoming>
      <semantic:outgoing>flow_sendpr_placeholder</semantic:outgoing> <!-- Placeholder for message flow -->
    </semantic:task>

    <semantic:task id="task_hod_receive_pr" name="Receive PR">
      <semantic:incoming>flow_receivepr_placeholder</semantic:incoming> <!-- Placeholder for message flow -->
      <semantic:outgoing>flow_receivepr_to_reviewpr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_hod_review_pr" name="Review PR">
      <semantic:incoming>flow_receivepr_to_reviewpr</semantic:incoming>
      <semantic:outgoing>flow_reviewpr_to_docpr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_hod_doc_pr_decision" name="Document PR Decision">
      <semantic:incoming>flow_reviewpr_to_docpr</semantic:incoming>
      <semantic:outgoing>flow_docpr_to_gwprdecision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_hod_pr_decision" name="PR Decision by HoD?">
      <semantic:incoming>flow_docpr_to_gwprdecision</semantic:incoming>
      <semantic:outgoing>flow_gwprdecision_to_submitpr</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_hod_submit_pr_decision" name="Submit PR Decision">
      <semantic:incoming>flow_gwprdecision_to_submitpr</semantic:incoming>
      <semantic:outgoing>flow_submitpr_placeholder</semantic:outgoing> <!-- Placeholder for message flow -->
    </semantic:task>

    <semantic:exclusiveGateway id="gw_req_pr_approved" name="PR Approved by HoD?">
      <semantic:incoming>flow_gwreqpr_placeholder</semantic:incoming> <!-- Placeholder for message flow -->
      <semantic:outgoing>flow_gwpr_to_issueformalpr</semantic:outgoing>
      <semantic:outgoing>flow_gwpr_to_determine</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_issue_formal_pr" name="Issue Formal PR">
      <semantic:incoming>flow_gwpr_to_issueformalpr</semantic:incoming>
      <semantic:outgoing>flow_issueformalpr_to_createpo</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_create_po" name="Create PO">
      <semantic:incoming>flow_issueformalpr_to_createpo</semantic:incoming>
      <semantic:outgoing>flow_createpo_to_sendpo</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_send_po_poa" name="Send PO to PO Approvers">
      <semantic:incoming>flow_createpo_to_sendpo</semantic:incoming>
      <semantic:outgoing>flow_sendpo_placeholder</semantic:outgoing> <!-- Placeholder for message flow -->
    </semantic:task>

    <semantic:task id="task_poa_receive_po" name="Receive PO">
      <semantic:incoming>flow_receivepo_placeholder</semantic:incoming> <!-- Placeholder for message flow -->
      <semantic:outgoing>flow_receivepo_to_reviewpo</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_poa_review_po" name="Review PO">
      <semantic:incoming>flow_receivepo_to_reviewpo</semantic:incoming>
      <semantic:outgoing>flow_reviewpo_to_docpo</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_poa_doc_po_decision" name="Document PO Decision">
      <semantic:incoming>flow_reviewpo_to_docpo</semantic:incoming>
      <semantic:outgoing>flow_docpo_to_gwpodecision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_poa_po_decision" name="PO Decision by PO Approvers?">
      <semantic:incoming>flow_docpo_to_gwpodecision</semantic:incoming>
      <semantic:outgoing>flow_gwpodecision_to_submitpo</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_poa_submit_po_decision" name="Submit PO Decision">
      <semantic:incoming>flow_gwpodecision_to_submitpo</semantic:incoming>
      <semantic:outgoing>flow_submitpo_placeholder</semantic:outgoing> <!-- Placeholder for message flow -->
    </semantic:task>

    <semantic:exclusiveGateway id="gw_req_po_approved" name="PO Approved by PO Approvers?">
      <semantic:incoming>flow_gwreqpo_placeholder</semantic:incoming> <!-- Placeholder for message flow -->
      <semantic:outgoing>flow_gwpo_to_dispatch</semantic:outgoing>
      <semantic:outgoing>flow_gwpo_to_determine</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_dispatch_po_vendor" name="Dispatch PO to Vendor">
      <semantic:incoming>flow_gwpo_to_dispatch</semantic:incoming>
      <semantic:outgoing>flow_dispatch_placeholder</semantic:outgoing> <!-- Placeholder for message flow -->
    </semantic:task>

    <semantic:exclusiveGateway id="gw_req_vendor_decision" name="Vendor Approved/Negotiated PO?">
      <semantic:incoming>flow_gwreqven_placeholder</semantic:incoming> <!-- Placeholder for message flow -->
      <semantic:outgoing>flow_gwven_to_receiveitems</semantic:outgoing>
      <semantic:outgoing>flow_gwven_to_searchvendor</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_search_vendor" name="Search for New Vendor">
      <semantic:incoming>flow_gwven_to_searchvendor</semantic:incoming>
      <semantic:outgoing>flow_searchvendor_to_dispatch</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_receive_items" name="Receive Goods/Services">
      <semantic:incoming>flow_gwven_to_receiveitems</semantic:incoming>
      <semantic:outgoing>flow_receiveitems_to_gwitemtype</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_item_type" name="Goods or Service?">
      <semantic:incoming>flow_receiveitems_to_gwitemtype</semantic:incoming>
      <semantic:outgoing>flow_gwitemtype_to_issuegr</semantic:outgoing>
      <semantic:outgoing>flow_gwitemtype_to_issueses</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_issue_gr" name="Issue Goods Receipt (GR)">
      <semantic:incoming>flow_gwitemtype_to_issuegr</semantic:incoming>
      <semantic:outgoing>flow_issuegr_to_gwmergegrses</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_issue_ses" name="Issue Service Entry Sheet (SES)">
      <semantic:incoming>flow_gwitemtype_to_issueses</semantic:incoming>
      <semantic:outgoing>flow_issueses_to_gwmergegrses</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_merge_gr_ses" name="Merge GR/SES Path">
      <semantic:incoming>flow_issuegr_to_gwmergegrses</semantic:incoming>
      <semantic:incoming>flow_issueses_to_gwmergegrses</semantic:incoming>
      <semantic:outgoing>flow_gwmergegrses_to_receiveinvoice</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_receive_invoice" name="Receive Invoice">
      <semantic:incoming>flow_gwmergegrses_to_receiveinvoice</semantic:incoming>
      <semantic:incoming>flow_receiveinvoice_placeholder</semantic:incoming> <!-- Placeholder for message flow -->
      <semantic:outgoing>flow_receiveinvoice_to_3waymatch</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_3way_match" name="Perform 3-Way Match">
      <semantic:incoming>flow_receiveinvoice_to_3waymatch</semantic:incoming>
      <semantic:outgoing>flow_3waymatch_to_gwinvoiceok</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_invoice_ok" name="Invoice Correct?">
      <semantic:incoming>flow_3waymatch_to_gwinvoiceok</semantic:incoming>
      <semantic:outgoing>flow_gwinvoiceok_to_mergeinvoicecheck</semantic:outgoing>
      <semantic:outgoing>flow_gwinvoiceok_to_returninvoice</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_return_invoice_vendor" name="Return Wrong Invoice to Vendor">
      <semantic:incoming>flow_gwinvoiceok_to_returninvoice</semantic:incoming>
      <semantic:outgoing>flow_returninvoice_placeholder</semantic:outgoing> <!-- Placeholder for message flow -->
    </semantic:task>
    <semantic:task id="task_req_receive_good_invoice" name="Receive Corrected Invoice">
      <semantic:incoming>flow_receivegoodinvoice_placeholder</semantic:incoming> <!-- Placeholder for message flow -->
      <semantic:outgoing>flow_receivegoodinvoice_to_mergeinvoicecheck</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_merge_invoice_check" name="Merge Invoice Paths">
      <semantic:incoming>flow_gwinvoiceok_to_mergeinvoicecheck</semantic:incoming>
      <semantic:incoming>flow_receivegoodinvoice_to_mergeinvoicecheck</semantic:incoming>
      <semantic:outgoing>flow_mergeinvoicecheck_to_postinvoice</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_post_invoice_sap" name="Post Invoice in SAP">
      <semantic:incoming>flow_mergeinvoicecheck_to_postinvoice</semantic:incoming>
      <semantic:outgoing>flow_postinvoice_to_sendinvoiceap</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_send_invoice_ap" name="Send Invoice to AP">
      <semantic:incoming>flow_postinvoice_to_sendinvoiceap</semantic:incoming>
      <semantic:outgoing>flow_sendinvoiceap_placeholder</semantic:outgoing> <!-- Placeholder for message flow -->
    </semantic:task>

    <semantic:task id="task_ap_receive_invoice" name="Receive Invoice from Requestor">
      <semantic:incoming>flow_receiveinvoiceap_placeholder</semantic:incoming> <!-- Placeholder for message flow -->
      <semantic:outgoing>flow_receiveinvoiceap_to_decidepayment</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ap_decide_payment" name="Decide Payment Method">
      <semantic:incoming>flow_receiveinvoiceap_to_decidepayment</semantic:incoming>
      <semantic:outgoing>flow_decidepayment_to_gwpaymenttype</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_ap_payment_type" name="Payment Method?">
      <semantic:incoming>flow_decidepayment_to_gwpaymenttype</semantic:incoming>
      <semantic:outgoing>flow_gwpaymenttype_to_payfull</semantic:outgoing>
      <semantic:outgoing>flow_gwpaymenttype_to_payinstall</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_ap_pay_full" name="Perform Payment in Full">
      <semantic:incoming>flow_gwpaymenttype_to_payfull</semantic:incoming>
      <semantic:outgoing>flow_payfull_to_gwmergepayment</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ap_pay_install" name="Perform Payment in Installments">
      <semantic:incoming>flow_gwpaymenttype_to_payinstall</semantic:incoming>
      <semantic:outgoing>flow_payinstall_to_gwmergepayment</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_ap_merge_payment" name="Merge Payment Path">
      <semantic:incoming>flow_payfull_to_gwmergepayment</semantic:incoming>
      <semantic:incoming>flow_payinstall_to_gwmergepayment</semantic:incoming>
      <semantic:outgoing>flow_gwmergepayment_to_end</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:endEvent id="event_end_p2p_success" name="P2P Process Successfully Completed">
      <semantic:incoming>flow_gwmergepayment_to_end</semantic:incoming>
    </semantic:endEvent>

    <!-- Sequence Flows for Purchasing Department -->
    <semantic:sequenceFlow id="flow_start_to_determine" sourceRef="event_start_p2p" targetRef="task_req_determine_needs"/>
    <semantic:sequenceFlow id="flow_determine_to_createpr" sourceRef="task_req_determine_needs" targetRef="task_req_create_pr_draft"/>
    <semantic:sequenceFlow id="flow_createpr_to_sendpr" sourceRef="task_req_create_pr_draft" targetRef="task_req_send_pr_hod"/>
    <semantic:sequenceFlow id="flow_sendpr_placeholder" sourceRef="task_req_send_pr_hod" targetRef="task_hod_receive_pr" name="PR for Approval"/> <!-- Connects to HoD lane via message flow -->

    <semantic:sequenceFlow id="flow_receivepr_placeholder" sourceRef="task_hod_receive_pr" targetRef="task_hod_review_pr"/>
    <semantic:sequenceFlow id="flow_receivepr_to_reviewpr" sourceRef="task_hod_receive_pr" targetRef="task_hod_review_pr"/>
    <semantic:sequenceFlow id="flow_reviewpr_to_docpr" sourceRef="task_hod_review_pr" targetRef="task_hod_doc_pr_decision"/>
    <semantic:sequenceFlow id="flow_docpr_to_gwprdecision" sourceRef="task_hod_doc_pr_decision" targetRef="gw_hod_pr_decision"/>
    <semantic:sequenceFlow id="flow_gwprdecision_to_submitpr" sourceRef="gw_hod_pr_decision" targetRef="task_hod_submit_pr_decision" name="Any Decision"/>
    <semantic:sequenceFlow id="flow_submitpr_placeholder" sourceRef="task_hod_submit_pr_decision" targetRef="gw_req_pr_approved" name="PR Decision"/> <!-- Connects to Requestor lane via message flow -->

    <semantic:sequenceFlow id="flow_gwreqpr_placeholder" sourceRef="gw_req_pr_approved" targetRef="task_req_issue_formal_pr" name="Approved"/>
    <semantic:sequenceFlow id="flow_gwpr_to_issueformalpr" sourceRef="gw_req_pr_approved" targetRef="task_req_issue_formal_pr" name="Approved"/>
    <semantic:sequenceFlow id="flow_gwpr_to_determine" sourceRef="gw_req_pr_approved" targetRef="task_req_determine_needs" name="Not Approved"/>
    <semantic:sequenceFlow id="flow_issueformalpr_to_createpo" sourceRef="task_req_issue_formal_pr" targetRef="task_req_create_po"/>
    <semantic:sequenceFlow id="flow_createpo_to_sendpo" sourceRef="task_req_create_po" targetRef="task_req_send_po_poa"/>
    <semantic:sequenceFlow id="flow_sendpo_placeholder" sourceRef="task_req_send_po_poa" targetRef="task_poa_receive_po" name="PO for Approval"/> <!-- Connects to POA lane via message flow -->

    <semantic:sequenceFlow id="flow_receivepo_placeholder" sourceRef="task_poa_receive_po" targetRef="task_poa_review_po"/>
    <semantic:sequenceFlow id="flow_receivepo_to_reviewpo" sourceRef="task_poa_receive_po" targetRef="task_poa_review_po"/>
    <semantic:sequenceFlow id="flow_reviewpo_to_docpo" sourceRef="task_poa_review_po" targetRef="task_poa_doc_po_decision"/>
    <semantic:sequenceFlow id="flow_docpo_to_gwpodecision" sourceRef="task_poa_doc_po_decision" targetRef="gw_poa_po_decision"/>
    <semantic:sequenceFlow id="flow_gwpodecision_to_submitpo" sourceRef="gw_poa_po_decision" targetRef="task_poa_submit_po_decision" name="Any Decision"/>
    <semantic:sequenceFlow id="flow_submitpo_placeholder" sourceRef="task_poa_submit_po_decision" targetRef="gw_req_po_approved" name="PO Decision"/> <!-- Connects to Requestor lane via message flow -->

    <semantic:sequenceFlow id="flow_gwreqpo_placeholder" sourceRef="gw_req_po_approved" targetRef="task_req_dispatch_po_vendor" name="Approved"/>
    <semantic:sequenceFlow id="flow_gwpo_to_dispatch" sourceRef="gw_req_po_approved" targetRef="task_req_dispatch_po_vendor" name="Approved"/>
    <semantic:sequenceFlow id="flow_gwpo_to_determine" sourceRef="gw_req_po_approved" targetRef="task_req_determine_needs" name="Not Approved"/>
    <semantic:sequenceFlow id="flow_dispatch_placeholder" sourceRef="task_req_dispatch_po_vendor" targetRef="task_ven_receive_po" name="PO"/> <!-- Connects to Vendor pool via message flow -->

    <semantic:sequenceFlow id="flow_gwreqven_placeholder" sourceRef="gw_req_vendor_decision" targetRef="task_req_receive_items" name="Approved/Negotiated"/>
    <semantic:sequenceFlow id="flow_gwven_to_receiveitems" sourceRef="gw_req_vendor_decision" targetRef="task_req_receive_items" name="Approved/Negotiated"/>
    <semantic:sequenceFlow id="flow_gwven_to_searchvendor" sourceRef="gw_req_vendor_decision" targetRef="task_req_search_vendor" name="Rejected"/>
    <semantic:sequenceFlow id="flow_searchvendor_to_dispatch" sourceRef="task_req_search_vendor" targetRef="task_req_dispatch_po_vendor"/>
    <semantic:sequenceFlow id="flow_receiveitems_to_gwitemtype" sourceRef="task_req_receive_items" targetRef="gw_req_item_type"/>
    <semantic:sequenceFlow id="flow_gwitemtype_to_issuegr" sourceRef="gw_req_item_type" targetRef="task_req_issue_gr" name="Goods"/>
    <semantic:sequenceFlow id="flow_gwitemtype_to_issueses" sourceRef="gw_req_item_type" targetRef="task_req_issue_ses" name="Service"/>
    <semantic:sequenceFlow id="flow_issuegr_to_gwmergegrses" sourceRef="task_req_issue_gr" targetRef="gw_req_merge_gr_ses"/>
    <semantic:sequenceFlow id="flow_issueses_to_gwmergegrses" sourceRef="task_req_issue_ses" targetRef="gw_req_merge_gr_ses"/>
    <semantic:sequenceFlow id="flow_gwmergegrses_to_receiveinvoice" sourceRef="gw_req_merge_gr_ses" targetRef="task_req_receive_invoice"/>
