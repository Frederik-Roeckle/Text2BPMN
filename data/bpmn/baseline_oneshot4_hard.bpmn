<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="P2P_Process_Definition"
    targetNamespace="http://example.com/bpmn/p2p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_p2p_process">
    <semantic:participant id="participant_p2p_pool" name="P2P Process Pool" processRef="p2pProcess"/>
  </semantic:collaboration>

  <semantic:process id="p2pProcess" isExecutable="true">
    <semantic:laneSet id="p2pLaneSet">
      <semantic:lane id="lane_requestor" name="Requestor">
        <semantic:flowNodeRef>start_event_need_identified</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_determine_needs</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_prepare_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_pr_decision_received</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_issue_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_create_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_po_decision_received</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_dispatch_po_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_vendor_decision_on_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_search_new_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_receive_goods_services</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_delivery_type</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_issue_gr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_issue_ses</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_merge_gr_ses</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_receive_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_perform_3way_match</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_invoice_correct</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_return_invoice_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_receive_corrected_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_post_invoice_sap</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_send_invoice_to_ap</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_hod" name="Heads of Departments">
        <semantic:flowNodeRef>task_review_pr_hod</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_pr_review_outcome_hod</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_submit_pr_decision_hod</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_po_approver" name="PO Approvers">
        <semantic:flowNodeRef>task_review_po_poappr</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_po_review_outcome_poappr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_submit_po_decision_poappr</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_vendor" name="Vendor">
        <semantic:flowNodeRef>task_evaluate_po_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_vendor_po_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_inform_requestor_po_decision_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_deliver_goods_services_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_send_invoice_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_receive_wrong_invoice_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_correct_invoice_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_send_corrected_invoice_vendor</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_ap_processor" name="AP Processor">
        <semantic:flowNodeRef>task_ap_receive_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_decide_payment_method</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_perform_payment</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_event_p2p_completed</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <!-- Requestor Lane Elements -->
    <semantic:startEvent id="start_event_need_identified" name="Business Need Identified">
      <semantic:outgoing>flow_to_determine_needs</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_determine_needs" name="Determine Business Requirements">
      <semantic:incoming>flow_to_determine_needs</semantic:incoming>
      <semantic:incoming>flow_pr_not_approved_loop</semantic:incoming>
      <semantic:incoming>flow_po_rejected_loop</semantic:incoming>
      <semantic:outgoing>flow_to_prepare_pr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_prepare_pr" name="Prepare Purchase Requisition">
      <semantic:incoming>flow_to_prepare_pr</semantic:incoming>
      <semantic:outgoing>flow_pr_to_hod_review</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_pr_decision_received" name="PR Decision from HoD?">
      <semantic:incoming>flow_hod_decision_to_req</semantic:incoming>
      <semantic:outgoing>flow_pr_approved</semantic:outgoing>
      <semantic:outgoing>flow_pr_not_approved_loop</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_issue_pr" name="Formally Issue Approved PR">
      <semantic:incoming>flow_pr_approved</semantic:incoming>
      <semantic:outgoing>flow_to_create_po</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_create_po" name="Create Purchase Order">
      <semantic:incoming>flow_to_create_po</semantic:incoming>
      <semantic:incoming>flow_search_vendor_to_create_po</semantic:incoming>
      <semantic:outgoing>flow_po_to_po_approver_review</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_po_decision_received" name="PO Decision from Approvers?">
      <semantic:incoming>flow_po_approver_decision_to_req</semantic:incoming>
      <semantic:outgoing>flow_po_approved</semantic:outgoing>
      <semantic:outgoing>flow_po_rejected_loop</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_dispatch_po_vendor" name="Dispatch PO to Vendor">
      <semantic:incoming>flow_po_approved</semantic:incoming>
      <semantic:outgoing>flow_req_dispatch_po_to_vendor_eval</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_vendor_decision_on_po" name="Vendor Decision on PO?">
      <semantic:incoming>flow_vendor_decision_to_req</semantic:incoming>
      <semantic:outgoing>flow_vendor_po_approved_negotiated</semantic:outgoing>
      <semantic:outgoing>flow_vendor_po_rejected_req_path</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_search_new_vendor" name="Search for New Vendor">
      <semantic:incoming>flow_vendor_po_rejected_req_path</semantic:incoming>
      <semantic:outgoing>flow_search_vendor_to_create_po</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_receive_goods_services" name="Receive Goods/Services">
      <semantic:incoming>flow_vendor_po_approved_negotiated</semantic:incoming>
      <semantic:incoming>flow_vendor_delivers_to_req_receive</semantic:incoming> <!-- conceptually linked to vendor delivery -->
      <semantic:outgoing>flow_to_delivery_type_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_delivery_type" name="Goods or Service Received?">
      <semantic:incoming>flow_to_delivery_type_gw</semantic:incoming>
      <semantic:outgoing>flow_to_issue_gr</semantic:outgoing>
      <semantic:outgoing>flow_to_issue_ses</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_issue_gr" name="Issue Goods Receipt (GR)">
      <semantic:incoming>flow_to_issue_gr</semantic:incoming>
      <semantic:outgoing>flow_gr_to_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_issue_ses" name="Issue Service Entry Sheet (SES)">
      <semantic:incoming>flow_to_issue_ses</semantic:incoming>
      <semantic:outgoing>flow_ses_to_merge</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_merge_gr_ses" name="GR/SES Issued">
      <semantic:incoming>flow_gr_to_merge</semantic:incoming>
      <semantic:incoming>flow_ses_to_merge</semantic:incoming>
      <semantic:outgoing>flow_to_receive_invoice</semantic:outgoing> <!-- This might be too direct, receive invoice is separate trigger -->
    </semantic:exclusiveGateway>
    <semantic:task id="task_receive_invoice" name="Receive Invoice from Vendor">
      <semantic:incoming>flow_to_receive_invoice</semantic:incoming> <!-- From merge GR/SES to signify readiness -->
      <semantic:incoming>flow_vendor_sends_invoice_to_req</semantic:incoming> <!-- Actual invoice arrival -->
      <semantic:outgoing>flow_to_3way_match</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_perform_3way_match" name="Perform 3-Way Match">
      <semantic:incoming>flow_to_3way_match</semantic:incoming>
      <semantic:incoming>flow_corrected_invoice_to_3way_match</semantic:incoming>
      <semantic:outgoing>flow_to_invoice_correct_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_invoice_correct" name="Invoice Correct?">
      <semantic:incoming>flow_to_invoice_correct_gw</semantic:incoming>
      <semantic:outgoing>flow_invoice_correct_yes</semantic:outgoing>
      <semantic:outgoing>flow_invoice_correct_no</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_return_invoice_vendor" name="Return Wrong Invoice to Vendor">
      <semantic:incoming>flow_invoice_correct_no</semantic:incoming>
      <semantic:outgoing>flow_req_returns_invoice_to_vendor</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_receive_corrected_invoice" name="Receive Corrected Invoice">
      <semantic:incoming>flow_vendor_sends_corrected_invoice_to_req</semantic:incoming>
      <semantic:outgoing>flow_corrected_invoice_to_3way_match</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_post_invoice_sap" name="Post Invoice in SAP">
      <semantic:incoming>flow_invoice_correct_yes</semantic:incoming>
      <semantic:outgoing>flow_to_send_invoice_ap</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_send_invoice_to_ap" name="Send Invoice to AP Team">
      <semantic:incoming>flow_to_send_invoice_ap</semantic:incoming>
      <semantic:outgoing>flow_req_sends_invoice_to_ap</semantic:outgoing>
    </semantic:task>

    <!-- Heads of Departments Lane Elements -->
    <semantic:task id="task_review_pr_hod" name="Review PR (Needs, Budget, Form)">
      <semantic:incoming>flow_pr_to_hod_review</semantic:incoming>
      <semantic:outgoing>flow_to_hod_pr_review_outcome_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_pr_review_outcome_hod" name="PR Review Outcome?">
      <semantic:incoming>flow_to_hod_pr_review_outcome_gw</semantic:incoming>
      <semantic:outgoing>flow_hod_pr_approved</semantic:outgoing>
      <semantic:outgoing>flow_hod_pr_declined</semantic:outgoing>
      <semantic:outgoing>flow_hod_pr_rejected_incomplete</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_submit_pr_decision_hod" name="Submit PR Decision to Requestor">
      <semantic:incoming>flow_hod_pr_approved</semantic:incoming>
      <semantic:incoming>flow_hod_pr_declined</semantic:incoming>
      <semantic:incoming>flow_hod_pr_rejected_incomplete</semantic:incoming>
      <semantic:outgoing>flow_hod_decision_to_req</semantic:outgoing>
    </semantic:task>

    <!-- PO Approvers Lane Elements -->
    <semantic:task id="task_review_po_poappr" name="Review PO Specifications">
      <semantic:incoming>flow_po_to_po_approver_review</semantic:incoming>
      <semantic:outgoing>flow_to_poappr_po_review_outcome_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_po_review_outcome_poappr" name="PO Review Outcome?">
      <semantic:incoming>flow_to_poappr_po_review_outcome_gw</semantic:incoming>
      <semantic:outgoing>flow_poappr_po_approved</semantic:outgoing>
      <semantic:outgoing>flow_poappr_po_rejected</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_submit_po_decision_poappr" name="Submit PO Decision to Requestor">
      <semantic:incoming>flow_poappr_po_approved</semantic:incoming>
      <semantic:incoming>flow_poappr_po_rejected</semantic:incoming>
      <semantic:outgoing>flow_po_approver_decision_to_req</semantic:outgoing>
    </semantic:task>

    <!-- Vendor Lane Elements -->
    <semantic:task id="task_evaluate_po_vendor" name="Evaluate Received PO">
      <semantic:incoming>flow_req_dispatch_po_to_vendor_eval</semantic:incoming>
      <semantic:outgoing>flow_to_vendor_po_decision_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_vendor_po_decision" name="Vendor PO Decision?">
      <semantic:incoming>flow_to_vendor_po_decision_gw</semantic:incoming>
      <semantic:outgoing>flow_vendor_po_approved</semantic:outgoing>
      <semantic:outgoing>flow_vendor_po_negotiated</semantic:outgoing>
      <semantic:outgoing>flow_vendor_po_rejected</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_inform_requestor_po_decision_vendor" name="Inform Requestor of PO Decision">
      <semantic:incoming>flow_vendor_po_approved</semantic:incoming>
      <semantic:incoming>flow_vendor_po_negotiated</semantic:incoming>
      <semantic:incoming>flow_vendor_po_rejected</semantic:incoming>
      <semantic:outgoing>flow_vendor_decision_to_req</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_deliver_goods_services_vendor" name="Deliver Goods/Services">
      <!-- This task is triggered if PO is approved/negotiated. It's parallel to Inform Requestor or sequential after it.
           Let's assume it can start after the decision to approve/negotiate is made.
           The flow from gateway_vendor_po_decision for approved/negotiated should also go here.
           And it should also connect to task_inform_requestor_po_decision_vendor.
           This implies a parallel split after vendor decides to approve/negotiate.
           For simplicity, let's make Inform Decision first, then Deliver if positive.
      -->
      <semantic:incoming>flow_vendor_decision_to_deliver</semantic:incoming> <!-- This new flow comes from Inform task if positive -->
      <semantic:outgoing>flow_vendor_delivers_to_req_receive</semantic:outgoing>
      <semantic:outgoing>flow_to_vendor_send_invoice</semantic:outgoing> <!-- Invoice sent after delivery -->
    </semantic:task>
    <semantic:task id="task_send_invoice_vendor" name="Send Invoice to Requestor">
      <semantic:incoming>flow_to_vendor_send_invoice</semantic:incoming>
      <semantic:outgoing>flow_vendor_sends_invoice_to_req</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_receive_wrong_invoice_vendor" name="Receive Wrong Invoice">
      <semantic:incoming>flow_req_returns_invoice_to_vendor</semantic:incoming>
      <semantic:outgoing>flow_to_vendor_correct_invoice</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_correct_invoice_vendor" name="Correct Invoice">
      <semantic:incoming>flow_to_vendor_correct_invoice</semantic:incoming>
      <semantic:outgoing>flow_to_vendor_send_corrected_invoice</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_send_corrected_invoice_vendor" name="Send Corrected Invoice to Requestor">
      <semantic:incoming>flow_to_vendor_send_corrected_invoice</semantic:incoming>
      <semantic:outgoing>flow_vendor_sends_corrected_invoice_to_req</semantic:outgoing>
    </semantic:task>

    <!-- AP Processor Lane Elements -->
    <semantic:task id="task_ap_receive_invoice" name="AP Receives Invoice for Payment">
      <semantic:incoming>flow_req_sends_invoice_to_ap</semantic:incoming>
      <semantic:outgoing>flow_to_ap_decide_payment</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ap_decide_payment_method" name="AP Decides Payment Method">
      <semantic:incoming>flow_to_ap_decide_payment</semantic:incoming>
      <semantic:outgoing>flow_to_ap_perform_payment</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ap_perform_payment" name="AP Performs Payment to Vendor">
      <semantic:incoming>flow_to_ap_perform_payment</semantic:incoming>
      <semantic:outgoing>flow_to_p2p_end</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="end_event_p2p_completed" name="P2P Process Successfully Completed">
      <semantic:incoming>flow_to_p2p_end</semantic:incoming>
    </semantic:endEvent>

    <!-- Sequence Flows -->
    <semantic:sequenceFlow id="flow_to_determine_needs" sourceRef="start_event_need_identified" targetRef="task_determine_needs"/>
    <semantic:sequenceFlow id="flow_to_prepare_pr" sourceRef="task_determine_needs" targetRef="task_prepare_pr"/>
    <semantic:sequenceFlow id="flow_pr_to_hod_review" sourceRef="task_prepare_pr" targetRef="task_review_pr_hod"/>
    <semantic:sequenceFlow id="flow_to_hod_pr_review_outcome_gw" sourceRef="task_review_pr_hod" targetRef="gateway_pr_review_outcome_hod"/>
    <semantic:sequenceFlow id="flow_hod_pr_approved" name="Approved" sourceRef="gateway_pr_review_outcome_hod" targetRef="task_submit_pr_decision_hod"/>
    <semantic:sequenceFlow id="flow_hod_pr_declined" name="Declined" sourceRef="gateway_pr_review_outcome_hod" targetRef="task_submit_pr_decision_hod"/>
    <semantic:sequenceFlow id="flow_hod_pr_rejected_incomplete" name="Rejected Incomplete" sourceRef="gateway_pr_review_outcome_hod" targetRef="task_submit_pr_decision_hod"/>
    <semantic:sequenceFlow id="flow_hod_decision_to_req" sourceRef="task_submit_pr_decision_hod" targetRef="gateway_pr_decision_received"/>
    <semantic:sequenceFlow id="flow_pr_approved" name="PR Approved" sourceRef="gateway_pr_decision_received" targetRef="task_issue_pr"/>
    <semantic:sequenceFlow id="flow_pr_not_approved_loop" name="PR Not Approved" sourceRef="gateway_pr_decision_received" targetRef="task_determine_needs"/>
    <semantic:sequenceFlow id="flow_to_create_po" sourceRef="task_issue_pr" targetRef="task_create_po"/>
    <semantic:sequenceFlow id="flow_po_to_po_approver_review" sourceRef="task_create_po" targetRef="task_review_po_poappr"/>
    <semantic:sequenceFlow id="flow_to_poappr_po_review_outcome_gw" sourceRef="task_review_po_poappr" targetRef="gateway_po_review_outcome_poappr"/>
    <semantic:sequenceFlow id="flow_poappr_po_approved" name="Approved" sourceRef="gateway_po_review_outcome_poappr" targetRef="task_submit_po_decision_poappr"/>
    <semantic:sequenceFlow id="flow_poappr_po_rejected" name="Rejected" sourceRef="gateway_po_review_outcome_poappr" targetRef="task_submit_po_decision_poappr"/>
    <semantic:sequenceFlow id="flow_po_approver_decision_to_req" sourceRef="task_submit_po_decision_poappr" targetRef="gateway_po_decision_received"/>
    <semantic:sequenceFlow id="flow_po_approved" name="PO Approved" sourceRef="gateway_po_decision_received" targetRef="task_dispatch_po_vendor"/>
    <semantic:sequenceFlow id="flow_po_rejected_loop" name="PO Rejected" sourceRef="gateway_po_decision_received" targetRef="task_determine_needs"/>
    <semantic:sequenceFlow id="flow_req_dispatch_po_to_vendor_eval" sourceRef="task_dispatch_po_vendor" targetRef="task_evaluate_po_vendor"/>
    <semantic:sequenceFlow id="flow_to_vendor_po_decision_gw" sourceRef="task_evaluate_po_vendor" targetRef="gateway_vendor_po_decision"/>
    <semantic:sequenceFlow id="flow_vendor_po_approved" name="Approved" sourceRef="gateway_vendor_po_decision" targetRef="task_inform_requestor_po_decision_vendor"/>
    <semantic:sequenceFlow id="flow_vendor_po_negotiated" name="Negotiated" sourceRef="gateway_vendor_po_decision" targetRef="task_inform_requestor_po_decision_vendor"/>
    <semantic:sequenceFlow id="flow_vendor_po_rejected" name="Rejected" sourceRef="gateway_vendor_po_decision" targetRef="task_inform_requestor_po_decision_vendor"/>
    <semantic:sequenceFlow id="flow_vendor_decision_to_req" sourceRef="task_inform_requestor_po_decision_vendor" targetRef="gateway_vendor_decision_on_po"/>

    <!-- Flow from Inform to Deliver if positive -->
    <semantic:sequenceFlow id="flow_vendor_decision_to_deliver" sourceRef="task_inform_requestor_po_decision_vendor" targetRef="task_deliver_goods_services_vendor"/>
    <!-- This flow_vendor_decision_to_deliver should ideally be conditional based on the decision (approved/negotiated).
         A gateway after task_inform_requestor_po_decision_vendor or changing logic:
         gateway_vendor_po_decision -> (if appr/neg) -> task_deliver_goods_services_vendor -> task_inform_requestor_po_decision_vendor
         Let's simplify: if vendor informs, and it was positive, they proceed to deliver.
         The current model: gateway -> inform -> gateway_vendor_decision_on_po (in requestor).
         And gateway -> inform -> deliver (in vendor). This means inform has two outgoing if positive.
         This is better:
         task_inform_requestor_po_decision_vendor has one outgoing to Requestor.
         task_deliver_goods_services_vendor is triggered if gateway_vendor_po_decision is appr/neg.
         So, flow_vendor_po_approved and flow_vendor_po_negotiated should also target task_deliver_goods_services_vendor if they are parallel.
         Or sequential: gateway -> task_deliver_goods_services (if appr/neg) -> task_inform_requestor -> task_send_invoice.
         The text: "If... approved or negotiated, he has to deliver... and send the invoice". "document decision to requestor" is separate.
         Let's make Inform and Deliver parallel if PO is good:
         Remove flow_vendor_decision_to_deliver.
         Add new flows from gateway_vendor_po_decision if appr/neg to task_deliver_goods_services_vendor.
    -->
    <semantic:sequenceFlow id="flow_vendor_po_approved_to_deliver" sourceRef="gateway_vendor_po_decision" targetRef="task_deliver_goods_services_vendor"/>
    <semantic:sequenceFlow id="flow_vendor_po_negotiated_to_deliver" sourceRef="gateway_vendor_po_decision" targetRef="task_deliver_goods_services_vendor"/>


    <semantic:sequenceFlow id="flow_vendor_po_approved_negotiated" name="Vendor Approved/Negotiated" sourceRef="gateway_vendor_decision_on_po" targetRef="task_receive_goods_services"/>
    <semantic:sequenceFlow id="flow_vendor_po_rejected_req_path" name="Vendor Rejected PO" sourceRef="gateway_vendor_decision_on_po" targetRef="task_search_new_vendor"/>
    <semantic:sequenceFlow id="flow_search_vendor_to_create_po" sourceRef="task_search_new_vendor" targetRef="task_create_po"/>

    <semantic:sequenceFlow id="flow_vendor_delivers_to_req_receive" sourceRef="task_deliver_goods_services_vendor" targetRef="task_receive_goods_services"/>
    <semantic:sequenceFlow id="flow_to_delivery_type_gw" sourceRef="task_receive_goods_services" targetRef="gateway_delivery_type"/>
    <semantic:sequenceFlow id="flow_to_issue_gr" name="Goods" sourceRef="gateway_delivery_type" targetRef="task_issue_gr"/>
    <semantic:sequenceFlow id="flow_to_issue_ses" name="Service" sourceRef="gateway_delivery_type" targetRef="task_issue_ses"/>
