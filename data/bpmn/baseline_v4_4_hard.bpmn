<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="P2P_Process_Definition"
    targetNamespace="http://example.com/bpmn/p2p-process"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_p2p">
    <semantic:participant id="participant_purchasing" name="Purchasing Department" processRef="purchasing_process"/>
    <semantic:participant id="participant_vendor" name="Vendor" processRef="vendor_process"/>
    <semantic:participant id="participant_finance" name="Finance Department" processRef="finance_process"/>

    <semantic:messageFlow id="msg_pr_to_hod" name="PR for Approval" sourceRef="T3_Req" targetRef="SE_HoD_Start"/>
    <semantic:messageFlow id="msg_pr_decision_to_req" name="PR Decision" sourceRef="T_HoD_SendDecision" targetRef="T4_Req"/>
    <semantic:messageFlow id="msg_po_to_po_approver" name="PO for Approval" sourceRef="T7_Req" targetRef="SE_PO_App_Start"/>
    <semantic:messageFlow id="msg_po_decision_to_req" name="PO Decision" sourceRef="T_PO_App_SendDecision" targetRef="T8_Req"/>
    <semantic:messageFlow id="msg_po_to_vendor" name="Dispatched PO" sourceRef="T9_Req" targetRef="SE_Vendor_PO_Start"/>
    <semantic:messageFlow id="msg_vendor_po_decision_to_req" name="Vendor PO Decision" sourceRef="T_Vendor_Doc_Decision" targetRef="T10_Req"/>
    <semantic:messageFlow id="msg_goods_services_to_req" name="Goods/Services Delivery" sourceRef="T_Vendor_Deliver" targetRef="T11_Req"/>
    <semantic:messageFlow id="msg_invoice_to_req" name="Vendor Invoice" sourceRef="T_Vendor_Send_Invoice" targetRef="T14_Req"/>
    <semantic:messageFlow id="msg_wrong_invoice_to_vendor" name="Incorrect Invoice" sourceRef="T17_Req" targetRef="SE_Vendor_InvoiceCorr_Start"/>
    <semantic:messageFlow id="msg_corrected_invoice_to_req" name="Corrected Invoice" sourceRef="T_Vendor_Send_Corrected_Invoice" targetRef="T18_Req"/>
    <semantic:messageFlow id="msg_invoice_to_ap" name="Invoice for Payment" sourceRef="T19_Req" targetRef="SE_AP_Start"/>
  </semantic:collaboration>

  <!-- Purchasing Process -->
  <semantic:process id="purchasing_process" isExecutable="true">
    <semantic:laneSet id="purchasing_laneSet">
      <semantic:lane id="lane_requestor" name="Requestor">
        <semantic:flowNodeRef>SE1_Req_Start</semantic:flowNodeRef>
        <semantic:flowNodeRef>T1_Req</semantic:flowNodeRef>
        <semantic:flowNodeRef>T2_Req</semantic:flowNodeRef>
        <semantic:flowNodeRef>T3_Req</semantic:flowNodeRef>
        <semantic:flowNodeRef>T4_Req</semantic:flowNodeRef>
        <semantic:flowNodeRef>GW1_Req_PR_Approved_HoD</semantic:flowNodeRef>
        <semantic:flowNodeRef>T5_Req_IssuePR</semantic:flowNodeRef>
        <semantic:flowNodeRef>BE_PR_Discrepancy</semantic:flowNodeRef>
        <semantic:flowNodeRef>T5_BE_Handle_PR_Discrepancy</semantic:flowNodeRef>
        <semantic:flowNodeRef>T6_Req_CreatePO</semantic:flowNodeRef>
        <semantic:flowNodeRef>T7_Req</semantic:flowNodeRef>
        <semantic:flowNodeRef>T8_Req</semantic:flowNodeRef>
        <semantic:flowNodeRef>GW2_Req_PO_Approved_App</semantic:flowNodeRef>
        <semantic:flowNodeRef>T9_Req</semantic:flowNodeRef>
        <semantic:flowNodeRef>T10_Req</semantic:flowNodeRef>
        <semantic:flowNodeRef>GW3_Req_Vendor_Action</semantic:flowNodeRef>
        <semantic:flowNodeRef>T_Req_SearchNewVendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>T11_Req</semantic:flowNodeRef>
        <semantic:flowNodeRef>GW4_Req_ItemType</semantic:flowNodeRef>
        <semantic:flowNodeRef>T12_Req_IssueGR</semantic:flowNodeRef>
        <semantic:flowNodeRef>T13_Req_IssueSES</semantic:flowNodeRef>
        <semantic:flowNodeRef>GW_Req_Merge_GR_SES</semantic:flowNodeRef>
        <semantic:flowNodeRef>T14_Req</semantic:flowNodeRef>
        <semantic:flowNodeRef>T15_Req_3WayMatch</semantic:flowNodeRef>
        <semantic:flowNodeRef>GW5_Req_InvoiceCorrect</semantic:flowNodeRef>
        <semantic:flowNodeRef>T16_Req_PostInvoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>T17_Req</semantic:flowNodeRef>
        <semantic:flowNodeRef>T18_Req</semantic:flowNodeRef>
        <semantic:flowNodeRef>T19_Req</semantic:flowNodeRef>
        <semantic:flowNodeRef>EE_Req_Process_Ends_After_Invoice_Sent</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_hod" name="Heads of Departments">
        <semantic:flowNodeRef>SE_HoD_Start</semantic:flowNodeRef>
        <semantic:flowNodeRef>T_HoD_ReviewPR</semantic:flowNodeRef>
        <semantic:flowNodeRef>GW_HoD_ReviewOutcome</semantic:flowNodeRef>
        <semantic:flowNodeRef>T_HoD_Doc_App</semantic:flowNodeRef>
        <semantic:flowNodeRef>T_HoD_Doc_Dec</semantic:flowNodeRef>
        <semantic:flowNodeRef>T_HoD_Doc_Rej</semantic:flowNodeRef>
        <semantic:flowNodeRef>GW_HoD_Merge_Doc</semantic:flowNodeRef>
        <semantic:flowNodeRef>T_HoD_SendDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>EE_HoD_Review_Complete</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_po_approvers" name="PO Approvers">
        <semantic:flowNodeRef>SE_PO_App_Start</semantic:flowNodeRef>
        <semantic:flowNodeRef>T_PO_App_ReviewPO</semantic:flowNodeRef>
        <semantic:flowNodeRef>GW_PO_App_Decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>T_PO_App_Doc_App</semantic:flowNodeRef>
        <semantic:flowNodeRef>T_PO_App_Doc_Rej</semantic:flowNodeRef>
        <semantic:flowNodeRef>GW_PO_App_Merge_Doc</semantic:flowNodeRef>
        <semantic:flowNodeRef>T_PO_App_SendDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>EE_PO_App_Review_Complete</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <!-- Requestor Lane Elements -->
    <semantic:startEvent id="SE1_Req_Start" name="P2P Process Initiated">
      <semantic:outgoing>flow_req_s1_t1</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="T1_Req" name="Determine Business Requirements">
      <semantic:incoming>flow_req_s1_t1</semantic:incoming>
      <semantic:incoming>flow_req_gw1_t1</semantic:incoming>
      <semantic:incoming>flow_req_gw2_t1</semantic:incoming>
      <semantic:outgoing>flow_req_t1_t2</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T2_Req" name="Create PR Draft">
      <semantic:incoming>flow_req_t1_t2</semantic:incoming>
      <semantic:outgoing>flow_req_t2_t3</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T3_Req" name="Submit PR for HoD Approval"> <!-- Source for msg_pr_to_hod -->
      <semantic:incoming>flow_req_t2_t3</semantic:incoming>
      <semantic:outgoing>flow_req_t3_t4</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T4_Req" name="Receive PR Decision from HoD"> <!-- Target for msg_pr_decision_to_req -->
      <semantic:incoming>flow_req_t3_t4</semantic:incoming>
      <semantic:outgoing>flow_req_t4_gw1</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW1_Req_PR_Approved_HoD" name="PR Approved by HoD?">
      <semantic:incoming>flow_req_t4_gw1</semantic:incoming>
      <semantic:outgoing>flow_req_gw1_t5</semantic:outgoing> <!-- Yes -->
      <semantic:outgoing>flow_req_gw1_t1</semantic:outgoing> <!-- No -->
    </semantic:exclusiveGateway>
    <semantic:task id="T5_Req_IssuePR" name="Finalize and Issue PR">
      <semantic:incoming>flow_req_gw1_t5</semantic:incoming>
      <semantic:outgoing>flow_req_t5_t6</semantic:outgoing>
    </semantic:task>
    <semantic:boundaryEvent id="BE_PR_Discrepancy" name="PR Discrepancy Occurred" attachedToRef="T5_Req_IssuePR" cancelActivity="false">
      <semantic:outgoing>flow_be_pr_discrepancy_handle</semantic:outgoing>
      <semantic:errorEventDefinition id="error_pr_discrepancy" />
    </semantic:boundaryEvent>
    <semantic:task id="T5_BE_Handle_PR_Discrepancy" name="Log PR Discrepancy">
        <semantic:incoming>flow_be_pr_discrepancy_handle</semantic:incoming>
        <semantic:outgoing>flow_be_pr_discrepancy_end</semantic:outgoing>
    </semantic:task>
     <semantic:endEvent id="EE_PR_Discrepancy_Logged" name="PR Discrepancy Logged">
        <semantic:incoming>flow_be_pr_discrepancy_end</semantic:incoming>
    </semantic:endEvent>
    <semantic:task id="T6_Req_CreatePO" name="Create Purchase Order">
      <semantic:incoming>flow_req_t5_t6</semantic:incoming>
      <semantic:outgoing>flow_req_t6_t7</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T7_Req" name="Submit PO for Approver Review"> <!-- Source for msg_po_to_po_approver -->
      <semantic:incoming>flow_req_t6_t7</semantic:incoming>
      <semantic:outgoing>flow_req_t7_t8</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T8_Req" name="Receive PO Decision from Approvers"> <!-- Target for msg_po_decision_to_req -->
      <semantic:incoming>flow_req_t7_t8</semantic:incoming>
      <semantic:outgoing>flow_req_t8_gw2</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW2_Req_PO_Approved_App" name="PO Approved by PO Approvers?">
      <semantic:incoming>flow_req_t8_gw2</semantic:incoming>
      <semantic:outgoing>flow_req_gw2_t9</semantic:outgoing> <!-- Yes -->
      <semantic:outgoing>flow_req_gw2_t1</semantic:outgoing> <!-- No -->
    </semantic:exclusiveGateway>
    <semantic:task id="T9_Req" name="Dispatch PO to Vendor"> <!-- Source for msg_po_to_vendor -->
      <semantic:incoming>flow_req_gw2_t9</semantic:incoming>
      <semantic:incoming>flow_req_searchvendor_t9</semantic:incoming>
      <semantic:outgoing>flow_req_t9_t10</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T10_Req" name="Receive Vendor Communication on PO"> <!-- Target for msg_vendor_po_decision_to_req -->
      <semantic:incoming>flow_req_t9_t10</semantic:incoming>
      <semantic:outgoing>flow_req_t10_gw3</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW3_Req_Vendor_Action" name="Vendor Action on PO?">
      <semantic:incoming>flow_req_t10_gw3</semantic:incoming>
      <semantic:outgoing>flow_req_gw3_t11</semantic:outgoing> <!-- Approved/Negotiated -->
      <semantic:outgoing>flow_req_gw3_searchvendor</semantic:outgoing> <!-- Rejected -->
    </semantic:exclusiveGateway>
    <semantic:task id="T_Req_SearchNewVendor" name="Search for New Vendor">
      <semantic:incoming>flow_req_gw3_searchvendor</semantic:incoming>
      <semantic:outgoing>flow_req_searchvendor_t9</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T11_Req" name="Receive Goods/Services"> <!-- Target for msg_goods_services_to_req -->
      <semantic:incoming>flow_req_gw3_t11</semantic:incoming>
      <semantic:outgoing>flow_req_t11_gw4</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW4_Req_ItemType" name="Item Type?">
      <semantic:incoming>flow_req_t11_gw4</semantic:incoming>
      <semantic:outgoing>flow_req_gw4_t12</semantic:outgoing> <!-- Goods -->
      <semantic:outgoing>flow_req_gw4_t13</semantic:outgoing> <!-- Service -->
    </semantic:exclusiveGateway>
    <semantic:task id="T12_Req_IssueGR" name="Issue Goods Receipt">
      <semantic:incoming>flow_req_gw4_t12</semantic:incoming>
      <semantic:outgoing>flow_req_t12_gw_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T13_Req_IssueSES" name="Issue Service Entry Sheet">
      <semantic:incoming>flow_req_gw4_t13</semantic:incoming>
      <semantic:outgoing>flow_req_t13_gw_merge</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW_Req_Merge_GR_SES">
        <semantic:incoming>flow_req_t12_gw_merge</semantic:incoming>
        <semantic:incoming>flow_req_t13_gw_merge</semantic:incoming>
        <semantic:outgoing>flow_req_gw_merge_t14</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="T14_Req" name="Receive Vendor Invoice"> <!-- Target for msg_invoice_to_req -->
      <semantic:incoming>flow_req_gw_merge_t14</semantic:incoming>
      <semantic:outgoing>flow_req_t14_t15</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T15_Req_3WayMatch" name="Perform 3-Way Match">
      <semantic:incoming>flow_req_t14_t15</semantic:incoming>
      <semantic:incoming>flow_req_t18_t15</semantic:incoming>
      <semantic:outgoing>flow_req_t15_gw5</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW5_Req_InvoiceCorrect" name="Invoice Correct?">
      <semantic:incoming>flow_req_t15_gw5</semantic:incoming>
      <semantic:outgoing>flow_req_gw5_t16</semantic:outgoing> <!-- Yes -->
      <semantic:outgoing>flow_req_gw5_t17</semantic:outgoing> <!-- No -->
    </semantic:exclusiveGateway>
    <semantic:task id="T16_Req_PostInvoice" name="Post Invoice in SAP System">
      <semantic:incoming>flow_req_gw5_t16</semantic:incoming>
      <semantic:outgoing>flow_req_t16_t19</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T17_Req" name="Return Incorrect Invoice to Vendor"> <!-- Source for msg_wrong_invoice_to_vendor -->
      <semantic:incoming>flow_req_gw5_t17</semantic:incoming>
      <semantic:outgoing>flow_req_t17_t18</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T18_Req" name="Receive Corrected Invoice"> <!-- Target for msg_corrected_invoice_to_req -->
      <semantic:incoming>flow_req_t17_t18</semantic:incoming>
      <semantic:outgoing>flow_req_t18_t15</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T19_Req" name="Forward Invoice to AP Team"> <!-- Source for msg_invoice_to_ap -->
      <semantic:incoming>flow_req_t16_t19</semantic:incoming>
      <semantic:outgoing>flow_req_t19_ee1</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="EE_Req_Process_Ends_After_Invoice_Sent" name="Requestor Tasks Concluded">
      <semantic:incoming>flow_req_t19_ee1</semantic:incoming>
    </semantic:endEvent>

    <!-- HoD Lane Elements -->
    <semantic:startEvent id="SE_HoD_Start" name="Start PR Approval by HoD"> <!-- Target for msg_pr_to_hod -->
      <semantic:outgoing>flow_hod_s1_t1</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="T_HoD_ReviewPR" name="Review Purchase Requisition">
      <semantic:incoming>flow_hod_s1_t1</semantic:incoming>
      <semantic:outgoing>flow_hod_t1_gw1</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW_HoD_ReviewOutcome" name="PR Review Outcome?">
      <semantic:incoming>flow_hod_t1_gw1</semantic:incoming>
      <semantic:outgoing>flow_hod_gw1_app</semantic:outgoing> <!-- Approved -->
      <semantic:outgoing>flow_hod_gw1_dec</semantic:outgoing> <!-- Declined -->
      <semantic:outgoing>flow_hod_gw1_rej</semantic:outgoing> <!-- Rejected (Incomplete) -->
    </semantic:exclusiveGateway>
    <semantic:task id="T_HoD_Doc_App" name="Document PR Approval">
      <semantic:incoming>flow_hod_gw1_app</semantic:incoming>
      <semantic:outgoing>flow_hod_doc_app_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T_HoD_Doc_Dec" name="Document PR Declination">
      <semantic:incoming>flow_hod_gw1_dec</semantic:incoming>
      <semantic:outgoing>flow_hod_doc_dec_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T_HoD_Doc_Rej" name="Document PR Rejection (Incomplete)">
      <semantic:incoming>flow_hod_gw1_rej</semantic:incoming>
      <semantic:outgoing>flow_hod_doc_rej_merge</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW_HoD_Merge_Doc">
        <semantic:incoming>flow_hod_doc_app_merge</semantic:incoming>
        <semantic:incoming>flow_hod_doc_dec_merge</semantic:incoming>
        <semantic:incoming>flow_hod_doc_rej_merge</semantic:incoming>
        <semantic:outgoing>flow_hod_merge_send</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="T_HoD_SendDecision" name="Communicate PR Decision to Requestor"> <!-- Source for msg_pr_decision_to_req -->
      <semantic:incoming>flow_hod_merge_send</semantic:incoming>
      <semantic:outgoing>flow_hod_send_ee1</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="EE_HoD_Review_Complete" name="HoD PR Review Concluded">
      <semantic:incoming>flow_hod_send_ee1</semantic:incoming>
    </semantic:endEvent>

    <!-- PO Approvers Lane Elements -->
    <semantic:startEvent id="SE_PO_App_Start" name="Start PO Approval by PO Approvers"> <!-- Target for msg_po_to_po_approver -->
      <semantic:outgoing>flow_po_app_s1_t1</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="T_PO_App_ReviewPO" name="Review Purchase Order">
      <semantic:incoming>flow_po_app_s1_t1</semantic:incoming>
      <semantic:outgoing>flow_po_app_t1_gw1</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW_PO_App_Decision" name="PO Approved?">
      <semantic:incoming>flow_po_app_t1_gw1</semantic:incoming>
      <semantic:outgoing>flow_po_app_gw1_app</semantic:outgoing> <!-- Approved -->
      <semantic:outgoing>flow_po_app_gw1_rej</semantic:outgoing> <!-- Rejected -->
    </semantic:exclusiveGateway>
    <semantic:task id="T_PO_App_Doc_App" name="Document PO Approval">
      <semantic:incoming>flow_po_app_gw1_app</semantic:incoming>
      <semantic:outgoing>flow_po_app_doc_app_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="T_PO_App_Doc_Rej" name="Document PO Rejection">
      <semantic:incoming>flow_po_app_gw1_rej</semantic:incoming>
      <semantic:outgoing>flow_po_app_doc_rej_merge</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW_PO_App_Merge_Doc">
        <semantic:incoming>flow_po_app_doc_app_merge</semantic:incoming>
        <semantic:incoming>flow_po_app_doc_rej_merge</semantic:incoming>
        <semantic:outgoing>flow_po_app_merge_send</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="T_PO_App_SendDecision" name="Communicate PO Decision to Requestor"> <!-- Source for msg_po_decision_to_req -->
      <semantic:incoming>flow_po_app_merge_send</semantic:incoming>
      <semantic:outgoing>flow_po_app_send_ee1</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="EE_PO_App_Review_Complete" name="PO Approval Concluded">
      <semantic:incoming>flow_po_app_send_ee1</semantic:incoming>
    </semantic:endEvent>

    <!-- Sequence Flows for Purchasing Process -->
    <semantic:sequenceFlow id="flow_req_s1_t1" sourceRef="SE1_Req_Start" targetRef="T1_Req"/>
    <semantic:sequenceFlow id="flow_req_t1_t2" sourceRef="T1_Req" targetRef="T2_Req"/>
    <semantic:sequenceFlow id="flow_req_t2_t3" sourceRef="T2_Req" targetRef="T3_Req"/>
    <semantic:sequenceFlow id="flow_req_t3_t4" sourceRef="T3_Req" targetRef="T4_Req"/>
    <semantic:sequenceFlow id="flow_req_t4_gw1" sourceRef="T4_Req" targetRef="GW1_Req_PR_Approved_HoD"/>
    <semantic:sequenceFlow id="flow_req_gw1_t5" name="Yes" sourceRef="GW1_Req_PR_Approved_HoD" targetRef="T5_Req_IssuePR"/>
    <semantic:sequenceFlow id="flow_req_gw1_t1" name="No" sourceRef="GW1_Req_PR_Approved_HoD" targetRef="T1_Req"/>
    <semantic:sequenceFlow id="flow_be_pr_discrepancy_handle" sourceRef="BE_PR_Discrepancy" targetRef="T5_BE_Handle_PR_Discrepancy"/>
    <semantic:sequenceFlow id="flow_be_pr_discrepancy_end" sourceRef="T5_BE_Handle_PR_Discrepancy" targetRef="EE_PR_Discrepancy_Logged"/>
    <semantic:sequenceFlow id="flow_req_t5_t6" sourceRef="T5_Req_IssuePR" targetRef="T6_Req_CreatePO"/>
    <semantic:sequenceFlow id="flow_req_t6_t7" sourceRef="T6_Req_CreatePO" targetRef="T7_Req"/>
    <semantic:sequenceFlow id="flow_req_t7_t8" sourceRef="T7_Req" targetRef="T8_Req"/>
    <semantic:sequenceFlow id="flow_req_t8_gw2" sourceRef="T8_Req" targetRef="GW2_Req_PO_Approved_App"/>
    <semantic:sequenceFlow id="flow_req_gw2_t9" name="Yes" sourceRef="GW2_Req_PO_Approved_App" targetRef="T9_Req"/>
    <semantic:sequenceFlow id="flow_req_gw2_t1" name="No" sourceRef="GW2_Req_PO_Approved_App" targetRef="T1_Req"/>
    <semantic:sequenceFlow id="flow_req_t9_t10" sourceRef="T9_Req" targetRef="T10_Req"/>
    <semantic:sequenceFlow id="flow_req_t10_gw3" sourceRef="T10_Req" targetRef="GW3_Req_Vendor_Action"/>
    <semantic:sequenceFlow id="flow_req_gw3_t11" name="Approved/Negotiated" sourceRef="GW3_Req_Vendor_Action" targetRef="T11_Req"/>
    <semantic:sequenceFlow id="flow_req_gw3_searchvendor" name="Rejected" sourceRef="GW3_Req_Vendor_Action" targetRef="T_Req_SearchNewVendor"/>
    <semantic:sequenceFlow id="flow_req_searchvendor_t9" sourceRef="T_Req_SearchNewVendor" targetRef="T9_Req"/>
    <semantic:sequenceFlow id="flow_req_t11_gw4" sourceRef="T11_Req" targetRef="GW4_Req_ItemType"/>
    <semantic:sequenceFlow id="flow_req_gw4_t12" name="Goods" sourceRef="GW4_Req_ItemType" targetRef="T12_Req_IssueGR"/>
    <semantic:sequenceFlow id="flow_req_gw4_t13" name="Service" sourceRef="GW4_Req_ItemType" targetRef="T13_Req_IssueSES"/>
