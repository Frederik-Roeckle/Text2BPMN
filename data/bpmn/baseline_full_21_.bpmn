<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="ProductionProcessDefs"
    targetNamespace="http://example.com/bpmn/production-process"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_ProductionProcess">
    <semantic:participant id="participant_ProductionDepartment" name="Production Department" processRef="process_Production"/>
  </semantic:collaboration>

  <semantic:process id="process_Production" isExecutable="true">
    <semantic:laneSet id="laneSet_Production">
      <semantic:lane id="lane_ProductionManager" name="Production Manager">
        <semantic:flowNodeRef>startEvent_OrderRequestReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ReleaseProdOrderInSAP</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_OrderReleaseDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>callActivity_CheckProdOrderLogs</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SaveReleasingProcess</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ConfirmProdOrder</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_ConfirmationMethod</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_ManualConfirmationType</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_EnterOrderNoForProgressConfirm</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SpecifyConfirmTypeProgress</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_JoinManualConfirmTypes</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_JoinConfirmMethods</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SaveConfirmationProcess</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_PostProdOrderConfirmation</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_PostSettlement</semantic:flowNodeRef>
        <semantic:flowNodeRef>parallelGateway_DefineSettlementParamsFork</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_DefineOrderNoForSettlement</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_DefinePeriodForSettlement</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_DeselectTestRunForSettlement</semantic:flowNodeRef>
        <semantic:flowNodeRef>parallelGateway_DefineSettlementParamsJoin</semantic:flowNodeRef>
        <semantic:flowNodeRef>callActivity_CheckSettledCosts</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_CheckQuantityAndEvaluation</semantic:flowNodeRef>
        <semantic:flowNodeRef>endEvent_OrderProcessedSuccessfully</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_ProductionAssistant" name="Production Assistant">
        <semantic:flowNodeRef>task_WithdrawMaterialsFromStock</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_PostGoodsIssueMIGO</semantic:flowNodeRef>
        <semantic:flowNodeRef>parallelGateway_DefineGIParamsFork</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_DefineMovementType261</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SpecifyPlantStorageAreaForGI</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_EnterOrderNoForGI</semantic:flowNodeRef>
        <semantic:flowNodeRef>parallelGateway_DefineGIParamsJoin</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SaveGoodsIssueProcess</semantic:flowNodeRef>
        <semantic:flowNodeRef>callActivity_CheckActualCosts</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_PostGoodsReceiptInWarehouse</semantic:flowNodeRef>
        <semantic:flowNodeRef>parallelGateway_DefineGRParamsFork</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_DefineMovementType101</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SpecifyProdOrderForGR</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_IndicatePlantStorageAreaForGR</semantic:flowNodeRef>
        <semantic:flowNodeRef>parallelGateway_DefineGRParamsJoin</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SaveGoodsReceiptProcess</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ComputeOverheadsActualCosts</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_OverheadProcessingType</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_JoinOverheadProcessingTypes</semantic:flowNodeRef>
        <semantic:flowNodeRef>parallelGateway_SpecifyOverheadParamsFork</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SpecifyOrderPeriodForOverheads</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SetTestRunFieldForOverheads</semantic:flowNodeRef>
        <semantic:flowNodeRef>parallelGateway_SpecifyOverheadParamsJoin</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ExecuteOverheadCalculation</semantic:flowNodeRef>
        <semantic:flowNodeRef>callActivity_CheckOverheadSurcharge</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <!-- Production Manager Lane Elements -->
    <semantic:startEvent id="startEvent_OrderRequestReceived" name="Production Order Request Received">
      <semantic:outgoing>flow_StartToReleaseOrder</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_ReleaseProdOrderInSAP" name="Release Production Order in SAP ERP">
      <semantic:incoming>flow_StartToReleaseOrder</semantic:incoming>
      <semantic:outgoing>flow_ReleaseOrderToDecision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_OrderReleaseDecision" name="Order Release Successful?">
      <semantic:incoming>flow_ReleaseOrderToDecision</semantic:incoming>
      <semantic:outgoing>flow_OrderReleased</semantic:outgoing>
      <semantic:outgoing>flow_OrderNotReleased</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:callActivity id="callActivity_CheckProdOrderLogs" name="Check Production Order Logs" calledElement="process_CheckingProductionOrder">
      <semantic:incoming>flow_OrderNotReleased</semantic:incoming>
      <semantic:outgoing>flow_CheckLogsToSaveRelease</semantic:outgoing>
    </semantic:callActivity>
    <semantic:task id="task_SaveReleasingProcess" name="Save Releasing Process">
      <semantic:incoming>flow_OrderReleased</semantic:incoming>
      <semantic:incoming>flow_CheckLogsToSaveRelease</semantic:incoming>
      <semantic:outgoing>flow_SaveReleaseToWithdrawMaterials</semantic:outgoing>
    </semantic:task>

    <semantic:task id="task_ConfirmProdOrder" name="Confirm Production Order">
      <semantic:incoming>flow_SaveGIToConfirmOrder</semantic:incoming>
      <semantic:outgoing>flow_ConfirmOrderToMethodDecision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_ConfirmationMethod" name="Confirmation Method?">
      <semantic:incoming>flow_ConfirmOrderToMethodDecision</semantic:incoming>
      <semantic:outgoing>flow_MethodExternalPDC</semantic:outgoing>
      <semantic:outgoing>flow_MethodManual</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:exclusiveGateway id="exclusiveGateway_ManualConfirmationType" name="Manual Confirmation Type?">
      <semantic:incoming>flow_MethodManual</semantic:incoming>
      <semantic:outgoing>flow_ManualTypeTimeTicket</semantic:outgoing>
      <semantic:outgoing>flow_ManualTypeTimeEvent</semantic:outgoing>
      <semantic:outgoing>flow_ManualTypeCollective</semantic:outgoing>
      <semantic:outgoing>flow_ManualTypeProgress</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_EnterOrderNoForProgressConfirm" name="Enter Order No. for Progress Confirmation">
      <semantic:incoming>flow_ManualTypeProgress</semantic:incoming>
      <semantic:outgoing>flow_EnterOrderToSpecifyType</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_SpecifyConfirmTypeProgress" name="Specify Confirmation Type for Progress">
      <semantic:incoming>flow_EnterOrderToSpecifyType</semantic:incoming>
      <semantic:outgoing>flow_SpecifyTypeToJoinManual</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_JoinManualConfirmTypes" name="Join Manual Confirmation Paths">
      <semantic:incoming>flow_ManualTypeTimeTicket</semantic:incoming>
      <semantic:incoming>flow_ManualTypeTimeEvent</semantic:incoming>
      <semantic:incoming>flow_ManualTypeCollective</semantic:incoming>
      <semantic:incoming>flow_SpecifyTypeToJoinManual</semantic:incoming>
      <semantic:outgoing>flow_JoinManualToJoinMethods</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:exclusiveGateway id="exclusiveGateway_JoinConfirmMethods" name="Join Confirmation Methods">
      <semantic:incoming>flow_MethodExternalPDC</semantic:incoming>
      <semantic:incoming>flow_JoinManualToJoinMethods</semantic:incoming>
      <semantic:outgoing>flow_JoinMethodsToSaveConfirm</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_SaveConfirmationProcess" name="Save Confirmation Process">
      <semantic:incoming>flow_JoinMethodsToSaveConfirm</semantic:incoming>
      <semantic:outgoing>flow_SaveConfirmToPostConfirm</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_PostProdOrderConfirmation" name="Post Production Order Confirmation">
      <semantic:incoming>flow_SaveConfirmToPostConfirm</semantic:incoming>
      <semantic:outgoing>flow_PostConfirmToCheckActualCosts</semantic:outgoing>
    </semantic:task>

    <semantic:task id="task_PostSettlement" name="Post Settlement">
      <semantic:incoming>flow_CheckOverheadToPostSettlement</semantic:incoming>
      <semantic:outgoing>flow_PostSettlementToDefineParams</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="parallelGateway_DefineSettlementParamsFork" name="Define Settlement Parameters Fork">
      <semantic:incoming>flow_PostSettlementToDefineParams</semantic:incoming>
      <semantic:outgoing>flow_ForkToDefineOrderNoSettlement</semantic:outgoing>
      <semantic:outgoing>flow_ForkToDefinePeriodSettlement</semantic:outgoing>
      <semantic:outgoing>flow_ForkToDeselectTestRunSettlement</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:task id="task_DefineOrderNoForSettlement" name="Define Order Number for Settlement">
      <semantic:incoming>flow_ForkToDefineOrderNoSettlement</semantic:incoming>
      <semantic:outgoing>flow_DefineOrderNoToJoinSettlement</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_DefinePeriodForSettlement" name="Define Period for Settlement">
      <semantic:incoming>flow_ForkToDefinePeriodSettlement</semantic:incoming>
      <semantic:outgoing>flow_DefinePeriodToJoinSettlement</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_DeselectTestRunForSettlement" name="Deselect 'Test Run' for Settlement">
      <semantic:incoming>flow_ForkToDeselectTestRunSettlement</semantic:incoming>
      <semantic:outgoing>flow_DeselectTestRunToJoinSettlement</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="parallelGateway_DefineSettlementParamsJoin" name="Define Settlement Parameters Join">
      <semantic:incoming>flow_DefineOrderNoToJoinSettlement</semantic:incoming>
      <semantic:incoming>flow_DefinePeriodToJoinSettlement</semantic:incoming>
      <semantic:incoming>flow_DeselectTestRunToJoinSettlement</semantic:incoming>
      <semantic:outgoing>flow_JoinSettlementToCheckSettled</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:callActivity id="callActivity_CheckSettledCosts" name="Check Settled Costs via Cost Analysis" calledElement="process_CheckingProductionOrder">
      <semantic:incoming>flow_JoinSettlementToCheckSettled</semantic:incoming>
      <semantic:outgoing>flow_CheckSettledToFinalCheck</semantic:outgoing>
    </semantic:callActivity>
    <semantic:task id="task_CheckQuantityAndEvaluation" name="Check Quantity Produced and Evaluation">
      <semantic:incoming>flow_CheckSettledToFinalCheck</semantic:incoming>
      <semantic:outgoing>flow_FinalCheckToEnd</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="endEvent_OrderProcessedSuccessfully" name="Production Order Processed Successfully">
      <semantic:incoming>flow_FinalCheckToEnd</semantic:incoming>
    </semantic:endEvent>

    <!-- Production Assistant Lane Elements -->
    <semantic:task id="task_WithdrawMaterialsFromStock" name="Withdraw Materials from Stock">
      <semantic:incoming>flow_SaveReleaseToWithdrawMaterials</semantic:incoming>
      <semantic:outgoing>flow_WithdrawToPostGI</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_PostGoodsIssueMIGO" name="Post Goods Issue (MIGO) in System">
      <semantic:incoming>flow_WithdrawToPostGI</semantic:incoming>
      <semantic:outgoing>flow_PostGIToDefineGIParams</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="parallelGateway_DefineGIParamsFork" name="Define Goods Issue Parameters Fork">
      <semantic:incoming>flow_PostGIToDefineGIParams</semantic:incoming>
      <semantic:outgoing>flow_ForkToDefineMovementType261</semantic:outgoing>
      <semantic:outgoing>flow_ForkToSpecifyPlantStorageGI</semantic:outgoing>
      <semantic:outgoing>flow_ForkToEnterOrderNoGI</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:task id="task_DefineMovementType261" name="Define Movement Type 261">
      <semantic:incoming>flow_ForkToDefineMovementType261</semantic:incoming>
      <semantic:outgoing>flow_DefineMovementTypeToJoinGI</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_SpecifyPlantStorageAreaForGI" name="Specify Plant and Storage Area for GI">
      <semantic:incoming>flow_ForkToSpecifyPlantStorageGI</semantic:incoming>
      <semantic:outgoing>flow_SpecifyPlantToJoinGI</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_EnterOrderNoForGI" name="Enter Order Number for GI">
      <semantic:incoming>flow_ForkToEnterOrderNoGI</semantic:incoming>
      <semantic:outgoing>flow_EnterOrderNoToJoinGI</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="parallelGateway_DefineGIParamsJoin" name="Define Goods Issue Parameters Join">
      <semantic:incoming>flow_DefineMovementTypeToJoinGI</semantic:incoming>
      <semantic:incoming>flow_SpecifyPlantToJoinGI</semantic:incoming>
      <semantic:incoming>flow_EnterOrderNoToJoinGI</semantic:incoming>
      <semantic:outgoing>flow_JoinGIToSaveGI</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:task id="task_SaveGoodsIssueProcess" name="Save Goods Issue Process">
      <semantic:incoming>flow_JoinGIToSaveGI</semantic:incoming>
      <semantic:outgoing>flow_SaveGIToConfirmOrder</semantic:outgoing>
    </semantic:task>

    <semantic:callActivity id="callActivity_CheckActualCosts" name="Check Actual Costs via Cost Analysis" calledElement="process_CheckingProductionOrder">
      <semantic:incoming>flow_PostConfirmToCheckActualCosts</semantic:incoming>
      <semantic:outgoing>flow_CheckActualCostsToPostGR</semantic:outgoing>
    </semantic:callActivity>
    <semantic:task id="task_PostGoodsReceiptInWarehouse" name="Post Goods Receipt in Warehouse">
      <semantic:incoming>flow_CheckActualCostsToPostGR</semantic:incoming>
      <semantic:outgoing>flow_PostGRToDefineGRParams</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="parallelGateway_DefineGRParamsFork" name="Define Goods Receipt Parameters Fork">
      <semantic:incoming>flow_PostGRToDefineGRParams</semantic:incoming>
      <semantic:outgoing>flow_ForkToDefineMovementType101</semantic:outgoing>
      <semantic:outgoing>flow_ForkToSpecifyProdOrderGR</semantic:outgoing>
      <semantic:outgoing>flow_ForkToIndicatePlantStorageGR</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:task id="task_DefineMovementType101" name="Define Movement Type 101 for GR">
      <semantic:incoming>flow_ForkToDefineMovementType101</semantic:incoming>
      <semantic:outgoing>flow_DefineMovementTypeGRToJoinGR</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_SpecifyProdOrderForGR" name="Specify Production Order for GR">
      <semantic:incoming>flow_ForkToSpecifyProdOrderGR</semantic:incoming>
      <semantic:outgoing>flow_SpecifyProdOrderToJoinGR</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_IndicatePlantStorageAreaForGR" name="Indicate Plant and Storage Area for GR">
      <semantic:incoming>flow_ForkToIndicatePlantStorageGR</semantic:incoming>
      <semantic:outgoing>flow_IndicatePlantGRToJoinGR</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="parallelGateway_DefineGRParamsJoin" name="Define Goods Receipt Parameters Join">
      <semantic:incoming>flow_DefineMovementTypeGRToJoinGR</semantic:incoming>
      <semantic:incoming>flow_SpecifyProdOrderToJoinGR</semantic:incoming>
      <semantic:incoming>flow_IndicatePlantGRToJoinGR</semantic:incoming>
      <semantic:outgoing>flow_JoinGRToSaveGR</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:task id="task_SaveGoodsReceiptProcess" name="Save Goods Receipt Process">
      <semantic:incoming>flow_JoinGRToSaveGR</semantic:incoming>
      <semantic:outgoing>flow_SaveGRToComputeOverheads</semantic:outgoing>
    </semantic:task>

    <semantic:task id="task_ComputeOverheadsActualCosts" name="Compute Overheads in Actual Costs">
      <semantic:incoming>flow_SaveGRToComputeOverheads</semantic:incoming>
      <semantic:outgoing>flow_ComputeOverheadsToProcessingType</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_OverheadProcessingType" name="Overhead Processing Type?">
      <semantic:incoming>flow_ComputeOverheadsToProcessingType</semantic:incoming>
      <semantic:outgoing>flow_OverheadTypeIndividual</semantic:outgoing>
      <semantic:outgoing>flow_OverheadTypeCollective</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:exclusiveGateway id="exclusiveGateway_JoinOverheadProcessingTypes" name="Join Overhead Processing Types">
      <semantic:incoming>flow_OverheadTypeIndividual</semantic:incoming>
      <semantic:incoming>flow_OverheadTypeCollective</semantic:incoming>
      <semantic:outgoing>flow_JoinOverheadTypesToSpecifyParams</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:parallelGateway id="parallelGateway_SpecifyOverheadParamsFork" name="Specify Overhead Parameters Fork">
      <semantic:incoming>flow_JoinOverheadTypesToSpecifyParams</semantic:incoming>
      <semantic:outgoing>flow_ForkToSpecifyOrderPeriodOverhead</semantic:outgoing>
      <semantic:outgoing>flow_ForkToSetTestRunOverhead</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:task id="task_SpecifyOrderPeriodForOverheads" name="Specify Order Number/Period for Overheads">
      <semantic:incoming>flow_ForkToSpecifyOrderPeriodOverhead</semantic:incoming>
      <semantic:outgoing>flow_SpecifyOrderPeriodToJoinOverhead</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_SetTestRunFieldForOverheads" name="Set 'Test Run' Field for Overheads">
      <semantic:incoming>flow_ForkToSetTestRunOverhead</semantic:incoming>
      <semantic:outgoing>flow_SetTestRunToJoinOverhead</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="parallelGateway_SpecifyOverheadParamsJoin" name="Specify Overhead Parameters Join">
      <semantic:incoming>flow_SpecifyOrderPeriodToJoinOverhead</semantic:incoming>
      <semantic:incoming>flow_SetTestRunToJoinOverhead</semantic:incoming>
      <semantic:outgoing>flow_JoinOverheadParamsToExecute</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:task id="task_ExecuteOverheadCalculation" name="Execute Overhead Calculation">
      <semantic:incoming>flow_JoinOverheadParamsToExecute</semantic:incoming>
      <semantic:outgoing>flow_ExecuteOverheadToCheckSurcharge</semantic:outgoing>
    </semantic:task>
    <semantic:callActivity id="callActivity_CheckOverheadSurcharge" name="Check Overhead Surcharge via Cost Analysis" calledElement="process_CheckingProductionOrder">
      <semantic:incoming>flow_ExecuteOverheadToCheckSurcharge</semantic:incoming>
      <semantic:outgoing>flow_CheckOverheadToPostSettlement</semantic:outgoing>
    </semantic:callActivity>

    <!-- Sequence Flows -->
    <semantic:sequenceFlow id="flow_StartToReleaseOrder" sourceRef="startEvent_OrderRequestReceived" targetRef="task_ReleaseProdOrderInSAP"/>
    <semantic:sequenceFlow id="flow_ReleaseOrderToDecision" sourceRef="task_ReleaseProdOrderInSAP" targetRef="exclusiveGateway_OrderReleaseDecision"/>
    <semantic:sequenceFlow id="flow_OrderReleased" name="Order Released" sourceRef="exclusiveGateway_OrderReleaseDecision" targetRef="task_SaveReleasingProcess"/>
    <semantic:sequenceFlow id="flow_OrderNotReleased" name="Order Not Released" sourceRef="exclusiveGateway_OrderReleaseDecision" targetRef="callActivity_CheckProdOrderLogs"/>
    <semantic:sequenceFlow id="flow_CheckLogsToSaveRelease" sourceRef="callActivity_CheckProdOrderLogs" targetRef="task_SaveReleasingProcess"/>
    <semantic:sequenceFlow id="flow_SaveReleaseToWithdrawMaterials" sourceRef="task_SaveReleasingProcess" targetRef="task_WithdrawMaterialsFromStock"/>

    <semantic:sequenceFlow id="flow_WithdrawToPostGI" sourceRef="task_WithdrawMaterialsFromStock" targetRef="task_PostGoodsIssueMIGO"/>
    <semantic:sequenceFlow id="flow_PostGIToDefineGIParams" sourceRef="task_PostGoodsIssueMIGO" targetRef="parallelGateway_DefineGIParamsFork"/>
    <semantic:sequenceFlow id="flow_ForkToDefineMovementType261" sourceRef="parallelGateway_DefineGIParamsFork" targetRef="task_DefineMovementType261"/>
    <semantic:sequenceFlow id="flow_ForkToSpecifyPlantStorageGI" sourceRef="parallelGateway_DefineGIParamsFork" targetRef="task_SpecifyPlantStorageAreaForGI"/>
    <semantic:sequenceFlow id="flow_ForkToEnterOrderNoGI" sourceRef="parallelGateway_DefineGIParamsFork" targetRef="task_EnterOrderNoForGI"/>
    <semantic:sequenceFlow id="flow_DefineMovementTypeToJoinGI" sourceRef="task_DefineMovementType261" targetRef="parallelGateway_DefineGIParamsJoin"/>
    <semantic:sequenceFlow id="flow_SpecifyPlantToJoinGI" sourceRef="task_SpecifyPlantStorageAreaForGI" targetRef="parallelGateway_DefineGIParamsJoin"/>
    <semantic:sequenceFlow id="flow_EnterOrderNoToJoinGI" sourceRef="task_EnterOrderNoForGI" targetRef="parallelGateway_DefineGIParamsJoin"/>
    <semantic:sequenceFlow id="flow_JoinGIToSaveGI" sourceRef="parallelGateway_DefineGIParamsJoin" targetRef="task_SaveGoodsIssueProcess"/>
    <semantic:sequenceFlow id="flow_SaveGIToConfirmOrder" sourceRef="task_SaveGoodsIssueProcess" targetRef="task_ConfirmProdOrder"/>

    <semantic:sequenceFlow id="flow_ConfirmOrderToMethodDecision" sourceRef="task_ConfirmProdOrder" targetRef="exclusiveGateway_ConfirmationMethod"/>
    <semantic:sequenceFlow id="flow_MethodExternalPDC" name="External PDC" sourceRef="exclusiveGateway_ConfirmationMethod" targetRef="exclusiveGateway_JoinConfirmMethods"/>
    <semantic:sequenceFlow id="flow_MethodManual" name="Manual" sourceRef="exclusiveGateway_ConfirmationMethod" targetRef="exclusiveGateway_ManualConfirmationType"/>
    <semantic:sequenceFlow id="flow_ManualTypeTimeTicket" name="Time Ticket" sourceRef="exclusiveGateway_ManualConfirmationType" targetRef="exclusiveGateway_JoinManualConfirmTypes"/>
    <semantic:sequenceFlow id="flow_ManualTypeTimeEvent" name="Time Event" sourceRef="exclusiveGateway_ManualConfirmationType" targetRef="exclusiveGateway_JoinManualConfirmTypes"/>
    <semantic:sequenceFlow id="flow_ManualTypeCollective" name="Collective Entry" sourceRef="exclusiveGateway_ManualConfirmationType" targetRef="exclusiveGateway_JoinManualConfirmTypes"/>
    <semantic:sequenceFlow id="flow_ManualTypeProgress" name="Progress Confirmation" sourceRef="exclusiveGateway_ManualConfirmationType" targetRef="task_EnterOrderNoForProgressConfirm"/>
    <semantic:sequenceFlow id="flow_EnterOrderToSpecifyType" sourceRef="task_EnterOrderNoForProgressConfirm" targetRef="task_SpecifyConfirmTypeProgress"/>
    <semantic:sequenceFlow id="flow_SpecifyTypeToJoinManual" sourceRef="task_SpecifyConfirmTypeProgress" targetRef="exclusiveGateway_JoinManualConfirmTypes"/>
    <semantic:sequenceFlow id="flow_JoinManualToJoinMethods" sourceRef="exclusiveGateway_JoinManualConfirmTypes" targetRef="exclusiveGateway_JoinConfirmMethods"/>
    <semantic:sequenceFlow id="flow_JoinMethodsToSaveConfirm" sourceRef="exclusiveGateway_JoinConfirmMethods" targetRef="task_SaveConfirmationProcess"/>
    <semantic:sequenceFlow id="flow_SaveConfirmToPostConfirm" sourceRef="task_SaveConfirmationProcess" targetRef="task_PostProdOrderConfirmation"/>
    <semantic:sequenceFlow id="flow_PostConfirmToCheckActualCosts" sourceRef="task_PostProdOrderConfirmation" targetRef="callActivity_CheckActualCosts"/>

    <semantic:sequenceFlow id="flow_CheckActualCostsToPostGR" sourceRef="callActivity_CheckActualCosts" targetRef="task_PostGoodsReceiptInWarehouse"/>
    <semantic:sequenceFlow id="flow_PostGRToDefineGRParams" sourceRef="task_PostGoodsReceiptInWarehouse" targetRef="parallelGateway_DefineGRParamsFork"/>
