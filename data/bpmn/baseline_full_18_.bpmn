<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="PunitiveMandateProcessDefs"
    targetNamespace="http://example.com/bpmn/punitive-mandate"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_punitiveMandate">
    <semantic:participant id="pool_PunitiveMandate" name="Punitive Mandate Process Pool" processRef="process_PunitiveMandate"/>
  </semantic:collaboration>

  <semantic:process id="process_PunitiveMandate" isExecutable="true">
    <semantic:laneSet id="laneSet_PunitiveMandate">
      <semantic:lane id="lane_MA" name="Municipal Authority">
        <semantic:flowNodeRef>startEvent_MA_PunishmentRealized</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_MA_IssueMandate</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_MA_SendMandateByPost</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_MA_CheckObjection</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_MA_ConcludeInitialInvestigation</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_MA_InvestigationSufficient</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_MA_DecisionAfterSufficientInv</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_MA_ImposeSentenceSufficient</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_MA_TerminateProceedingsSufficient</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_MA_MergeSufficientInvOutcome</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_MA_InvestigateFurther</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_MA_ConcludeFurtherInvestigation</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_MA_DecisionAfterFurtherInv</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_MA_ImposeSentenceFurther</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_MA_TerminateProceedingsFurther</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_MA_MergeFurtherInvOutcome</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_MA_ConvergeInvestigationPaths</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_MA_InformAccusedOfResults</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_Postman" name="Postman">
        <semantic:flowNodeRef>task_PS_AttemptDelivery</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_PS_DeliverySuccessful</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_PS_DepositAtPostOffice</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_AP" name="Accused Person">
        <semantic:flowNodeRef>task_AP_TakeOverMandateRSb</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AP_TakeOverMandatePostOffice</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_AP_MergeMandateReceipt</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AP_RaiseObjection</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AP_ReceiveInvestigationResults</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_AP_WantToComplain</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AP_PayPenaltyNoComplaint</semantic:flowNodeRef>
        <semantic:flowNodeRef>endEvent_AP_MandateHandledNoComplaint</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_AP_PenaltyType</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AP_RaiseComplaintAdminCourt</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AP_RaiseComplaintFedFinCourt</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_AP_MergeDisregardedComplaints</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AP_ReceiveDisregardedComplaint</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AP_PayPenaltyAfterDisregard</semantic:flowNodeRef>
        <semantic:flowNodeRef>endEvent_AP_MandateHandledAfterDisregard</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_AC" name="Administrative Court">
        <semantic:flowNodeRef>task_AC_ReceiveComplaint</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AC_CheckSubmissionTime</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_AC_SubmissionOnTime</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AC_FinalizeDecisionNoTime</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AC_DisregardComplaint</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AC_SendDisregardedBack</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AC_InvestigateComplaint</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AC_ArrangeTrial</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AC_SubmitDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>endEvent_AC_DecisionSubmitted</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_FFC" name="Federal Finance Court">
        <semantic:flowNodeRef>task_FFC_ReceiveComplaint</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_FFC_CheckSubmissionTime</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_FFC_SubmissionOnTime</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_FFC_FinalizeDecisionNoTime</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_FFC_DisregardComplaint</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_FFC_SendDisregardedBack</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_FFC_InvestigateComplaint</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_FFC_ArrangeTrial</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_FFC_SubmitDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>endEvent_FFC_DecisionSubmitted</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <!-- Municipal Authority Lane Elements -->
    <semantic:startEvent id="startEvent_MA_PunishmentRealized" name="Punishment Issuance Realized">
      <semantic:outgoing>flow_Start_to_IssueMandate</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_MA_IssueMandate" name="Issue Punitive Mandate">
      <semantic:incoming>flow_Start_to_IssueMandate</semantic:incoming>
      <semantic:outgoing>flow_IssueMandate_to_SendMandate</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_MA_SendMandateByPost" name="Send Punitive Mandate by Post">
      <semantic:incoming>flow_IssueMandate_to_SendMandate</semantic:incoming>
      <semantic:outgoing>flow_SendMandate_to_AttemptDelivery</semantic:outgoing>
    </semantic:task>

    <semantic:task id="task_MA_CheckObjection" name="Check Raised Objection">
      <semantic:incoming>flow_RaiseObjection_to_CheckObjection</semantic:incoming>
      <semantic:outgoing>flow_CheckObjection_to_ConcludeInitial</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_MA_ConcludeInitialInvestigation" name="Conclude Initial Investigation">
      <semantic:incoming>flow_CheckObjection_to_ConcludeInitial</semantic:incoming>
      <semantic:outgoing>flow_ConcludeInitial_to_GatewayInvSufficient</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_MA_InvestigationSufficient" name="Initial Investigation Sufficient?">
      <semantic:incoming>flow_ConcludeInitial_to_GatewayInvSufficient</semantic:incoming>
      <semantic:outgoing>flow_InvSufficient_Yes</semantic:outgoing>
      <semantic:outgoing>flow_InvSufficient_No</semantic:outgoing>
    </semantic:exclusiveGateway>
    
    <!-- Path: Investigation Sufficient = Yes -->
    <semantic:exclusiveGateway id="gateway_MA_DecisionAfterSufficientInv" name="Decision After Sufficient Investigation?">
      <semantic:incoming>flow_InvSufficient_Yes</semantic:incoming>
      <semantic:outgoing>flow_DecisionSufficient_Impose</semantic:outgoing>
      <semantic:outgoing>flow_DecisionSufficient_Terminate</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_MA_ImposeSentenceSufficient" name="Impose Punitive Sentence (Sufficient Inv.)">
      <semantic:incoming>flow_DecisionSufficient_Impose</semantic:incoming>
      <semantic:outgoing>flow_ImposeSufficient_to_MergeSufficient</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_MA_TerminateProceedingsSufficient" name="Terminate Proceedings (Sufficient Inv.)">
      <semantic:incoming>flow_DecisionSufficient_Terminate</semantic:incoming>
      <semantic:outgoing>flow_TerminateSufficient_to_MergeSufficient</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_MA_MergeSufficientInvOutcome" name="Merge Sufficient Investigation Outcome">
      <semantic:incoming>flow_ImposeSufficient_to_MergeSufficient</semantic:incoming>
      <semantic:incoming>flow_TerminateSufficient_to_MergeSufficient</semantic:incoming>
      <semantic:outgoing>flow_MergeSufficient_to_ConvergeInvPaths</semantic:outgoing>
    </semantic:exclusiveGateway>

    <!-- Path: Investigation Sufficient = No -->
    <semantic:task id="task_MA_InvestigateFurther" name="Investigate Objection Further">
      <semantic:incoming>flow_InvSufficient_No</semantic:incoming>
      <semantic:outgoing>flow_InvestigateFurther_to_ConcludeFurther</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_MA_ConcludeFurtherInvestigation" name="Conclude Further Investigation">
      <semantic:incoming>flow_InvestigateFurther_to_ConcludeFurther</semantic:incoming>
      <semantic:outgoing>flow_ConcludeFurther_to_GatewayDecisionFurther</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_MA_DecisionAfterFurtherInv" name="Decision After Further Investigation?">
      <semantic:incoming>flow_ConcludeFurther_to_GatewayDecisionFurther</semantic:incoming>
      <semantic:outgoing>flow_DecisionFurther_Impose</semantic:outgoing>
      <semantic:outgoing>flow_DecisionFurther_Terminate</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_MA_ImposeSentenceFurther" name="Impose Punitive Sentence (Further Inv.)">
      <semantic:incoming>flow_DecisionFurther_Impose</semantic:incoming>
      <semantic:outgoing>flow_ImposeFurther_to_MergeFurther</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_MA_TerminateProceedingsFurther" name="Terminate Proceedings (Further Inv.)">
      <semantic:incoming>flow_DecisionFurther_Terminate</semantic:incoming>
      <semantic:outgoing>flow_TerminateFurther_to_MergeFurther</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_MA_MergeFurtherInvOutcome" name="Merge Further Investigation Outcome">
      <semantic:incoming>flow_ImposeFurther_to_MergeFurther</semantic:incoming>
      <semantic:incoming>flow_TerminateFurther_to_MergeFurther</semantic:incoming>
      <semantic:outgoing>flow_MergeFurther_to_ConvergeInvPaths</semantic:outgoing>
    </semantic:exclusiveGateway>

    <semantic:exclusiveGateway id="gateway_MA_ConvergeInvestigationPaths" name="Converge Investigation Paths">
      <semantic:incoming>flow_MergeSufficient_to_ConvergeInvPaths</semantic:incoming>
      <semantic:incoming>flow_MergeFurther_to_ConvergeInvPaths</semantic:incoming>
      <semantic:outgoing>flow_ConvergeInvPaths_to_InformAccused</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_MA_InformAccusedOfResults" name="Inform Accused of Investigation Results">
      <semantic:incoming>flow_ConvergeInvPaths_to_InformAccused</semantic:incoming>
      <semantic:outgoing>flow_InformAccused_to_ReceiveResults</semantic:outgoing>
    </semantic:task>

    <!-- Postman Lane Elements -->
    <semantic:task id="task_PS_AttemptDelivery" name="Attempt Punitive Mandate Delivery">
      <semantic:incoming>flow_SendMandate_to_AttemptDelivery</semantic:incoming>
      <semantic:outgoing>flow_AttemptDelivery_to_GatewayDeliverySuccessful</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_PS_DeliverySuccessful" name="Delivery Successful?">
      <semantic:incoming>flow_AttemptDelivery_to_GatewayDeliverySuccessful</semantic:incoming>
      <semantic:outgoing>flow_DeliverySuccessful_Yes</semantic:outgoing>
      <semantic:outgoing>flow_DeliverySuccessful_No</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_PS_DepositAtPostOffice" name="Deposit Mandate at Post Office">
      <semantic:incoming>flow_DeliverySuccessful_No</semantic:incoming>
      <semantic:outgoing>flow_DepositPO_to_TakeOverPO</semantic:outgoing>
    </semantic:task>

    <!-- Accused Person Lane Elements -->
    <semantic:task id="task_AP_TakeOverMandateRSb" name="Take Over Mandate (Proof of Delivery RSb)">
      <semantic:incoming>flow_DeliverySuccessful_Yes</semantic:incoming>
      <semantic:outgoing>flow_TakeOverRSb_to_MergeMandateReceipt</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_AP_TakeOverMandatePostOffice" name="Take Over Mandate from Post Office">
      <semantic:incoming>flow_DepositPO_to_TakeOverPO</semantic:incoming>
      <semantic:outgoing>flow_TakeOverPO_to_MergeMandateReceipt</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_AP_MergeMandateReceipt" name="Merge Mandate Receipt Paths">
      <semantic:incoming>flow_TakeOverRSb_to_MergeMandateReceipt</semantic:incoming>
      <semantic:incoming>flow_TakeOverPO_to_MergeMandateReceipt</semantic:incoming>
      <semantic:outgoing>flow_MergeMandateReceipt_to_RaiseObjection</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_AP_RaiseObjection" name="Raise Objection Against Punitive Mandate">
      <semantic:incoming>flow_MergeMandateReceipt_to_RaiseObjection</semantic:incoming>
      <semantic:outgoing>flow_RaiseObjection_to_CheckObjection</semantic:outgoing>
    </semantic:task>

    <semantic:task id="task_AP_ReceiveInvestigationResults" name="Receive Investigation Results">
      <semantic:incoming>flow_InformAccused_to_ReceiveResults</semantic:incoming>
      <semantic:outgoing>flow_ReceiveResults_to_GatewayWantToComplain</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_AP_WantToComplain" name="Want to Raise Complaint?">
      <semantic:incoming>flow_ReceiveResults_to_GatewayWantToComplain</semantic:incoming>
      <semantic:outgoing>flow_WantToComplain_No</semantic:outgoing>
      <semantic:outgoing>flow_WantToComplain_Yes</semantic:outgoing>
    </semantic:exclusiveGateway>
    
    <!-- Path: Want to Complain = No -->
    <semantic:task id="task_AP_PayPenaltyNoComplaint" name="Pay Imposed Penalty">
      <semantic:incoming>flow_WantToComplain_No</semantic:incoming>
      <semantic:outgoing>flow_PayNoComplaint_to_EndNoComplaint</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="endEvent_AP_MandateHandledNoComplaint" name="Punitive Mandate Finalized (Payment)">
      <semantic:incoming>flow_PayNoComplaint_to_EndNoComplaint</semantic:incoming>
    </semantic:endEvent>

    <!-- Path: Want to Complain = Yes -->
    <semantic:exclusiveGateway id="gateway_AP_PenaltyType" name="Complaint Type?">
      <semantic:incoming>flow_WantToComplain_Yes</semantic:incoming>
      <semantic:outgoing>flow_PenaltyType_Traffic</semantic:outgoing>
      <semantic:outgoing>flow_PenaltyType_Parking</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_AP_RaiseComplaintAdminCourt" name="Raise Complaint to Administrative Court">
      <semantic:incoming>flow_PenaltyType_Traffic</semantic:incoming>
      <semantic:outgoing>flow_RaiseComplaintAC_to_ReceiveAC</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_AP_RaiseComplaintFedFinCourt" name="Raise Complaint to Federal Finance Court">
      <semantic:incoming>flow_PenaltyType_Parking</semantic:incoming>
      <semantic:outgoing>flow_RaiseComplaintFFC_to_ReceiveFFC</semantic:outgoing>
    </semantic:task>

    <semantic:exclusiveGateway id="gateway_AP_MergeDisregardedComplaints" name="Merge Disregarded Complaint Paths">
        <semantic:incoming>flow_SendBackAC_to_MergeDisregarded</semantic:incoming>
        <semantic:incoming>flow_SendBackFFC_to_MergeDisregarded</semantic:incoming>
        <semantic:outgoing>flow_MergeDisregarded_to_ReceiveDisregarded</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_AP_ReceiveDisregardedComplaint" name="Receive Disregarded Complaint">
      <semantic:incoming>flow_MergeDisregarded_to_ReceiveDisregarded</semantic:incoming>
      <semantic:outgoing>flow_ReceiveDisregarded_to_PayAfterDisregard</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_AP_PayPenaltyAfterDisregard" name="Pay Imposed Penalty (After Disregard)">
      <semantic:incoming>flow_ReceiveDisregarded_to_PayAfterDisregard</semantic:incoming>
      <semantic:outgoing>flow_PayAfterDisregard_to_EndDisregard</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="endEvent_AP_MandateHandledAfterDisregard" name="Punitive Mandate Finalized (Payment After Disregard)">
      <semantic:incoming>flow_PayAfterDisregard_to_EndDisregard</semantic:incoming>
    </semantic:endEvent>

    <!-- Administrative Court Lane Elements -->
    <semantic:task id="task_AC_ReceiveComplaint" name="Receive Complaint (Traffic)">
      <semantic:incoming>flow_RaiseComplaintAC_to_ReceiveAC</semantic:incoming>
      <semantic:outgoing>flow_ReceiveAC_to_CheckTimeAC</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_AC_CheckSubmissionTime" name="Check Complaint Submission Time (AC)">
      <semantic:incoming>flow_ReceiveAC_to_CheckTimeAC</semantic:incoming>
      <semantic:outgoing>flow_CheckTimeAC_to_GatewayOnTimeAC</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_AC_SubmissionOnTime" name="Complaint Submitted On Time? (AC)">
      <semantic:incoming>flow_CheckTimeAC_to_GatewayOnTimeAC</semantic:incoming>
      <semantic:outgoing>flow_OnTimeAC_No</semantic:outgoing>
      <semantic:outgoing>flow_OnTimeAC_Yes</semantic:outgoing>
    </semantic:exclusiveGateway>
    <!-- Path: On Time AC = No -->
    <semantic:task id="task_AC_FinalizeDecisionNoTime" name="Consider Decision Final (AC)">
      <semantic:incoming>flow_OnTimeAC_No</semantic:incoming>
      <semantic:outgoing>flow_FinalizeAC_to_DisregardAC</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_AC_DisregardComplaint" name="Disregard Complaint (AC)">
      <semantic:incoming>flow_FinalizeAC_to_DisregardAC</semantic:incoming>
      <semantic:outgoing>flow_DisregardAC_to_SendBackAC</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_AC_SendDisregardedBack" name="Send Disregarded Complaint Back (AC)">
      <semantic:incoming>flow_DisregardAC_to_SendBackAC</semantic:incoming>
      <semantic:outgoing>flow_SendBackAC_to_MergeDisregarded</semantic:outgoing>
    </semantic:task>
    <!-- Path: On Time AC = Yes -->
    <semantic:task id="task_AC_InvestigateComplaint" name="Investigate Complaint (AC)">
      <semantic:incoming>flow_OnTimeAC_Yes</semantic:incoming>
      <semantic:outgoing>flow_InvestigateAC_to_ArrangeTrialAC</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_AC_ArrangeTrial" name="Arrange Trial and Summon Persons (AC)">
      <semantic:incoming>flow_InvestigateAC_to_ArrangeTrialAC</semantic:incoming>
      <semantic:outgoing>flow_ArrangeTrialAC_to_SubmitDecisionAC</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_AC_SubmitDecision" name="Submit Court Decision (AC)">
      <semantic:incoming>flow_ArrangeTrialAC_to_SubmitDecisionAC</semantic:incoming>
      <semantic:outgoing>flow_SubmitDecisionAC_to_EndDecisionAC</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="endEvent_AC_DecisionSubmitted" name="Court Decision Issued (AC)">
      <semantic:incoming>flow_SubmitDecisionAC_to_EndDecisionAC</semantic:incoming>
    </semantic:endEvent>

    <!-- Federal Finance Court Lane Elements -->
    <semantic:task id="task_FFC_ReceiveComplaint" name="Receive Complaint (Parking)">
      <semantic:incoming>flow_RaiseComplaintFFC_to_ReceiveFFC</semantic:incoming>
      <semantic:outgoing>flow_ReceiveFFC_to_CheckTimeFFC</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_FFC_CheckSubmissionTime" name="Check Complaint Submission Time (FFC)">
      <semantic:incoming>flow_ReceiveFFC_to_CheckTimeFFC</semantic:incoming>
      <semantic:outgoing>flow_CheckTimeFFC_to_GatewayOnTimeFFC</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_FFC_SubmissionOnTime" name="Complaint Submitted On Time? (FFC)">
      <semantic:incoming>flow_CheckTimeFFC_to_GatewayOnTimeFFC</semantic:incoming>
      <semantic:outgoing>flow_OnTimeFFC_No</semantic:outgoing>
      <semantic:outgoing>flow_OnTimeFFC_Yes</semantic:outgoing>
    </semantic:exclusiveGateway>
    <!-- Path: On Time FFC = No -->
    <semantic:task id="task_FFC_FinalizeDecisionNoTime" name="Consider Decision Final (FFC)">
      <semantic:incoming>flow_OnTimeFFC_No</semantic:incoming>
      <semantic:outgoing>flow_FinalizeFFC_to_DisregardFFC</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_FFC_DisregardComplaint" name="Disregard Complaint (FFC)">
      <semantic:incoming>flow_FinalizeFFC_to_DisregardFFC</semantic:incoming>
      <semantic:outgoing>flow_DisregardFFC_to_SendBackFFC</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_FFC_SendDisregardedBack" name="Send Disregarded Complaint Back (FFC)">
      <semantic:incoming>flow_DisregardFFC_to_SendBackFFC</semantic:incoming>
      <semantic:outgoing>flow_SendBackFFC_to_MergeDisregarded</semantic:outgoing>
    </semantic:task>
    <!-- Path: On Time FFC = Yes -->
    <semantic:task id="task_FFC_InvestigateComplaint" name="Investigate Complaint (FFC)">
      <semantic:incoming>flow_OnTimeFFC_Yes</semantic:incoming>
      <semantic:outgoing>flow_InvestigateFFC_to_ArrangeTrialFFC</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_FFC_ArrangeTrial" name="Arrange Trial and Summon Persons (FFC)">
      <semantic:incoming>flow_InvestigateFFC_to_ArrangeTrialFFC</semantic:incoming>
      <semantic:outgoing>flow_ArrangeTrialFFC_to_SubmitDecisionFFC</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_FFC_SubmitDecision" name="Submit Court Decision (FFC)">
      <semantic:incoming>flow_ArrangeTrialFFC_to_SubmitDecisionFFC</semantic:incoming>
      <semantic:outgoing>flow_SubmitDecisionFFC_to_EndDecisionFFC</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="endEvent_FFC_DecisionSubmitted" name="Court Decision Issued (FFC)">
      <semantic:incoming>flow_SubmitDecisionFFC_to_EndDecisionFFC</semantic:incoming>
    </semantic:endEvent>

    <!-- Sequence Flows -->
    <semantic:sequenceFlow id="flow_Start_to_IssueMandate" sourceRef="startEvent_MA_PunishmentRealized" targetRef="task_MA_IssueMandate"/>
    <semantic:sequenceFlow id="flow_IssueMandate_to_SendMandate" sourceRef="task_MA_IssueMandate" targetRef="task_MA_SendMandateByPost"/>
    <semantic:sequenceFlow id="flow_SendMandate_to_AttemptDelivery" sourceRef="task_MA_SendMandateByPost" targetRef="task_PS_AttemptDelivery"/>
    
    <semantic:sequenceFlow id="flow_AttemptDelivery_to_GatewayDeliverySuccessful" sourceRef="task_PS_AttemptDelivery" targetRef="gateway_PS_DeliverySuccessful"/>
