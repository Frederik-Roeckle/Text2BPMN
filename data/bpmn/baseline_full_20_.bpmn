<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="P2PProcessDefs"
    targetNamespace="http://example.com/bpmn/p2p-process"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_P2P">
    <semantic:participant id="participant_purchasing" name="Purchasing Department" processRef="purchasingProcess"/>
    <semantic:participant id="participant_vendor" name="Vendor" processRef="vendorProcess"/>
    <semantic:participant id="participant_ap" name="Accounts Payable Department" processRef="apProcess"/>
  </semantic:collaboration>

  <semantic:process id="purchasingProcess" isExecutable="true">
    <semantic:laneSet id="purchasingLaneSet">
      <semantic:lane id="requestorLane" name="Requestor">
        <semantic:flowNodeRef>start_P2P</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_DetermineNeeds</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_RequestPRIssue</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_PRIssueApproved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_IssuePR</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SendPRToHoD</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_WaitForHoDDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>msg_HoDApprovedReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>msg_HoDDeclinedReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>msg_HoDRejectedReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_PRStatusCheck</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_CreatePO</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SubmitPOToApprovers</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_WaitForPODecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>msg_POApprovedReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>msg_PORejectedReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_POStatusCheck</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_DispatchPOToVendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_WaitForVendorResponse</semantic:flowNodeRef>
        <semantic:flowNodeRef>msg_VendorApprovedPOReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>msg_VendorNegotiatedPOReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>msg_VendorRejectedPOReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_VendorResponseOutcome</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SearchNewVendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ReceiveGoodsServices</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_DeliveryType</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_IssueGR</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_IssueSES</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_ReceiptIssued</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ReceiveInvoiceFromVendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_PerformThreeWayMatch</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_InvoiceCorrect</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_PostInvoiceSAP</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SendInvoiceToAP</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_RequestorInvoiceProcessing</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ReturnWrongInvoiceToVendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>event_CorrectedInvoiceReceived</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="hodLane" name="Heads of Departments">
        <semantic:flowNodeRef>event_PRReceivedFromRequestor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ReviewPR</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_DocumentPRDecisionHoD</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_HoDPRDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SendApprovedPRToRequestor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SendDeclinedPRToRequestor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SendRejectedPRToRequestor</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_HoDDecisionSent</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_HoDPRReview</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="poApproverLane" name="PO Approvers">
        <semantic:flowNodeRef>event_POReceivedForApproval</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ReviewPOSpecs</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_DocumentPODecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_POApproverDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SendApprovedPOToRequestor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SendRejectedPOToRequestor</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_POApproverDecisionSent</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_POApprovalProcess</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <!-- Requestor Lane Elements -->
    <semantic:startEvent id="start_P2P" name="P2P Process Started">
      <semantic:outgoing>flow_start_to_determineneeds</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_DetermineNeeds" name="Determine Business Requirements &amp; Needs">
      <semantic:incoming>flow_start_to_determineneeds</semantic:incoming>
      <semantic:incoming>flow_gwPRissue_to_determineneeds</semantic:incoming>
      <semantic:incoming>flow_gwPRstatus_to_determineneeds</semantic:incoming>
      <semantic:incoming>flow_gwPOstatus_to_determineneeds</semantic:incoming>
      <semantic:outgoing>flow_determineneeds_to_requestpr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_RequestPRIssue" name="Request to Issue Purchase Requisition (PR)">
      <semantic:incoming>flow_determineneeds_to_requestpr</semantic:incoming>
      <semantic:outgoing>flow_requestpr_to_gwPRissue</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_PRIssueApproved" name="PR Issuance Approved?">
      <semantic:incoming>flow_requestpr_to_gwPRissue</semantic:incoming>
      <semantic:outgoing>flow_gwPRissue_yes</semantic:outgoing>
      <semantic:outgoing>flow_gwPRissue_to_determineneeds</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_IssuePR" name="Issue Purchase Requisition (PR)">
      <semantic:incoming>flow_gwPRissue_yes</semantic:incoming>
      <semantic:outgoing>flow_issuePR_to_sendPRtoHoD</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_SendPRToHoD" name="Send PR to HoD for Approval">
      <semantic:incoming>flow_issuePR_to_sendPRtoHoD</semantic:incoming>
      <semantic:outgoing>flow_SendPRToHoD_to_gwWaitForHoD</semantic:outgoing> <!-- Connects to EventBasedGateway trigger conceptually -->
    </semantic:task>

    <semantic:eventBasedGateway id="gw_WaitForHoDDecision" name="Wait for HoD Decision">
      <semantic:incoming>flow_SendPRToHoD_to_gwWaitForHoD</semantic:incoming> <!-- Triggered after sending PR -->
      <semantic:outgoing>flow_gwWaitForHoD_to_msgHoDApproved</semantic:outgoing>
      <semantic:outgoing>flow_gwWaitForHoD_to_msgHoDDeclined</semantic:outgoing>
      <semantic:outgoing>flow_gwWaitForHoD_to_msgHoDRejected</semantic:outgoing>
    </semantic:eventBasedGateway>
    <semantic:intermediateCatchEvent id="msg_HoDApprovedReceived" name="Approved PR Received from HoD">
      <semantic:incoming>flow_gwWaitForHoD_to_msgHoDApproved</semantic:incoming>
      <semantic:outgoing>flow_msgHoDApproved_to_gwPRStatus</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_HoD_Approved"/>
    </semantic:intermediateCatchEvent>
    <semantic:intermediateCatchEvent id="msg_HoDDeclinedReceived" name="Declined PR Received from HoD">
      <semantic:incoming>flow_gwWaitForHoD_to_msgHoDDeclined</semantic:incoming>
      <semantic:outgoing>flow_msgHoDDeclined_to_gwPRStatus</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_HoD_Declined"/>
    </semantic:intermediateCatchEvent>
    <semantic:intermediateCatchEvent id="msg_HoDRejectedReceived" name="Rejected PR Received from HoD">
      <semantic:incoming>flow_gwWaitForHoD_to_msgHoDRejected</semantic:incoming>
      <semantic:outgoing>flow_msgHoDRejected_to_gwPRStatus</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_HoD_Rejected"/>
    </semantic:intermediateCatchEvent>

    <semantic:exclusiveGateway id="gw_PRStatusCheck" name="PR Approved by HoD?">
      <semantic:incoming>flow_msgHoDApproved_to_gwPRStatus</semantic:incoming>
      <semantic:incoming>flow_msgHoDDeclined_to_gwPRStatus</semantic:incoming>
      <semantic:incoming>flow_msgHoDRejected_to_gwPRStatus</semantic:incoming>
      <semantic:outgoing>flow_gwPRstatus_yes</semantic:outgoing> <!-- Approved -->
      <semantic:outgoing>flow_gwPRstatus_to_determineneeds</semantic:outgoing> <!-- Declined/Rejected -->
    </semantic:exclusiveGateway>
    <semantic:task id="task_CreatePO" name="Create Purchase Order (PO)">
      <semantic:incoming>flow_gwPRstatus_yes</semantic:incoming>
      <semantic:outgoing>flow_createPO_to_submitPO</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_SubmitPOToApprovers" name="Submit PO to PO Approvers for Approval">
      <semantic:incoming>flow_createPO_to_submitPO</semantic:incoming>
      <semantic:outgoing>flow_SubmitPOToApprovers_to_gwWaitForPO</semantic:outgoing>
    </semantic:task>

    <semantic:eventBasedGateway id="gw_WaitForPODecision" name="Wait for PO Approver Decision">
      <semantic:incoming>flow_SubmitPOToApprovers_to_gwWaitForPO</semantic:incoming>
      <semantic:outgoing>flow_gwWaitForPO_to_msgPOApproved</semantic:outgoing>
      <semantic:outgoing>flow_gwWaitForPO_to_msgPORejected</semantic:outgoing>
    </semantic:eventBasedGateway>
    <semantic:intermediateCatchEvent id="msg_POApprovedReceived" name="Approved PO Received">
      <semantic:incoming>flow_gwWaitForPO_to_msgPOApproved</semantic:incoming>
      <semantic:outgoing>flow_msgPOApproved_to_gwPOStatus</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_POApprover_Approved"/>
    </semantic:intermediateCatchEvent>
    <semantic:intermediateCatchEvent id="msg_PORejectedReceived" name="Rejected PO Received">
      <semantic:incoming>flow_gwWaitForPO_to_msgPORejected</semantic:incoming>
      <semantic:outgoing>flow_msgPORejected_to_gwPOStatus</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_POApprover_Rejected"/>
    </semantic:intermediateCatchEvent>

    <semantic:exclusiveGateway id="gw_POStatusCheck" name="PO Approved by PO Approver?">
      <semantic:incoming>flow_msgPOApproved_to_gwPOStatus</semantic:incoming>
      <semantic:incoming>flow_msgPORejected_to_gwPOStatus</semantic:incoming>
      <semantic:outgoing>flow_gwPOstatus_yes</semantic:outgoing> <!-- Approved -->
      <semantic:outgoing>flow_gwPOstatus_to_determineneeds</semantic:outgoing> <!-- Rejected -->
    </semantic:exclusiveGateway>
    <semantic:task id="task_DispatchPOToVendor" name="Dispatch Approved PO to Vendor">
      <semantic:incoming>flow_gwPOstatus_yes</semantic:incoming>
      <semantic:incoming>flow_searchvendor_to_dispatch</semantic:incoming>
      <semantic:outgoing>flow_DispatchPOToVendor_to_gwWaitForVendor</semantic:outgoing>
    </semantic:task>

    <semantic:eventBasedGateway id="gw_WaitForVendorResponse" name="Wait for Vendor PO Response">
      <semantic:incoming>flow_DispatchPOToVendor_to_gwWaitForVendor</semantic:incoming>
      <semantic:outgoing>flow_gwWaitForVendor_to_msgVendorApproved</semantic:outgoing>
      <semantic:outgoing>flow_gwWaitForVendor_to_msgVendorNegotiated</semantic:outgoing>
      <semantic:outgoing>flow_gwWaitForVendor_to_msgVendorRejected</semantic:outgoing>
    </semantic:eventBasedGateway>
    <semantic:intermediateCatchEvent id="msg_VendorApprovedPOReceived" name="Vendor Approved PO">
      <semantic:incoming>flow_gwWaitForVendor_to_msgVendorApproved</semantic:incoming>
      <semantic:outgoing>flow_msgVendorApproved_to_gwVendorResponse</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_Vendor_Approved"/>
    </semantic:intermediateCatchEvent>
    <semantic:intermediateCatchEvent id="msg_VendorNegotiatedPOReceived" name="Vendor Negotiated PO">
      <semantic:incoming>flow_gwWaitForVendor_to_msgVendorNegotiated</semantic:incoming>
      <semantic:outgoing>flow_msgVendorNegotiated_to_gwVendorResponse</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_Vendor_Negotiated"/>
    </semantic:intermediateCatchEvent>
    <semantic:intermediateCatchEvent id="msg_VendorRejectedPOReceived" name="Vendor Rejected PO">
      <semantic:incoming>flow_gwWaitForVendor_to_msgVendorRejected</semantic:incoming>
      <semantic:outgoing>flow_msgVendorRejected_to_gwVendorResponse</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_Vendor_Rejected"/>
    </semantic:intermediateCatchEvent>

    <semantic:exclusiveGateway id="gw_VendorResponseOutcome" name="Vendor PO Response Outcome?">
      <semantic:incoming>flow_msgVendorApproved_to_gwVendorResponse</semantic:incoming>
      <semantic:incoming>flow_msgVendorNegotiated_to_gwVendorResponse</semantic:incoming>
      <semantic:incoming>flow_msgVendorRejected_to_gwVendorResponse</semantic:incoming>
      <semantic:outgoing>flow_gwVendorResponse_approved</semantic:outgoing> <!-- Approved/Negotiated -->
      <semantic:outgoing>flow_gwVendorResponse_rejected</semantic:outgoing> <!-- Rejected -->
    </semantic:exclusiveGateway>
    <semantic:task id="task_SearchNewVendor" name="Search for New Vendor">
      <semantic:incoming>flow_gwVendorResponse_rejected</semantic:incoming>
      <semantic:outgoing>flow_searchvendor_to_dispatch</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ReceiveGoodsServices" name="Receive Agreed Goods/Services">
      <semantic:incoming>flow_gwVendorResponse_approved</semantic:incoming>
      <!-- This task is also triggered by vendor's delivery message -->
      <semantic:outgoing>flow_receivegoods_to_gwDeliveryType</semantic:outgoing>
    </semantic:task>
    <semantic:intermediateCatchEvent id="event_GoodsServicesDelivered" name="Goods/Services Delivered by Vendor">
        <semantic:messageEventDefinition messageRef="msg_Vendor_Delivered"/>
        <semantic:outgoing>flow_deliverynotice_to_receivegoods</semantic:outgoing>
    </semantic:intermediateCatchEvent>

    <semantic:exclusiveGateway id="gw_DeliveryType" name="Type of Delivery Received?">
      <semantic:incoming>flow_receivegoods_to_gwDeliveryType</semantic:incoming>
      <semantic:outgoing>flow_gwDeliveryType_goods</semantic:outgoing>
      <semantic:outgoing>flow_gwDeliveryType_service</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_IssueGR" name="Issue Goods Receipt (GR)">
      <semantic:incoming>flow_gwDeliveryType_goods</semantic:incoming>
      <semantic:outgoing>flow_issueGR_to_gwReceiptIssued</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_IssueSES" name="Issue Service Entry Sheet (SES)">
      <semantic:incoming>flow_gwDeliveryType_service</semantic:incoming>
      <semantic:outgoing>flow_issueSES_to_gwReceiptIssued</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_ReceiptIssued" name="Receipt Issued">
      <semantic:incoming>flow_issueGR_to_gwReceiptIssued</semantic:incoming>
      <semantic:incoming>flow_issueSES_to_gwReceiptIssued</semantic:incoming>
      <semantic:outgoing>flow_gwReceiptIssued_to_receiveinvoice</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_ReceiveInvoiceFromVendor" name="Receive Invoice from Vendor">
      <semantic:incoming>flow_gwReceiptIssued_to_receiveinvoice</semantic:incoming>
      <!-- This task is also triggered by vendor's invoice message -->
      <semantic:outgoing>flow_receiveinvoice_to_3waymatch</semantic:outgoing>
    </semantic:task>
    <semantic:intermediateCatchEvent id="event_InvoiceReceivedFromVendor" name="Invoice Received from Vendor">
        <semantic:messageEventDefinition messageRef="msg_Vendor_InvoiceSent"/>
        <semantic:outgoing>flow_invoicenotice_to_receiveinvoice</semantic:outgoing>
    </semantic:intermediateCatchEvent>

    <semantic:task id="task_PerformThreeWayMatch" name="Perform Three-Way Match (Invoice, PO, GR/SES)">
      <semantic:incoming>flow_receiveinvoice_to_3waymatch</semantic:incoming>
      <semantic:incoming>flow_correctedInvoice_to_3waymatch</semantic:incoming>
      <semantic:outgoing>flow_3waymatch_to_gwInvoiceCorrect</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_InvoiceCorrect" name="Invoice Correct?">
      <semantic:incoming>flow_3waymatch_to_gwInvoiceCorrect</semantic:incoming>
      <semantic:outgoing>flow_gwInvoiceCorrect_yes</semantic:outgoing>
      <semantic:outgoing>flow_gwInvoiceCorrect_no</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_PostInvoiceSAP" name="Post Invoice in SAP">
      <semantic:incoming>flow_gwInvoiceCorrect_yes</semantic:incoming>
      <semantic:outgoing>flow_postinvoice_to_sendtoAP</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_SendInvoiceToAP" name="Send Invoice to AP Team">
      <semantic:incoming>flow_postinvoice_to_sendtoAP</semantic:incoming>
      <semantic:outgoing>flow_sendtoAP_to_endRequestor</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="end_RequestorInvoiceProcessing" name="Requestor Invoice Processing Completed">
      <semantic:incoming>flow_sendtoAP_to_endRequestor</semantic:incoming>
    </semantic:endEvent>
    <semantic:task id="task_ReturnWrongInvoiceToVendor" name="Return Wrong Invoice to Vendor for Correction">
      <semantic:incoming>flow_gwInvoiceCorrect_no</semantic:incoming>
      <semantic:outgoing>flow_returnInvoice_to_waitCorrected</semantic:outgoing>
    </semantic:task>
    <semantic:intermediateCatchEvent id="event_CorrectedInvoiceReceived" name="Corrected Invoice Received from Vendor">
      <semantic:incoming>flow_returnInvoice_to_waitCorrected</semantic:incoming>
      <semantic:outgoing>flow_correctedInvoice_to_3waymatch</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_Vendor_CorrectedInvoice"/>
    </semantic:intermediateCatchEvent>

    <!-- HoD Lane Elements -->
    <semantic:intermediateCatchEvent id="event_PRReceivedFromRequestor" name="PR Received from Requestor">
      <semantic:outgoing>flow_prreceivedHoD_to_reviewPR</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_Requestor_PRForApproval"/>
    </semantic:intermediateCatchEvent>
    <semantic:task id="task_ReviewPR" name="Review PR (Needs, Budget, Form)">
      <semantic:incoming>flow_prreceivedHoD_to_reviewPR</semantic:incoming>
      <semantic:outgoing>flow_reviewPR_to_docPRdecision</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_DocumentPRDecisionHoD" name="Document Final PR Decision">
      <semantic:incoming>flow_reviewPR_to_docPRdecision</semantic:incoming>
      <semantic:outgoing>flow_docPRdecision_to_gwHoDdecision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_HoDPRDecision" name="HoD PR Decision?">
      <semantic:incoming>flow_docPRdecision_to_gwHoDdecision</semantic:incoming>
      <semantic:outgoing>flow_gwHoDdecision_approved</semantic:outgoing>
      <semantic:outgoing>flow_gwHoDdecision_declined</semantic:outgoing>
      <semantic:outgoing>flow_gwHoDdecision_rejected</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_SendApprovedPRToRequestor" name="Submit Approved PR to Requestor">
      <semantic:incoming>flow_gwHoDdecision_approved</semantic:incoming>
      <semantic:outgoing>flow_sendApprovedPR_to_gwHoDsent</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_SendDeclinedPRToRequestor" name="Submit Declined PR to Requestor">
      <semantic:incoming>flow_gwHoDdecision_declined</semantic:incoming>
      <semantic:outgoing>flow_sendDeclinedPR_to_gwHoDsent</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_SendRejectedPRToRequestor" name="Submit Rejected (Incomplete) PR to Requestor">
      <semantic:incoming>flow_gwHoDdecision_rejected</semantic:incoming>
      <semantic:outgoing>flow_sendRejectedPR_to_gwHoDsent</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_HoDDecisionSent" name="HoD Decision Submitted">
      <semantic:incoming>flow_sendApprovedPR_to_gwHoDsent</semantic:incoming>
      <semantic:incoming>flow_sendDeclinedPR_to_gwHoDsent</semantic:incoming>
      <semantic:incoming>flow_sendRejectedPR_to_gwHoDsent</semantic:incoming>
      <semantic:outgoing>flow_gwHoDsent_to_endHoD</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:endEvent id="end_HoDPRReview" name="HoD PR Review Completed">
      <semantic:incoming>flow_gwHoDsent_to_endHoD</semantic:incoming>
    </semantic:endEvent>

    <!-- PO Approver Lane Elements -->
    <semantic:intermediateCatchEvent id="event_POReceivedForApproval" name="PO Received for Approval">
      <semantic:outgoing>flow_poreceivedAPPR_to_reviewPO</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_Requestor_POForApproval"/>
    </semantic:intermediateCatchEvent>
    <semantic:task id="task_ReviewPOSpecs" name="Review PO Specifications">
      <semantic:incoming>flow_poreceivedAPPR_to_reviewPO</semantic:incoming>
      <semantic:outgoing>flow_reviewPO_to_docPOdecision</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_DocumentPODecision" name="Document Final PO Decision">
      <semantic:incoming>flow_reviewPO_to_docPOdecision</semantic:incoming>
      <semantic:outgoing>flow_docPOdecision_to_gwPOapprdecision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_POApproverDecision" name="PO Approver Decision?">
      <semantic:incoming>flow_docPOdecision_to_gwPOapprdecision</semantic:incoming>
      <semantic:outgoing>flow_gwPOapprdecision_approved</semantic:outgoing>
      <semantic:outgoing>flow_gwPOapprdecision_rejected</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_SendApprovedPOToRequestor" name="Submit Approved PO to Requestor">
      <semantic:incoming>flow_gwPOapprdecision_approved</semantic:incoming>
      <semantic:outgoing>flow_sendApprovedPO_to_gwPOapprsent</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_SendRejectedPOToRequestor" name="Submit Rejected PO to Requestor">
      <semantic:incoming>flow_gwPOapprdecision_rejected</semantic:incoming>
      <semantic:outgoing>flow_sendRejectedPO_to_gwPOapprsent</semantic:outgoing>
    </semantic:task>
