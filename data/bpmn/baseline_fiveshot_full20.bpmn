<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="P2P_Process_Definition"
    targetNamespace="http://example.com/bpmn/p2p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="Collab_P2P">
    <semantic:participant id="Pool_Requestor" name="Requestor" processRef="Process_Requestor"/>
    <semantic:participant id="Pool_HoD" name="Heads of Departments (PR Approvers)" processRef="Process_HoD"/>
    <semantic:participant id="Pool_POApprovers" name="PO Approvers" processRef="Process_POApprovers"/>
    <semantic:participant id="Pool_Vendor" name="Vendor" processRef="Process_Vendor"/>
    <semantic:participant id="Pool_APTeam" name="Accounts Payable (AP) Team" processRef="Process_APTeam"/>

    <semantic:messageFlow id="MsgFlow_PR_ReqToHoD" sourceRef="SendTask_Req_SendPRForApproval" targetRef="ReceiveTask_HoD_ReceivePR"/>
    <semantic:messageFlow id="MsgFlow_PRDecision_HoDToReq_Approved" sourceRef="SendTask_HoD_SendPRApproved" targetRef="ReceiveTask_Req_ReceivePRDecision"/>
    <semantic:messageFlow id="MsgFlow_PRDecision_HoDToReq_Declined" sourceRef="SendTask_HoD_SendPRDeclined" targetRef="ReceiveTask_Req_ReceivePRDecision"/>
    <semantic:messageFlow id="MsgFlow_PRDecision_HoDToReq_RejectedIncomplete" sourceRef="SendTask_HoD_SendPRRejectedIncomplete" targetRef="ReceiveTask_Req_ReceivePRDecision"/>

    <semantic:messageFlow id="MsgFlow_PO_ReqToPOApprover" sourceRef="SendTask_Req_SendPOForApproval" targetRef="ReceiveTask_POApp_ReceivePO"/>
    <semantic:messageFlow id="MsgFlow_PODecision_POAppToReq_Approved" sourceRef="SendTask_POApp_SendPOApproved" targetRef="ReceiveTask_Req_ReceivePODecision"/>
    <semantic:messageFlow id="MsgFlow_PODecision_POAppToReq_Rejected" sourceRef="SendTask_POApp_SendPORejected" targetRef="ReceiveTask_Req_ReceivePODecision"/>

    <semantic:messageFlow id="MsgFlow_PO_ReqToVendor" sourceRef="SendTask_Req_DispatchPOToVendor" targetRef="ReceiveTask_Ven_ReceivePOForReview"/>
    <semantic:messageFlow id="MsgFlow_POEval_VenToReq_Approved" sourceRef="SendTask_Ven_SendPOApprovedDecision" targetRef="ReceiveTask_Req_ReceivePOEvaluationVendor"/>
    <semantic:messageFlow id="MsgFlow_POEval_VenToReq_Negotiated" sourceRef="SendTask_Ven_SendPONegotiatedDecision" targetRef="ReceiveTask_Req_ReceivePOEvaluationVendor"/>
    <semantic:messageFlow id="MsgFlow_POEval_VenToReq_Rejected" sourceRef="SendTask_Ven_SendPORejectedDecision" targetRef="ReceiveTask_Req_ReceivePOEvaluationVendor"/>
    <semantic:messageFlow id="MsgFlow_Invoice_VenToReq" sourceRef="SendTask_Ven_SendInvoice" targetRef="ReceiveTask_Req_ReceiveInvoice"/>

    <semantic:messageFlow id="MsgFlow_WrongInvoice_ReqToVen" sourceRef="SendTask_Req_ReturnWrongInvoice" targetRef="ReceiveTask_Ven_ReceiveWrongInvoice"/>
    <semantic:messageFlow id="MsgFlow_CorrectedInvoice_VenToReq" sourceRef="SendTask_Ven_SendCorrectedInvoice" targetRef="ReceiveTask_Req_ReceiveCorrectedInvoice"/>

    <semantic:messageFlow id="MsgFlow_Invoice_ReqToAP" sourceRef="SendTask_Req_SendInvoiceToAP" targetRef="ReceiveTask_AP_ReceiveInvoice"/>
  </semantic:collaboration>

  <!-- Process for Requestor -->
  <semantic:process id="Process_Requestor" isExecutable="true">
    <semantic:startEvent id="Start_Req_NeedIdentified" name="Need Identified">
      <semantic:outgoing>Flow_Req_StartToDefineNeeds</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="Task_Req_DefineNeeds" name="Determine and Define Business Requirements &amp; Needs">
      <semantic:incoming>Flow_Req_StartToDefineNeeds</semantic:incoming>
      <semantic:incoming>Flow_Req_PRDeclinedToDefineNeeds</semantic:incoming>
      <semantic:incoming>Flow_Req_PONotApprovedToDefineNeeds</semantic:incoming>
      <semantic:outgoing>Flow_Req_DefineNeedsToCreatePR</semantic:outgoing>
    </semantic:task>
    <semantic:task id="Task_Req_CreatePR" name="Create Purchase Requisition (PR)">
      <semantic:incoming>Flow_Req_DefineNeedsToCreatePR</semantic:incoming>
      <semantic:incoming>Flow_Req_PRRejectedIncompleteToCorrectPR</semantic:incoming> <!-- Assuming correction leads to re-creation or update -->
      <semantic:outgoing>Flow_Req_CreatePRToSendPRApproval</semantic:outgoing>
    </semantic:task>
    <semantic:sendTask id="SendTask_Req_SendPRForApproval" name="Send PR for Approval">
      <semantic:incoming>Flow_Req_CreatePRToSendPRApproval</semantic:incoming>
      <semantic:outgoing>Flow_Req_SendPRApprovalToReceivePRDecision</semantic:outgoing>
    </semantic:sendTask>
    <semantic:receiveTask id="ReceiveTask_Req_ReceivePRDecision" name="Receive PR Decision from HoD">
      <semantic:incoming>Flow_Req_SendPRApprovalToReceivePRDecision</semantic:incoming>
      <semantic:outgoing>Flow_Req_ReceivePRDecisionToGWPRStatus</semantic:outgoing>
    </semantic:receiveTask>
    <semantic:exclusiveGateway id="GW_Req_PRStatusHoD" name="PR Status from HoD?">
      <semantic:incoming>Flow_Req_ReceivePRDecisionToGWPRStatus</semantic:incoming>
      <semantic:outgoing>Flow_Req_PRApproved</semantic:outgoing>
      <semantic:outgoing>Flow_Req_PRDeclined</semantic:outgoing>
      <semantic:outgoing>Flow_Req_PRRejectedIncomplete</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:sequenceFlow id="Flow_Req_PRApproved" name="Approved" sourceRef="GW_Req_PRStatusHoD" targetRef="Task_Req_CreatePO"/>
    <semantic:sequenceFlow id="Flow_Req_PRDeclined" name="Declined" sourceRef="GW_Req_PRStatusHoD" targetRef="Task_Req_DefineNeeds"/>
    <semantic:sequenceFlow id="Flow_Req_PRRejectedIncomplete" name="Rejected (Incomplete)" sourceRef="GW_Req_PRStatusHoD" targetRef="Task_Req_CreatePR"/> <!-- Simplified: Re-do PR creation/correction -->

    <semantic:task id="Task_Req_CreatePO" name="Create Purchase Order (PO)">
      <semantic:incoming>Flow_Req_PRApproved</semantic:incoming>
      <semantic:outgoing>Flow_Req_CreatePOToSendPOApproval</semantic:outgoing>
    </semantic:task>
    <semantic:sendTask id="SendTask_Req_SendPOForApproval" name="Send PO for Approval">
      <semantic:incoming>Flow_Req_CreatePOToSendPOApproval</semantic:incoming>
      <semantic:outgoing>Flow_Req_SendPOApprovalToReceivePODecision</semantic:outgoing>
    </semantic:sendTask>
    <semantic:receiveTask id="ReceiveTask_Req_ReceivePODecision" name="Receive PO Decision from PO Approvers">
      <semantic:incoming>Flow_Req_SendPOApprovalToReceivePODecision</semantic:incoming>
      <semantic:outgoing>Flow_Req_ReceivePODecisionToGWPOApproved</semantic:outgoing>
    </semantic:receiveTask>
    <semantic:exclusiveGateway id="GW_Req_POApproved" name="PO Approved by PO Approvers?">
      <semantic:incoming>Flow_Req_ReceivePODecisionToGWPOApproved</semantic:incoming>
      <semantic:outgoing>Flow_Req_POApproved_Yes</semantic:outgoing>
      <semantic:outgoing>Flow_Req_POApproved_No</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:sequenceFlow id="Flow_Req_POApproved_Yes" name="Yes" sourceRef="GW_Req_POApproved" targetRef="SendTask_Req_DispatchPOToVendor"/>
    <semantic:sequenceFlow id="Flow_Req_POApproved_No" name="No" sourceRef="GW_Req_POApproved" targetRef="Task_Req_DefineNeeds"/>

    <semantic:sendTask id="SendTask_Req_DispatchPOToVendor" name="Dispatch Approved PO to Vendor">
      <semantic:incoming>Flow_Req_POApproved_Yes</semantic:incoming>
      <semantic:incoming>Flow_Req_SearchVendorToDispatchPO</semantic:incoming>
      <semantic:outgoing>Flow_Req_DispatchPOToReceiveVendorEval</semantic:outgoing>
    </semantic:sendTask>
    <semantic:receiveTask id="ReceiveTask_Req_ReceivePOEvaluationVendor" name="Receive PO Evaluation from Vendor">
      <semantic:incoming>Flow_Req_DispatchPOToReceiveVendorEval</semantic:incoming>
      <semantic:outgoing>Flow_Req_ReceiveVendorEvalToGWVendorAction</semantic:outgoing>
    </semantic:receiveTask>
    <semantic:exclusiveGateway id="GW_Req_VendorPOAction" name="Vendor Action on PO?">
      <semantic:incoming>Flow_Req_ReceiveVendorEvalToGWVendorAction</semantic:incoming>
      <semantic:outgoing>Flow_Req_VendorApprovedOrNegotiated</semantic:outgoing>
      <semantic:outgoing>Flow_Req_VendorRejected</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:sequenceFlow id="Flow_Req_VendorApprovedOrNegotiated" name="Approved/Negotiated" sourceRef="GW_Req_VendorPOAction" targetRef="Task_Req_ReceiveGoodsServices"/>
    <semantic:sequenceFlow id="Flow_Req_VendorRejected" name="Rejected" sourceRef="GW_Req_VendorPOAction" targetRef="Task_Req_SearchNewVendor"/>

    <semantic:task id="Task_Req_SearchNewVendor" name="Search for New Vendor">
      <semantic:incoming>Flow_Req_VendorRejected</semantic:incoming>
      <semantic:outgoing>Flow_Req_SearchVendorToDispatchPO</semantic:outgoing>
    </semantic:task>

    <semantic:task id="Task_Req_ReceiveGoodsServices" name="Receive Agreed Goods/Services">
      <semantic:incoming>Flow_Req_VendorApprovedOrNegotiated</semantic:incoming>
      <semantic:outgoing>Flow_Req_ReceiveGoodsToGWGoodOrService</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW_Req_GoodOrService" name="Goods or Service Received?">
      <semantic:incoming>Flow_Req_ReceiveGoodsToGWGoodOrService</semantic:incoming>
      <semantic:outgoing>Flow_Req_GoodsReceived</semantic:outgoing>
      <semantic:outgoing>Flow_Req_ServiceReceived</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="Task_Req_IssueGR" name="Issue Goods Receipt (GR)">
      <semantic:incoming>Flow_Req_GoodsReceived</semantic:incoming>
      <semantic:outgoing>Flow_Req_IssueGRToJoinReceipt</semantic:outgoing>
    </semantic:task>
    <semantic:task id="Task_Req_IssueSES" name="Issue Service Entry Sheet (SES)">
      <semantic:incoming>Flow_Req_ServiceReceived</semantic:incoming>
      <semantic:outgoing>Flow_Req_IssueSESToJoinReceipt</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="Join_Req_ReceiptConfirmed" name="Receipt Confirmed"> <!-- Implicit Join -->
      <semantic:incoming>Flow_Req_IssueGRToJoinReceipt</semantic:incoming>
      <semantic:incoming>Flow_Req_IssueSESToJoinReceipt</semantic:incoming>
      <semantic:outgoing>Flow_Req_JoinReceiptToReceiveInvoice</semantic:outgoing>
    </semantic:exclusiveGateway>

    <semantic:receiveTask id="ReceiveTask_Req_ReceiveInvoice" name="Receive Invoice from Vendor">
      <semantic:incoming>Flow_Req_JoinReceiptToReceiveInvoice</semantic:incoming>
      <semantic:outgoing>Flow_Req_ReceiveInvoiceToThreeWayMatch</semantic:outgoing>
    </semantic:receiveTask>
    <semantic:task id="Task_Req_ThreeWayMatch" name="Perform Three-Way Match">
      <semantic:incoming>Flow_Req_ReceiveInvoiceToThreeWayMatch</semantic:incoming>
      <semantic:incoming>Flow_Req_ReceiveCorrectedInvoiceToThreeWayMatch</semantic:incoming>
      <semantic:outgoing>Flow_Req_ThreeWayMatchToGWInvoiceCorrect</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW_Req_InvoiceCorrect" name="Invoice Correct?">
      <semantic:incoming>Flow_Req_ThreeWayMatchToGWInvoiceCorrect</semantic:incoming>
      <semantic:outgoing>Flow_Req_InvoiceCorrect_Yes</semantic:outgoing>
      <semantic:outgoing>Flow_Req_InvoiceCorrect_No</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:sequenceFlow id="Flow_Req_InvoiceCorrect_Yes" name="Yes" sourceRef="GW_Req_InvoiceCorrect" targetRef="Task_Req_PostInvoice"/>
    <semantic:sequenceFlow id="Flow_Req_InvoiceCorrect_No" name="No (Wrong)" sourceRef="GW_Req_InvoiceCorrect" targetRef="SendTask_Req_ReturnWrongInvoice"/>

    <semantic:sendTask id="SendTask_Req_ReturnWrongInvoice" name="Return Invoice to Vendor for Correction">
      <semantic:incoming>Flow_Req_InvoiceCorrect_No</semantic:incoming>
      <semantic:outgoing>Flow_Req_ReturnWrongInvoiceToReceiveCorrected</semantic:outgoing>
    </semantic:sendTask>
    <semantic:receiveTask id="ReceiveTask_Req_ReceiveCorrectedInvoice" name="Receive Corrected Invoice from Vendor">
      <semantic:incoming>Flow_Req_ReturnWrongInvoiceToReceiveCorrected</semantic:incoming>
      <semantic:outgoing>Flow_Req_ReceiveCorrectedInvoiceToThreeWayMatch</semantic:outgoing>
    </semantic:receiveTask>

    <semantic:task id="Task_Req_PostInvoice" name="Post Invoice in SAP">
      <semantic:incoming>Flow_Req_InvoiceCorrect_Yes</semantic:incoming>
      <semantic:outgoing>Flow_Req_PostInvoiceToSendToAP</semantic:outgoing>
    </semantic:task>
    <semantic:sendTask id="SendTask_Req_SendInvoiceToAP" name="Send Invoice to AP Team">
      <semantic:incoming>Flow_Req_PostInvoiceToSendToAP</semantic:incoming>
      <semantic:outgoing>Flow_Req_SendInvoiceToAPToEnd</semantic:outgoing>
    </semantic:sendTask>
    <semantic:endEvent id="End_Req_InvoiceSentToAP" name="Invoice Processed and Sent to AP">
      <semantic:incoming>Flow_Req_SendInvoiceToAPToEnd</semantic:incoming>
    </semantic:endEvent>

    <semantic:sequenceFlow id="Flow_Req_StartToDefineNeeds" sourceRef="Start_Req_NeedIdentified" targetRef="Task_Req_DefineNeeds"/>
    <semantic:sequenceFlow id="Flow_Req_DefineNeedsToCreatePR" sourceRef="Task_Req_DefineNeeds" targetRef="Task_Req_CreatePR"/>
    <semantic:sequenceFlow id="Flow_Req_CreatePRToSendPRApproval" sourceRef="Task_Req_CreatePR" targetRef="SendTask_Req_SendPRForApproval"/>
    <semantic:sequenceFlow id="Flow_Req_SendPRApprovalToReceivePRDecision" sourceRef="SendTask_Req_SendPRForApproval" targetRef="ReceiveTask_Req_ReceivePRDecision"/>
    <semantic:sequenceFlow id="Flow_Req_ReceivePRDecisionToGWPRStatus" sourceRef="ReceiveTask_Req_ReceivePRDecision" targetRef="GW_Req_PRStatusHoD"/>
    <semantic:sequenceFlow id="Flow_Req_PRDeclinedToDefineNeeds" sourceRef="GW_Req_PRStatusHoD" targetRef="Task_Req_DefineNeeds"/>
    <semantic:sequenceFlow id="Flow_Req_PRRejectedIncompleteToCorrectPR" sourceRef="GW_Req_PRStatusHoD" targetRef="Task_Req_CreatePR"/>
    <semantic:sequenceFlow id="Flow_Req_CreatePOToSendPOApproval" sourceRef="Task_Req_CreatePO" targetRef="SendTask_Req_SendPOForApproval"/>
    <semantic:sequenceFlow id="Flow_Req_SendPOApprovalToReceivePODecision" sourceRef="SendTask_Req_SendPOForApproval" targetRef="ReceiveTask_Req_ReceivePODecision"/>
    <semantic:sequenceFlow id="Flow_Req_ReceivePODecisionToGWPOApproved" sourceRef="ReceiveTask_Req_ReceivePODecision" targetRef="GW_Req_POApproved"/>
    <semantic:sequenceFlow id="Flow_Req_PONotApprovedToDefineNeeds" sourceRef="GW_Req_POApproved" targetRef="Task_Req_DefineNeeds"/>
    <semantic:sequenceFlow id="Flow_Req_DispatchPOToReceiveVendorEval" sourceRef="SendTask_Req_DispatchPOToVendor" targetRef="ReceiveTask_Req_ReceivePOEvaluationVendor"/>
    <semantic:sequenceFlow id="Flow_Req_ReceiveVendorEvalToGWVendorAction" sourceRef="ReceiveTask_Req_ReceivePOEvaluationVendor" targetRef="GW_Req_VendorPOAction"/>
    <semantic:sequenceFlow id="Flow_Req_SearchVendorToDispatchPO" sourceRef="Task_Req_SearchNewVendor" targetRef="SendTask_Req_DispatchPOToVendor"/>
    <semantic:sequenceFlow id="Flow_Req_ReceiveGoodsToGWGoodOrService" sourceRef="Task_Req_ReceiveGoodsServices" targetRef="GW_Req_GoodOrService"/>
    <semantic:sequenceFlow id="Flow_Req_GoodsReceived" name="Goods" sourceRef="GW_Req_GoodOrService" targetRef="Task_Req_IssueGR"/>
    <semantic:sequenceFlow id="Flow_Req_ServiceReceived" name="Service" sourceRef="GW_Req_GoodOrService" targetRef="Task_Req_IssueSES"/>
    <semantic:sequenceFlow id="Flow_Req_IssueGRToJoinReceipt" sourceRef="Task_Req_IssueGR" targetRef="Join_Req_ReceiptConfirmed"/>
    <semantic:sequenceFlow id="Flow_Req_IssueSESToJoinReceipt" sourceRef="Task_Req_IssueSES" targetRef="Join_Req_ReceiptConfirmed"/>
    <semantic:sequenceFlow id="Flow_Req_JoinReceiptToReceiveInvoice" sourceRef="Join_Req_ReceiptConfirmed" targetRef="ReceiveTask_Req_ReceiveInvoice"/>
    <semantic:sequenceFlow id="Flow_Req_ReceiveInvoiceToThreeWayMatch" sourceRef="ReceiveTask_Req_ReceiveInvoice" targetRef="Task_Req_ThreeWayMatch"/>
    <semantic:sequenceFlow id="Flow_Req_ThreeWayMatchToGWInvoiceCorrect" sourceRef="Task_Req_ThreeWayMatch" targetRef="GW_Req_InvoiceCorrect"/>
    <semantic:sequenceFlow id="Flow_Req_ReturnWrongInvoiceToReceiveCorrected" sourceRef="SendTask_Req_ReturnWrongInvoice" targetRef="ReceiveTask_Req_ReceiveCorrectedInvoice"/>
    <semantic:sequenceFlow id="Flow_Req_ReceiveCorrectedInvoiceToThreeWayMatch" sourceRef="ReceiveTask_Req_ReceiveCorrectedInvoice" targetRef="Task_Req_ThreeWayMatch"/>
    <semantic:sequenceFlow id="Flow_Req_PostInvoiceToSendToAP" sourceRef="Task_Req_PostInvoice" targetRef="SendTask_Req_SendInvoiceToAP"/>
    <semantic:sequenceFlow id="Flow_Req_SendInvoiceToAPToEnd" sourceRef="SendTask_Req_SendInvoiceToAP" targetRef="End_Req_InvoiceSentToAP"/>
  </semantic:process>

  <!-- Process for Heads of Departments -->
  <semantic:process id="Process_HoD" isExecutable="true">
    <semantic:receiveTask id="ReceiveTask_HoD_ReceivePR" name="Receive PR for Approval">
      <semantic:outgoing>Flow_HoD_ReceivePRToReviewPR</semantic:outgoing>
    </semantic:receiveTask>
    <semantic:task id="Task_HoD_ReviewPR" name="Review PR (Evaluate Needs, Verify Budget, Validate Form)">
      <semantic:incoming>Flow_HoD_ReceivePRToReviewPR</semantic:incoming>
      <semantic:outgoing>Flow_HoD_ReviewPRToDocumentDecision</semantic:outgoing>
    </semantic:task>
    <semantic:task id="Task_HoD_DocumentDecision" name="Document PR Decision">
      <semantic:incoming>Flow_HoD_ReviewPRToDocumentDecision</semantic:incoming>
      <semantic:outgoing>Flow_HoD_DocumentDecisionToGWPRDecision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW_HoD_PRDecision" name="PR Decision?">
      <semantic:incoming>Flow_HoD_DocumentDecisionToGWPRDecision</semantic:incoming>
      <semantic:outgoing>Flow_HoD_PRApproved</semantic:outgoing>
      <semantic:outgoing>Flow_HoD_PRDeclined</semantic:outgoing>
      <semantic:outgoing>Flow_HoD_PRRejectedIncomplete</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:sendTask id="SendTask_HoD_SendPRApproved" name="Send 'PR Approved' Decision">
      <semantic:incoming>Flow_HoD_PRApproved</semantic:incoming>
      <semantic:outgoing>Flow_HoD_SendApprovedToEnd</semantic:outgoing>
    </semantic:sendTask>
    <semantic:sendTask id="SendTask_HoD_SendPRDeclined" name="Send 'PR Declined' Decision">
      <semantic:incoming>Flow_HoD_PRDeclined</semantic:incoming>
      <semantic:outgoing>Flow_HoD_SendDeclinedToEnd</semantic:outgoing>
    </semantic:sendTask>
    <semantic:sendTask id="SendTask_HoD_SendPRRejectedIncomplete" name="Send 'PR Rejected (Incomplete)' Decision">
      <semantic:incoming>Flow_HoD_PRRejectedIncomplete</semantic:incoming>
      <semantic:outgoing>Flow_HoD_SendRejectedIncompleteToEnd</semantic:outgoing>
    </semantic:sendTask>
    <semantic:endEvent id="End_HoD_PRReviewProcessEnded" name="PR Review Process Ended">
      <semantic:incoming>Flow_HoD_SendApprovedToEnd</semantic:incoming>
      <semantic:incoming>Flow_HoD_SendDeclinedToEnd</semantic:incoming>
      <semantic:incoming>Flow_HoD_SendRejectedIncompleteToEnd</semantic:incoming>
    </semantic:endEvent>

    <semantic:sequenceFlow id="Flow_HoD_ReceivePRToReviewPR" sourceRef="ReceiveTask_HoD_ReceivePR" targetRef="Task_HoD_ReviewPR"/>
    <semantic:sequenceFlow id="Flow_HoD_ReviewPRToDocumentDecision" sourceRef="Task_HoD_ReviewPR" targetRef="Task_HoD_DocumentDecision"/>
    <semantic:sequenceFlow id="Flow_HoD_DocumentDecisionToGWPRDecision" sourceRef="Task_HoD_DocumentDecision" targetRef="GW_HoD_PRDecision"/>
    <semantic:sequenceFlow id="Flow_HoD_PRApproved" name="Approve" sourceRef="GW_HoD_PRDecision" targetRef="SendTask_HoD_SendPRApproved"/>
    <semantic:sequenceFlow id="Flow_HoD_PRDeclined" name="Decline" sourceRef="GW_HoD_PRDecision" targetRef="SendTask_HoD_SendPRDeclined"/>
    <semantic:sequenceFlow id="Flow_HoD_PRRejectedIncomplete" name="Reject (Incomplete)" sourceRef="GW_HoD_PRDecision" targetRef="SendTask_HoD_SendPRRejectedIncomplete"/>
    <semantic:sequenceFlow id="Flow_HoD_SendApprovedToEnd" sourceRef="SendTask_HoD_SendPRApproved" targetRef="End_HoD_PRReviewProcessEnded"/>
    <semantic:sequenceFlow id="Flow_HoD_SendDeclinedToEnd" sourceRef="SendTask_HoD_SendPRDeclined" targetRef="End_HoD_PRReviewProcessEnded"/>
    <semantic:sequenceFlow id="Flow_HoD_SendRejectedIncompleteToEnd" sourceRef="SendTask_HoD_SendPRRejectedIncomplete" targetRef="End_HoD_PRReviewProcessEnded"/>
  </semantic:process>

  <!-- Process for PO Approvers -->
  <semantic:process id="Process_POApprovers" isExecutable="true">
    <semantic:receiveTask id="ReceiveTask_POApp_ReceivePO" name="Receive PO for Approval">
      <semantic:outgoing>Flow_POApp_ReceivePOToReviewPO</semantic:outgoing>
    </semantic:receiveTask>
    <semantic:task id="Task_POApp_ReviewPO" name="Review Purchase Order Specifications">
      <semantic:incoming>Flow_POApp_ReceivePOToReviewPO</semantic:incoming>
      <semantic:outgoing>Flow_POApp_ReviewPOToDocumentPODecision</semantic:outgoing>
    </semantic:task>
    <semantic:task id="Task_POApp_DocumentPODecision" name="Document PO Decision">
      <semantic:incoming>Flow_POApp_ReviewPOToDocumentPODecision</semantic:incoming>
      <semantic:outgoing>Flow_POApp_DocumentPODecisionToGWPODecision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW_POApp_PODecision" name="PO Decision?">
      <semantic:incoming>Flow_POApp_DocumentPODecisionToGWPODecision</semantic:incoming>
      <semantic:outgoing>Flow_POApp_POApproved</semantic:outgoing>
      <semantic:outgoing>Flow_POApp_PORejected</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:sendTask id="SendTask_POApp_SendPOApproved" name="Send 'PO Approved' Decision">
      <semantic:incoming>Flow_POApp_POApproved</semantic:incoming>
      <semantic:outgoing>Flow_POApp_SendApprovedToEnd</semantic:outgoing>
    </semantic:sendTask>
    <semantic:sendTask id="SendTask_POApp_SendPORejected" name="Send 'PO Rejected' Decision">
      <semantic:incoming>Flow_POApp_PORejected</semantic:incoming>
      <semantic:outgoing>Flow_POApp_SendRejectedToEnd</semantic:outgoing>
    </semantic:sendTask>
    <semantic:endEvent id="End_POApp_POApprovalProcessEnded" name="PO Approval Process Ended">
      <semantic:incoming>Flow_POApp_SendApprovedToEnd</semantic:incoming>
      <semantic:incoming>Flow_POApp_SendRejectedToEnd</semantic:incoming>
    </semantic:endEvent>

    <semantic:sequenceFlow id="Flow_POApp_ReceivePOToReviewPO" sourceRef="ReceiveTask_POApp_ReceivePO" targetRef="Task_POApp_ReviewPO"/>
    <semantic:sequenceFlow id="Flow_POApp_ReviewPOToDocumentPODecision" sourceRef="Task_POApp_ReviewPO" targetRef="Task_POApp_DocumentPODecision"/>
    <semantic:sequenceFlow id="Flow_POApp_DocumentPODecisionToGWPODecision" sourceRef="Task_POApp_DocumentPODecision" targetRef="GW_POApp_PODecision"/>
    <semantic:sequenceFlow id="Flow_POApp_POApproved" name="Approve" sourceRef="GW_POApp_PODecision" targetRef="SendTask_POApp_SendPOApproved"/>
    <semantic:sequenceFlow id="Flow_POApp_PORejected" name="Reject" sourceRef="GW_POApp_PODecision" targetRef="SendTask_POApp_SendPORejected"/>
    <semantic:sequenceFlow id="Flow_POApp_SendApprovedToEnd" sourceRef="SendTask_POApp_SendPOApproved" targetRef="End_POApp_POApprovalProcessEnded"/>
    <semantic:sequenceFlow id="Flow_POApp_SendRejectedToEnd" sourceRef="SendTask_POApp_SendPORejected" targetRef="End_POApp_POApprovalProcessEnded"/>
  </semantic:process>

  <!-- Process for Vendor -->
  <semantic:process id="Process_Vendor" isExecutable="true">
    <semantic:receiveTask id="ReceiveTask_Ven_ReceivePOForReview" name="Receive PO for Review">
      <semantic:outgoing>Flow_Ven_ReceivePOToEvaluatePO</semantic:outgoing>
    </semantic:receiveTask>
    <semantic:task id="Task_Ven_EvaluatePO" name="Evaluate PO">
      <semantic:incoming>Flow_Ven_ReceivePOToEvaluatePO</semantic:incoming>
      <semantic:outgoing>Flow_Ven_EvaluatePOToDocumentPODecision</semantic:outgoing>
    </semantic:task>
    <semantic:task id="Task_Ven_DocumentPODecision" name="Document PO Decision">
      <semantic:incoming>Flow_Ven_EvaluatePOToDocumentPODecision</semantic:incoming>
      <semantic:outgoing>Flow_Ven_DocumentPODecisionToGWPODecision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="GW_Ven_PODecision" name="Vendor PO Decision?">
      <semantic:incoming>Flow_Ven_DocumentPODecisionToGWPODecision</semantic:incoming>
      <semantic:outgoing>Flow_Ven_POApproved</semantic:outgoing>
      <semantic:outgoing>Flow_Ven_PONegotiated</semantic:outgoing>
      <semantic:outgoing>Flow_Ven_PORejected</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:sendTask id="SendTask_Ven_SendPOApprovedDecision" name="Send 'PO Approved' Decision">
      <semantic:incoming>Flow_Ven_POApproved</semantic:incoming>
      <semantic:outgoing>Flow_Ven_SendApprovedToDeliver</semantic:outgoing>
    </semantic:sendTask>
    <semantic:sendTask id="SendTask_Ven_SendPONegotiatedDecision" name="Send 'PO Negotiated' Decision">
      <semantic:incoming>Flow_Ven_PONegotiated</semantic:incoming>
      <semantic:outgoing>Flow_Ven_SendNegotiatedToDeliver</semantic:outgoing>
    </semantic:sendTask>
    <semantic:sendTask id="SendTask_Ven_SendPORejectedDecision" name="Send 'PO Rejected' Decision">
      <semantic:incoming>Flow_Ven_PORejected</semantic:incoming>
      <semantic:outgoing>Flow_Ven_SendRejectedToEndPORejected</semantic:outgoing>
    </semantic:sendTask>
    <semantic:endEvent id="End_Ven_PORejectedByVendor" name="PO Rejected by Vendor">
      <semantic:incoming>Flow_Ven_SendRejectedToEndPORejected</semantic:incoming>
    </semantic:endEvent>

    <semantic:task id="Task_Ven_DeliverGoodsServices" name="Deliver Agreed Goods/Services">
      <semantic:incoming>Flow_Ven_SendApprovedToDeliver</semantic:incoming>
      <semantic:incoming>Flow_Ven_SendNegotiatedToDeliver</semantic:incoming>
      <semantic:outgoing>Flow_Ven_DeliverToInvoice</semantic:outgoing>
    </semantic:task>
    <semantic:sendTask id="SendTask_Ven_SendInvoice" name="Send Invoice for Delivery">
      <semantic:incoming>Flow_Ven_DeliverToInvoice</semantic:incoming>
      <semantic:outgoing>Flow_Ven_SendInvoiceToEndDelivery</semantic:outgoing>
    </semantic:sendTask>
    <semantic:endEvent id="End_Ven_DeliveryAndInvoicingComplete" name="Delivery and Invoicing Complete">
      <semantic:incoming>Flow_Ven_SendInvoiceToEndDelivery</semantic:incoming>
    </semantic:endEvent>

    <!-- Invoice Correction Sub-Process -->
    <semantic:receiveTask id="ReceiveTask_Ven_ReceiveWrongInvoice" name="Receive Wrong Invoice for Correction">
      <semantic:outgoing>Flow_Ven_ReceiveWrongInvoiceToCorrect</semantic:outgoing>
    </semantic:receiveTask>
    <semantic:task id="Task_Ven_CorrectInvoice" name="Correct Invoice">
      <semantic:incoming>Flow_Ven_ReceiveWrongInvoiceToCorrect</semantic:incoming>
      <semantic:outgoing>Flow_Ven_CorrectInvoiceToSendCorrected</semantic:outgoing>
    </semantic:task>
    <semantic:sendTask id="SendTask_Ven_SendCorrectedInvoice" name="Send Corrected Invoice to Requestor">
      <semantic:incoming>Flow_Ven_CorrectInvoiceToSendCorrected</semantic:incoming>
      <semantic:outgoing>Flow_Ven_SendCorrectedInvoiceToEndCorrection</semantic:outgoing>
    </semantic:sendTask>
    <semantic:endEvent id="End_Ven_InvoiceCorrectedAndResent" name="Invoice Corrected and Resent">
      <semantic:incoming>Flow_Ven_SendCorrectedInvoiceToEndCorrection</semantic:incoming>
    </semantic:endEvent>

    <semantic:sequenceFlow id="Flow_Ven_ReceivePOToEvaluatePO" sourceRef="ReceiveTask_Ven_ReceivePOForReview" targetRef="Task_Ven_EvaluatePO"/>
    <semantic:sequenceFlow id="Flow_Ven_EvaluatePOToDocumentPODecision" sourceRef="Task_Ven_EvaluatePO" targetRef="Task_Ven_DocumentPODecision"/>
    <semantic:sequenceFlow id="Flow_Ven_DocumentPODecisionToGWPODecision" sourceRef="Task_Ven_DocumentPODecision" targetRef="GW_Ven_PODecision"/>
    <semantic:sequenceFlow id="Flow_Ven_POApproved" name="Approve" sourceRef="GW_Ven_PODecision" targetRef="SendTask_Ven_SendPOApprovedDecision"/>
    <semantic:sequenceFlow id="Flow_Ven_PONegotiated" name="Negotiate" sourceRef="GW_Ven_PODecision" targetRef="SendTask_Ven_SendPONegotiatedDecision"/>
    <semantic:sequenceFlow id="Flow_Ven_PORejected" name="Reject" sourceRef="GW_Ven_PODecision" targetRef="SendTask_Ven_SendPORejectedDecision"/>
    <semantic:sequenceFlow id="Flow_Ven_SendApprovedToDeliver" sourceRef="SendTask_Ven_SendPOApprovedDecision" targetRef="Task_Ven_DeliverGoodsServices"/>
    <semantic:sequenceFlow id="Flow_Ven_SendNegotiatedToDeliver" sourceRef="SendTask_Ven_SendPONegotiatedDecision" targetRef="Task_Ven_DeliverGoodsServices"/>
    <semantic:sequenceFlow id="Flow_Ven_SendRejectedToEndPORejected" sourceRef="SendTask_Ven_SendPORejectedDecision" targetRef="End_Ven_PORejectedByVendor"/>
    <semantic:sequenceFlow id="Flow_Ven_DeliverToInvoice" sourceRef="Task_Ven_DeliverGoodsServices" targetRef="SendTask_Ven_SendInvoice"/>
    <semantic:sequenceFlow id="Flow_Ven_SendInvoiceToEndDelivery" sourceRef="SendTask_Ven_SendInvoice" targetRef="End_Ven_DeliveryAndInvoicingComplete"/>
    <semantic:sequenceFlow id="Flow_Ven_ReceiveWrongInvoiceToCorrect" sourceRef="ReceiveTask_Ven_ReceiveWrongInvoice" targetRef="Task_Ven_CorrectInvoice"/>
    <semantic:sequenceFlow id="Flow_Ven_CorrectInvoiceToSendCorrected" sourceRef="Task_Ven_CorrectInvoice" targetRef="SendTask_Ven_SendCorrectedInvoice"/>
    <semantic:sequenceFlow id="Flow_Ven_SendCorrectedInvoiceToEndCorrection" sourceRef="SendTask_Ven_SendCorrectedInvoice" targetRef="End_Ven_InvoiceCorrectedAndResent"/>
  </semantic:process>

  <!-- Process for Accounts Payable Team -->
  <semantic:process id="Process_APTeam" isExecutable="true">
    <semantic:receiveTask id="ReceiveTask_AP_ReceiveInvoice" name="Receive Invoice from Requestor">
      <semantic:outgoing>Flow_AP_ReceiveInvoiceToDecidePayment</semantic:outgoing>
    </semantic:receiveTask>
    <semantic:userTask id="Task_AP_DecidePaymentMethod" name="Decide on Payment Method">
      <semantic:incoming>Flow_AP_ReceiveInvoiceToDecidePayment</semantic:incoming>
      <semantic:outgoing>Flow_AP_DecidePaymentToGWPaymentMethod</semantic:outgoing>
    </semantic:userTask>
    <semantic:exclusiveGateway id="GW_AP_PaymentMethod" name="Payment Method?">
      <semantic:incoming>Flow_AP_DecidePaymentToGWPaymentMethod</semantic:incoming>
      <semantic:outgoing>Flow_AP_PayFull</semantic:outgoing>
      <semantic:outgoing>Flow_AP_PayInstallments</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="Task_AP_PayFull" name="Perform Payment to Vendor (Full)">
      <semantic:incoming>Flow_AP_PayFull</semantic:incoming>
      <semantic:outgoing>Flow_AP_PaymentToJoin</semantic:outgoing>
    </semantic:task>
    <semantic:task id="Task_AP_PayInstallments" name="Perform Payment to Vendor (Installments)">
      <semantic:incoming>Flow_AP_PayInstallments</semantic:incoming>
      <semantic:outgoing>Flow_AP_PaymentToJoin_Install</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="Join_AP_PaymentPerformed" name="Payment Performed"> <!-- Implicit Join -->
      <semantic:incoming>Flow_AP_PaymentToJoin</semantic:incoming>
      <semantic:incoming>Flow_AP_PaymentToJoin_Install</semantic:incoming>
      <semantic:outgoing>Flow_AP_JoinToP2PComplete</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:endEvent id="End_AP_P2PComplete" name="P2P Process Successfully Completed">
      <semantic:incoming>Flow_AP_JoinToP2PComplete</semantic:incoming>
    </semantic:endEvent>

    <semantic:sequenceFlow id="Flow_AP_ReceiveInvoiceToDecidePayment" sourceRef="ReceiveTask_AP_ReceiveInvoice" targetRef="Task_AP_DecidePaymentMethod"/>
    <semantic:sequenceFlow id="Flow_AP_DecidePaymentToGWPaymentMethod" sourceRef="Task_AP_DecidePaymentMethod" targetRef="GW_AP_PaymentMethod"/>
    <semantic:sequenceFlow id="Flow_AP_PayFull" name="Full" sourceRef="GW_AP_PaymentMethod" targetRef="Task_AP_PayFull"/>
    <semantic:sequenceFlow id="Flow_AP_PayInstallments" name="Installments" sourceRef="GW_AP_PaymentMethod" targetRef="Task_AP_PayInstallments"/>
    <semantic:sequenceFlow id="Flow_AP_PaymentToJoin" sourceRef="Task_AP_PayFull" targetRef="Join_AP_PaymentPerformed"/>
    <semantic:sequenceFlow id="Flow_AP_PaymentToJoin_Install" sourceRef="Task_AP_PayInstallments" targetRef="Join_AP_PaymentPerformed"/>
    <semantic:sequenceFlow id="Flow_AP_JoinToP2PComplete" sourceRef="Join_AP_PaymentPerformed" targetRef="End_AP_P2PComplete"/>
  </semantic:process>

