        <semantic:conditionExpression xsi:type="semantic:tFormalExpression">Vendor Rejected PO</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="flow_seq_25" name="New vendor found, dispatch same PO" sourceRef="task_req_8_search_new_vendor" targetRef="task_req_7_dispatch_po_to_vendor"/>
    <semantic:sequenceFlow id="flow_seq_26" name="Vendor Approved/Negotiated PO" sourceRef="gw_req_4_vendor_po_outcome" targetRef="task_req_9_receive_goods_services">
