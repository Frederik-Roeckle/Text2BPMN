      <semantic:errorEventDefinition errorRef="DATA_BREACH_ERROR"/>
    </semantic:boundaryEvent>
    <semantic:endEvent id="end_event_data_breach_error" name="PR Process Terminated (Data Breach)">
        <semantic:incoming>flow_seq_err_3</semantic:incoming>
    </semantic:endEvent>
    <semantic:task id="task_hod_2_document_submit_decision" name="Document/submit HoD final decision to requestor">
      <semantic:incoming>flow_seq_8</semantic:incoming>
      <semantic:outgoing>flow_seq_9</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_hod_1_hod_pr_decision" name="HoD PR Decision">
      <semantic:incoming>flow_seq_9</semantic:incoming>
      <semantic:outgoing>flow_seq_10</semantic:outgoing>
      <semantic:outgoing>flow_seq_11</semantic:outgoing>
      <semantic:outgoing>flow_seq_12</semantic:outgoing>
    </semantic:exclusiveGateway>
