      <semantic:outgoing>flow_seq_34</semantic:outgoing>
    </semantic:task>
    <semantic:boundaryEvent id="error_be_invoice_discrepancy" name="Invoice Discrepancy Occurred" attachedToRef="task_req_12_process_invoice_3way_match" cancelActivity="true">
      <semantic:outgoing>flow_seq_err_4</semantic:outgoing>
      <semantic:errorEventDefinition errorRef="INVOICE_DISCREPANCY_ERROR"/>
    </semantic:boundaryEvent>
    <semantic:endEvent id="end_event_invoice_error" name="Invoice Process Terminated (Discrepancy)">
        <semantic:incoming>flow_seq_err_4</semantic:incoming>
    </semantic:endEvent>
    <semantic:exclusiveGateway id="gw_req_7_invoice_correct" name="Invoice Correct?">
      <semantic:incoming>flow_seq_34</semantic:incoming>
      <semantic:outgoing>flow_seq_35</semantic:outgoing>
      <semantic:outgoing>flow_seq_37</semantic:outgoing>
