      <semantic:errorEventDefinition errorRef="PR_DISCREPANCY_ERROR"/>
    </semantic:boundaryEvent>
    <semantic:endEvent id="end_event_pr_error" name="PR Process Terminated (Discrepancy)">
        <semantic:incoming>flow_seq_err_1</semantic:incoming>
    </semantic:endEvent>
    <semantic:task id="task_req_4_send_pr_to_hod" name="Send issued PR to Heads of Departments for approval">
      <semantic:incoming>flow_seq_6</semantic:incoming>
      <semantic:outgoing>flow_seq_7</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_hod_1_review_pr" name="Review PR (needs, budget, form)">
      <semantic:incoming>flow_seq_7</semantic:incoming>
      <semantic:outgoing>flow_seq_8</semantic:outgoing>
    </semantic:task>
