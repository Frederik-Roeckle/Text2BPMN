      <semantic:outgoing>flow_seq_28</semantic:outgoing>
      <semantic:outgoing>flow_seq_29</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_10_issue_gr" name="Issue Goods Receipt (GR)">
      <semantic:incoming>flow_seq_28</semantic:incoming>
      <semantic:outgoing>flow_seq_30</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_11_issue_ses" name="Issue Service Entry Sheet (SES)">
      <semantic:incoming>flow_seq_29</semantic:incoming>
      <semantic:outgoing>flow_seq_31</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_6_merge_gr_ses" name="Merge GR/SES Path">
      <semantic:incoming>flow_seq_30</semantic:incoming>
      <semantic:incoming>flow_seq_31</semantic:incoming>
      <semantic:outgoing>flow_seq_32</semantic:outgoing>
    </semantic:exclusiveGateway>
