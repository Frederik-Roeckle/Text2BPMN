      <semantic:incoming>flow_seq_45</semantic:incoming>
      <semantic:outgoing>flow_seq_46</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="end_event_ap_1_process_completed" name="P2P Process Successfully Completed">
      <semantic:incoming>flow_seq_46</semantic:incoming>
    </semantic:endEvent>

    <semantic:sequenceFlow id="flow_seq_1" sourceRef="start_event_req_1" targetRef="task_req_1_define_needs"/>
    <semantic:sequenceFlow id="flow_seq_2" sourceRef="task_req_1_define_needs" targetRef="task_req_2_create_pr"/>
    <semantic:sequenceFlow id="flow_seq_3" sourceRef="task_req_2_create_pr" targetRef="gw_req_1_initial_pr_approval"/>
    <semantic:sequenceFlow id="flow_seq_4" name="Approved" sourceRef="gw_req_1_initial_pr_approval" targetRef="task_req_3_issue_pr">
        <semantic:conditionExpression xsi:type="semantic:tFormalExpression">Approved</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="flow_seq_5" name="Not Approved" sourceRef="gw_req_1_initial_pr_approval" targetRef="task_req_1_define_needs">
        <semantic:conditionExpression xsi:type="semantic:tFormalExpression">Not Approved</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="flow_seq_6" sourceRef="task_req_3_issue_pr" targetRef="task_req_4_send_pr_to_hod"/>
    <semantic:sequenceFlow id="flow_seq_err_1" sourceRef="error_be_pr_discrepancy" targetRef="end_event_pr_error"/>
    <semantic:sequenceFlow id="flow_seq_7" sourceRef="task_req_4_send_pr_to_hod" targetRef="task_hod_1_review_pr"/>
    <semantic:sequenceFlow id="flow_seq_8" sourceRef="task_hod_1_review_pr" targetRef="task_hod_2_document_submit_decision"/>
    <semantic:sequenceFlow id="flow_seq_err_2" sourceRef="error_be_budget_overdraft" targetRef="end_event_budget_error"/>
    <semantic:sequenceFlow id="flow_seq_err_3" sourceRef="error_be_data_breach" targetRef="end_event_data_breach_error"/>
    <semantic:sequenceFlow id="flow_seq_9" sourceRef="task_hod_2_document_submit_decision" targetRef="gw_hod_1_hod_pr_decision"/>
    <semantic:sequenceFlow id="flow_seq_10" name="PR Approved" sourceRef="gw_hod_1_hod_pr_decision" targetRef="task_req_5_create_po">
        <semantic:conditionExpression xsi:type="semantic:tFormalExpression">PR Approved</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="flow_seq_11" name="PR Declined" sourceRef="gw_hod_1_hod_pr_decision" targetRef="task_req_1_define_needs">
        <semantic:conditionExpression xsi:type="semantic:tFormalExpression">PR Declined</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="flow_seq_12" name="PR Rejected Incomplete" sourceRef="gw_hod_1_hod_pr_decision" targetRef="task_req_1_define_needs">
        <semantic:conditionExpression xsi:type="semantic:tFormalExpression">PR Rejected Incomplete</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="flow_seq_13" sourceRef="task_req_5_create_po" targetRef="task_req_6_send_po_to_poa"/>
    <semantic:sequenceFlow id="flow_seq_14" sourceRef="task_req_6_send_po_to_poa" targetRef="task_poa_1_review_po_specs"/>
    <semantic:sequenceFlow id="flow_seq_15" sourceRef="task_poa_1_review_po_specs" targetRef="task_poa_2_document_submit_decision"/>
    <semantic:sequenceFlow id="flow_seq_16a" sourceRef="task_poa_2_document_submit_decision" targetRef="gw_poa_1_poa_po_decision"/>
    <semantic:sequenceFlow id="flow_seq_17" name="PO Approved" sourceRef="gw_poa_1_poa_po_decision" targetRef="task_req_7_dispatch_po_to_vendor">
        <semantic:conditionExpression xsi:type="semantic:tFormalExpression">PO Approved</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="flow_seq_16" name="PO Rejected" sourceRef="gw_poa_1_poa_po_decision" targetRef="task_req_1_define_needs">
        <semantic:conditionExpression xsi:type="semantic:tFormalExpression">PO Rejected</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="flow_seq_18" name="Wait for Vendor Decision" sourceRef="task_req_7_dispatch_po_to_vendor" targetRef="event_mc_req_vendor_po_decision"/>
    <semantic:sequenceFlow id="flow_seq_23" sourceRef="event_mc_req_vendor_po_decision" targetRef="gw_req_4_vendor_po_outcome"/>
