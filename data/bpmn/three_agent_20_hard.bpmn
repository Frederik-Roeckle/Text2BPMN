    </semantic:startEvent>
    <semantic:task id="task_req_1_define_needs" name="Determine and define business requirements &amp; needs">
      <semantic:incoming>flow_seq_1</semantic:incoming>
      <semantic:incoming>flow_seq_5</semantic:incoming>
      <semantic:incoming>flow_seq_11</semantic:incoming>
      <semantic:incoming>flow_seq_12</semantic:incoming>
      <semantic:incoming>flow_seq_16</semantic:incoming>
      <semantic:outgoing>flow_seq_2</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_2_create_pr" name="Request to issue Purchase Requisition (PR)">
      <semantic:incoming>flow_seq_2</semantic:incoming>
      <semantic:outgoing>flow_seq_3</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_1_initial_pr_approval" name="Initial PR Approved?">
      <semantic:incoming>flow_seq_3</semantic:incoming>
      <semantic:outgoing>flow_seq_4</semantic:outgoing>
      <semantic:outgoing>flow_seq_5</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_3_issue_pr" name="Issue Purchase Requisition (PR)">
      <semantic:incoming>flow_seq_4</semantic:incoming>
      <semantic:outgoing>flow_seq_6</semantic:outgoing>
    </semantic:task>
    <semantic:boundaryEvent id="error_be_pr_discrepancy" name="PR Discrepancy Occurred" attachedToRef="task_req_3_issue_pr" cancelActivity="true">
