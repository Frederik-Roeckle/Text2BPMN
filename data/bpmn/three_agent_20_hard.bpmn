    <semantic:exclusiveGateway id="gw_req_8_merge_invoice_paths" name="Merge Invoice Paths">
      <semantic:incoming>flow_seq_37</semantic:incoming>
      <semantic:incoming>flow_seq_38</semantic:incoming>
      <semantic:outgoing>flow_seq_39</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_14_post_invoice_sap" name="Post invoice in SAP">
      <semantic:incoming>flow_seq_39</semantic:incoming>
      <semantic:outgoing>flow_seq_40</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_15_send_invoice_to_ap" name="Send invoice to Accounts Payable (AP) team">
      <semantic:incoming>flow_seq_40</semantic:incoming>
      <semantic:outgoing>flow_seq_41</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ap_1_receive_invoice" name="Receive invoice (AP)">
      <semantic:incoming>flow_seq_41</semantic:incoming>
      <semantic:outgoing>flow_seq_42</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ap_2_decide_payment_method" name="Decide on payment method">
      <semantic:incoming>flow_seq_42</semantic:incoming>
      <semantic:outgoing>flow_seq_43</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_ap_1_payment_method" name="Payment Method Decision">
      <semantic:incoming>flow_seq_43</semantic:incoming>
      <semantic:outgoing>flow_seq_44</semantic:outgoing>
      <semantic:outgoing>flow_seq_45</semantic:outgoing>
