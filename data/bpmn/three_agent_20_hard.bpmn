      <semantic:outgoing>flow_seq_13</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_6_send_po_to_poa" name="Send PO to PO approvers for approval">
      <semantic:incoming>flow_seq_13</semantic:incoming>
      <semantic:outgoing>flow_seq_14</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_poa_1_review_po_specs" name="Review PO specifications">
      <semantic:incoming>flow_seq_14</semantic:incoming>
      <semantic:outgoing>flow_seq_15</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_poa_2_document_submit_decision" name="Document/submit POA final decision to requestor">
      <semantic:incoming>flow_seq_15</semantic:incoming>
      <semantic:outgoing>flow_seq_16a</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_poa_1_poa_po_decision" name="POA PO Decision">
      <semantic:incoming>flow_seq_16a</semantic:incoming>
      <semantic:outgoing>flow_seq_17</semantic:outgoing>
      <semantic:outgoing>flow_seq_16</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_7_dispatch_po_to_vendor" name="Dispatch approved PO to vendor">
      <semantic:incoming>flow_seq_17</semantic:incoming>
      <semantic:incoming>flow_seq_25</semantic:incoming>
      <semantic:outgoing>flow_seq_18</semantic:outgoing>
    </semantic:task>
    <semantic:intermediateCatchEvent id="event_mc_req_vendor_po_decision" name="Receive Vendor PO Decision">
      <semantic:incoming>flow_seq_18</semantic:incoming>
      <semantic:outgoing>flow_seq_23</semantic:outgoing>
      <semantic:messageEventDefinition/>
    </semantic:intermediateCatchEvent>
    <semantic:exclusiveGateway id="gw_req_4_vendor_po_outcome" name="Vendor PO Outcome?">
      <semantic:incoming>flow_seq_23</semantic:incoming>
      <semantic:outgoing>flow_seq_24</semantic:outgoing>
      <semantic:outgoing>flow_seq_26</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_8_search_new_vendor" name="Search for a new vendor">
      <semantic:incoming>flow_seq_24</semantic:incoming>
      <semantic:outgoing>flow_seq_25</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_9_receive_goods_services" name="Receive agreed goods/services">
