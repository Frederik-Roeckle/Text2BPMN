    <semantic:messageFlow id="msgflow_2" name="Vendor PO Decision" sourceRef="task_ven_2_document_send_decision_to_req" targetRef="event_mc_req_vendor_po_decision"/>
    <semantic:messageFlow id="msgflow_3" name="Invoice" sourceRef="task_ven_4_send_invoice_for_delivery" targetRef="event_mc_req_invoice_from_vendor"/>
    <semantic:messageFlow id="msgflow_4" name="Wrong Invoice for Correction" sourceRef="task_req_13_return_invoice_to_vendor" targetRef="event_mc_ven_receive_wrong_invoice"/>
    <semantic:messageFlow id="msgflow_5" name="Corrected Invoice" sourceRef="task_ven_6_send_corrected_invoice_to_req" targetRef="event_mc_req_corrected_invoice"/>
  </semantic:collaboration>

