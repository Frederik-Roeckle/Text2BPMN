<?xml version="1.0" encoding="UTF-8"?>
<semantic:definitions xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
                      xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                      xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                      xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                      id="definitions_p2p"
                      targetNamespace="http://example.com/bpmn/p2p">

  <semantic:collaboration id="collab_p2p">
    <semantic:participant id="part_company" name="Company P2P Operations" processRef="process_company_p2p"/>
    <semantic:participant id="part_vendor" name="Vendor" processRef="process_vendor_p2p"/>
  </semantic:collaboration>

  <semantic:process id="process_company_p2p" name="Company P2P Process" isExecutable="false">
    <semantic:laneSet id="laneset_company">
      <semantic:lane id="lane_requestor" name="Requestor">
        <semantic:flowNodeRef>start_need_identified</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_det_requirements</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_pr_issue</semantic:flowNodeRef>
        <semantic:flowNodeRef>gate_req_pr_issue_approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_issue_submit_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>catch_pr_decision_from_hod</semantic:flowNodeRef>
        <semantic:flowNodeRef>gate_pr_approved_by_hod</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_create_submit_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>catch_po_decision_from_poa</semantic:flowNodeRef>
        <semantic:flowNodeRef>gate_po_approved_by_poa</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_dispatch_po_to_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>catch_vendor_po_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gate_vendor_po_decision_outcome</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_search_new_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>gate_await_delivery_invoice_split</semantic:flowNodeRef>
        <semantic:flowNodeRef>catch_delivery_from_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_receive_goods_services</semantic:flowNodeRef>
        <semantic:flowNodeRef>gate_receipt_type</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_issue_gr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_issue_ses</semantic:flowNodeRef>
        <semantic:flowNodeRef>gate_merge_gr_ses</semantic:flowNodeRef>
        <semantic:flowNodeRef>catch_invoice_from_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_receive_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>gate_sync_delivery_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_perform_3_way_match</semantic:flowNodeRef>
        <semantic:flowNodeRef>gate_invoice_correct</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_return_invoice_to_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>catch_corrected_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_receive_corrected_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_post_invoice_sap</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_send_invoice_to_ap</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_hod" name="Heads of Departments">
        <semantic:flowNodeRef>catch_pr_for_approval_hod</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_hod_review_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_hod_submit_pr_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gate_hod_pr_decision</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_po_approvers" name="PO Approvers">
        <semantic:flowNodeRef>catch_po_for_approval_poa</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_poa_review_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_poa_submit_po_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gate_poa_po_decision</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_ap_team" name="Accounts Payable Team">
        <semantic:flowNodeRef>catch_invoice_for_payment_ap</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_decide_payment_method</semantic:flowNodeRef>
        <semantic:flowNodeRef>gate_ap_payment_method_choice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_perform_payment</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_p2p_completed</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <!-- Requestor Lane Elements -->
    <semantic:startEvent id="start_need_identified" name="Need Identified">
      <semantic:outgoing>flow_start_to_det_req</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_det_requirements" name="Determine Business Requirements &amp; Needs">
      <semantic:incoming>flow_start_to_det_req</semantic:incoming>
      <semantic:incoming>flow_gate_req_pr_issue_not_approved_to_det_req</semantic:incoming>
      <semantic:incoming>flow_gate_pr_not_approved_hod_to_det_req</semantic:incoming>
      <semantic:incoming>flow_gate_po_not_approved_poa_to_det_req</semantic:incoming>
      <semantic:outgoing>flow_det_req_to_req_pr_issue</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_pr_issue" name="Request to Issue Purchase Requisition">
      <semantic:incoming>flow_det_req_to_req_pr_issue</semantic:incoming>
      <semantic:outgoing>flow_req_pr_issue_to_gate_req_approve</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gate_req_pr_issue_approved" name="PR Issuance Approved by Requestor?">
      <semantic:incoming>flow_req_pr_issue_to_gate_req_approve</semantic:incoming>
      <semantic:outgoing>flow_gate_req_pr_issue_approved_to_issue_pr</semantic:outgoing>
      <semantic:outgoing>flow_gate_req_pr_issue_not_approved_to_det_req</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_issue_submit_pr" name="Issue and Submit PR for Approval">
      <semantic:incoming>flow_gate_req_pr_issue_approved_to_issue_pr</semantic:incoming>
      <semantic:outgoing>flow_issue_pr_to_catch_pr_decision</semantic:outgoing> <!-- Connects via MF -->
    </semantic:task>
    <semantic:intermediateCatchEvent id="catch_pr_decision_from_hod" name="Receive PR Decision from HoD">
      <semantic:incoming>flow_issue_pr_to_catch_pr_decision</semantic:incoming> <!-- Placeholder, actual trigger is MF -->
      <semantic:outgoing>flow_catch_pr_decision_to_gate_pr_approved_hod</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_pr_decision"/>
    </semantic:intermediateCatchEvent>
    <semantic:exclusiveGateway id="gate_pr_approved_by_hod" name="PR Approved by HoD?">
      <semantic:incoming>flow_catch_pr_decision_to_gate_pr_approved_hod</semantic:incoming>
      <semantic:outgoing>flow_gate_pr_approved_hod_to_create_po</semantic:outgoing>
      <semantic:outgoing>flow_gate_pr_not_approved_hod_to_det_req</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_create_submit_po" name="Create and Submit PO for Approval">
      <semantic:incoming>flow_gate_pr_approved_hod_to_create_po</semantic:incoming>
      <semantic:outgoing>flow_create_po_to_catch_po_decision</semantic:outgoing> <!-- Connects via MF -->
    </semantic:task>
    <semantic:intermediateCatchEvent id="catch_po_decision_from_poa" name="Receive PO Decision from POA">
      <semantic:incoming>flow_create_po_to_catch_po_decision</semantic:incoming> <!-- Placeholder -->
      <semantic:outgoing>flow_catch_po_decision_to_gate_po_approved_poa</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_po_decision"/>
    </semantic:intermediateCatchEvent>
    <semantic:exclusiveGateway id="gate_po_approved_by_poa" name="PO Approved by POA?">
      <semantic:incoming>flow_catch_po_decision_to_gate_po_approved_poa</semantic:incoming>
      <semantic:outgoing>flow_gate_po_approved_poa_to_dispatch_po</semantic:outgoing>
      <semantic:outgoing>flow_gate_po_not_approved_poa_to_det_req</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_dispatch_po_to_vendor" name="Dispatch Approved PO to Vendor">
      <semantic:incoming>flow_gate_po_approved_poa_to_dispatch_po</semantic:incoming>
      <semantic:incoming>flow_search_vendor_to_dispatch_po</semantic:incoming>
      <semantic:outgoing>flow_dispatch_po_to_catch_vendor_decision</semantic:outgoing> <!-- Connects via MF -->
    </semantic:task>
    <semantic:intermediateCatchEvent id="catch_vendor_po_decision" name="Receive Vendor PO Decision">
      <semantic:incoming>flow_dispatch_po_to_catch_vendor_decision</semantic:incoming> <!-- Placeholder -->
      <semantic:outgoing>flow_catch_vendor_decision_to_gate_vendor_outcome</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_vendor_po_decision"/>
    </semantic:intermediateCatchEvent>
    <semantic:exclusiveGateway id="gate_vendor_po_decision_outcome" name="Vendor PO Decision Outcome?">
      <semantic:incoming>flow_catch_vendor_decision_to_gate_vendor_outcome</semantic:incoming>
      <semantic:outgoing>flow_gate_vendor_approved_to_await_split</semantic:outgoing> <!-- Approved/Negotiated -->
      <semantic:outgoing>flow_gate_vendor_rejected_to_search_vendor</semantic:outgoing> <!-- Rejected -->
    </semantic:exclusiveGateway>
    <semantic:task id="task_search_new_vendor" name="Search for New Vendor">
      <semantic:incoming>flow_gate_vendor_rejected_to_search_vendor</semantic:incoming>
      <semantic:outgoing>flow_search_vendor_to_dispatch_po</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="gate_await_delivery_invoice_split" name="Await Delivery and Invoice">
      <semantic:incoming>flow_gate_vendor_approved_to_await_split</semantic:incoming>
      <semantic:outgoing>flow_await_split_to_catch_delivery</semantic:outgoing>
      <semantic:outgoing>flow_await_split_to_catch_invoice</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:intermediateCatchEvent id="catch_delivery_from_vendor" name="Receive Delivery from Vendor">
      <semantic:incoming>flow_await_split_to_catch_delivery</semantic:incoming>
      <semantic:outgoing>flow_catch_delivery_to_receive_goods</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_delivery"/>
    </semantic:intermediateCatchEvent>
    <semantic:task id="task_receive_goods_services" name="Receive Agreed Goods/Services">
      <semantic:incoming>flow_catch_delivery_to_receive_goods</semantic:incoming>
      <semantic:outgoing>flow_receive_goods_to_gate_receipt_type</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gate_receipt_type" name="Receipt Type?">
      <semantic:incoming>flow_receive_goods_to_gate_receipt_type</semantic:incoming>
      <semantic:outgoing>flow_gate_receipt_type_to_issue_gr</semantic:outgoing> <!-- Goods -->
      <semantic:outgoing>flow_gate_receipt_type_to_issue_ses</semantic:outgoing> <!-- Service -->
    </semantic:exclusiveGateway>
    <semantic:task id="task_issue_gr" name="Issue Goods Receipt (GR)">
      <semantic:incoming>flow_gate_receipt_type_to_issue_gr</semantic:incoming>
      <semantic:outgoing>flow_issue_gr_to_merge_gr_ses</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_issue_ses" name="Issue Service Entry Sheet (SES)">
      <semantic:incoming>flow_gate_receipt_type_to_issue_ses</semantic:incoming>
      <semantic:outgoing>flow_issue_ses_to_merge_gr_ses</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gate_merge_gr_ses" name="Merge GR/SES Path">
      <semantic:incoming>flow_issue_gr_to_merge_gr_ses</semantic:incoming>
      <semantic:incoming>flow_issue_ses_to_merge_gr_ses</semantic:incoming>
      <semantic:outgoing>flow_merge_gr_ses_to_sync_join</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:intermediateCatchEvent id="catch_invoice_from_vendor" name="Receive Invoice from Vendor">
      <semantic:incoming>flow_await_split_to_catch_invoice</semantic:incoming>
      <semantic:outgoing>flow_catch_invoice_to_receive_invoice</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_invoice"/>
    </semantic:intermediateCatchEvent>
    <semantic:task id="task_receive_invoice" name="Receive Invoice">
      <semantic:incoming>flow_catch_invoice_to_receive_invoice</semantic:incoming>
      <semantic:outgoing>flow_receive_invoice_to_sync_join</semantic:outgoing>
    </semantic:task>
    <semantic:parallelGateway id="gate_sync_delivery_invoice" name="Synchronize Delivery and Invoice">
      <semantic:incoming>flow_merge_gr_ses_to_sync_join</semantic:incoming>
      <semantic:incoming>flow_receive_invoice_to_sync_join</semantic:incoming>
      <semantic:outgoing>flow_sync_join_to_3_way_match</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:task id="task_perform_3_way_match" name="Perform Three-Way Match">
      <semantic:incoming>flow_sync_join_to_3_way_match</semantic:incoming>
      <semantic:incoming>flow_receive_corrected_invoice_to_3_way_match</semantic:incoming>
      <semantic:outgoing>flow_3_way_match_to_gate_invoice_correct</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gate_invoice_correct" name="Invoice Correct?">
      <semantic:incoming>flow_3_way_match_to_gate_invoice_correct</semantic:incoming>
      <semantic:outgoing>flow_gate_invoice_correct_to_post_invoice</semantic:outgoing> <!-- Yes -->
      <semantic:outgoing>flow_gate_invoice_incorrect_to_return_invoice</semantic:outgoing> <!-- No -->
    </semantic:exclusiveGateway>
    <semantic:task id="task_return_invoice_to_vendor" name="Return Invoice to Vendor for Correction">
      <semantic:incoming>flow_gate_invoice_incorrect_to_return_invoice</semantic:incoming>
      <semantic:outgoing>flow_return_invoice_to_catch_corrected</semantic:outgoing> <!-- Connects via MF -->
    </semantic:task>
    <semantic:intermediateCatchEvent id="catch_corrected_invoice" name="Receive Corrected Invoice from Vendor">
      <semantic:incoming>flow_return_invoice_to_catch_corrected</semantic:incoming> <!-- Placeholder -->
      <semantic:outgoing>flow_catch_corrected_to_receive_corrected</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_corrected_invoice"/>
    </semantic:intermediateCatchEvent>
    <semantic:task id="task_receive_corrected_invoice" name="Receive Corrected Invoice">
      <semantic:incoming>flow_catch_corrected_to_receive_corrected</semantic:incoming>
      <semantic:outgoing>flow_receive_corrected_invoice_to_3_way_match</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_post_invoice_sap" name="Post Invoice in SAP">
      <semantic:incoming>flow_gate_invoice_correct_to_post_invoice</semantic:incoming>
      <semantic:outgoing>flow_post_invoice_to_send_to_ap</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_send_invoice_to_ap" name="Send Invoice to AP Team">
      <semantic:incoming>flow_post_invoice_to_send_to_ap</semantic:incoming>
      <semantic:outgoing>flow_send_to_ap_to_ap_lane</semantic:outgoing> <!-- Connects via MF -->
    </semantic:task>

    <!-- Heads of Departments Lane Elements -->
    <semantic:intermediateCatchEvent id="catch_pr_for_approval_hod" name="Receive PR for Approval">
      <semantic:outgoing>flow_catch_pr_hod_to_review_pr</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_pr_for_approval"/>
    </semantic:intermediateCatchEvent>
    <semantic:task id="task_hod_review_pr" name="Review PR (Needs, Budget, Form)">
      <semantic:incoming>flow_catch_pr_hod_to_review_pr</semantic:incoming>
      <semantic:outgoing>flow_hod_review_pr_to_submit_decision</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_hod_submit_pr_decision" name="Document &amp; Submit PR Decision">
      <semantic:incoming>flow_hod_review_pr_to_submit_decision</semantic:incoming>
      <semantic:outgoing>flow_hod_submit_decision_to_gate_decision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gate_hod_pr_decision" name="PR Decision by HoD">
      <semantic:incoming>flow_hod_submit_decision_to_gate_decision</semantic:incoming>
      <semantic:outgoing>flow_hod_pr_approved</semantic:outgoing> <!-- Approved -->
      <semantic:outgoing>flow_hod_pr_declined</semantic:outgoing> <!-- Declined -->
      <semantic:outgoing>flow_hod_pr_rejected_incomplete</semantic:outgoing> <!-- Rejected Incomplete -->
    </semantic:exclusiveGateway>

    <!-- PO Approvers Lane Elements -->
    <semantic:intermediateCatchEvent id="catch_po_for_approval_poa" name="Receive PO for Approval">
      <semantic:outgoing>flow_catch_po_poa_to_review_po</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_po_for_approval"/>
    </semantic:intermediateCatchEvent>
    <semantic:task id="task_poa_review_po" name="Review PO Specifications">
      <semantic:incoming>flow_catch_po_poa_to_review_po</semantic:incoming>
      <semantic:outgoing>flow_poa_review_po_to_submit_decision</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_poa_submit_po_decision" name="Document &amp; Submit PO Decision">
      <semantic:incoming>flow_poa_review_po_to_submit_decision</semantic:incoming>
      <semantic:outgoing>flow_poa_submit_decision_to_gate_decision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gate_poa_po_decision" name="PO Decision by PO Approvers">
      <semantic:incoming>flow_poa_submit_decision_to_gate_decision</semantic:incoming>
      <semantic:outgoing>flow_poa_po_approved</semantic:outgoing> <!-- Approved -->
      <semantic:outgoing>flow_poa_po_rejected</semantic:outgoing> <!-- Rejected -->
    </semantic:exclusiveGateway>

    <!-- Accounts Payable Team Lane Elements -->
    <semantic:intermediateCatchEvent id="catch_invoice_for_payment_ap" name="Receive Invoice for Payment">
      <semantic:outgoing>flow_catch_invoice_ap_to_decide_payment</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="msg_invoice_for_payment"/>
    </semantic:intermediateCatchEvent>
    <semantic:task id="task_ap_decide_payment_method" name="Decide on Payment Method">
      <semantic:incoming>flow_catch_invoice_ap_to_decide_payment</semantic:incoming>
      <semantic:outgoing>flow_ap_decide_payment_to_gate_method</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gate_ap_payment_method_choice" name="Payment Method Choice">
      <semantic:incoming>flow_ap_decide_payment_to_gate_method</semantic:incoming>
      <semantic:outgoing>flow_ap_payment_full</semantic:outgoing> <!-- Full -->
      <semantic:outgoing>flow_ap_payment_installments</semantic:outgoing> <!-- Installments -->
    </semantic:exclusiveGateway>
    <semantic:task id="task_ap_perform_payment" name="Perform Payment to Vendor">
      <semantic:incoming>flow_ap_payment_full</semantic:incoming>
      <semantic:incoming>flow_ap_payment_installments</semantic:incoming>
      <semantic:outgoing>flow_ap_perform_payment_to_end</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="end_p2p_completed" name="P2P Process Successfully Completed">
      <semantic:incoming>flow_ap_perform_payment_to_end</semantic:incoming>
    </semantic:endEvent>

    <!-- Sequence Flows for Requestor Lane -->
    <semantic:sequenceFlow id="flow_start_to_det_req" sourceRef="start_need_identified" targetRef="task_det_requirements"/>
    <semantic:sequenceFlow id="flow_det_req_to_req_pr_issue" sourceRef="task_det_requirements" targetRef="task_req_pr_issue"/>
    <semantic:sequenceFlow id="flow_req_pr_issue_to_gate_req_approve" sourceRef="task_req_pr_issue" targetRef="gate_req_pr_issue_approved"/>
    <semantic:sequenceFlow id="flow_gate_req_pr_issue_approved_to_issue_pr" name="Approved" sourceRef="gate_req_pr_issue_approved" targetRef="task_issue_submit_pr"/>
    <semantic:sequenceFlow id="flow_gate_req_pr_issue_not_approved_to_det_req" name="Not Approved" sourceRef="gate_req_pr_issue_approved" targetRef="task_det_requirements"/>
    <semantic:sequenceFlow id="flow_issue_pr_to_catch_pr_decision" sourceRef="task_issue_submit_pr" targetRef="catch_pr_decision_from_hod"/>
    <semantic:sequenceFlow id="flow_catch_pr_decision_to_gate_pr_approved_hod" sourceRef="catch_pr_decision_from_hod" targetRef="gate_pr_approved_by_hod"/>
    <semantic:sequenceFlow id="flow_gate_pr_approved_hod_to_create_po" name="Approved by HoD" sourceRef="gate_pr_approved_by_hod" targetRef="task_create_submit_po"/>
    <semantic:sequenceFlow id="flow_gate_pr_not_approved_hod_to_det_req" name="Not Approved by HoD" sourceRef="gate_pr_approved_by_hod" targetRef="task_det_requirements"/>
    <semantic:sequenceFlow id="flow_create_po_to_catch_po_decision" sourceRef="task_create_submit_po" targetRef="catch_po_decision_from_poa"/>
    <semantic:sequenceFlow id="flow_catch_po_decision_to_gate_po_approved_poa" sourceRef="catch_po_decision_from_poa" targetRef="gate_po_approved_by_poa"/>
    <semantic:sequenceFlow id="flow_gate_po_approved_poa_to_dispatch_po" name="Approved by POA" sourceRef="gate_po_approved_by_poa" targetRef="task_dispatch_po_to_vendor"/>
    <semantic:sequenceFlow id="flow_gate_po_not_approved_poa_to_det_req" name="Not Approved by POA" sourceRef="gate_po_approved_by_poa" targetRef="task_det_requirements"/>
    <semantic:sequenceFlow id="flow_dispatch_po_to_catch_vendor_decision" sourceRef="task_dispatch_po_to_vendor" targetRef="catch_vendor_po_decision"/>
    <semantic:sequenceFlow id="flow_catch_vendor_decision_to_gate_vendor_outcome" sourceRef="catch_vendor_po_decision" targetRef="gate_vendor_po_decision_outcome"/>
    <semantic:sequenceFlow id="flow_gate_vendor_approved_to_await_split" name="Approved/Negotiated" sourceRef="gate_vendor_po_decision_outcome" targetRef="gate_await_delivery_invoice_split"/>
    <semantic:sequenceFlow id="flow_gate_vendor_rejected_to_search_vendor" name="Rejected by Vendor" sourceRef="gate_vendor_po_decision_outcome" targetRef="task_search_new_vendor"/>
    <semantic:sequenceFlow id="flow_search_vendor_to_dispatch_po" sourceRef="task_search_new_vendor" targetRef="task_dispatch_po_to_vendor"/>
    <semantic:sequenceFlow id="flow_await_split_to_catch_delivery" sourceRef="gate_await_delivery_invoice_split" targetRef="catch_delivery_from_vendor"/>
    <semantic:sequenceFlow id="flow_await_split_to_catch_invoice" sourceRef="gate_await_delivery_invoice_split" targetRef="catch_invoice_from_vendor"/>
    <semantic:sequenceFlow id="flow_catch_delivery_to_receive_goods" sourceRef="catch_delivery_from_vendor" targetRef="task_receive_goods_services"/>
    <semantic:sequenceFlow id="flow_receive_goods_to_gate_receipt_type" sourceRef="task_receive_goods_services" targetRef="gate_receipt_type"/>
    <semantic:sequenceFlow id="flow_gate_receipt_type_to_issue_gr" name="Goods" sourceRef="gate_receipt_type" targetRef="task_issue_gr"/>
