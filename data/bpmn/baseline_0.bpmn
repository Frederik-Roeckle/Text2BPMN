<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="Definitions_OfferProcess"
    targetNamespace="http://example.com/bpmn/offer-process"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_OfferProcess">
    <semantic:participant id="participant_company" name="Company" processRef="OfferCreationProcess"/>
  </semantic:collaboration>

  <semantic:process id="OfferCreationProcess" isExecutable="true">
    <semantic:laneSet id="laneSet_company">
      <semantic:lane id="lane_sales_dept" name="Sales Department">
        <semantic:flowNodeRef>start_event_offer_required</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_inform_customer_not_feasible</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_event_customer_informed</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_comm_manager" name="Commercial Manager">
        <semantic:flowNodeRef>task_perform_feasibility_check</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_feasibility_outcome</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_check_personnel_availability</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_comm_employee" name="Commercial Employee">
        <semantic:flowNodeRef>task_create_offer</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_event_offer_created</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <semantic:startEvent id="start_event_offer_required" name="Offer Required">
      <semantic:outgoing>flow_start_to_feasibility_check</semantic:outgoing>
    </semantic:startEvent>

    <semantic:task id="task_perform_feasibility_check" name="Perform Feasibility Check">
      <semantic:incoming>flow_start_to_feasibility_check</semantic:incoming>
      <semantic:outgoing>flow_feasibility_check_to_gateway</semantic:outgoing>
    </semantic:task>

    <semantic:exclusiveGateway id="gateway_feasibility_outcome" name="Is Offer Feasible?">
      <semantic:incoming>flow_feasibility_check_to_gateway</semantic:incoming>
      <semantic:outgoing>flow_gateway_to_inform_customer</semantic:outgoing>
      <semantic:outgoing>flow_gateway_to_check_availability</semantic:outgoing>
    </semantic:exclusiveGateway>

    <semantic:task id="task_inform_customer_not_feasible" name="Inform Customer (Offer Not Feasible)">
      <semantic:incoming>flow_gateway_to_inform_customer</semantic:incoming>
      <semantic:outgoing>flow_inform_customer_to_end</semantic:outgoing>
    </semantic:task>

    <semantic:endEvent id="end_event_customer_informed" name="Process Ended - Customer Informed">
      <semantic:incoming>flow_inform_customer_to_end</semantic:incoming>
    </semantic:endEvent>

    <semantic:task id="task_check_personnel_availability" name="Check Personnel Needs &amp; Availability">
      <semantic:incoming>flow_gateway_to_check_availability</semantic:incoming>
      <semantic:outgoing>flow_check_availability_to_create_offer</semantic:outgoing>
    </semantic:task>

    <semantic:task id="task_create_offer" name="Create Offer">
      <semantic:incoming>flow_check_availability_to_create_offer</semantic:incoming>
      <semantic:outgoing>flow_create_offer_to_end</semantic:outgoing>
    </semantic:task>

    <semantic:endEvent id="end_event_offer_created" name="Process Ended - Offer Created">
      <semantic:incoming>flow_create_offer_to_end</semantic:incoming>
    </semantic:endEvent>

    <semantic:sequenceFlow id="flow_start_to_feasibility_check" sourceRef="start_event_offer_required" targetRef="task_perform_feasibility_check"/>
    <semantic:sequenceFlow id="flow_feasibility_check_to_gateway" sourceRef="task_perform_feasibility_check" targetRef="gateway_feasibility_outcome"/>
    <semantic:sequenceFlow id="flow_gateway_to_inform_customer" name="Not Feasible" sourceRef="gateway_feasibility_outcome" targetRef="task_inform_customer_not_feasible"/>
    <semantic:sequenceFlow id="flow_inform_customer_to_end" sourceRef="task_inform_customer_not_feasible" targetRef="end_event_customer_informed"/>
    <semantic:sequenceFlow id="flow_gateway_to_check_availability" name="Feasible" sourceRef="gateway_feasibility_outcome" targetRef="task_check_personnel_availability"/>
    <semantic:sequenceFlow id="flow_check_availability_to_create_offer" sourceRef="task_check_personnel_availability" targetRef="task_create_offer"/>
    <semantic:sequenceFlow id="flow_create_offer_to_end" sourceRef="task_create_offer" targetRef="end_event_offer_created"/>
  </semantic:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_OfferProcess" name="Offer Creation Process Diagram" resolution="96.0">
    <bpmndi:BPMNPlane bpmnElement="collab_OfferProcess" id="BPMNPlane_OfferProcess">
      <bpmndi:BPMNShape id="shape_participant_company" bpmnElement="participant_company" isHorizontal="true">
        <dc:Bounds x="20" y="20" width="980" height="600"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_lane_sales_dept" bpmnElement="lane_sales_dept" isHorizontal="true">
        <dc:Bounds x="50" y="20" width="950" height="200"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_lane_comm_manager" bpmnElement="lane_comm_manager" isHorizontal="true">
        <dc:Bounds x="50" y="220" width="950" height="200"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_lane_comm_employee" bpmnElement="lane_comm_employee" isHorizontal="true">
        <dc:Bounds x="50" y="420" width="950" height="200"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="shape_start_event_offer_required" bpmnElement="start_event_offer_required">
        <dc:Bounds x="70" y="102" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_task_perform_feasibility_check" bpmnElement="task_perform_feasibility_check">
        <dc:Bounds x="210" y="292" width="100" height="56"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_gateway_feasibility_outcome" bpmnElement="gateway_feasibility_outcome" isMarkerVisible="true">
        <dc:Bounds x="380" y="295" width="50" height="50"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_task_inform_customer_not_feasible" bpmnElement="task_inform_customer_not_feasible">
        <dc:Bounds x="530" y="52" width="100" height="56"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_end_event_customer_informed" bpmnElement="end_event_customer_informed">
        <dc:Bounds x="710" y="62" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_task_check_personnel_availability" bpmnElement="task_check_personnel_availability">
        <dc:Bounds x="530" y="292" width="100" height="56"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_task_create_offer" bpmnElement="task_create_offer">
        <dc:Bounds x="710" y="492" width="100" height="56"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_end_event_offer_created" bpmnElement="end_event_offer_created">
        <dc:Bounds x="890" y="502" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNEdge id="edge_flow_start_to_feasibility_check" bpmnElement="flow_start_to_feasibility_check">
        <di:waypoint x="106" y="120"/>
        <di:waypoint x="160" y="120"/>
        <di:waypoint x="160" y="320"/>
        <di:waypoint x="210" y="320"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="edge_flow_feasibility_check_to_gateway" bpmnElement="flow_feasibility_check_to_gateway">
        <di:waypoint x="310" y="320"/>
        <di:waypoint x="380" y="320"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="edge_flow_gateway_to_inform_customer" bpmnElement="flow_gateway_to_inform_customer">
        <di:waypoint x="405" y="295"/>
        <di:waypoint x="405" y="80"/>
        <di:waypoint x="530" y="80"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds x="410" y="170" width="70" height="14"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="edge_flow_inform_customer_to_end" bpmnElement="flow_inform_customer_to_end">
        <di:waypoint x="630" y="80"/>
        <di:waypoint x="710" y="80"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="edge_flow_gateway_to_check_availability" bpmnElement="flow_gateway_to_check_availability">
        <di:waypoint x="430" y="320"/>
        <di:waypoint x="530" y="320"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds x="470" y="300" width="50" height="14"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="edge_flow_check_availability_to_create_offer" bpmnElement="flow_check_availability_to_create_offer">
        <di:waypoint x="630" y="320"/>
        <di:waypoint x="670" y="320"/>
        <di:waypoint x="670" y="520"/>
        <di:waypoint x="710" y="520"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="edge_flow_create_offer_to_end" bpmnElement="flow_create_offer_to_end">
        <di:waypoint x="810" y="520"/>
        <di:waypoint x="890" y="520"/>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</semantic:definitions> (Routing to: end)
