<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="OfferFeasibilityProcess"
    targetNamespace="http://example.com/bpmn/offer-feasibility"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_offerFeasibility">
    <semantic:participant id="participant_company" name="Company" processRef="process_offerFeasibility"/>
  </semantic:collaboration>

  <semantic:process id="process_offerFeasibility" isExecutable="true">
    <semantic:laneSet id="laneSet_company">
      <semantic:lane id="lane_salesDepartment" name="Sales Department">
        <semantic:flowNodeRef>start_offerRequired</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_checkPersonnelNeeds</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_createOffer</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_offerCreated</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_commercialManager" name="Commercial Manager">
        <semantic:flowNodeRef>task_performFeasibilityCheck</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_offerFeasible</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_customer" name="Customer">
        <semantic:flowNodeRef>task_informCustomer</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_processEndsAfterInforming</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <semantic:startEvent id="start_offerRequired" name="Offer Required">
      <semantic:outgoing>flow_1</semantic:outgoing>
    </semantic:startEvent>

    <semantic:task id="task_performFeasibilityCheck" name="Perform Feasibility Check">
      <semantic:incoming>flow_1</semantic:incoming>
      <semantic:outgoing>flow_2</semantic:outgoing>
    </semantic:task>

    <semantic:exclusiveGateway id="gateway_offerFeasible" name="Is Offer Feasible?">
      <semantic:incoming>flow_2</semantic:incoming>
      <semantic:outgoing>flow_3</semantic:outgoing>
      <semantic:outgoing>flow_4</semantic:outgoing>
    </semantic:exclusiveGateway>

    <semantic:task id="task_informCustomer" name="Inform Customer">
      <semantic:incoming>flow_4</semantic:incoming>
      <semantic:outgoing>flow_5</semantic:outgoing>
    </semantic:task>

    <semantic:endEvent id="end_processEndsAfterInforming" name="Process Ends After Informing Customer">
      <semantic:incoming>flow_5</semantic:incoming>
    </semantic:endEvent>

    <semantic:task id="task_checkPersonnelNeeds" name="Check Personnel Needs and Availability">
      <semantic:incoming>flow_3</semantic:incoming>
      <semantic:outgoing>flow_6</semantic:outgoing>
    </semantic:task>

    <semantic:task id="task_createOffer" name="Create Offer">
      <semantic:incoming>flow_6</semantic:incoming>
      <semantic:outgoing>flow_7</semantic:outgoing>
    </semantic:task>

    <semantic:endEvent id="end_offerCreated" name="Process Ends After Creating Offer">
      <semantic:incoming>flow_7</semantic:incoming>
    </semantic:endEvent>

    <semantic:sequenceFlow id="flow_1" sourceRef="start_offerRequired" targetRef="task_performFeasibilityCheck"/>
    <semantic:sequenceFlow id="flow_2" sourceRef="task_performFeasibilityCheck" targetRef="gateway_offerFeasible"/>
    <semantic:sequenceFlow id="flow_3" sourceRef="gateway_offerFeasible" targetRef="task_checkPersonnelNeeds"/>
    <semantic:sequenceFlow id="flow_4" sourceRef="gateway_offerFeasible" targetRef="task_informCustomer"/>
    <semantic:sequenceFlow id="flow_5" sourceRef="task_informCustomer" targetRef="end_processEndsAfterInforming"/>
    <semantic:sequenceFlow id="flow_6" sourceRef="task_checkPersonnelNeeds" targetRef="task_createOffer"/>
    <semantic:sequenceFlow id="flow_7" sourceRef="task_createOffer" targetRef="end_offerCreated"/>
  </semantic:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_offerFeasibility" name="Offer Feasibility Diagram" resolution="96.0">
    <bpmndi:BPMNPlane bpmnElement="collab_offerFeasibility">
      <bpmndi:BPMNShape id="participant_company_shape" bpmnElement="participant_company" isHorizontal="true">
        <dc:Bounds x="10" y="10" width="900" height="300"/>
      </bpmndi:BPMNShape>

      <!-- Sales Department Lane -->
      <bpmndi:BPMNShape id="lane_salesDepartment_shape" bpmnElement="lane_salesDepartment" isHorizontal="false">
        <dc:Bounds x="10" y="10" width="280" height="280"/>
      </bpmndi:BPMNShape>

      <!-- Commercial Manager Lane -->
      <bpmndi:BPMNShape id="lane_commercialManager_shape" bpmnElement="lane_commercialManager" isHorizontal="false">
        <dc:Bounds x="290" y="10" width="280" height="280"/>
      </bpmndi:BPMNShape>

      <!-- Customer Lane -->
      <bpmndi:BPMNShape id="lane_customer_shape" bpmnElement="lane_customer" isHorizontal="false">
        <dc:Bounds x="570" y="10" width="280" height="280"/>
      </bpmndi:BPMNShape>

      <!-- Start Event -->
      <bpmndi:BPMNShape id="start_offerRequired_shape" bpmnElement="start_offerRequired">
        <dc:Bounds x="50" y="130" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <!-- Task: Perform Feasibility Check -->
      <bpmndi:BPMNShape id="task_performFeasibilityCheck_shape" bpmnElement="task_performFeasibilityCheck">
        <dc:Bounds x="330" y="120" width="120" height="56"/>
      </bpmndi:BPMNShape>

      <!-- Exclusive Gateway: Is Offer Feasible? -->
      <bpmndi:BPMNShape id="gateway_offerFeasible_shape" bpmnElement="gateway_offerFeasible" isMarkerVisible="true">
        <dc:Bounds x="480" y="130" width="50" height="50"/>
      </bpmndi:BPMNShape>

      <!-- Task: Inform Customer -->
      <bpmndi:BPMNShape id="task_informCustomer_shape" bpmnElement="task_informCustomer">
        <dc:Bounds x="630" y="120" width="120" height="56"/>
      </bpmndi:BPMNShape>

      <!-- End Event: Process Ends After Informing Customer -->
      <bpmndi:BPMNShape id="end_processEndsAfterInforming_shape" bpmnElement="end_processEndsAfterInforming">
        <dc:Bounds x="780" y="130" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <!-- Task: Check Personnel Needs and Availability -->
      <bpmndi:BPMNShape id="task_checkPersonnelNeeds_shape" bpmnElement="task_checkPersonnelNeeds">
        <dc:Bounds x="100" y="120" width="140" height="56"/>
      </bpmndi:BPMNShape>

      <!-- Task: Create Offer -->
      <bpmndi:BPMNShape id="task_createOffer_shape" bpmnElement="task_createOffer">
        <dc:Bounds x="100" y="200" width="140" height="56"/>
      </bpmndi:BPMNShape>

      <!-- End Event: Process Ends After Creating Offer -->
      <bpmndi:BPMNShape id="end_offerCreated_shape" bpmnElement="end_offerCreated">
        <dc:Bounds x="260" y="210" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <!-- Sequence Flows -->
      <bpmndi:BPMNEdge id="flow_1_edge" bpmnElement="flow_1">
        <di:waypoint x="86" y="148"/>
        <di:waypoint x="330" y="148"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_2_edge" bpmnElement="flow_2">
        <di:waypoint x="450" y="148"/>
        <di:waypoint x="480" y="155"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_3_edge" bpmnElement="flow_3">
        <di:waypoint x="530" y="155"/>
        <di:waypoint x="240" y="148"/>
        <di:waypoint x="240" y="148"/>
        <di:waypoint x="170" y="148"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_4_edge" bpmnElement="flow_4">
        <di:waypoint x="530" y="180"/>
        <di:waypoint x="630" y="148"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_5_edge" bpmnElement="flow_5">
        <di:waypoint x="750" y="148"/>
        <di:waypoint x="780" y="148"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_6_edge" bpmnElement="flow_6">
        <di:waypoint x="240" y="148"/>
        <di:waypoint x="100" y="148"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="flow_7_edge" bpmnElement="flow_7">
        <di:waypoint x="170" y="176"/>
        <di:waypoint x="170" y="210"/>
        <di:waypoint x="260" y="210"/>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</semantic:definitions>
