<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="P2PProcessDefinitions"
    targetNamespace="http://example.com/bpmn/p2p-process"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_p2p">
    <semantic:participant id="participant_company" name="Company P2P Operations" processRef="process_company"/>
    <semantic:participant id="participant_vendor" name="Vendor" processRef="process_vendor"/>

    <semantic:messageFlow id="mf_PO_to_Vendor" name="PO Dispatched" sourceRef="task_req_7" targetRef="task_vendor_1"/>
    <semantic:messageFlow id="mf_VendorDecision_to_Company" name="Vendor PO Decision" sourceRef="task_vendor_2" targetRef="ev_Catch_VendorDecision"/>
    <semantic:messageFlow id="mf_GoodsServices_to_Company" name="Goods/Services Delivered" sourceRef="task_vendor_3" targetRef="task_req_9"/>
    <semantic:messageFlow id="mf_Invoice_to_Company" name="Invoice Sent" sourceRef="task_vendor_4" targetRef="task_req_12"/>
    <semantic:messageFlow id="mf_IncorrectInvoice_to_Vendor" name="Incorrect Invoice Returned" sourceRef="task_req_14" targetRef="task_vendor_5"/>
    <semantic:messageFlow id="mf_CorrectedInvoice_to_Company" name="Corrected Invoice Sent" sourceRef="task_vendor_7" targetRef="task_req_15"/>
  </semantic:collaboration>

  <semantic:process id="process_company" isExecutable="true">
    <semantic:laneSet id="laneSet_company">
      <semantic:lane id="lane_requestor" name="Requestor">
        <semantic:flowNodeRef>se_1</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_1</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_2</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_1</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_3</semantic:flowNodeRef>
        <semantic:flowNodeRef>err_pr_discrepancy</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_log_pr_d</semantic:flowNodeRef>
        <semantic:flowNodeRef>ee_log_pr_d</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_4</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_2</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_5</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_6</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_3</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_7</semantic:flowNodeRef>
        <semantic:flowNodeRef>ev_Catch_VendorDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_4</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_8</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_9</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_5</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_10</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_11</semantic:flowNodeRef>
        <semantic:flowNodeRef>merge_gr_ses_complete</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_12</semantic:flowNodeRef>
        <semantic:flowNodeRef>err_invoice_discrepancy</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_log_inv_d</semantic:flowNodeRef>
        <semantic:flowNodeRef>ee_log_inv_d</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_13</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_req_6</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_14</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_15</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_16</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_req_17</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_hod" name="Heads of Departments">
        <semantic:flowNodeRef>task_hod_1</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_hod_2</semantic:flowNodeRef>
        <semantic:flowNodeRef>err_budget_overdraft</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_log_bud_o</semantic:flowNodeRef>
        <semantic:flowNodeRef>ee_log_bud_o</semantic:flowNodeRef>
        <semantic:flowNodeRef>err_data_breach</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_log_dat_b</semantic:flowNodeRef>
        <semantic:flowNodeRef>ee_log_dat_b</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_hod_3</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_po_approvers" name="PO Approvers">
        <semantic:flowNodeRef>task_poa_1</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_poa_2</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_poa_3</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_ap_team" name="Accounts Payable Team">
        <semantic:flowNodeRef>task_ap_1</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_2</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_ap_1</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_3</semantic:flowNodeRef>
        <semantic:flowNodeRef>ee_1</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <semantic:startEvent id="se_1" name="P2P Process Started">
      <semantic:outgoing>flow_se1_taskreq1</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_req_1" name="Determine and define business requirements &amp; needs">
      <semantic:incoming>flow_se1_taskreq1</semantic:incoming>
      <semantic:incoming>flow_gwreq1_taskreq1</semantic:incoming>
      <semantic:incoming>flow_gwreq2_taskreq1</semantic:incoming>
      <semantic:incoming>flow_gwreq3_taskreq1</semantic:incoming>
      <semantic:incoming>flow_taskreq8_taskreq1</semantic:incoming>
      <semantic:outgoing>flow_taskreq1_taskreq2</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_2" name="Request to issue Purchase Requisition (PR)">
      <semantic:incoming>flow_taskreq1_taskreq2</semantic:incoming>
      <semantic:outgoing>flow_taskreq2_gwreq1</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_1" name="Initial PR Request Approved?">
      <semantic:incoming>flow_taskreq2_gwreq1</semantic:incoming>
      <semantic:outgoing>flow_gwreq1_taskreq3</semantic:outgoing>
      <semantic:outgoing>flow_gwreq1_taskreq1</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_3" name="Issue Purchase Requisition (PR)">
      <semantic:incoming>flow_gwreq1_taskreq3</semantic:incoming>
      <semantic:outgoing>flow_taskreq3_taskreq4</semantic:outgoing>
    </semantic:task>
    <semantic:boundaryEvent id="err_pr_discrepancy" name="PR Discrepancy Occurred (Risk attached to Issue PR)" attachedToRef="task_req_3" cancelActivity="false">
      <semantic:outgoing>flow_err_pr_d_log</semantic:outgoing>
    </semantic:boundaryEvent>
    <semantic:task id="task_log_pr_d" name="Log PR Discrepancy">
        <semantic:incoming>flow_err_pr_d_log</semantic:incoming>
        <semantic:outgoing>flow_log_pr_d_end</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="ee_log_pr_d" name="PR Discrepancy Logged">
        <semantic:incoming>flow_log_pr_d_end</semantic:incoming>
    </semantic:endEvent>
    <semantic:task id="task_req_4" name="Send issued PR to HoD for approval">
      <semantic:incoming>flow_taskreq3_taskreq4</semantic:incoming>
      <semantic:outgoing>flow_taskreq4_taskhod1</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_hod_1" name="Receive PR for approval">
      <semantic:incoming>flow_taskreq4_taskhod1</semantic:incoming>
      <semantic:outgoing>flow_taskhod1_taskhod2</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_hod_2" name="Review PR (evaluate needs, verify budget, validate form)">
      <semantic:incoming>flow_taskhod1_taskhod2</semantic:incoming>
      <semantic:outgoing>flow_taskhod2_taskhod3</semantic:outgoing>
    </semantic:task>
    <semantic:boundaryEvent id="err_budget_overdraft" name="Budget Overdraft Risk (Attached to Review PR)" attachedToRef="task_hod_2" cancelActivity="false">
      <semantic:outgoing>flow_err_bud_o_log</semantic:outgoing>
    </semantic:boundaryEvent>
    <semantic:task id="task_log_bud_o" name="Log Budget Overdraft">
        <semantic:incoming>flow_err_bud_o_log</semantic:incoming>
        <semantic:outgoing>flow_log_bud_o_end</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="ee_log_bud_o" name="Budget Overdraft Logged">
        <semantic:incoming>flow_log_bud_o_end</semantic:incoming>
    </semantic:endEvent>
    <semantic:boundaryEvent id="err_data_breach" name="Data Breach Risk (Attached to Review PR)" attachedToRef="task_hod_2" cancelActivity="false">
      <semantic:outgoing>flow_err_dat_b_log</semantic:outgoing>
    </semantic:boundaryEvent>
    <semantic:task id="task_log_dat_b" name="Log Data Breach">
        <semantic:incoming>flow_err_dat_b_log</semantic:incoming>
        <semantic:outgoing>flow_log_dat_b_end</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="ee_log_dat_b" name="Data Breach Logged">
        <semantic:incoming>flow_log_dat_b_end</semantic:incoming>
    </semantic:endEvent>
    <semantic:task id="task_hod_3" name="Document/submit final PR decision to requestor">
      <semantic:incoming>flow_taskhod2_taskhod3</semantic:incoming>
      <semantic:outgoing>flow_taskhod3_gwreq2</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_2" name="PR Approved by HoD?">
      <semantic:incoming>flow_taskhod3_gwreq2</semantic:incoming>
      <semantic:outgoing>flow_gwreq2_taskreq5</semantic:outgoing>
      <semantic:outgoing>flow_gwreq2_taskreq1</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_5" name="Create Purchase Order (PO)">
      <semantic:incoming>flow_gwreq2_taskreq5</semantic:incoming>
      <semantic:outgoing>flow_taskreq5_taskreq6</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_6" name="Send PO to PO approvers for approval">
      <semantic:incoming>flow_taskreq5_taskreq6</semantic:incoming>
      <semantic:outgoing>flow_taskreq6_taskpoa1</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_poa_1" name="Receive PO for approval">
      <semantic:incoming>flow_taskreq6_taskpoa1</semantic:incoming>
      <semantic:outgoing>flow_taskpoa1_taskpoa2</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_poa_2" name="Review PO specifications">
      <semantic:incoming>flow_taskpoa1_taskpoa2</semantic:incoming>
      <semantic:outgoing>flow_taskpoa2_taskpoa3</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_poa_3" name="Document/submit final PO decision to requestor">
      <semantic:incoming>flow_taskpoa2_taskpoa3</semantic:incoming>
      <semantic:outgoing>flow_taskpoa3_gwreq3</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_3" name="PO Approved by PO Approvers?">
      <semantic:incoming>flow_taskpoa3_gwreq3</semantic:incoming>
      <semantic:outgoing>flow_gwreq3_taskreq7</semantic:outgoing>
      <semantic:outgoing>flow_gwreq3_taskreq1</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:sendTask id="task_req_7" name="Dispatch approved PO to vendor">
      <semantic:incoming>flow_gwreq3_taskreq7</semantic:incoming>
      <semantic:outgoing>flow_taskreq7_evCatchVendorDecision</semantic:outgoing>
    </semantic:sendTask>
    <semantic:intermediateCatchEvent id="ev_Catch_VendorDecision" name="Vendor PO Decision Received">
        <semantic:incoming>flow_taskreq7_evCatchVendorDecision</semantic:incoming>
        <semantic:outgoing>flow_evCatchVendorDecision_gwreq4</semantic:outgoing>
        <semantic:messageEventDefinition />
    </semantic:intermediateCatchEvent>
    <semantic:exclusiveGateway id="gw_req_4" name="Vendor's Decision on PO Received?">
      <semantic:incoming>flow_evCatchVendorDecision_gwreq4</semantic:incoming>
      <semantic:outgoing>flow_gwreq4_taskreq8</semantic:outgoing>
      <semantic:outgoing>flow_gwreq4_taskreq9</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_8" name="Search for a new vendor">
      <semantic:incoming>flow_gwreq4_taskreq8</semantic:incoming>
      <semantic:outgoing>flow_taskreq8_taskreq1</semantic:outgoing>
    </semantic:task>
    <semantic:receiveTask id="task_req_9" name="Receive agreed goods/services">
      <semantic:incoming>flow_gwreq4_taskreq9</semantic:incoming>
      <semantic:outgoing>flow_taskreq9_gwreq5</semantic:outgoing>
    </semantic:receiveTask>
    <semantic:exclusiveGateway id="gw_req_5" name="Goods or Service Received?">
      <semantic:incoming>flow_taskreq9_gwreq5</semantic:incoming>
      <semantic:outgoing>flow_gwreq5_taskreq10</semantic:outgoing>
      <semantic:outgoing>flow_gwreq5_taskreq11</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_req_10" name="Issue a Goods Receipt (GR)">
      <semantic:incoming>flow_gwreq5_taskreq10</semantic:incoming>
      <semantic:outgoing>flow_taskreq10_mergegrses</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_11" name="Issue a Service Entry Sheet (SES)">
      <semantic:incoming>flow_gwreq5_taskreq11</semantic:incoming>
      <semantic:outgoing>flow_taskreq11_mergegrses</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="merge_gr_ses_complete" name="GR/SES Issuance Complete">
      <semantic:incoming>flow_taskreq10_mergegrses</semantic:incoming>
      <semantic:incoming>flow_taskreq11_mergegrses</semantic:incoming>
      <semantic:outgoing>flow_mergegrses_taskreq12</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:receiveTask id="task_req_12" name="Receive invoice from vendor">
      <semantic:incoming>flow_mergegrses_taskreq12</semantic:incoming>
      <semantic:outgoing>flow_taskreq12_taskreq13</semantic:outgoing>
    </semantic:receiveTask>
    <semantic:boundaryEvent id="err_invoice_discrepancy" name="Invoice Discrepancy Occurred (Risk attached to Receive Invoice)" attachedToRef="task_req_12" cancelActivity="false">
      <semantic:outgoing>flow_err_inv_d_log</semantic:outgoing>
    </semantic:boundaryEvent>
    <semantic:task id="task_log_inv_d" name="Log Invoice Discrepancy">
        <semantic:incoming>flow_err_inv_d_log</semantic:incoming>
        <semantic:outgoing>flow_log_inv_d_end</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="ee_log_inv_d" name="Invoice Discrepancy Logged">
        <semantic:incoming>flow_log_inv_d_end</semantic:incoming>
    </semantic:endEvent>
    <semantic:task id="task_req_13" name="Perform three-way match (Invoice, PO, GR/SES)">
      <semantic:incoming>flow_taskreq12_taskreq13</semantic:incoming>
      <semantic:outgoing>flow_taskreq13_gwreq6</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_req_6" name="Invoice Correct?">
      <semantic:incoming>flow_taskreq13_gwreq6</semantic:incoming>
      <semantic:outgoing>flow_gwreq6_taskreq14</semantic:outgoing>
      <semantic:outgoing>flow_gwreq6_taskreq16</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:sendTask id="task_req_14" name="Return incorrect invoice to vendor for correction">
      <semantic:incoming>flow_gwreq6_taskreq14</semantic:incoming>
      <semantic:outgoing>flow_taskreq14_taskreq15</semantic:outgoing>
    </semantic:sendTask>
    <semantic:receiveTask id="task_req_15" name="Receive corrected invoice from vendor">
      <semantic:incoming>flow_taskreq14_taskreq15</semantic:incoming>
      <semantic:outgoing>flow_taskreq15_taskreq16</semantic:outgoing>
    </semantic:receiveTask>
    <semantic:task id="task_req_16" name="Post invoice in SAP">
      <semantic:incoming>flow_gwreq6_taskreq16</semantic:incoming>
      <semantic:incoming>flow_taskreq15_taskreq16</semantic:incoming>
      <semantic:outgoing>flow_taskreq16_taskreq17</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_req_17" name="Send invoice to AP team">
      <semantic:incoming>flow_taskreq16_taskreq17</semantic:incoming>
      <semantic:outgoing>flow_taskreq17_taskap1</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ap_1" name="Receive posted invoice">
      <semantic:incoming>flow_taskreq17_taskap1</semantic:incoming>
      <semantic:outgoing>flow_taskap1_taskap2</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ap_2" name="Decide on payment method">
      <semantic:incoming>flow_taskap1_taskap2</semantic:incoming>
      <semantic:outgoing>flow_taskap2_gwap1</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_ap_1" name="Payment Method Selected">
      <semantic:incoming>flow_taskap2_gwap1</semantic:incoming>
      <semantic:outgoing>flow_gwap1_taskap3_full</semantic:outgoing>
      <semantic:outgoing>flow_gwap1_taskap3_install</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_ap_3" name="Perform payment to vendor">
      <semantic:incoming>flow_gwap1_taskap3_full</semantic:incoming>
      <semantic:incoming>flow_gwap1_taskap3_install</semantic:incoming>
      <semantic:outgoing>flow_taskap3_ee1</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="ee_1" name="P2P Process Successfully Completed">
      <semantic:incoming>flow_taskap3_ee1</semantic:incoming>
    </semantic:endEvent>

    <semantic:sequenceFlow id="flow_se1_taskreq1" sourceRef="se_1" targetRef="task_req_1"/>
    <semantic:sequenceFlow id="flow_taskreq1_taskreq2" sourceRef="task_req_1" targetRef="task_req_2"/>
    <semantic:sequenceFlow id="flow_taskreq2_gwreq1" sourceRef="task_req_2" targetRef="gw_req_1"/>
    <semantic:sequenceFlow id="flow_gwreq1_taskreq3" name="Approved" sourceRef="gw_req_1" targetRef="task_req_3"/>
    <semantic:sequenceFlow id="flow_gwreq1_taskreq1" name="Not Approved" sourceRef="gw_req_1" targetRef="task_req_1"/>
    <semantic:sequenceFlow id="flow_taskreq3_taskreq4" sourceRef="task_req_3" targetRef="task_req_4"/>
    <semantic:sequenceFlow id="flow_err_pr_d_log" sourceRef="err_pr_discrepancy" targetRef="task_log_pr_d"/>
    <semantic:sequenceFlow id="flow_log_pr_d_end" sourceRef="task_log_pr_d" targetRef="ee_log_pr_d"/>
    <semantic:sequenceFlow id="flow_taskreq4_taskhod1" sourceRef="task_req_4" targetRef="task_hod_1"/>
    <semantic:sequenceFlow id="flow_taskhod1_taskhod2" sourceRef="task_hod_1" targetRef="task_hod_2"/>
    <semantic:sequenceFlow id="flow_taskhod2_taskhod3" sourceRef="task_hod_2" targetRef="task_hod_3"/>
    <semantic:sequenceFlow id="flow_err_bud_o_log" sourceRef="err_budget_overdraft" targetRef="task_log_bud_o"/>
    <semantic:sequenceFlow id="flow_log_bud_o_end" sourceRef="task_log_bud_o" targetRef="ee_log_bud_o"/>
    <semantic:sequenceFlow id="flow_err_dat_b_log" sourceRef="err_data_breach" targetRef="task_log_dat_b"/>
    <semantic:sequenceFlow id="flow_log_dat_b_end" sourceRef="task_log_dat_b" targetRef="ee_log_dat_b"/>
    <semantic:sequenceFlow id="flow_taskhod3_gwreq2" sourceRef="task_hod_3" targetRef="gw_req_2"/>
    <semantic:sequenceFlow id="flow_gwreq2_taskreq5" name="PR Approved by HoD" sourceRef="gw_req_2" targetRef="task_req_5"/>
    <semantic:sequenceFlow id="flow_gwreq2_taskreq1" name="PR Not Approved by HoD" sourceRef="gw_req_2" targetRef="task_req_1"/>
    <semantic:sequenceFlow id="flow_taskreq5_taskreq6" sourceRef="task_req_5" targetRef="task_req_6"/>
    <semantic:sequenceFlow id="flow_taskreq6_taskpoa1" sourceRef="task_req_6" targetRef="task_poa_1"/>
    <semantic:sequenceFlow id="flow_taskpoa1_taskpoa2" sourceRef="task_poa_1" targetRef="task_poa_2"/>
    <semantic:sequenceFlow id="flow_taskpoa2_taskpoa3" sourceRef="task_poa_2" targetRef="task_poa_3"/>
    <semantic:sequenceFlow id="flow_taskpoa3_gwreq3" sourceRef="task_poa_3" targetRef="gw_req_3"/>
    <semantic:sequenceFlow id="flow_gwreq3_taskreq7" name="PO Approved by PO Approvers" sourceRef="gw_req_3" targetRef="task_req_7"/>
    <semantic:sequenceFlow id="flow_gwreq3_taskreq1" name="PO Rejected by PO Approvers" sourceRef="gw_req_3" targetRef="task_req_1"/>
    <semantic:sequenceFlow id="flow_taskreq7_evCatchVendorDecision" sourceRef="task_req_7" targetRef="ev_Catch_VendorDecision"/>
    <semantic:sequenceFlow id="flow_evCatchVendorDecision_gwreq4" sourceRef="ev_Catch_VendorDecision" targetRef="gw_req_4"/>
    <semantic:sequenceFlow id="flow_gwreq4_taskreq8" name="Vendor Rejected PO" sourceRef="gw_req_4" targetRef="task_req_8"/>
    <semantic:sequenceFlow id="flow_taskreq8_taskreq1" sourceRef="task_req_8" targetRef="task_req_1"/>
    <semantic:sequenceFlow id="flow_gwreq4_taskreq9" name="Vendor Approved or Negotiated PO" sourceRef="gw_req_4" targetRef="task_req_9"/>
    <semantic:sequenceFlow id="flow_taskreq9_gwreq5" sourceRef="task_req_9" targetRef="gw_req_5"/>
    <semantic:sequenceFlow id="flow_gwreq5_taskreq10" name="Goods Received" sourceRef="gw_req_5" targetRef="task_req_10"/>
    <semantic:sequenceFlow id="flow_gwreq5_taskreq11" name="Service Received" sourceRef="gw_req_5" targetRef="task_req_11"/>
    <semantic:sequenceFlow id="flow_taskreq10_mergegrses" sourceRef="task_req_10" targetRef="merge_gr_ses_complete"/>
