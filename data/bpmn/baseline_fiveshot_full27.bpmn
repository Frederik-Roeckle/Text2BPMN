  </semantic:collaboration>
  <semantic:process id="Process_InternalRefurbishment" isExecutable="false">
    <semantic:laneSet id="LaneSet_Main">
      <semantic:lane id="Lane_ResponsibleMaintenance" name="Responsible Maintenance">
        <semantic:flowNodeRef>StartEvent_RequestReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_CreatePMNotification</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_ChooseNotificationTypeP1</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineNotificationCharacteristics</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_ConfirmCharacteristicsEntries</semantic:flowNodeRef>
        <semantic:flowNodeRef>ServiceTask_PerformWarrantyCheck</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineReferenceObjectTab</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineMalfunctionDataTab</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_IsBreakdownCase</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_MarkBreakdownCheckbox</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_MergeBreakdownPath</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_SavePMNotification</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_PrepareApprovalDetails_Split</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineReceiverDetails</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineNoticeText</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_PrepareApprovalDetails_Merge</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_ConfirmApprovalPreparation</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_SaveAndSendApprovalRequest</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_ReceiveManagerDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_WasRequestApprovedByManager</semantic:flowNodeRef>
        <semantic:flowNodeRef>EndEvent_RequestNotApproved</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_ManageWorkOrder</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_CreateInternalRefurbishmentOrder</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_TransferInfoToWorkOrder</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineOrderTypeMC04</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_DefineRefurbDetails_Split</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineRefurbishmentPlantsInterval</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineValuationType</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_DefineRefurbDetails_Merge</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_MaintainOperations_Split</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_MaintainExternalOperations</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_MaintainOperations_Merge</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineEstimatedCosts</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_MaintainInvolvedPartners</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineAllocatedObjects</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineAdditionalData</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineLocationData</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_SaveWorkOrder</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_AssignWorkOrderToWBS</semantic:flowNodeRef>
        <semantic:flowNodeRef>EndEvent_RequestHandledSuccessfully</semantic:flowNodeRef>
        <semantic:flowNodeRef>TextAnnotation_BudgetOverdraftCosts</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="Lane_MaintenanceManager" name="Maintenance Manager">
        <semantic:flowNodeRef>Task_ReceiveAndEvaluateRequest</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DocumentSubmitDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_ManagerDecisionApproved</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_ChangePMNotification_Approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_SetStatusToApproved</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_ConfirmApprovedStatusEntries</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_SelectPutInProcessIcon</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_SaveApprovedNotification</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_ChangePMNotification_NotApproved</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_SetStatusToCancelled</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_SaveCancelledNotification</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_MergeManagerDecision</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="Lane_MaintenancePlanner" name="Maintenance Planner">
        <semantic:flowNodeRef>StartEvent_PlanningRequestReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_PlanInternalOperations</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_DefineOperationParams_Split</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefinePlant_Sub</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineDescription_Sub</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineControlKeyPM01_Sub</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineWorkingHours_Sub</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_DefineOperationParams_Merge</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_CreateWorkOrderApproval</semantic:flowNodeRef>
        <semantic:flowNodeRef>ServiceTask_ActivateWorkApprovalAction</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_PlanResources</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_CheckMaterialAvailability</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_DefineMaterialCheckParams_Split</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefinePlantForMaterialCheck</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DefineMaterialForCheck</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_DefineMaterialCheckParams_Merge</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_ExecuteMaterialCheck</semantic:flowNodeRef>
        <semantic:flowNodeRef>ServiceTask_DisplayMaterialReservationsList</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_DisplayReservationForWorkOrder</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_MaterialsAvailable</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_ReserveMaterials</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_OrderRequiredMaterials</semantic:flowNodeRef>
        <semantic:flowNodeRef>SendTask_DispatchOrderToPurchasing</semantic:flowNodeRef>
        <semantic:flowNodeRef>Gateway_MergeMaterialAction</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_PlanCapacity</semantic:flowNodeRef>
        <semantic:flowNodeRef>ServiceTask_DisplayCapacityEvaluation</semantic:flowNodeRef>
        <semantic:flowNodeRef>Task_ScheduleWorkOrder_Sub</semantic:flowNodeRef>
        <semantic:flowNodeRef>EndEvent_PlanningCompleted</semantic:flowNodeRef>
        <semantic:flowNodeRef>TextAnnotation_MaterialShortage</semantic:flowNodeRef>
        <semantic:flowNodeRef>TextAnnotation_CapacityOverdraft</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <!-- Responsible Maintenance Flow -->
    <semantic:startEvent id="StartEvent_RequestReceived" name="Internal Refurbishment Request Received">
      <semantic:outgoing>Flow_StartToCreatePM</semantic:outgoing>
    </semantic:startEvent>
    <semantic:userTask id="Task_CreatePMNotification" name="Create PM Notification in SAP">
      <semantic:incoming>Flow_StartToCreatePM</semantic:incoming>
      <semantic:outgoing>Flow_CreatePMToChooseType</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_ChooseNotificationTypeP1" name="Choose Notification Type P1">
      <semantic:incoming>Flow_CreatePMToChooseType</semantic:incoming>
      <semantic:outgoing>Flow_ChooseTypeToDefineChars</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_DefineNotificationCharacteristics" name="Define Notification Characteristics Tab">
      <semantic:incoming>Flow_ChooseTypeToDefineChars</semantic:incoming>
      <semantic:outgoing>Flow_DefineCharsToConfirmChars</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_ConfirmCharacteristicsEntries" name="Confirm Characteristics Entries">
      <semantic:incoming>Flow_DefineCharsToConfirmChars</semantic:incoming>
      <semantic:outgoing>Flow_ConfirmCharsToWarrantyCheck</semantic:outgoing>
    </semantic:userTask>
    <semantic:serviceTask id="ServiceTask_PerformWarrantyCheck" name="Perform Warranty Check by System">
      <semantic:incoming>Flow_ConfirmCharsToWarrantyCheck</semantic:incoming>
      <semantic:outgoing>Flow_WarrantyCheckToDefineRefObj</semantic:outgoing>
    </semantic:serviceTask>
    <semantic:userTask id="Task_DefineReferenceObjectTab" name="Define Reference Object Tab &amp; Attach Docs">
      <semantic:incoming>Flow_WarrantyCheckToDefineRefObj</semantic:incoming>
      <semantic:outgoing>Flow_DefineRefObjToDefineMalfunction</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_DefineMalfunctionDataTab" name="Define Malfunction Data Tab">
      <semantic:incoming>Flow_DefineRefObjToDefineMalfunction</semantic:incoming>
      <semantic:outgoing>Flow_DefineMalfunctionToBreakdownCase</semantic:outgoing>
    </semantic:userTask>
    <semantic:exclusiveGateway id="Gateway_IsBreakdownCase" name="Is it a Breakdown Case?">
      <semantic:incoming>Flow_DefineMalfunctionToBreakdownCase</semantic:incoming>
      <semantic:outgoing>Flow_BreakdownCase_Yes</semantic:outgoing>
      <semantic:outgoing>Flow_BreakdownCase_No</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:userTask id="Task_MarkBreakdownCheckbox" name="Mark Breakdown Checkbox">
      <semantic:incoming>Flow_BreakdownCase_Yes</semantic:incoming>
      <semantic:outgoing>Flow_MarkBreakdownToMerge</semantic:outgoing>
    </semantic:userTask>
    <semantic:exclusiveGateway id="Gateway_MergeBreakdownPath" name="Merge Breakdown Path">
      <semantic:incoming>Flow_MarkBreakdownToMerge</semantic:incoming>
      <semantic:incoming>Flow_BreakdownCase_No</semantic:incoming>
      <semantic:outgoing>Flow_MergeBreakdownToSavePM</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:userTask id="Task_SavePMNotification" name="Save PM Notification">
      <semantic:incoming>Flow_MergeBreakdownToSavePM</semantic:incoming>
      <semantic:outgoing>Flow_SavePMToPrepareApprovalSplit</semantic:outgoing>
    </semantic:userTask>
    <semantic:parallelGateway id="Gateway_PrepareApprovalDetails_Split" name="Prepare Approval Details">
      <semantic:incoming>Flow_SavePMToPrepareApprovalSplit</semantic:incoming>
      <semantic:outgoing>Flow_SplitToDefineReceiver</semantic:outgoing>
      <semantic:outgoing>Flow_SplitToDefineNotice</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:userTask id="Task_DefineReceiverDetails" name="Define Receiver Details">
      <semantic:incoming>Flow_SplitToDefineReceiver</semantic:incoming>
      <semantic:outgoing>Flow_DefineReceiverToMerge</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_DefineNoticeText" name="Define Notice Text">
      <semantic:incoming>Flow_SplitToDefineNotice</semantic:incoming>
      <semantic:outgoing>Flow_DefineNoticeToMerge</semantic:outgoing>
    </semantic:userTask>
    <semantic:parallelGateway id="Gateway_PrepareApprovalDetails_Merge" name="Merge Approval Details">
      <semantic:incoming>Flow_DefineReceiverToMerge</semantic:incoming>
      <semantic:incoming>Flow_DefineNoticeToMerge</semantic:incoming>
      <semantic:outgoing>Flow_MergeToConfirmApprovalPrep</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:userTask id="Task_ConfirmApprovalPreparation" name="Confirm Approval Preparation Entries">
      <semantic:incoming>Flow_MergeToConfirmApprovalPrep</semantic:incoming>
      <semantic:outgoing>Flow_ConfirmApprovalPrepToSaveSend</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_SaveAndSendApprovalRequest" name="Save Approval Process &amp; Send Request">
      <semantic:incoming>Flow_ConfirmApprovalPrepToSaveSend</semantic:incoming>
      <semantic:outgoing>Flow_SendRequestToReceiveEval</semantic:outgoing>
    </semantic:userTask>

    <!-- Maintenance Manager Flow -->
    <semantic:userTask id="Task_ReceiveAndEvaluateRequest" name="Receive and Evaluate Request">
      <semantic:incoming>Flow_SendRequestToReceiveEval</semantic:incoming>
      <semantic:outgoing>Flow_ReceiveEvalToDocSubmit</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_DocumentSubmitDecision" name="Document/Submit Decision">
      <semantic:incoming>Flow_ReceiveEvalToDocSubmit</semantic:incoming>
      <semantic:outgoing>Flow_DocSubmitToManagerDecision</semantic:outgoing>
    </semantic:userTask>
    <semantic:exclusiveGateway id="Gateway_ManagerDecisionApproved" name="Request Approved by Manager?">
      <semantic:incoming>Flow_DocSubmitToManagerDecision</semantic:incoming>
      <semantic:outgoing>Flow_ManagerDecision_Yes</semantic:outgoing>
      <semantic:outgoing>Flow_ManagerDecision_No</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:userTask id="Task_ChangePMNotification_Approved" name="Change PM Notification (Approved)">
      <semantic:incoming>Flow_ManagerDecision_Yes</semantic:incoming>
      <semantic:outgoing>Flow_ChangePMApprovedToSetStatusApproved</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_SetStatusToApproved" name="Set Notification Status to Approved (0002)">
      <semantic:incoming>Flow_ChangePMApprovedToSetStatusApproved</semantic:incoming>
      <semantic:outgoing>Flow_SetStatusApprovedToConfirmApproved</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_ConfirmApprovedStatusEntries" name="Confirm Approved Status Entries">
      <semantic:incoming>Flow_SetStatusApprovedToConfirmApproved</semantic:incoming>
      <semantic:outgoing>Flow_ConfirmApprovedToPutInProcess</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_SelectPutInProcessIcon" name="Select 'Put in Process' Icon">
      <semantic:incoming>Flow_ConfirmApprovedToPutInProcess</semantic:incoming>
      <semantic:outgoing>Flow_PutInProcessToSaveApproved</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_SaveApprovedNotification" name="Save Approved Notification">
      <semantic:incoming>Flow_PutInProcessToSaveApproved</semantic:incoming>
      <semantic:outgoing>Flow_SaveApprovedToMergeManagerDecision</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_ChangePMNotification_NotApproved" name="Change PM Notification (Not Approved)">
      <semantic:incoming>Flow_ManagerDecision_No</semantic:incoming>
      <semantic:outgoing>Flow_ChangePMNotApprovedToSetStatusCancelled</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_SetStatusToCancelled" name="Set Notification Status to Cancelled (0005)">
      <semantic:incoming>Flow_ChangePMNotApprovedToSetStatusCancelled</semantic:incoming>
      <semantic:outgoing>Flow_SetStatusCancelledToSaveCancelled</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_SaveCancelledNotification" name="Save Cancelled Notification">
      <semantic:incoming>Flow_SetStatusCancelledToSaveCancelled</semantic:incoming>
      <semantic:outgoing>Flow_SaveCancelledToMergeManagerDecision</semantic:outgoing>
    </semantic:userTask>
    <semantic:exclusiveGateway id="Gateway_MergeManagerDecision" name="Merge Manager Decision">
      <semantic:incoming>Flow_SaveApprovedToMergeManagerDecision</semantic:incoming>
      <semantic:incoming>Flow_SaveCancelledToMergeManagerDecision</semantic:incoming>
      <semantic:outgoing>Flow_MergeManagerDecisionToReceiveRMDecision</semantic:outgoing>
    </semantic:exclusiveGateway>

    <!-- Responsible Maintenance Flow (Post-Manager) -->
    <semantic:userTask id="Task_ReceiveManagerDecision" name="Receive Maintenance Request Decision">
      <semantic:incoming>Flow_MergeManagerDecisionToReceiveRMDecision</semantic:incoming>
      <semantic:outgoing>Flow_ReceiveRMDecisionToWasApproved</semantic:outgoing>
    </semantic:userTask>
    <semantic:exclusiveGateway id="Gateway_WasRequestApprovedByManager" name="Was Request Approved?">
      <semantic:incoming>Flow_ReceiveRMDecisionToWasApproved</semantic:incoming>
      <semantic:outgoing>Flow_WasApproved_Yes</semantic:outgoing>
      <semantic:outgoing>Flow_WasApproved_No</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:endEvent id="EndEvent_RequestNotApproved" name="Request Not Approved - Process Ends">
      <semantic:incoming>Flow_WasApproved_No</semantic:incoming>
    </semantic:endEvent>
    <semantic:userTask id="Task_ManageWorkOrder" name="Manage Work Order">
      <semantic:incoming>Flow_WasApproved_Yes</semantic:incoming>
      <semantic:outgoing>Flow_ManageWOToCreateInternalRefurb</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_CreateInternalRefurbishmentOrder" name="Create Internal Refurbishment Order">
      <semantic:incoming>Flow_ManageWOToCreateInternalRefurb</semantic:incoming>
      <semantic:outgoing>Flow_CreateInternalRefurbToTransferInfo</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_TransferInfoToWorkOrder" name="Transfer Information to Work Order">
      <semantic:incoming>Flow_CreateInternalRefurbToTransferInfo</semantic:incoming>
      <semantic:outgoing>Flow_TransferInfoToDefineOrderType</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_DefineOrderTypeMC04" name="Define Order Type MC04">
      <semantic:incoming>Flow_TransferInfoToDefineOrderType</semantic:incoming>
      <semantic:outgoing>Flow_DefineOrderTypeToRefurbDetailsSplit</semantic:outgoing>
    </semantic:userTask>
    <semantic:parallelGateway id="Gateway_DefineRefurbDetails_Split" name="Define Refurbishment Details">
      <semantic:incoming>Flow_DefineOrderTypeToRefurbDetailsSplit</semantic:incoming>
      <semantic:outgoing>Flow_SplitToDefinePlantsInterval</semantic:outgoing>
      <semantic:outgoing>Flow_SplitToDefineValuationType</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:userTask id="Task_DefineRefurbishmentPlantsInterval" name="Define Interval of Refurbishment Plants">
      <semantic:incoming>Flow_SplitToDefinePlantsInterval</semantic:incoming>
      <semantic:outgoing>Flow_DefinePlantsIntervalToMerge</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_DefineValuationType" name="Define Valuation Type">
      <semantic:incoming>Flow_SplitToDefineValuationType</semantic:incoming>
      <semantic:outgoing>Flow_DefineValuationTypeToMerge</semantic:outgoing>
    </semantic:userTask>
    <semantic:parallelGateway id="Gateway_DefineRefurbDetails_Merge" name="Merge Refurbishment Details">
      <semantic:incoming>Flow_DefinePlantsIntervalToMerge</semantic:incoming>
      <semantic:incoming>Flow_DefineValuationTypeToMerge</semantic:incoming>
      <semantic:outgoing>Flow_MergeRefurbDetailsToMaintainOpsSplit</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:parallelGateway id="Gateway_MaintainOperations_Split" name="Maintain Operations">
      <semantic:incoming>Flow_MergeRefurbDetailsToMaintainOpsSplit</semantic:incoming>
      <semantic:outgoing>Flow_SplitToPlanningRequest</semantic:outgoing> <!-- To Maintenance Planner Lane -->
      <semantic:outgoing>Flow_SplitToMaintainExternalOps</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:userTask id="Task_MaintainExternalOperations" name="Maintain External Operations">
      <semantic:incoming>Flow_SplitToMaintainExternalOps</semantic:incoming>
      <semantic:outgoing>Flow_MaintainExternalOpsToMergeOps</semantic:outgoing>
    </semantic:userTask>
    <semantic:parallelGateway id="Gateway_MaintainOperations_Merge" name="Merge Operations">
      <semantic:incoming>Flow_PlanningCompletedToMergeOps</semantic:incoming> <!-- From Maintenance Planner Lane -->
      <semantic:incoming>Flow_MaintainExternalOpsToMergeOps</semantic:incoming>
      <semantic:outgoing>Flow_MergeOpsToDefineCosts</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:userTask id="Task_DefineEstimatedCosts" name="Define Estimated Costs">
      <semantic:incoming>Flow_MergeOpsToDefineCosts</semantic:incoming>
      <semantic:outgoing>Flow_DefineCostsToMaintainPartners</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_MaintainInvolvedPartners" name="Maintain Involved Partners (External)">
      <semantic:incoming>Flow_DefineCostsToMaintainPartners</semantic:incoming>
      <semantic:outgoing>Flow_MaintainPartnersToDefineAllocObjects</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_DefineAllocatedObjects" name="Define Allocated Objects to Work Order">
      <semantic:incoming>Flow_MaintainPartnersToDefineAllocObjects</semantic:incoming>
      <semantic:outgoing>Flow_DefineAllocObjectsToDefineAddData</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_DefineAdditionalData" name="Define Additional Data for Work Order">
      <semantic:incoming>Flow_DefineAllocObjectsToDefineAddData</semantic:incoming>
      <semantic:outgoing>Flow_DefineAddDataToDefineLocationData</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_DefineLocationData" name="Define Location Data for Work Order">
      <semantic:incoming>Flow_DefineAddDataToDefineLocationData</semantic:incoming>
      <semantic:outgoing>Flow_DefineLocationDataToSaveWO</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_SaveWorkOrder" name="Save Work Order">
      <semantic:incoming>Flow_DefineLocationDataToSaveWO</semantic:incoming>
      <semantic:outgoing>Flow_SaveWOToAssignWBS</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_AssignWorkOrderToWBS" name="Assign Work Order to WBS Element">
      <semantic:incoming>Flow_SaveWOToAssignWBS</semantic:incoming>
      <semantic:outgoing>Flow_AssignWBSToEndHandled</semantic:outgoing>
    </semantic:userTask>
    <semantic:endEvent id="EndEvent_RequestHandledSuccessfully" name="Internal Refurbishment Request Handled">
      <semantic:incoming>Flow_AssignWBSToEndHandled</semantic:incoming>
    </semantic:endEvent>

    <!-- Maintenance Planner Flow (Sub-Process like) -->
    <semantic:startEvent id="StartEvent_PlanningRequestReceived" name="Request for Planning Internal Operations Received">
      <semantic:incoming>Flow_SplitToPlanningRequest</semantic:incoming>
      <semantic:outgoing>Flow_PlanningStartToPlanInternalOps</semantic:outgoing>
    </semantic:startEvent>
    <semantic:userTask id="Task_PlanInternalOperations" name="Plan Internal Operations">
      <semantic:incoming>Flow_PlanningStartToPlanInternalOps</semantic:incoming>
      <semantic:outgoing>Flow_PlanInternalOpsToDefineOpParamsSplit</semantic:outgoing>
    </semantic:userTask>
    <semantic:parallelGateway id="Gateway_DefineOperationParams_Split" name="Define Operation Parameters">
      <semantic:incoming>Flow_PlanInternalOpsToDefineOpParamsSplit</semantic:incoming>
      <semantic:outgoing>Flow_SplitToDefinePlantSub</semantic:outgoing>
      <semantic:outgoing>Flow_SplitToDefineDescSub</semantic:outgoing>
      <semantic:outgoing>Flow_SplitToDefineCtrlKeySub</semantic:outgoing>
      <semantic:outgoing>Flow_SplitToDefineHoursSub</semantic:outgoing>
    </semantic:parallelGateway>
    <semantic:userTask id="Task_DefinePlant_Sub" name="Define Plant (Sub)">
      <semantic:incoming>Flow_SplitToDefinePlantSub</semantic:incoming>
      <semantic:outgoing>Flow_DefinePlantSubToMergeOpParams</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_DefineDescription_Sub" name="Define Description (Sub)">
      <semantic:incoming>Flow_SplitToDefineDescSub</semantic:incoming>
      <semantic:outgoing>Flow_DefineDescSubToMergeOpParams</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_DefineControlKeyPM01_Sub" name="Define Control Key PM01 (Sub)">
      <semantic:incoming>Flow_SplitToDefineCtrlKeySub</semantic:incoming>
      <semantic:outgoing>Flow_DefineCtrlKeySubToMergeOpParams</semantic:outgoing>
    </semantic:userTask>
    <semantic:userTask id="Task_DefineWorkingHours_Sub" name="Define Working Hours (Sub)">
      <semantic:incoming>Flow_SplitToDefineHoursSub</semantic:incoming>
