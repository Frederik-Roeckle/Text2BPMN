        <semantic:flowNodeRef>exclusiveGateway_HoD_PRReviewOutcome</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_HoD_DocPRApproval</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_HoD_DocPRDecline</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_HoD_DocPRRejectIncomplete</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_HoD_MergeDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_HoD_SubmitDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>endEvent_HoD_DecisionSent</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_POApprovers" name="PO Approvers">
        <semantic:flowNodeRef>intermediateCatchEvent_POApprover_POReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_POApprover_ReviewPO</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_POApprover_POReviewOutcome</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_POApprover_DocPOApproval</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_POApprover_DocPORejection</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_POApprover_MergeDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_POApprover_SubmitDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>endEvent_POApprover_DecisionSent</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_Vendor" name="Vendor">
        <semantic:flowNodeRef>intermediateCatchEvent_Vendor_POReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Vendor_EvaluatePO</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_Vendor_POEvaluationOutcome</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Vendor_DocPOApproval</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Vendor_DocPONegotiation</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Vendor_DocPORejection</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Vendor_CommunicatePORejection</semantic:flowNodeRef>
        <semantic:flowNodeRef>endEvent_Vendor_PORejected</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_Vendor_MergeApproveNegotiate</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Vendor_DeliverGoodsServices</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Vendor_SendInvoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>endEvent_Vendor_DeliveryAndInvoiceSent</semantic:flowNodeRef>
        <semantic:flowNodeRef>intermediateCatchEvent_Vendor_WrongInvoiceReceived</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Vendor_CorrectInvoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Vendor_SendCorrectedInvoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>endEvent_Vendor_CorrectedInvoiceSent</semantic:flowNodeRef>
      </semantic:lane>
