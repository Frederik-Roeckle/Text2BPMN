      <semantic:outgoing>flow_HoDDocApproval_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_HoD_DocPRDecline" name="Document PR Decline">
      <semantic:incoming>flow_gwHoDOutcome_docDecline</semantic:incoming>
      <semantic:outgoing>flow_HoDDocDecline_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_HoD_DocPRRejectIncomplete" name="Document PR Rejection (Incomplete)">
      <semantic:incoming>flow_gwHoDOutcome_docReject</semantic:incoming>
