      <semantic:incoming>flow_POApproverPOReceived_reviewPO</semantic:incoming>
      <semantic:outgoing>flow_POApproverReviewPO_gwOutcome</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_POApprover_POReviewOutcome" name="PO Review Outcome?">
      <semantic:incoming>flow_POApproverReviewPO_gwOutcome</semantic:incoming>
      <semantic:outgoing>flow_gwPOApproverOutcome_docApproval</semantic:outgoing>
      <semantic:outgoing>flow_gwPOApproverOutcome_docRejection</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_POApprover_DocPOApproval" name="Document PO Approval">
      <semantic:incoming>flow_gwPOApproverOutcome_docApproval</semantic:incoming>
      <semantic:outgoing>flow_POApproverDocApproval_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_POApprover_DocPORejection" name="Document PO Rejection">
      <semantic:incoming>flow_gwPOApproverOutcome_docRejection</semantic:incoming>
      <semantic:outgoing>flow_POApproverDocRejection_merge</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_POApprover_MergeDecision" name="Decision Documented">
