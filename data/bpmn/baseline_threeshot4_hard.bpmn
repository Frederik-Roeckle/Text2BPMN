      <semantic:outgoing>flow_gwPOApproverDecision_determineNeeds</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_Requestor_DispatchPOToVendor" name="Dispatch Approved PO to Vendor">
      <semantic:incoming>flow_gwPOApproverDecision_dispatchPO</semantic:incoming>
