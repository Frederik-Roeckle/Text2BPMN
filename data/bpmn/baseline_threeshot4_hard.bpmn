        <semantic:flowNodeRef>exclusiveGateway_AP_PaymentMethod</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AP_SetupFullPayment</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AP_SetupInstallmentPayment</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_AP_MergePaymentSetup</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_AP_PerformPayment</semantic:flowNodeRef>
        <semantic:flowNodeRef>endEvent_AP_ProcessCompleted</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>
    <!-- Requestor Lane Elements -->
    <semantic:startEvent id="startEvent_Requestor_P2PInitiated" name="P2P Process Initiated">
      <semantic:outgoing>flow_start_determineNeeds</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_Requestor_DetermineNeeds" name="Determine Business Requirements &amp; Needs">
      <semantic:incoming>flow_start_determineNeeds</semantic:incoming>
      <semantic:incoming>flow_gwPRApprovalToIssue_determineNeeds</semantic:incoming>
      <semantic:incoming>flow_gwHoDDecision_determineNeeds</semantic:incoming>
      <semantic:incoming>flow_gwPOApproverDecision_determineNeeds</semantic:incoming>
      <semantic:outgoing>flow_determineNeeds_requestIssuePR</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_Requestor_RequestIssuePR" name="Request to Issue Purchase Requisition (PR)">
      <semantic:incoming>flow_determineNeeds_requestIssuePR</semantic:incoming>
      <semantic:outgoing>flow_requestIssuePR_gwPRApprovalToIssue</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_Requestor_PRApprovalToIssue" name="PR Issuance Approved?">
      <semantic:incoming>flow_requestIssuePR_gwPRApprovalToIssue</semantic:incoming>
      <semantic:outgoing>flow_gwPRApprovalToIssue_issuePR</semantic:outgoing>
      <semantic:outgoing>flow_gwPRApprovalToIssue_determineNeeds</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_Requestor_IssuePR" name="Issue Purchase Requisition (PR)">
      <semantic:incoming>flow_gwPRApprovalToIssue_issuePR</semantic:incoming>
      <semantic:outgoing>flow_issuePR_submitPRToHoD</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_Requestor_SubmitPRToHoD" name="Submit PR for Approval">
      <semantic:incoming>flow_issuePR_submitPRToHoD</semantic:incoming>
      <semantic:outgoing>flow_submitPRToHoD_gwHoDDecision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_Requestor_HoDDecisionOnPR" name="PR Approved by HoD?">
      <semantic:incoming>flow_submitPRToHoD_gwHoDDecision</semantic:incoming>
