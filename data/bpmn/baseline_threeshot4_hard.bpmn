      <semantic:outgoing>flow_dispatchPO_gwVendorDecision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_Requestor_VendorDecisionOnPO" name="Vendor Approved Dispatched PO?">
