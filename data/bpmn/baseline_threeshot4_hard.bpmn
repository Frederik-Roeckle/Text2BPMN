    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_Requestor_GoodsOrService" name="Goods or Service Received?">
      <semantic:incoming>flow_receiveGoodsServices_gwGoodsOrService</semantic:incoming>
      <semantic:outgoing>flow_gwGoodsOrService_issueGR</semantic:outgoing>
      <semantic:outgoing>flow_gwGoodsOrService_issueSES</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_Requestor_IssueGR" name="Issue Goods Receipt (GR)">
      <semantic:incoming>flow_gwGoodsOrService_issueGR</semantic:incoming>
      <semantic:outgoing>flow_issueGR_mergeAfterGRSES</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_Requestor_IssueSES" name="Issue Service Entry Sheet (SES)">
      <semantic:incoming>flow_gwGoodsOrService_issueSES</semantic:incoming>
      <semantic:outgoing>flow_issueSES_mergeAfterGRSES</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_Requestor_MergeAfterGRSES" name="GR/SES Issued">
      <semantic:incoming>flow_issueGR_mergeAfterGRSES</semantic:incoming>
