      <semantic:outgoing>flow_receiveCorrectedInvoice_perform3WayMatch</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_Requestor_PostInvoiceSAP" name="Post Invoice in SAP">
      <semantic:incoming>flow_gwInvoiceCorrect_postInvoiceSAP</semantic:incoming>
      <semantic:outgoing>flow_postInvoiceSAP_sendInvoiceToAP</semantic:outgoing>
    </semantic:task>
