      <semantic:outgoing>flow_sendInvoiceToAP_endRequestor</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="endEvent_Requestor_HandedToAP" name="Invoice Processing Handed to AP">
      <semantic:incoming>flow_sendInvoiceToAP_endRequestor</semantic:incoming>
    </semantic:endEvent>
    <!-- Heads of Departments Lane Elements -->
    <semantic:intermediateCatchEvent id="intermediateCatchEvent_HoD_PRReceived" name="PR Approval Request Received">
      <semantic:incoming>flow_msg_RequestorSubmitPR_HoDReceive</semantic:incoming>
      <semantic:outgoing>flow_HoDPRReceived_reviewPR</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="message_PRApprovalRequest"/>
    </semantic:intermediateCatchEvent>
    <semantic:task id="task_HoD_ReviewPR" name="Review PR (Evaluate Needs, Verify Budget, Validate Form)">
      <semantic:incoming>flow_HoDPRReceived_reviewPR</semantic:incoming>
      <semantic:outgoing>flow_HoDReviewPR_gwOutcome</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_HoD_PRReviewOutcome" name="PR Review Outcome?">
      <semantic:incoming>flow_HoDReviewPR_gwOutcome</semantic:incoming>
