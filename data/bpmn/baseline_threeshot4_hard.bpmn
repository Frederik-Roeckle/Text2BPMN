      <semantic:outgoing>flow_gwHoDDecision_determineNeeds</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_Requestor_CreatePO" name="Create Purchase Order (PO)">
      <semantic:incoming>flow_gwHoDDecision_createPO</semantic:incoming>
      <semantic:outgoing>flow_createPO_submitPOToApprovers</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_Requestor_SubmitPOToApprovers" name="Submit PO for Approval">
      <semantic:incoming>flow_createPO_submitPOToApprovers</semantic:incoming>
      <semantic:outgoing>flow_submitPOToApprovers_gwPOApproverDecision</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_Requestor_POApproverDecisionOnPO" name="PO Approved by PO Approvers?">
