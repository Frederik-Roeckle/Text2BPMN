    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_HoD_MergeDecision" name="Decision Documented">
      <semantic:incoming>flow_HoDDocApproval_merge</semantic:incoming>
      <semantic:incoming>flow_HoDDocDecline_merge</semantic:incoming>
      <semantic:incoming>flow_HoDDocReject_merge</semantic:incoming>
      <semantic:outgoing>flow_HoDMerge_submitDecision</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_HoD_SubmitDecision" name="Submit Final PR Decision to Requestor">
      <semantic:incoming>flow_HoDMerge_submitDecision</semantic:incoming>
      <semantic:outgoing>flow_HoDSubmitDecision_endHoD</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="endEvent_HoD_DecisionSent" name="PR Decision Submitted">
      <semantic:incoming>flow_HoDSubmitDecision_endHoD</semantic:incoming>
    </semantic:endEvent>
    <!-- PO Approvers Lane Elements -->
    <semantic:intermediateCatchEvent id="intermediateCatchEvent_POApprover_POReceived" name="PO Approval Request Received">
      <semantic:incoming>flow_msg_RequestorSubmitPO_POApproverReceive</semantic:incoming>
      <semantic:outgoing>flow_POApproverPOReceived_reviewPO</semantic:outgoing>
      <semantic:messageEventDefinition messageRef="message_POApprovalRequest"/>
