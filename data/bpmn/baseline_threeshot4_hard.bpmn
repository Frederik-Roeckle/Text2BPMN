      <semantic:outgoing>flow_mergeAfterGRSES_receiveInvoice</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_Requestor_ReceiveInvoice" name="Receive Invoice from Vendor">
