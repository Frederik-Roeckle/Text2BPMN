      <semantic:messageEventDefinition messageRef="message_PODispatched"/>
    </semantic:intermediateCatchEvent>
    <semantic:task id="task_Vendor_EvaluatePO" name="Evaluate PO">
      <semantic:incoming>flow_VendorPOReceived_evaluatePO</semantic:incoming>
      <semantic:outgoing>flow_VendorEvaluatePO_gwOutcome</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="exclusiveGateway_Vendor_POEvaluationOutcome" name="PO Evaluation Outcome?">
      <semantic:incoming>flow_VendorEvaluatePO_gwOutcome</semantic:incoming>
      <semantic:outgoing>flow_gwVendorOutcome_docApproval</semantic:outgoing>
      <semantic:outgoing>flow_gwVendorOutcome_docNegotiation</semantic:outgoing>
      <semantic:outgoing>flow_gwVendorOutcome_docRejection</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_Vendor_DocPOApproval" name="Document PO Approval (Vendor)">
      <semantic:incoming>flow_gwVendorOutcome_docApproval</semantic:incoming>
