        <semantic:flowNodeRef>exclusiveGateway_Requestor_PRApprovalToIssue</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_IssuePR</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_SubmitPRToHoD</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_Requestor_HoDDecisionOnPR</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_CreatePO</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_SubmitPOToApprovers</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_Requestor_POApproverDecisionOnPO</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_DispatchPOToVendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_Requestor_VendorDecisionOnPO</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_SearchNewVendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_ReceiveGoodsServices</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_Requestor_GoodsOrService</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_IssueGR</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_IssueSES</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_Requestor_MergeAfterGRSES</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_ReceiveInvoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_Perform3WayMatch</semantic:flowNodeRef>
        <semantic:flowNodeRef>exclusiveGateway_Requestor_InvoiceCorrect</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_ReturnInvoiceToVendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_ReceiveCorrectedInvoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_PostInvoiceSAP</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Requestor_SendInvoiceToAP</semantic:flowNodeRef>
        <semantic:flowNodeRef>endEvent_Requestor_HandedToAP</semantic:flowNodeRef>
