<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="P2PProcessDefs"
    targetNamespace="http://example.com/bpmn/p2p-process"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:collaboration id="collab_P2P">
    <semantic:participant id="participant_PurchasingDept" name="Purchasing Department" processRef="process_Purchasing"/>
    <semantic:participant id="participant_Vendor" name="Vendor" processRef="process_Vendor"/>
    <semantic:participant id="participant_APTeam" name="Accounts Payable (AP) Team" processRef="process_AP"/>
  </semantic:collaboration>

  <!-- Process for Purchasing Department -->
  <semantic:process id="process_Purchasing" isExecutable="true">
    <semantic:laneSet id="laneSet_Purchasing">
      <semantic:lane id="lane_Requestor" name="Requestor">
        <semantic:flowNodeRef>start_P2PProcess</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_DefineRequirements</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_CreatePR</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SendPRToHoD</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ReceiveHoDDecisionOnPR</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_IsPRApprovedByHoD</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_CreatePO</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SendPOToApprovers</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ReceivePODecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_IsPOApprovedByApprovers</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_DispatchPOToVendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ReceiveVendorDecisionOnPO</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_VendorPODecisionOutcome</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SearchNewVendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ReceiveGoodsServices</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_GoodsOrService</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_IssueGR</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_IssueSES</semantic:flowNodeRef>
        <semantic:flowNodeRef>merge_GRSES</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ConfirmReceipt</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ReceiveInvoiceFromVendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_Perform3WayMatch</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_IsInvoiceCorrect</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ReturnWrongInvoiceToVendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ReceiveCorrectedInvoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_PostInvoiceSAP</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_SendInvoiceToAP</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_RequestorTasksComplete</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_HoD" name="Heads of Departments">
        <semantic:flowNodeRef>task_HoDReceivePR</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_HoDReviewPR</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_HoDPRDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_HoDNotifyApproved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_HoDNotifyDeclined</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_HoDNotifyRejectedIncomplete</semantic:flowNodeRef>
        <semantic:flowNodeRef>merge_HoDNotification</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_HoDSendDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_HoDReviewComplete</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_POApprovers" name="PO Approvers">
        <semantic:flowNodeRef>task_POAReceivePO</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_POAReviewPO</semantic:flowNodeRef>
        <semantic:flowNodeRef>gateway_POADecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_POANotifyApproved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_POANotifyRejected</semantic:flowNodeRef>
        <semantic:flowNodeRef>merge_POANotification</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_POASendDecision</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_POApprovalComplete</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <!-- Requestor Lane Elements -->
    <semantic:startEvent id="start_P2PProcess" name="P2P Process Initiated">
      <semantic:outgoing>flow_start_to_definereq</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_DefineRequirements" name="Determine Business Requirements">
      <semantic:incoming>flow_start_to_definereq</semantic:incoming>
      <semantic:incoming>flow_pr_not_approved_to_definereq</semantic:incoming>
      <semantic:incoming>flow_po_not_approved_to_definereq</semantic:incoming>
      <semantic:outgoing>flow_definereq_to_createpr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_CreatePR" name="Create Purchase Requisition">
      <semantic:incoming>flow_definereq_to_createpr</semantic:incoming>
      <semantic:outgoing>flow_createpr_to_sendpr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_SendPRToHoD" name="Send PR to HoD">
      <semantic:incoming>flow_createpr_to_sendpr</semantic:incoming>
      <semantic:outgoing>flow_sendpr_to_receivehoddecision</semantic:outgoing> <!-- Placeholder, actual link is message flow -->
    </semantic:task>
    <semantic:task id="task_ReceiveHoDDecisionOnPR" name="Receive HoD Decision on PR">
      <semantic:incoming>flow_sendpr_to_receivehoddecision</semantic:incoming> <!-- Placeholder, actual link is message flow -->
      <semantic:outgoing>flow_receivehoddecision_to_gatewaypr</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_IsPRApprovedByHoD" name="PR Approved by HoD?" isMarkerVisible="true">
      <semantic:incoming>flow_receivehoddecision_to_gatewaypr</semantic:incoming>
      <semantic:outgoing>flow_pr_approved_yes</semantic:outgoing>
      <semantic:outgoing>flow_pr_not_approved_to_definereq</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_CreatePO" name="Create Purchase Order">
      <semantic:incoming>flow_pr_approved_yes</semantic:incoming>
      <semantic:outgoing>flow_createpo_to_sendpo</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_SendPOToApprovers" name="Send PO to PO Approvers">
      <semantic:incoming>flow_createpo_to_sendpo</semantic:incoming>
      <semantic:outgoing>flow_sendpo_to_receivepodecision</semantic:outgoing> <!-- Placeholder -->
    </semantic:task>
    <semantic:task id="task_ReceivePODecision" name="Receive PO Decision">
      <semantic:incoming>flow_sendpo_to_receivepodecision</semantic:incoming> <!-- Placeholder -->
      <semantic:outgoing>flow_receivepodecision_to_gatewaypo</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_IsPOApprovedByApprovers" name="PO Approved by Approvers?" isMarkerVisible="true">
      <semantic:incoming>flow_receivepodecision_to_gatewaypo</semantic:incoming>
      <semantic:outgoing>flow_po_approved_yes</semantic:outgoing>
      <semantic:outgoing>flow_po_not_approved_to_definereq</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_DispatchPOToVendor" name="Dispatch PO to Vendor">
      <semantic:incoming>flow_po_approved_yes</semantic:incoming>
      <semantic:incoming>flow_searchvendor_to_dispatch</semantic:incoming>
      <semantic:outgoing>flow_dispatchpo_to_receivevendordecision</semantic:outgoing> <!-- Placeholder -->
    </semantic:task>
    <semantic:task id="task_ReceiveVendorDecisionOnPO" name="Receive Vendor Decision on PO">
      <semantic:incoming>flow_dispatchpo_to_receivevendordecision</semantic:incoming> <!-- Placeholder -->
      <semantic:outgoing>flow_receivevendordecision_to_gatewayvendor</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_VendorPODecisionOutcome" name="Vendor Approved/Negotiated PO?" isMarkerVisible="true">
      <semantic:incoming>flow_receivevendordecision_to_gatewayvendor</semantic:incoming>
      <semantic:outgoing>flow_vendor_approved_yes</semantic:outgoing>
      <semantic:outgoing>flow_vendor_rejected_no</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_SearchNewVendor" name="Search for New Vendor">
      <semantic:incoming>flow_vendor_rejected_no</semantic:incoming>
      <semantic:outgoing>flow_searchvendor_to_dispatch</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ReceiveGoodsServices" name="Receive Agreed Goods/Services">
      <semantic:incoming>flow_vendor_approved_yes</semantic:incoming> <!-- Placeholder for message from Vendor -->
      <semantic:outgoing>flow_receivegoods_to_goodsservicegateway</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_GoodsOrService" name="Goods or Service Received?" isMarkerVisible="true">
      <semantic:incoming>flow_receivegoods_to_goodsservicegateway</semantic:incoming>
      <semantic:outgoing>flow_goodsservice_goods</semantic:outgoing>
      <semantic:outgoing>flow_goodsservice_service</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_IssueGR" name="Issue Goods Receipt (GR)">
      <semantic:incoming>flow_goodsservice_goods</semantic:incoming>
      <semantic:outgoing>flow_issuegr_to_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_IssueSES" name="Issue Service Entry Sheet (SES)">
      <semantic:incoming>flow_goodsservice_service</semantic:incoming>
      <semantic:outgoing>flow_issueses_to_merge</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="merge_GRSES" isMarkerVisible="true">
        <semantic:incoming>flow_issuegr_to_merge</semantic:incoming>
        <semantic:incoming>flow_issueses_to_merge</semantic:incoming>
        <semantic:outgoing>flow_merge_to_confirmreceipt</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_ConfirmReceipt" name="Confirm Receipt of Goods/Services">
      <semantic:incoming>flow_merge_to_confirmreceipt</semantic:incoming>
      <semantic:outgoing>flow_confirmreceipt_to_receiveinvoice</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ReceiveInvoiceFromVendor" name="Receive Invoice from Vendor">
      <semantic:incoming>flow_confirmreceipt_to_receiveinvoice</semantic:incoming> <!-- Placeholder for message from Vendor -->
      <semantic:outgoing>flow_receiveinvoice_to_3waymatch</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_Perform3WayMatch" name="Perform Three-Way Match">
      <semantic:incoming>flow_receiveinvoice_to_3waymatch</semantic:incoming>
      <semantic:incoming>flow_receivecorrectedinvoice_to_3waymatch</semantic:incoming>
      <semantic:outgoing>flow_3waymatch_to_invoicecorrectgateway</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_IsInvoiceCorrect" name="Invoice Correct?" isMarkerVisible="true">
      <semantic:incoming>flow_3waymatch_to_invoicecorrectgateway</semantic:incoming>
      <semantic:outgoing>flow_invoicecorrect_yes</semantic:outgoing>
      <semantic:outgoing>flow_invoicecorrect_no</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_ReturnWrongInvoiceToVendor" name="Return Wrong Invoice to Vendor">
      <semantic:incoming>flow_invoicecorrect_no</semantic:incoming>
      <semantic:outgoing>flow_returninvoice_to_receivecorrected</semantic:outgoing> <!-- Placeholder -->
    </semantic:task>
    <semantic:task id="task_ReceiveCorrectedInvoice" name="Receive Corrected Invoice">
      <semantic:incoming>flow_returninvoice_to_receivecorrected</semantic:incoming> <!-- Placeholder -->
      <semantic:outgoing>flow_receivecorrectedinvoice_to_3waymatch</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_PostInvoiceSAP" name="Post Invoice in SAP">
      <semantic:incoming>flow_invoicecorrect_yes</semantic:incoming>
      <semantic:outgoing>flow_postinvoice_to_sendtoap</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_SendInvoiceToAP" name="Send Invoice to AP Team">
      <semantic:incoming>flow_postinvoice_to_sendtoap</semantic:incoming>
      <semantic:outgoing>flow_sendtoap_to_endrequestor</semantic:outgoing> <!-- Placeholder -->
    </semantic:task>
    <semantic:endEvent id="end_RequestorTasksComplete" name="Requestor P2P Tasks Complete">
      <semantic:incoming>flow_sendtoap_to_endrequestor</semantic:incoming>
    </semantic:endEvent>

    <!-- HoD Lane Elements -->
    <semantic:task id="task_HoDReceivePR" name="Receive PR from Requestor">
      <semantic:outgoing>flow_hodreceive_to_hodreview</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_HoDReviewPR" name="Review PR (Needs, Budget, Form)">
      <semantic:incoming>flow_hodreceive_to_hodreview</semantic:incoming>
      <semantic:outgoing>flow_hodreview_to_hoddecisiongateway</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_HoDPRDecision" name="HoD PR Decision?" isMarkerVisible="true">
      <semantic:incoming>flow_hodreview_to_hoddecisiongateway</semantic:incoming>
      <semantic:outgoing>flow_hod_approved</semantic:outgoing>
      <semantic:outgoing>flow_hod_declined</semantic:outgoing>
      <semantic:outgoing>flow_hod_rejected_incomplete</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_HoDNotifyApproved" name="Prepare PR Approval Notification">
        <semantic:incoming>flow_hod_approved</semantic:incoming>
        <semantic:outgoing>flow_hodnotifyapproved_to_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_HoDNotifyDeclined" name="Prepare PR Decline Notification">
        <semantic:incoming>flow_hod_declined</semantic:incoming>
        <semantic:outgoing>flow_hodnotifydeclined_to_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_HoDNotifyRejectedIncomplete" name="Prepare PR Rejection Notification">
        <semantic:incoming>flow_hod_rejected_incomplete</semantic:incoming>
        <semantic:outgoing>flow_hodnotifyrejected_to_merge</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="merge_HoDNotification" isMarkerVisible="true">
        <semantic:incoming>flow_hodnotifyapproved_to_merge</semantic:incoming>
        <semantic:incoming>flow_hodnotifydeclined_to_merge</semantic:incoming>
        <semantic:incoming>flow_hodnotifyrejected_to_merge</semantic:incoming>
        <semantic:outgoing>flow_hodmerge_to_senddecision</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_HoDSendDecision" name="Send PR Decision to Requestor">
      <semantic:incoming>flow_hodmerge_to_senddecision</semantic:incoming>
      <semantic:outgoing>flow_hodsend_to_endhod</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="end_HoDReviewComplete" name="HoD PR Review Concluded">
      <semantic:incoming>flow_hodsend_to_endhod</semantic:incoming>
    </semantic:endEvent>

    <!-- PO Approvers Lane Elements -->
    <semantic:task id="task_POAReceivePO" name="Receive PO from Requestor">
      <semantic:outgoing>flow_poareceive_to_poareview</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_POAReviewPO" name="Review PO Specifications">
      <semantic:incoming>flow_poareceive_to_poareview</semantic:incoming>
      <semantic:outgoing>flow_poareview_to_poadecisiongateway</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gateway_POADecision" name="PO Approved?" isMarkerVisible="true">
      <semantic:incoming>flow_poareview_to_poadecisiongateway</semantic:incoming>
      <semantic:outgoing>flow_poa_approved</semantic:outgoing>
      <semantic:outgoing>flow_poa_rejected</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_POANotifyApproved" name="Prepare PO Approval Notification">
        <semantic:incoming>flow_poa_approved</semantic:incoming>
        <semantic:outgoing>flow_poanotifyapproved_to_merge</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_POANotifyRejected" name="Prepare PO Rejection Notification">
        <semantic:incoming>flow_poa_rejected</semantic:incoming>
        <semantic:outgoing>flow_poanotifyrejected_to_merge</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="merge_POANotification" isMarkerVisible="true">
        <semantic:incoming>flow_poanotifyapproved_to_merge</semantic:incoming>
        <semantic:incoming>flow_poanotifyrejected_to_merge</semantic:incoming>
        <semantic:outgoing>flow_poamerge_to_senddecision</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_POASendDecision" name="Send PO Decision to Requestor">
      <semantic:incoming>flow_poamerge_to_senddecision</semantic:incoming>
      <semantic:outgoing>flow_poasend_to_endpoa</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="end_POApprovalComplete" name="PO Approval Concluded">
      <semantic:incoming>flow_poasend_to_endpoa</semantic:incoming>
    </semantic:endEvent>

    <!-- Sequence Flows for Purchasing Process -->
    <semantic:sequenceFlow id="flow_start_to_definereq" sourceRef="start_P2PProcess" targetRef="task_DefineRequirements"/>
    <semantic:sequenceFlow id="flow_definereq_to_createpr" sourceRef="task_DefineRequirements" targetRef="task_CreatePR"/>
    <semantic:sequenceFlow id="flow_createpr_to_sendpr" sourceRef="task_CreatePR" targetRef="task_SendPRToHoD"/>
    <semantic:sequenceFlow id="flow_sendpr_to_receivehoddecision" sourceRef="task_SendPRToHoD" targetRef="task_ReceiveHoDDecisionOnPR"/>
    <semantic:sequenceFlow id="flow_receivehoddecision_to_gatewaypr" sourceRef="task_ReceiveHoDDecisionOnPR" targetRef="gateway_IsPRApprovedByHoD"/>
    <semantic:sequenceFlow id="flow_pr_approved_yes" name="Yes" sourceRef="gateway_IsPRApprovedByHoD" targetRef="task_CreatePO"/>
    <semantic:sequenceFlow id="flow_pr_not_approved_to_definereq" name="No/Rejected" sourceRef="gateway_IsPRApprovedByHoD" targetRef="task_DefineRequirements"/>
    <semantic:sequenceFlow id="flow_createpo_to_sendpo" sourceRef="task_CreatePO" targetRef="task_SendPOToApprovers"/>
    <semantic:sequenceFlow id="flow_sendpo_to_receivepodecision" sourceRef="task_SendPOToApprovers" targetRef="task_ReceivePODecision"/>
    <semantic:sequenceFlow id="flow_receivepodecision_to_gatewaypo" sourceRef="task_ReceivePODecision" targetRef="gateway_IsPOApprovedByApprovers"/>
    <semantic:sequenceFlow id="flow_po_approved_yes" name="Yes" sourceRef="gateway_IsPOApprovedByApprovers" targetRef="task_DispatchPOToVendor"/>
    <semantic:sequenceFlow id="flow_po_not_approved_to_definereq" name="No" sourceRef="gateway_IsPOApprovedByApprovers" targetRef="task_DefineRequirements"/>
    <semantic:sequenceFlow id="flow_dispatchpo_to_receivevendordecision" sourceRef="task_DispatchPOToVendor" targetRef="task_ReceiveVendorDecisionOnPO"/>
    <semantic:sequenceFlow id="flow_receivevendordecision_to_gatewayvendor" sourceRef="task_ReceiveVendorDecisionOnPO" targetRef="gateway_VendorPODecisionOutcome"/>
    <semantic:sequenceFlow id="flow_vendor_approved_yes" name="Approved/Negotiated" sourceRef="gateway_VendorPODecisionOutcome" targetRef="task_ReceiveGoodsServices"/>
    <semantic:sequenceFlow id="flow_vendor_rejected_no" name="Rejected" sourceRef="gateway_VendorPODecisionOutcome" targetRef="task_SearchNewVendor"/>
    <semantic:sequenceFlow id="flow_searchvendor_to_dispatch" sourceRef="task_SearchNewVendor" targetRef="task_DispatchPOToVendor"/>
    <semantic:sequenceFlow id="flow_receivegoods_to_goodsservicegateway" sourceRef="task_ReceiveGoodsServices" targetRef="gateway_GoodsOrService"/>
    <semantic:sequenceFlow id="flow_goodsservice_goods" name="Goods" sourceRef="gateway_GoodsOrService" targetRef="task_IssueGR"/>
    <semantic:sequenceFlow id="flow_goodsservice_service" name="Service" sourceRef="gateway_GoodsOrService" targetRef="task_IssueSES"/>
    <semantic:sequenceFlow id="flow_issuegr_to_merge" sourceRef="task_IssueGR" targetRef="merge_GRSES"/>
    <semantic:sequenceFlow id="flow_issueses_to_merge" sourceRef="task_IssueSES" targetRef="merge_GRSES"/>
    <semantic:sequenceFlow id="flow_merge_to_confirmreceipt" sourceRef="merge_GRSES" targetRef="task_ConfirmReceipt"/>
    <semantic:sequenceFlow id="flow_confirmreceipt_to_receiveinvoice" sourceRef="task_ConfirmReceipt" targetRef="task_ReceiveInvoiceFromVendor"/>
    <semantic:sequenceFlow id="flow_receiveinvoice_to_3waymatch" sourceRef="task_ReceiveInvoiceFromVendor" targetRef="task_Perform3WayMatch"/>
    <semantic:sequenceFlow id="flow_3waymatch_to_invoicecorrectgateway" sourceRef="task_Perform3WayMatch" targetRef="gateway_IsInvoiceCorrect"/>
    <semantic:sequenceFlow id="flow_invoicecorrect_yes" name="Yes" sourceRef="gateway_IsInvoiceCorrect" targetRef="task_PostInvoiceSAP"/>
    <semantic:sequenceFlow id="flow_invoicecorrect_no" name="No (Wrong)" sourceRef="gateway_IsInvoiceCorrect" targetRef="task_ReturnWrongInvoiceToVendor"/>
    <semantic:sequenceFlow id="flow_returninvoice_to_receivecorrected" sourceRef="task_ReturnWrongInvoiceToVendor" targetRef="task_ReceiveCorrectedInvoice"/>
    <semantic:sequenceFlow id="flow_receivecorrectedinvoice_to_3waymatch" sourceRef="task_ReceiveCorrectedInvoice" targetRef="task_Perform3WayMatch"/>
    <semantic:sequenceFlow id="flow_postinvoice_to_sendtoap" sourceRef="task_PostInvoiceSAP" targetRef="task_SendInvoiceToAP"/>
    <semantic:sequenceFlow id="flow_sendtoap_to_endrequestor" sourceRef="task_SendInvoiceToAP" targetRef="end_RequestorTasksComplete"/>

    <semantic:sequenceFlow id="flow_hodreceive_to_hodreview" sourceRef="task_HoDReceivePR" targetRef="task_HoDReviewPR"/>
    <semantic:sequenceFlow id="flow_hodreview_to_hoddecisiongateway" sourceRef="task_HoDReviewPR" targetRef="gateway_HoDPRDecision"/>
    <semantic:sequenceFlow id="flow_hod_approved" name="Approve" sourceRef="gateway_HoDPRDecision" targetRef="task_HoDNotifyApproved"/>
    <semantic:sequenceFlow id="flow_hod_declined" name="Decline" sourceRef="gateway_HoDPRDecision" targetRef="task_HoDNotifyDeclined"/>
    <semantic:sequenceFlow id="flow_hod_rejected_incomplete" name="Reject Incomplete" sourceRef="gateway_HoDPRDecision" targetRef="task_HoDNotifyRejectedIncomplete"/>
    <semantic:sequenceFlow id="flow_hodnotifyapproved_to_merge" sourceRef="task_HoDNotifyApproved" targetRef="merge_HoDNotification"/>
    <semantic:sequenceFlow id="flow_hodnotifydeclined_to_merge" sourceRef="task_HoDNotifyDeclined" targetRef="merge_HoDNotification"/>
    <semantic:sequenceFlow id="flow_hodnotifyrejected_to_merge" sourceRef="task_HoDNotifyRejectedIncomplete" targetRef="merge_HoDNotification"/>
    <semantic:sequenceFlow id="flow_hodmerge_to_senddecision" sourceRef="merge_HoDNotification" targetRef="task_HoDSendDecision"/>
    <semantic:sequenceFlow id="flow_hodsend_to_endhod" sourceRef="task_HoDSendDecision" targetRef="end_HoDReviewComplete"/>

    <semantic:sequenceFlow id="flow_poareceive_to_poareview" sourceRef="task_POAReceivePO" targetRef="task_POAReviewPO"/>
    <semantic:sequenceFlow id="flow_poareview_to_poadecisiongateway" sourceRef="task_POAReviewPO" targetRef="gateway_POADecision"/>
    <semantic:sequenceFlow id="flow_poa_approved" name="Approve" sourceRef="gateway_POADecision" targetRef="task_POANotifyApproved"/>
