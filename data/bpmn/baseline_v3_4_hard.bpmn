```xml
<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_P2P" targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:collaboration id="Collaboration_P2P">
    <bpmn:participant id="Participant_P2P_Process" name="Purchase to Pay Process" processRef="Process_P2P" />
  </bpmn:collaboration>
  <bpmn:process id="Process_P2P" isExecutable="true">
    <bpmn:laneSet id="LaneSet_P2P">
      <bpmn:lane id="Lane_Requestor" name="Requestor">
        <bpmn:flowNodeRef>StartEvent_P2PInitiated</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_DetermineNeeds</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_RequestIssuePR</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_PRIssuanceApproved</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_IssuePR</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Boundary_PRDisc</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CorrectPRDiscrepancy</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SendPRForApproval</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_ReceivePRDecision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Req_PRHoDApproved</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_CreatePO</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_SendPOApproval</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_ReceivePODecision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Req_POAppApproved</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_DispatchPO</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_ReceiveVendorDecision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Req_VendorApproved</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_SearchVendor</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_ReceiveGoods</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Req_GoodService</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_IssueGR</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_IssueSES</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Req_ConvergeGRSES</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_ReceiveInvoice</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_3WayMatch</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Req_InvCorrect</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_IdentifyInvDisc</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_ReturnInvoice</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_ReceiveCorrInv</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_PostInvSAP</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Req_SendInvAP</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_HoD" name="Heads of Departments">
        <bpmn:flowNodeRef>Task_HoD_ReceivePR</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_HoD_ReviewPR</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Boundary_Budget</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_HandleBudget</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Boundary_Data</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_HandleData</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_HoD_Outcome</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_HoD_DocApprove</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_HoD_DocDecline</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_HoD_DocReject</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_HoD_SubmitDecision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_HoD</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_POApprovers" name="PO Approvers">
        <bpmn:flowNodeRef>Task_POApp_ReceivePO</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_POApp_ReviewPO</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_POApp_Outcome</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_POApp_DocApprove</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_POApp_DocReject</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_POApp_SubmitDecision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_POApp</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Vendor" name="Vendor">
        <bpmn:flowNodeRef>Task_Vendor_ReceivePO</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Vendor_EvalPO</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Vendor_Outcome</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Vendor_DocApprove</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Vendor_DocNegotiate</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Vendor_DocReject</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Vendor_SendDecision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Vendor_Reject</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Vendor_Deliver</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Vendor_SendInv</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Vendor_ReceiveWrongInv</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Vendor_CorrectInv</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Vendor_SendCorrectedInv</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Vendor_InvCorr</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_APTeam" name="Accounts Payable Team">
        <bpmn:flowNodeRef>Task_AP_ReceiveInv</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AP_DecidePayment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_AP_PaymentMethod</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AP_PayFull</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AP_PayInstallments</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_AP_ConvergePayment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Main</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>

    <!-- Requestor Lane Elements -->
    <bpmn:startEvent id="StartEvent_P2PInitiated" name="P2P Process Initiated">
      <bpmn:outgoing>Flow_Start_To_DetNeeds</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="Task_DetermineNeeds" name="Determine Business Requirements &amp; Needs">
      <bpmn:incoming>Flow_Start_To_DetNeeds</bpmn:incoming>
      <bpmn:incoming>Flow_GatewayPRAppr_No_To_DetNeeds</bpmn:incoming>
      <bpmn:incoming>Flow_GatewayReqPRHoD_No_To_DetNeeds</bpmn:incoming>
      <bpmn:incoming>Flow_GatewayReqPOApp_No_To_DetNeeds</bpmn:incoming>
      <bpmn:outgoing>Flow_DetNeeds_To_ReqIssuePR</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_RequestIssuePR" name="Request to Issue Purchase Requisition">
      <bpmn:incoming>Flow_DetNeeds_To_ReqIssuePR</bpmn:incoming>
      <bpmn:outgoing>Flow_ReqIssuePR_To_GatewayPRAppr</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_PRIssuanceApproved" name="PR Issuance Approved?">
      <bpmn:incoming>Flow_ReqIssuePR_To_GatewayPRAppr</bpmn:incoming>
      <bpmn:outgoing>Flow_GatewayPRAppr_Yes_To_IssuePR</bpmn:outgoing>
      <bpmn:outgoing>Flow_GatewayPRAppr_No_To_DetNeeds</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_IssuePR" name="Issue Purchase Requisition (PR)">
      <bpmn:incoming>Flow_GatewayPRAppr_Yes_To_IssuePR</bpmn:incoming>
      <bpmn:incoming>Flow_CorrectPRDisc_To_IssuePR</bpmn:incoming>
      <bpmn:outgoing>Flow_IssuePR_To_SendPRApproval</bpmn:outgoing>
    </bpmn:task>
    <bpmn:boundaryEvent id="Boundary_PRDisc" name="PR Discrepancy Occurred" attachedToRef="Task_IssuePR">
      <bpmn:outgoing>Flow_BoundaryPRDisc_To_CorrectPRDisc</bpmn:outgoing>
      <bpmn:errorEventDefinition id="ErrorEvDef_PRDisc" errorRef="Error_PR_Discrepancy" />
    </bpmn:boundaryEvent>
    <bpmn:task id="Task_CorrectPRDiscrepancy" name="Correct PR Discrepancy">
      <bpmn:incoming>Flow_BoundaryPRDisc_To_CorrectPRDisc</bpmn:incoming>
      <bpmn:outgoing>Flow_CorrectPRDisc_To_IssuePR</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_SendPRForApproval" name="Send PR for Approval">
      <bpmn:incoming>Flow_IssuePR_To_SendPRApproval</bpmn:incoming>
      <bpmn:outgoing>Flow_SendPRApproval_To_HoDReceivePR</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_Req_ReceivePRDecision" name="Receive PR Decision from HoD">
      <bpmn:incoming>Flow_HoDSubmitDecision_To_ReqReceivePRDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_ReqReceivePRDecision_To_GatewayReqPRHoD</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_Req_PRHoDApproved" name="PR Approved by HoD?">
      <bpmn:incoming>Flow_ReqReceivePRDecision_To_GatewayReqPRHoD</bpmn:incoming>
      <bpmn:outgoing>Flow_GatewayReqPRHoD_Yes_To_CreatePO</bpmn:outgoing>
      <bpmn:outgoing>Flow_GatewayReqPRHoD_No_To_DetNeeds</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_Req_CreatePO" name="Create Purchase Order (PO)">
      <bpmn:incoming>Flow_GatewayReqPRHoD_Yes_To_CreatePO</bpmn:incoming>
      <bpmn:outgoing>Flow_CreatePO_To_SendPOApproval</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_Req_SendPOApproval" name="Send PO for Approval">
      <bpmn:incoming>Flow_CreatePO_To_SendPOApproval</bpmn:incoming>
      <bpmn:outgoing>Flow_SendPOApproval_To_POAppReceivePO</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_Req_ReceivePODecision" name="Receive PO Decision from PO Approvers">
      <bpmn:incoming>Flow_POAppSubmitDecision_To_ReqReceivePODecision</bpmn:incoming>
      <bpmn:outgoing>Flow_ReqReceivePODecision_To_GatewayReqPOApp</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_Req_POAppApproved" name="PO Approved by PO Approvers?">
      <bpmn:incoming>Flow_ReqReceivePODecision_To_GatewayReqPOApp</bpmn:incoming>
      <bpmn:outgoing>Flow_GatewayReqPOApp_Yes_To_DispatchPO</bpmn:outgoing>
      <bpmn:outgoing>Flow_GatewayReqPOApp_No_To_DetNeeds</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_Req_DispatchPO" name="Dispatch Approved PO to Vendor">
      <bpmn:incoming>Flow_GatewayReqPOApp_Yes_To_DispatchPO</bpmn:incoming>
      <bpmn:incoming>Flow_SearchVendor_To_DispatchPO</bpmn:incoming>
      <bpmn:outgoing>Flow_DispatchPO_To_VendorReceivePO</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_Req_ReceiveVendorDecision" name="Receive Vendor PO Decision">
      <bpmn:incoming>Flow_VendorSendDecision_To_ReqReceiveVendorDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_ReqReceiveVendorDecision_To_GatewayVendorApproved</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_Req_VendorApproved" name="Vendor Approved/Negotiated PO?">
      <bpmn:incoming>Flow_ReqReceiveVendorDecision_To_GatewayVendorApproved</bpmn:incoming>
      <bpmn:outgoing>Flow_GatewayVendorApproved_Yes_To_ReceiveGoods</bpmn:outgoing>
      <bpmn:outgoing>Flow_GatewayVendorApproved_No_To_SearchVendor</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_Req_SearchVendor" name="Search for New Vendor">
      <bpmn:incoming>Flow_GatewayVendorApproved_No_To_SearchVendor</bpmn:incoming>
      <bpmn:outgoing>Flow_SearchVendor_To_DispatchPO</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_Req_ReceiveGoods" name="Receive Agreed Goods/Services">
      <bpmn:incoming>Flow_GatewayVendorApproved_Yes_To_ReceiveGoods</bpmn:incoming>
      <bpmn:incoming>Flow_VendorDeliver_To_ReqReceiveGoods</bpmn:incoming> <!-- Added this connection -->
      <bpmn:outgoing>Flow_ReceiveGoods_To_GatewayGoodService</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_Req_GoodService" name="Goods or Service Received?">
      <bpmn:incoming>Flow_ReceiveGoods_To_GatewayGoodService</bpmn:incoming>
      <bpmn:outgoing>Flow_GatewayGoodService_Goods_To_IssueGR</bpmn:outgoing>
      <bpmn:outgoing>Flow_GatewayGoodService_Service_To_IssueSES</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_Req_IssueGR" name="Issue Goods Receipt (GR)">
      <bpmn:incoming>Flow_GatewayGoodService_Goods_To_IssueGR</bpmn:incoming>
      <bpmn:outgoing>Flow_IssueGR_To_ConvergeGRSES</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_Req_IssueSES" name="Issue Service Entry Sheet (SES)">
      <bpmn:incoming>Flow_GatewayGoodService_Service_To_IssueSES</bpmn:incoming>
      <bpmn:outgoing>Flow_IssueSES_To_ConvergeGRSES</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_Req_ConvergeGRSES" name="GR/SES Issued">
      <bpmn:incoming>Flow_IssueGR_To_ConvergeGRSES</bpmn:incoming>
      <bpmn:incoming>Flow_IssueSES_To_ConvergeGRSES</bpmn:incoming>
      <bpmn:outgoing>Flow_ConvergeGRSES_To_ReceiveInvoice</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_Req_ReceiveInvoice" name="Receive Invoice from Vendor">
      <bpmn:incoming>Flow_ConvergeGRSES_To_ReceiveInvoice</bpmn:incoming>
      <bpmn:incoming>Flow_VendorSendInv_To_ReqReceiveInvoice</bpmn:incoming>
      <bpmn:outgoing>Flow_ReceiveInvoice_To_3WayMatch</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_Req_3WayMatch" name="Perform Three-Way Match">
      <bpmn:incoming>Flow_ReceiveInvoice_To_3WayMatch</bpmn:incoming>
      <bpmn:incoming>Flow_ReqReceiveCorrInv_To_3WayMatch</bpmn:incoming>
      <bpmn:outgoing>Flow_3WayMatch_To_GatewayInvCorrect</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_Req_InvCorrect" name="Invoice Correct?">
      <bpmn:incoming>Flow_3WayMatch_To_GatewayInvCorrect</bpmn:incoming>
      <bpmn:outgoing>Flow_GatewayInvCorrect_Yes_To_PostInvSAP</bpmn:outgoing>
      <bpmn:outgoing>Flow_GatewayInvCorrect_No_To_IdentifyInvDisc</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_Req_IdentifyInvDisc" name="Identify Invoice Discrepancy Details">
      <bpmn:incoming>Flow_GatewayInvCorrect_No_To_IdentifyInvDisc</bpmn:incoming>
      <bpmn:outgoing>Flow_IdentifyInvDisc_To_ReturnInvoice</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_Req_ReturnInvoice" name="Return Wrong Invoice to Vendor">
      <bpmn:incoming>Flow_IdentifyInvDisc_To_ReturnInvoice</bpmn:incoming>
      <bpmn:outgoing>Flow_ReturnInvoice_To_VendorReceiveWrongInv</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_Req_ReceiveCorrInv" name="Receive Corrected Invoice from Vendor">
      <bpmn:incoming>Flow_VendorSendCorrectedInv_To_ReqReceiveCorrInv</bpmn:incoming>
      <bpmn:outgoing>Flow_ReqReceiveCorrInv_To_3WayMatch</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_Req_PostInvSAP" name="Post Invoice in SAP">
      <bpmn:incoming>Flow_GatewayInvCorrect_Yes_To_PostInvSAP</bpmn:incoming>
      <bpmn:outgoing>Flow_PostInvSAP_To_SendInvAP</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_Req_SendInvAP" name="Send Invoice to Accounts Payable (AP) Team">
      <bpmn:incoming>Flow_PostInvSAP_To_SendInvAP</bpmn:incoming>
      <bpmn:outgoing>Flow_SendInvAP_To_APReceiveInv</bpmn:outgoing>
    </bpmn:task>

    <!-- Heads of Departments Lane Elements -->
    <bpmn:task id="Task_HoD_ReceivePR" name="Receive PR Approval Request">
      <bpmn:incoming>Flow_SendPRApproval_To_HoDReceivePR</bpmn:incoming>
      <bpmn:outgoing>Flow_HoDReceivePR_To_HoDReviewPR</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_HoD_ReviewPR" name="Review PR (Needs, Budget, Form)">
      <bpmn:incoming>Flow_HoDReceivePR_To_HoDReviewPR</bpmn:incoming>
      <bpmn:outgoing>Flow_HoDReviewPR_To_GatewayHoDOutcome</bpmn:outgoing>
    </bpmn:task>
    <bpmn:boundaryEvent id="Boundary_Budget" name="Budget Overdraft Occurred" attachedToRef="Task_HoD_ReviewPR">
      <bpmn:outgoing>Flow_BoundaryBudget_To_HandleBudget</bpmn:outgoing>
      <bpmn:errorEventDefinition id="ErrorEvDef_Budget" errorRef="Error_Budget_Overdraft"/>
    </bpmn:boundaryEvent>
    <bpmn:task id="Task_HandleBudget" name="Handle Budget Overdraft">
      <bpmn:incoming>Flow_BoundaryBudget_To_HandleBudget</bpmn:incoming>
      <bpmn:outgoing>Flow_HandleBudget_To_HoDDocDecline</bpmn:outgoing>
    </bpmn:task>
    <bpmn:boundaryEvent id="Boundary_Data" name="Data Breach Occurred" attachedToRef="Task_HoD_ReviewPR">
      <bpmn:outgoing>Flow_BoundaryData_To_HandleData</bpmn:outgoing>
      <bpmn:errorEventDefinition id="ErrorEvDef_Data" errorRef="Error_Data_Breach"/>
    </bpmn:boundaryEvent>
    <bpmn:task id="Task_HandleData" name="Handle Data Breach">
      <bpmn:incoming>Flow_BoundaryData_To_HandleData</bpmn:incoming>
      <bpmn:outgoing>Flow_HandleData_To_HoDDocDecline</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_HoD_Outcome" name="PR Review Outcome?">
      <bpmn:incoming>Flow_HoDReviewPR_To_GatewayHoDOutcome</bpmn:incoming>
      <bpmn:outgoing>Flow_GatewayHoDOutcome_Approve_To_DocApprove</bpmn:outgoing>
      <bpmn:outgoing>Flow_GatewayHoDOutcome_Decline_To_DocDecline</bpmn:outgoing>
      <bpmn:outgoing>Flow_GatewayHoDOutcome_Reject_To_DocReject</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_HoD_DocApprove" name="Document Approved PR Decision">
      <bpmn:incoming>Flow_GatewayHoDOutcome_Approve_To_DocApprove</bpmn:incoming>
      <bpmn:outgoing>Flow_HoDDocApprove_To_SubmitDecision</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_HoD_DocDecline" name="Document Declined PR Decision">
      <bpmn:incoming>Flow_GatewayHoDOutcome_Decline_To_DocDecline</bpmn:incoming>
      <bpmn:incoming>Flow_HandleBudget_To_HoDDocDecline</bpmn:incoming>
      <bpmn:incoming>Flow_HandleData_To_HoDDocDecline</bpmn:incoming>
      <bpmn:outgoing>Flow_HoDDocDecline_To_SubmitDecision</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_HoD_DocReject" name="Document Rejected (Incomplete) PR Decision">
      <bpmn:incoming>Flow_GatewayHoDOutcome_Reject_To_DocReject</bpmn:incoming>
      <bpmn:outgoing>Flow_HoDDocReject_To_SubmitDecision</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_HoD_SubmitDecision" name="Submit Final PR Decision to Requestor">
      <bpmn:incoming>Flow_HoDDocApprove_To_SubmitDecision</bpmn:incoming>
      <bpmn:incoming>Flow_HoDDocDecline_To_SubmitDecision</bpmn:incoming>
      <bpmn:incoming>Flow_HoDDocReject_To_SubmitDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_HoDSubmitDecision_To_ReqReceivePRDecision</bpmn:outgoing>
      <bpmn:outgoing>Flow_HoDSubmitDecision_To_EndHoD</bpmn:outgoing>
    </bpmn:task>
    <bpmn:endEvent id="EndEvent_HoD" name="PR Approval Process Ended for HoD">
      <bpmn:incoming>Flow_HoDSubmitDecision_To_EndHoD</bpmn:incoming>
    </bpmn:endEvent>

    <!-- PO Approvers Lane Elements -->
    <bpmn:task id="Task_POApp_ReceivePO" name="Receive PO Approval Request">
      <bpmn:incoming>Flow_SendPOApproval_To_POAppReceivePO</bpmn:incoming>
      <bpmn:outgoing>Flow_POAppReceivePO_To_POAppReviewPO</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_POApp_ReviewPO" name="Review PO Specifications">
      <bpmn:incoming>Flow_POAppReceivePO_To_POAppReviewPO</bpmn:incoming>
      <bpmn:outgoing>Flow_POAppReviewPO_To_GatewayPOAppOutcome</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_POApp_Outcome" name="PO Review Outcome?">
      <bpmn:incoming>Flow_POAppReviewPO_To_GatewayPOAppOutcome</bpmn:incoming>
      <bpmn:outgoing>Flow_GatewayPOAppOutcome_Approve_To_DocApprove</bpmn:outgoing>
      <bpmn:outgoing>Flow_GatewayPOAppOutcome_Reject_To_DocReject</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_POApp_DocApprove" name="Document Approved PO Decision">
      <bpmn:incoming>Flow_GatewayPOAppOutcome_Approve_To_DocApprove</bpmn:incoming>
      <bpmn:outgoing>Flow_POAppDocApprove_To_SubmitDecision</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_POApp_DocReject" name="Document Rejected PO Decision">
