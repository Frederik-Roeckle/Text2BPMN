<?xml version="1.0" encoding="UTF-8"?>
<semantic:definitions xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="definitions_p2p" targetNamespace="http://example.com/bpmn/p2p">
  <semantic:collaboration id="collab_p2p">
    <semantic:participant id="pool_p2p_process" name="P2P Process" processRef="process_p2p"/>
  </semantic:collaboration>
  <semantic:process id="process_p2p" isExecutable="false">
    <semantic:laneSet id="laneSet_p2p">
      <semantic:lane id="lane_requestor" name="Requestor">
        <semantic:flowNodeRef>start_event_p2p</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_determine_needs</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_request_issue_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_pr_issuance_approved</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_issue_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_submit_pr_to_hod</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_receive_hod_pr_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_pr_approved_by_hod</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_create_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_submit_po_to_approvers</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_receive_po_approver_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_po_approved_by_po_approver</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_dispatch_po_to_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_receive_vendor_po_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_vendor_po_decision_req_side</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_search_new_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_receive_goods_services</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_goods_or_service</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_issue_gr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_issue_ses</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_gr_ses_issued_join</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_receive_invoice_from_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_perform_3way_match</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_invoice_correct</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_return_invoice_to_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_receive_corrected_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_post_invoice_sap</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_send_invoice_to_ap</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_hod" name="Heads of Departments">
        <semantic:flowNodeRef>task_hod_receive_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_hod_review_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_hod_submit_pr_decision</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_po_approvers" name="PO Approvers">
        <semantic:flowNodeRef>task_poapp_receive_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_poapp_review_po_specs</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_poapp_submit_po_decision</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_vendor" name="Vendor">
        <semantic:flowNodeRef>task_vendor_receive_po_for_review</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_vendor_evaluate_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_vendor_document_po_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_vendor_po_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_vendor_deliver_goods_services</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_vendor_send_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_event_vendor_po_rejected</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_event_vendor_invoice_sent</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_vendor_receive_wrong_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_vendor_correct_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_vendor_send_corrected_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_event_vendor_invoice_corrected</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_ap_team" name="Accounts Payable Team">
        <semantic:flowNodeRef>task_ap_receive_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_decide_payment_method</semantic:flowNodeRef>
        <semantic:flowNodeRef>gw_ap_payment_method</semantic:flowNodeRef>
        <semantic:flowNodeRef>task_ap_perform_payment</semantic:flowNodeRef>
        <semantic:flowNodeRef>end_event_p2p_completed</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <!-- Requestor Lane Elements -->
    <semantic:startEvent id="start_event_p2p" name="Start P2P Process">
      <semantic:outgoing>flow_start_to_determine_needs</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="task_determine_needs" name="Determine Business Needs">
      <semantic:incoming>flow_start_to_determine_needs</semantic:incoming>
      <semantic:incoming>flow_gw_pr_issue_no_to_needs</semantic:incoming>
      <semantic:incoming>flow_gw_hod_pr_no_to_needs</semantic:incoming>
      <semantic:incoming>flow_gw_po_app_no_to_needs</semantic:incoming>
      <semantic:outgoing>flow_needs_to_request_pr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_request_issue_pr" name="Request to Issue PR">
      <semantic:incoming>flow_needs_to_request_pr</semantic:incoming>
      <semantic:outgoing>flow_request_pr_to_gw_issue</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_pr_issuance_approved" name="PR Issuance Approved?">
      <semantic:incoming>flow_request_pr_to_gw_issue</semantic:incoming>
      <semantic:outgoing>flow_gw_pr_issue_yes_to_issue_pr</semantic:outgoing>
      <semantic:outgoing>flow_gw_pr_issue_no_to_needs</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_issue_pr" name="Issue Purchase Requisition (PR)">
      <semantic:incoming>flow_gw_pr_issue_yes_to_issue_pr</semantic:incoming>
      <semantic:outgoing>flow_issue_pr_to_submit_pr</semantic:outgoing>
    </semantic:task>
    <semantic:textAnnotation id="anno_pr_incomplete">
      <semantic:text>Risk: PR Incompletely Issued</semantic:text>
    </semantic:textAnnotation>
    <semantic:association id="assoc_pr_incomplete" sourceRef="task_issue_pr" targetRef="anno_pr_incomplete"/>
    <semantic:task id="task_submit_pr_to_hod" name="Submit PR to HoD">
      <semantic:incoming>flow_issue_pr_to_submit_pr</semantic:incoming>
      <semantic:outgoing>flow_submit_pr_to_hod_receive</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_receive_hod_pr_decision" name="Receive HoD PR Decision">
      <semantic:incoming>flow_hod_submit_decision_to_req_receive</semantic:incoming>
      <semantic:outgoing>flow_req_receive_hod_decision_to_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_pr_approved_by_hod" name="PR Approved by HoD?">
      <semantic:incoming>flow_req_receive_hod_decision_to_gw</semantic:incoming>
      <semantic:outgoing>flow_gw_hod_pr_yes_to_create_po</semantic:outgoing>
      <semantic:outgoing>flow_gw_hod_pr_no_to_needs</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_create_po" name="Create Purchase Order (PO)">
      <semantic:incoming>flow_gw_hod_pr_yes_to_create_po</semantic:incoming>
      <semantic:outgoing>flow_create_po_to_submit_po</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_submit_po_to_approvers" name="Submit PO to PO Approvers">
      <semantic:incoming>flow_create_po_to_submit_po</semantic:incoming>
      <semantic:outgoing>flow_submit_po_to_poapp_receive</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_receive_po_approver_decision" name="Receive PO Approver Decision">
      <semantic:incoming>flow_poapp_submit_decision_to_req_receive</semantic:incoming>
      <semantic:outgoing>flow_req_receive_poapp_decision_to_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_po_approved_by_po_approver" name="PO Approved by PO Approver?">
      <semantic:incoming>flow_req_receive_poapp_decision_to_gw</semantic:incoming>
      <semantic:outgoing>flow_gw_po_app_yes_to_dispatch</semantic:outgoing>
      <semantic:outgoing>flow_gw_po_app_no_to_needs</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_dispatch_po_to_vendor" name="Dispatch PO to Vendor">
      <semantic:incoming>flow_gw_po_app_yes_to_dispatch</semantic:incoming>
      <semantic:incoming>flow_search_vendor_to_dispatch</semantic:incoming>
      <semantic:outgoing>flow_dispatch_po_to_vendor_receive</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_receive_vendor_po_decision" name="Receive Vendor PO Decision">
      <semantic:incoming>flow_vendor_doc_decision_to_req_receive</semantic:incoming>
      <semantic:outgoing>flow_req_receive_vendor_decision_to_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_vendor_po_decision_req_side" name="Vendor Approved/Negotiated PO?">
      <semantic:incoming>flow_req_receive_vendor_decision_to_gw</semantic:incoming>
      <semantic:outgoing>flow_gw_vendor_dec_yes_to_receive_goods</semantic:outgoing>
      <semantic:outgoing>flow_gw_vendor_dec_no_to_search_vendor</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_search_new_vendor" name="Search for New Vendor">
      <semantic:incoming>flow_gw_vendor_dec_no_to_search_vendor</semantic:incoming>
      <semantic:outgoing>flow_search_vendor_to_dispatch</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_receive_goods_services" name="Receive Goods/Services">
      <semantic:incoming>flow_gw_vendor_dec_yes_to_receive_goods</semantic:incoming>
      <semantic:incoming>flow_vendor_delivers_to_req_receive_goods</semantic:incoming> <!-- From Vendor delivery -->
      <semantic:outgoing>flow_receive_goods_to_gw_type</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_goods_or_service" name="Goods or Service?">
      <semantic:incoming>flow_receive_goods_to_gw_type</semantic:incoming>
      <semantic:outgoing>flow_gw_type_goods_to_issue_gr</semantic:outgoing>
      <semantic:outgoing>flow_gw_type_service_to_issue_ses</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_issue_gr" name="Issue Goods Receipt (GR)">
      <semantic:incoming>flow_gw_type_goods_to_issue_gr</semantic:incoming>
      <semantic:outgoing>flow_issue_gr_to_join</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_issue_ses" name="Issue Service Entry Sheet (SES)">
      <semantic:incoming>flow_gw_type_service_to_issue_ses</semantic:incoming>
      <semantic:outgoing>flow_issue_ses_to_join</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_gr_ses_issued_join" name="GR/SES Issued">
      <semantic:incoming>flow_issue_gr_to_join</semantic:incoming>
      <semantic:incoming>flow_issue_ses_to_join</semantic:incoming>
      <semantic:outgoing>flow_join_to_receive_invoice</semantic:outgoing> <!-- This should lead to expecting invoice -->
    </semantic:exclusiveGateway>
    <semantic:task id="task_receive_invoice_from_vendor" name="Receive Invoice from Vendor">
      <semantic:incoming>flow_join_to_receive_invoice</semantic:incoming> <!-- After GR/SES -->
      <semantic:incoming>flow_vendor_sends_invoice_to_req_receive</semantic:incoming> <!-- From Vendor -->
      <semantic:outgoing>flow_receive_invoice_to_3way</semantic:outgoing>
    </semantic:task>
    <semantic:textAnnotation id="anno_invoice_discrepancy">
      <semantic:text>Risk: Invoice Discrepancy</semantic:text>
    </semantic:textAnnotation>
    <semantic:association id="assoc_invoice_discrepancy" sourceRef="task_receive_invoice_from_vendor" targetRef="anno_invoice_discrepancy"/>
    <semantic:task id="task_perform_3way_match" name="Perform Three-Way Match">
      <semantic:incoming>flow_receive_invoice_to_3way</semantic:incoming>
      <semantic:incoming>flow_req_receive_corrected_invoice_to_3way</semantic:incoming>
      <semantic:outgoing>flow_3way_to_gw_invoice_correct</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_invoice_correct" name="Invoice Correct?">
      <semantic:incoming>flow_3way_to_gw_invoice_correct</semantic:incoming>
      <semantic:outgoing>flow_gw_invoice_yes_to_post</semantic:outgoing>
      <semantic:outgoing>flow_gw_invoice_no_to_return</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_return_invoice_to_vendor" name="Return Invoice to Vendor">
      <semantic:incoming>flow_gw_invoice_no_to_return</semantic:incoming>
      <semantic:outgoing>flow_return_invoice_to_vendor_receive_wrong</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_receive_corrected_invoice" name="Receive Corrected Invoice">
      <semantic:incoming>flow_vendor_sends_corrected_to_req_receive</semantic:incoming>
      <semantic:outgoing>flow_req_receive_corrected_invoice_to_3way</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_post_invoice_sap" name="Post Invoice in SAP">
      <semantic:incoming>flow_gw_invoice_yes_to_post</semantic:incoming>
      <semantic:outgoing>flow_post_invoice_to_send_ap</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_send_invoice_to_ap" name="Send Invoice to AP Team">
      <semantic:incoming>flow_post_invoice_to_send_ap</semantic:incoming>
      <semantic:outgoing>flow_send_invoice_to_ap_receive</semantic:outgoing>
    </semantic:task>

    <!-- Heads of Departments Lane Elements -->
    <semantic:task id="task_hod_receive_pr" name="HoD Receives PR">
      <semantic:incoming>flow_submit_pr_to_hod_receive</semantic:incoming>
      <semantic:outgoing>flow_hod_receive_to_review</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_hod_review_pr" name="HoD Reviews PR">
      <semantic:incoming>flow_hod_receive_to_review</semantic:incoming>
      <semantic:outgoing>flow_hod_review_to_submit_decision</semantic:outgoing>
    </semantic:task>
    <semantic:textAnnotation id="anno_budget_overdraft">
      <semantic:text>Risk: Budget Overdraft</semantic:text>
    </semantic:textAnnotation>
    <semantic:association id="assoc_budget_overdraft" sourceRef="task_hod_review_pr" targetRef="anno_budget_overdraft"/>
    <semantic:textAnnotation id="anno_data_breach">
      <semantic:text>Risk: Data Breach</semantic:text>
    </semantic:textAnnotation>
    <semantic:association id="assoc_data_breach" sourceRef="task_hod_review_pr" targetRef="anno_data_breach"/>
    <semantic:task id="task_hod_submit_pr_decision" name="HoD Submits PR Decision">
      <semantic:incoming>flow_hod_review_to_submit_decision</semantic:incoming>
      <semantic:outgoing>flow_hod_submit_decision_to_req_receive</semantic:outgoing>
    </semantic:task>

    <!-- PO Approvers Lane Elements -->
    <semantic:task id="task_poapp_receive_po" name="PO Approver Receives PO">
      <semantic:incoming>flow_submit_po_to_poapp_receive</semantic:incoming>
      <semantic:outgoing>flow_poapp_receive_to_review</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_poapp_review_po_specs" name="PO Approver Reviews PO Specs">
      <semantic:incoming>flow_poapp_receive_to_review</semantic:incoming>
      <semantic:outgoing>flow_poapp_review_to_submit_decision</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_poapp_submit_po_decision" name="PO Approver Submits PO Decision">
      <semantic:incoming>flow_poapp_review_to_submit_decision</semantic:incoming>
      <semantic:outgoing>flow_poapp_submit_decision_to_req_receive</semantic:outgoing>
    </semantic:task>

    <!-- Vendor Lane Elements -->
    <semantic:task id="task_vendor_receive_po_for_review" name="Vendor Receives PO for Review">
      <semantic:incoming>flow_dispatch_po_to_vendor_receive</semantic:incoming>
      <semantic:outgoing>flow_vendor_receive_po_to_evaluate</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_vendor_evaluate_po" name="Vendor Evaluates PO">
      <semantic:incoming>flow_vendor_receive_po_to_evaluate</semantic:incoming>
      <semantic:outgoing>flow_vendor_evaluate_to_doc_decision</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_vendor_document_po_decision" name="Vendor Documents PO Decision">
      <semantic:incoming>flow_vendor_evaluate_to_doc_decision</semantic:incoming>
      <semantic:outgoing>flow_vendor_doc_decision_to_req_receive</semantic:outgoing> <!-- To Requestor -->
      <semantic:outgoing>flow_vendor_doc_decision_to_gw</semantic:outgoing> <!-- Internal Vendor Logic -->
    </semantic:task>
    <semantic:exclusiveGateway id="gw_vendor_po_decision" name="Vendor PO Decision?">
      <semantic:incoming>flow_vendor_doc_decision_to_gw</semantic:incoming>
      <semantic:outgoing>flow_gw_vendor_approve_to_deliver</semantic:outgoing>
      <semantic:outgoing>flow_gw_vendor_negotiate_to_deliver</semantic:outgoing>
      <semantic:outgoing>flow_gw_vendor_reject_to_end</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_vendor_deliver_goods_services" name="Vendor Delivers Goods/Services">
      <semantic:incoming>flow_gw_vendor_approve_to_deliver</semantic:incoming>
      <semantic:incoming>flow_gw_vendor_negotiate_to_deliver</semantic:incoming>
      <semantic:outgoing>flow_vendor_delivers_to_req_receive_goods</semantic:outgoing> <!-- To Requestor -->
      <semantic:outgoing>flow_vendor_deliver_to_send_invoice</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_vendor_send_invoice" name="Vendor Sends Invoice">
      <semantic:incoming>flow_vendor_deliver_to_send_invoice</semantic:incoming>
      <semantic:outgoing>flow_vendor_sends_invoice_to_req_receive</semantic:outgoing> <!-- To Requestor -->
      <semantic:outgoing>flow_vendor_send_invoice_to_end_invoice_sent</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="end_event_vendor_po_rejected" name="Vendor Rejects PO">
      <semantic:incoming>flow_gw_vendor_reject_to_end</semantic:incoming>
    </semantic:endEvent>
    <semantic:endEvent id="end_event_vendor_invoice_sent" name="Vendor Invoice Sent">
      <semantic:incoming>flow_vendor_send_invoice_to_end_invoice_sent</semantic:incoming>
    </semantic:endEvent>
    <semantic:task id="task_vendor_receive_wrong_invoice" name="Vendor Receives Wrong Invoice">
      <semantic:incoming>flow_return_invoice_to_vendor_receive_wrong</semantic:incoming>
      <semantic:outgoing>flow_vendor_receive_wrong_to_correct</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_vendor_correct_invoice" name="Vendor Corrects Invoice">
      <semantic:incoming>flow_vendor_receive_wrong_to_correct</semantic:incoming>
      <semantic:outgoing>flow_vendor_correct_to_send_corrected</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_vendor_send_corrected_invoice" name="Vendor Sends Corrected Invoice">
      <semantic:incoming>flow_vendor_correct_to_send_corrected</semantic:incoming>
      <semantic:outgoing>flow_vendor_sends_corrected_to_req_receive</semantic:outgoing> <!-- To Requestor -->
      <semantic:outgoing>flow_vendor_send_corrected_to_end_corrected</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="end_event_vendor_invoice_corrected" name="Vendor Invoice Corrected">
      <semantic:incoming>flow_vendor_send_corrected_to_end_corrected</semantic:incoming>
    </semantic:endEvent>

    <!-- Accounts Payable Team Lane Elements -->
    <semantic:task id="task_ap_receive_invoice" name="AP Team Receives Invoice">
      <semantic:incoming>flow_send_invoice_to_ap_receive</semantic:incoming>
      <semantic:outgoing>flow_ap_receive_to_decide_method</semantic:outgoing>
    </semantic:task>
    <semantic:task id="task_ap_decide_payment_method" name="AP Decides Payment Method">
      <semantic:incoming>flow_ap_receive_to_decide_method</semantic:incoming>
      <semantic:outgoing>flow_ap_decide_method_to_gw</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="gw_ap_payment_method" name="Payment Method?">
      <semantic:incoming>flow_ap_decide_method_to_gw</semantic:incoming>
      <semantic:outgoing>flow_gw_ap_full_to_perform</semantic:outgoing>
      <semantic:outgoing>flow_gw_ap_install_to_perform</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="task_ap_perform_payment" name="AP Performs Payment">
      <semantic:incoming>flow_gw_ap_full_to_perform</semantic:incoming>
      <semantic:incoming>flow_gw_ap_install_to_perform</semantic:incoming>
      <semantic:outgoing>flow_ap_perform_to_end</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="end_event_p2p_completed" name="P2P Process Completed">
      <semantic:incoming>flow_ap_perform_to_end</semantic:incoming>
    </semantic:endEvent>

    <!-- Sequence Flows -->
    <semantic:sequenceFlow id="flow_start_to_determine_needs" sourceRef="start_event_p2p" targetRef="task_determine_needs"/>
    <semantic:sequenceFlow id="flow_needs_to_request_pr" sourceRef="task_determine_needs" targetRef="task_request_issue_pr"/>
    <semantic:sequenceFlow id="flow_request_pr_to_gw_issue" sourceRef="task_request_issue_pr" targetRef="gw_pr_issuance_approved"/>
    <semantic:sequenceFlow id="flow_gw_pr_issue_yes_to_issue_pr" name="Yes" sourceRef="gw_pr_issuance_approved" targetRef="task_issue_pr"/>
