<?xml version="1.0" standalone="yes"?>
<semantic:definitions id="definitions_p2p"
    targetNamespace="http://example.com/bpmn/p2p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:semantic="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI">

  <semantic:message id="msg_requestor_po" name="Requestor PO"/>
  <semantic:message id="msg_vendor_po_decision" name="Vendor PO Decision"/>
  <semantic:message id="msg_vendor_invoice" name="Vendor Invoice"/>
  <semantic:message id="msg_invoice_return" name="Invoice Return"/>
  <semantic:message id="msg_corrected_vendor_invoice" name="Corrected Vendor Invoice"/>

  <semantic:collaboration id="collab_p2p">
    <semantic:participant id="pool_purchasing" name="Purchasing Department" processRef="process_purchasing"/>
    <semantic:participant id="pool_vendor" name="Vendor" processRef="process_vendor"/>

    <semantic:messageFlow id="mf_po_to_vendor" name="PO Dispatch" sourceRef="el_task_dispatch_po_to_vendor" targetRef="el_start_vendor_po_received"/>
    <semantic:messageFlow id="mf_vendor_decision_to_req" name="Vendor PO Decision" sourceRef="el_task_vendor_doc_decision_to_req" targetRef="el_event_po_decision_received"/>
    <semantic:messageFlow id="mf_invoice_to_req" name="Vendor Invoice" sourceRef="el_task_vendor_send_invoice" targetRef="el_task_receive_invoice"/>
    <semantic:messageFlow id="mf_return_invoice_to_vendor" name="Return Wrong Invoice" sourceRef="el_task_return_invoice_to_vendor" targetRef="el_event_invoice_issue_received"/>
    <semantic:messageFlow id="mf_corrected_invoice_to_req" name="Corrected Invoice" sourceRef="el_task_vendor_send_corrected_invoice" targetRef="el_event_corrected_invoice_received"/>
  </semantic:collaboration>

  <semantic:process id="process_purchasing" isExecutable="false">
    <semantic:laneSet id="laneSet_purchasing">
      <semantic:lane id="lane_requestor" name="Requestor">
        <semantic:flowNodeRef>el_start_requestor</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_det_needs</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_request_issue_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_gateway_internal_pr_approval</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_issue_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_send_pr_for_hod_approval</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_gateway_pr_hod_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_create_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_send_po_for_approval</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_gateway_po_approver_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_dispatch_po_to_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_event_po_decision_received</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_gateway_vendor_po_decision</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_search_new_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_receive_goods_services</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_gateway_goods_or_service</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_issue_gr</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_issue_ses</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_gateway_merge_gr_ses</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_receive_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_3_way_match</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_gateway_invoice_correct</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_return_invoice_to_vendor</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_event_corrected_invoice_received</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_receive_corrected_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_post_invoice_sap</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_send_invoice_to_ap</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_hod" name="Heads of Departments">
        <semantic:flowNodeRef>el_task_hod_receive_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_hod_review_pr</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_gateway_hod_pr_decision_type</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_hod_submit_decision</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_po_approvers" name="PO Approvers">
        <semantic:flowNodeRef>el_task_poa_receive_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_poa_review_po</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_gateway_poa_po_decision_type</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_poa_submit_decision</semantic:flowNodeRef>
      </semantic:lane>
      <semantic:lane id="lane_ap" name="Accounts Payable Team">
        <semantic:flowNodeRef>el_task_ap_receive_invoice</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_ap_decide_payment_method</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_task_ap_perform_payment</semantic:flowNodeRef>
        <semantic:flowNodeRef>el_end_process_final</semantic:flowNodeRef>
      </semantic:lane>
    </semantic:laneSet>

    <!-- Requestor Lane Elements -->
    <semantic:startEvent id="el_start_requestor" name="P2P Process Initiated">
      <semantic:outgoing>sf_start_to_det_needs</semantic:outgoing>
    </semantic:startEvent>
    <semantic:task id="el_task_det_needs" name="Determine Business Requirements &amp; Needs">
      <semantic:incoming>sf_start_to_det_needs</semantic:incoming>
      <semantic:incoming>sf_internal_pr_not_approved_to_det_needs</semantic:incoming>
      <semantic:incoming>sf_hod_pr_not_approved_to_det_needs</semantic:incoming>
      <semantic:incoming>sf_poa_po_rejected_to_det_needs</semantic:incoming>
      <semantic:outgoing>sf_det_needs_to_request_issue_pr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="el_task_request_issue_pr" name="Request to Issue PR (Draft PR)">
      <semantic:incoming>sf_det_needs_to_request_issue_pr</semantic:incoming>
      <semantic:outgoing>sf_request_issue_pr_to_gateway_internal</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="el_gateway_internal_pr_approval" name="PR Internally Approved by Requestor?">
      <semantic:incoming>sf_request_issue_pr_to_gateway_internal</semantic:incoming>
      <semantic:outgoing>sf_internal_pr_approved_to_issue_pr</semantic:outgoing>
      <semantic:outgoing>sf_internal_pr_not_approved_to_det_needs</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="el_task_issue_pr" name="Issue Purchase Requisition (PR)">
      <semantic:incoming>sf_internal_pr_approved_to_issue_pr</semantic:incoming>
      <semantic:outgoing>sf_issue_pr_to_send_pr_hod</semantic:outgoing>
    </semantic:task>
    <semantic:task id="el_task_send_pr_for_hod_approval" name="Send PR to HoD for Approval">
      <semantic:incoming>sf_issue_pr_to_send_pr_hod</semantic:incoming>
      <semantic:outgoing>sf_send_pr_hod_to_hod_receive_pr</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="el_gateway_pr_hod_decision" name="PR Approved by HoD?">
      <semantic:incoming>sf_hod_submit_to_gateway_pr_hod</semantic:incoming>
      <semantic:outgoing>sf_hod_pr_approved_to_create_po</semantic:outgoing>
      <semantic:outgoing>sf_hod_pr_not_approved_to_det_needs</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="el_task_create_po" name="Create Purchase Order (PO)">
      <semantic:incoming>sf_hod_pr_approved_to_create_po</semantic:incoming>
      <semantic:outgoing>sf_create_po_to_send_po_approval</semantic:outgoing>
    </semantic:task>
    <semantic:task id="el_task_send_po_for_approval" name="Send PO to PO Approvers for Approval">
      <semantic:incoming>sf_create_po_to_send_po_approval</semantic:incoming>
      <semantic:outgoing>sf_send_po_approval_to_poa_receive_po</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="el_gateway_po_approver_decision" name="PO Approved by PO Approvers?">
      <semantic:incoming>sf_poa_submit_to_gateway_poa</semantic:incoming>
      <semantic:outgoing>sf_poa_po_approved_to_dispatch_po</semantic:outgoing>
      <semantic:outgoing>sf_poa_po_rejected_to_det_needs</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="el_task_dispatch_po_to_vendor" name="Dispatch Approved PO to Vendor">
      <semantic:incoming>sf_poa_po_approved_to_dispatch_po</semantic:incoming>
      <semantic:incoming>sf_search_new_vendor_to_dispatch_po</semantic:incoming>
      <semantic:outgoing>sf_dispatch_po_to_wait_decision</semantic:outgoing>
    </semantic:task>
    <semantic:intermediateCatchEvent id="el_event_po_decision_received" name="PO Decision Received from Vendor">
        <semantic:incoming>sf_dispatch_po_to_wait_decision</semantic:incoming>
        <semantic:outgoing>sf_po_decision_to_gateway</semantic:outgoing>
        <semantic:messageEventDefinition messageRef="msg_vendor_po_decision"/>
    </semantic:intermediateCatchEvent>
    <semantic:exclusiveGateway id="el_gateway_vendor_po_decision" name="Vendor Decision on PO?">
      <semantic:incoming>sf_po_decision_to_gateway</semantic:incoming>
      <semantic:outgoing>sf_vendor_approved_po_to_receive_goods</semantic:outgoing>
      <semantic:outgoing>sf_vendor_rejected_po_to_search_vendor</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="el_task_search_new_vendor" name="Search for New Vendor">
      <semantic:incoming>sf_vendor_rejected_po_to_search_vendor</semantic:incoming>
      <semantic:outgoing>sf_search_new_vendor_to_dispatch_po</semantic:outgoing>
    </semantic:task>
    <semantic:task id="el_task_receive_goods_services" name="Receive Agreed Goods/Services">
      <semantic:incoming>sf_vendor_approved_po_to_receive_goods</semantic:incoming>
      <semantic:outgoing>sf_receive_goods_to_gateway_goods_service</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="el_gateway_goods_or_service" name="Goods or Service Received?">
      <semantic:incoming>sf_receive_goods_to_gateway_goods_service</semantic:incoming>
      <semantic:outgoing>sf_goods_received_to_issue_gr</semantic:outgoing>
      <semantic:outgoing>sf_service_received_to_issue_ses</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="el_task_issue_gr" name="Issue Goods Receipt (GR)">
      <semantic:incoming>sf_goods_received_to_issue_gr</semantic:incoming>
      <semantic:outgoing>sf_issue_gr_to_merge_gateway</semantic:outgoing>
    </semantic:task>
    <semantic:task id="el_task_issue_ses" name="Issue Service Entry Sheet (SES)">
      <semantic:incoming>sf_service_received_to_issue_ses</semantic:incoming>
      <semantic:outgoing>sf_issue_ses_to_merge_gateway</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="el_gateway_merge_gr_ses" name="Merge Receipt Confirmation">
      <semantic:incoming>sf_issue_gr_to_merge_gateway</semantic:incoming>
      <semantic:incoming>sf_issue_ses_to_merge_gateway</semantic:incoming>
      <semantic:outgoing>sf_merge_gateway_to_receive_invoice</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:receiveTask id="el_task_receive_invoice" name="Receive Invoice from Vendor" messageRef="msg_vendor_invoice">
      <semantic:incoming>sf_merge_gateway_to_receive_invoice</semantic:incoming>
      <semantic:outgoing>sf_receive_invoice_to_3_way_match</semantic:outgoing>
    </semantic:receiveTask>
    <semantic:task id="el_task_3_way_match" name="Perform Three-Way Match">
      <semantic:incoming>sf_receive_invoice_to_3_way_match</semantic:incoming>
      <semantic:incoming>sf_receive_corrected_invoice_to_3_way_match</semantic:incoming>
      <semantic:outgoing>sf_3_way_match_to_gateway_invoice_correct</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="el_gateway_invoice_correct" name="Invoice Correct?">
      <semantic:incoming>sf_3_way_match_to_gateway_invoice_correct</semantic:incoming>
      <semantic:outgoing>sf_invoice_correct_to_post_invoice</semantic:outgoing>
      <semantic:outgoing>sf_invoice_wrong_to_return_invoice</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="el_task_return_invoice_to_vendor" name="Return Invoice to Vendor for Correction">
      <semantic:incoming>sf_invoice_wrong_to_return_invoice</semantic:incoming>
      <semantic:outgoing>sf_return_invoice_to_wait_correction</semantic:outgoing>
    </semantic:task>
    <semantic:intermediateCatchEvent id="el_event_corrected_invoice_received" name="Corrected Invoice Received from Vendor">
        <semantic:incoming>sf_return_invoice_to_wait_correction</semantic:incoming>
        <semantic:outgoing>sf_corrected_invoice_to_task</semantic:outgoing>
        <semantic:messageEventDefinition messageRef="msg_corrected_vendor_invoice"/>
    </semantic:intermediateCatchEvent>
    <semantic:task id="el_task_receive_corrected_invoice" name="Receive Corrected Invoice from Vendor">
      <semantic:incoming>sf_corrected_invoice_to_task</semantic:incoming>
      <semantic:outgoing>sf_receive_corrected_invoice_to_3_way_match</semantic:outgoing>
    </semantic:task>
    <semantic:task id="el_task_post_invoice_sap" name="Post Invoice in SAP">
      <semantic:incoming>sf_invoice_correct_to_post_invoice</semantic:incoming>
      <semantic:outgoing>sf_post_invoice_to_send_invoice_ap</semantic:outgoing>
    </semantic:task>
    <semantic:task id="el_task_send_invoice_to_ap" name="Send Invoice to AP Team">
      <semantic:incoming>sf_post_invoice_to_send_invoice_ap</semantic:incoming>
      <semantic:outgoing>sf_send_invoice_ap_to_ap_receive_invoice</semantic:outgoing>
    </semantic:task>

    <!-- HoD Lane Elements -->
    <semantic:task id="el_task_hod_receive_pr" name="Receive PR for Approval">
      <semantic:incoming>sf_send_pr_hod_to_hod_receive_pr</semantic:incoming>
      <semantic:outgoing>sf_hod_receive_pr_to_hod_review_pr</semantic:outgoing>
    </semantic:task>
    <semantic:task id="el_task_hod_review_pr" name="Review PR (Needs, Budget, Form)">
      <semantic:incoming>sf_hod_receive_pr_to_hod_review_pr</semantic:incoming>
      <semantic:outgoing>sf_hod_review_pr_to_gateway_hod_decision_type</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="el_gateway_hod_pr_decision_type" name="HoD PR Decision">
      <semantic:incoming>sf_hod_review_pr_to_gateway_hod_decision_type</semantic:incoming>
      <semantic:outgoing>sf_hod_approved_pr_to_hod_submit</semantic:outgoing>
      <semantic:outgoing>sf_hod_declined_pr_to_hod_submit</semantic:outgoing>
      <semantic:outgoing>sf_hod_rejected_incomplete_pr_to_hod_submit</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="el_task_hod_submit_decision" name="Document/Submit PR Decision to Requestor">
      <semantic:incoming>sf_hod_approved_pr_to_hod_submit</semantic:incoming>
      <semantic:incoming>sf_hod_declined_pr_to_hod_submit</semantic:incoming>
      <semantic:incoming>sf_hod_rejected_incomplete_pr_to_hod_submit</semantic:incoming>
      <semantic:outgoing>sf_hod_submit_to_gateway_pr_hod</semantic:outgoing>
    </semantic:task>

    <!-- PO Approvers Lane Elements -->
    <semantic:task id="el_task_poa_receive_po" name="Receive PO for Approval">
      <semantic:incoming>sf_send_po_approval_to_poa_receive_po</semantic:incoming>
      <semantic:outgoing>sf_poa_receive_po_to_poa_review_po</semantic:outgoing>
    </semantic:task>
    <semantic:task id="el_task_poa_review_po" name="Review PO Specifications">
      <semantic:incoming>sf_poa_receive_po_to_poa_review_po</semantic:incoming>
      <semantic:outgoing>sf_poa_review_po_to_gateway_poa_decision_type</semantic:outgoing>
    </semantic:task>
    <semantic:exclusiveGateway id="el_gateway_poa_po_decision_type" name="PO Approver Decision">
      <semantic:incoming>sf_poa_review_po_to_gateway_poa_decision_type</semantic:incoming>
      <semantic:outgoing>sf_poa_approved_po_to_poa_submit</semantic:outgoing>
      <semantic:outgoing>sf_poa_rejected_po_to_poa_submit</semantic:outgoing>
    </semantic:exclusiveGateway>
    <semantic:task id="el_task_poa_submit_decision" name="Document/Submit PO Decision to Requestor">
      <semantic:incoming>sf_poa_approved_po_to_poa_submit</semantic:incoming>
      <semantic:incoming>sf_poa_rejected_po_to_poa_submit</semantic:incoming>
      <semantic:outgoing>sf_poa_submit_to_gateway_poa</semantic:outgoing>
    </semantic:task>

    <!-- AP Lane Elements -->
    <semantic:task id="el_task_ap_receive_invoice" name="Receive Invoice from Requestor">
      <semantic:incoming>sf_send_invoice_ap_to_ap_receive_invoice</semantic:incoming>
      <semantic:outgoing>sf_ap_receive_invoice_to_ap_decide_payment</semantic:outgoing>
    </semantic:task>
    <semantic:task id="el_task_ap_decide_payment_method" name="Decide on Payment Method">
      <semantic:incoming>sf_ap_receive_invoice_to_ap_decide_payment</semantic:incoming>
      <semantic:outgoing>sf_ap_decide_payment_to_ap_perform_payment</semantic:outgoing>
    </semantic:task>
    <semantic:task id="el_task_ap_perform_payment" name="Perform Payment to Vendor">
      <semantic:incoming>sf_ap_decide_payment_to_ap_perform_payment</semantic:incoming>
      <semantic:outgoing>sf_ap_perform_payment_to_end_process</semantic:outgoing>
    </semantic:task>
    <semantic:endEvent id="el_end_process_final" name="P2P Process Successfully Completed">
      <semantic:incoming>sf_ap_perform_payment_to_end_process</semantic:incoming>
    </semantic:endEvent>

    <!-- Sequence Flows for Purchasing Department -->
    <semantic:sequenceFlow id="sf_start_to_det_needs" sourceRef="el_start_requestor" targetRef="el_task_det_needs"/>
    <semantic:sequenceFlow id="sf_det_needs_to_request_issue_pr" sourceRef="el_task_det_needs" targetRef="el_task_request_issue_pr"/>
    <semantic:sequenceFlow id="sf_request_issue_pr_to_gateway_internal" sourceRef="el_task_request_issue_pr" targetRef="el_gateway_internal_pr_approval"/>
    <semantic:sequenceFlow id="sf_internal_pr_approved_to_issue_pr" name="Internally Approved" sourceRef="el_gateway_internal_pr_approval" targetRef="el_task_issue_pr">
      <semantic:conditionExpression xsi:type="semantic:tFormalExpression">Internally Approved</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="sf_internal_pr_not_approved_to_det_needs" name="Internally Not Approved" sourceRef="el_gateway_internal_pr_approval" targetRef="el_task_det_needs">
      <semantic:conditionExpression xsi:type="semantic:tFormalExpression">Internally Not Approved</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="sf_issue_pr_to_send_pr_hod" sourceRef="el_task_issue_pr" targetRef="el_task_send_pr_for_hod_approval"/>
    <semantic:sequenceFlow id="sf_send_pr_hod_to_hod_receive_pr" sourceRef="el_task_send_pr_for_hod_approval" targetRef="el_task_hod_receive_pr"/>
    <semantic:sequenceFlow id="sf_hod_receive_pr_to_hod_review_pr" sourceRef="el_task_hod_receive_pr" targetRef="el_task_hod_review_pr"/>
    <semantic:sequenceFlow id="sf_hod_review_pr_to_gateway_hod_decision_type" sourceRef="el_task_hod_review_pr" targetRef="el_gateway_hod_pr_decision_type"/>
    <semantic:sequenceFlow id="sf_hod_approved_pr_to_hod_submit" name="PR Approved by HoD" sourceRef="el_gateway_hod_pr_decision_type" targetRef="el_task_hod_submit_decision">
      <semantic:conditionExpression xsi:type="semantic:tFormalExpression">PR Approved by HoD</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="sf_hod_declined_pr_to_hod_submit" name="PR Declined by HoD" sourceRef="el_gateway_hod_pr_decision_type" targetRef="el_task_hod_submit_decision">
      <semantic:conditionExpression xsi:type="semantic:tFormalExpression">PR Declined by HoD</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="sf_hod_rejected_incomplete_pr_to_hod_submit" name="PR Rejected Incomplete by HoD" sourceRef="el_gateway_hod_pr_decision_type" targetRef="el_task_hod_submit_decision">
      <semantic:conditionExpression xsi:type="semantic:tFormalExpression">PR Rejected Incomplete by HoD</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="sf_hod_submit_to_gateway_pr_hod" sourceRef="el_task_hod_submit_decision" targetRef="el_gateway_pr_hod_decision"/>
    <semantic:sequenceFlow id="sf_hod_pr_approved_to_create_po" name="PR Approved" sourceRef="el_gateway_pr_hod_decision" targetRef="el_task_create_po">
      <semantic:conditionExpression xsi:type="semantic:tFormalExpression">PR Approved</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="sf_hod_pr_not_approved_to_det_needs" name="PR Not Approved (Declined/Rejected)" sourceRef="el_gateway_pr_hod_decision" targetRef="el_task_det_needs">
      <semantic:conditionExpression xsi:type="semantic:tFormalExpression">PR Not Approved (Declined/Rejected)</semantic:conditionExpression>
    </semantic:sequenceFlow>
    <semantic:sequenceFlow id="sf_create_po_to_send_po_approval" sourceRef="el_task_create_po" targetRef="el_task_send_po_for_approval"/>
    <semantic:sequenceFlow id="sf_send_po_approval_to_poa_receive_po" sourceRef="el_task_send_po_for_approval" targetRef="el_task_poa_receive_po"/>
    <semantic:sequenceFlow id="sf_poa_receive_po_to_poa_review_po" sourceRef="el_task_poa_receive_po" targetRef="el_task_poa_review_po"/>
    <semantic:sequenceFlow id="sf_poa_review_po_to_gateway_poa_decision_type" sourceRef="el_task_poa_review_po" targetRef="el_gateway_poa_po_decision_type"/>
    <semantic:sequenceFlow id="sf_poa_approved_po_to_poa_submit" name="PO Approved by PO Approver" sourceRef="el_gateway_poa_po_decision_type" targetRef="el_task_poa_submit_decision">
