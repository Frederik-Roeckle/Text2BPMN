<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" 
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
             targetNamespace="http://www.example.org/bpmn" 
             id="sampleDiagram" 
             exporter="Example Exporter" 
             exporterVersion="1.0">

  <collaboration id="Collaboration">
    <participant id="User" name="User" processRef="UserProcess" />
    <participant id="Employee" name="Employee" processRef="EmployeeProcess" />
    <participant id="Manager" name="Manager" processRef="ManagerProcess" />
  </collaboration>

  <process id="UserProcess" name="User Process">
    <startEvent id="StartEvent" name="Request Submitted" />
    <task id="SubmitRequest" name="Submit Request" />
    <sequenceFlow id="Flow1" sourceRef="StartEvent" targetRef="SubmitRequest" />
    <endEvent id="EndEvent" name="Request Processed" />
    <sequenceFlow id="Flow2" sourceRef="SubmitRequest" targetRef="EndEvent" />
  </process>

  <process id="EmployeeProcess" name="Employee Process">
    <task id="ReviewRequest" name="Review Request" />
    <exclusiveGateway id="ApprovalDecision" name="Approved?" />
    <task id="ForwardRequest" name="Forward Request for Approval" />
    <task id="RejectRequest" name="Reject Request" />
  
    <sequenceFlow id="Flow3" sourceRef="SubmitRequest" targetRef="ReviewRequest" />
    <sequenceFlow id="Flow4" sourceRef="ReviewRequest" targetRef="ApprovalDecision" />
    <sequenceFlow id="Flow5" sourceRef="ApprovalDecision" targetRef="RejectRequest" />
    <sequenceFlow id="Flow6" sourceRef="ApprovalDecision" targetRef="ForwardRequest" />
  </process>

  <process id="ManagerProcess" name="Manager Process">
    <startEvent id="ManagerStartEvent" name="Request Received" />
    <endEvent id="ManagerEndEvent" name="Final Approval" />
    <sequenceFlow id="Flow7" sourceRef="ManagerStartEvent" targetRef="ManagerEndEvent" />
  </process>

  <bpmndi:BPMNDiagram id="BPMNDiagram">
    <bpmndi:BPMNPlane bpmnElement="Collaboration">
      <bpmndi:BPMNShape id="UserShape" bpmnElement="User">
        <dc:Bounds x="50" y="50" width="100" height="350" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EmployeeShape" bpmnElement="Employee">
        <dc:Bounds x="200" y="50" width="100" height="350" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="ManagerShape" bpmnElement="Manager">
        <dc:Bounds x="350" y="50" width="100" height="350" />
      </bpmndi:BPMNShape>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</definitions>
